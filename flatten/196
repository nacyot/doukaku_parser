10</pre></td><td class="code"><div class="highlight"><pre><span class="k">private</span> <span class="k">import</span> <span class="n">std</span>.<span class="n">string</span>, <span class="n">std</span>.<span class="n">regexp</span>;

<span class="n">string</span> <span class="n">totitle</span>(<span class="k">in</span> <span class="n">string</span> <span class="n">str</span>) {
    <span class="k">return</span> <span class="n">str</span>.<span class="n">sub</span>(<span class="s">r&quot;\b[a-z]&quot;</span>, (<span class="n">RegExp</span> <span class="n">m</span>){<span class="k">return</span> <span class="n">m</span>[<span class="mi">0</span>].<span class="n">toupper</span>();}, <span class="s">&quot;g&quot;</span>);
}

<span class="k">unittest</span> {
    <span class="k">assert</span>(<span class="s">&quot;LL future&quot;</span>.<span class="n">totitle</span>() == <span class="s">&quot;LL Future&quot;</span>);
    <span class="k">assert</span>(<span class="s">&quot;LL day and night&quot;</span>.<span class="n">totitle</span>() == <span class="s">&quot;LL Day And Night&quot;</span>);
}
</pre></div>
</td></tr></table>
