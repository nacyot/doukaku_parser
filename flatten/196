      <table><tr><td class="linenos"><pre>1</pre></td><td class="code"><div class="highlight"><pre><span class="k">using</span> <span class="nn">System</span><span class="p">;</span><span class="k">using</span> <span class="nn">System.Linq</span><span class="p">;</span><span class="k">class</span> <span class="nc">d</span><span class="k">{static</span> <span class="k">void</span> <span class="n">Main</span><span class="p">(</span><span class="kt">string</span><span class="p">[]</span><span class="n">a</span><span class="p">)</span><span class="k">{</span><span class="kt">int</span> <span class="n">c</span><span class="p">=</span><span class="m">0</span><span class="p">;</span><span class="n">a</span><span class="p">[</span><span class="m">0</span><span class="p">].</span><span class="n">ToCharArray</span><span class="p">().</span><span class="n">ToList</span><span class="p">&lt;</span><span class="kt">char</span><span class="p">&gt;().</span><span class="n">ForEach</span><span class="p">(</span><span class="n">b</span><span class="p">=&gt;</span><span class="k">{if</span><span class="p">(</span><span class="n">c</span><span class="p">!=</span><span class="m">0</span><span class="p">&amp;&amp;</span><span class="n">a</span><span class="p">[</span><span class="m">0</span><span class="p">][</span><span class="n">c</span><span class="p">-</span><span class="m">1</span><span class="p">]!=</span><span class="sc">&#39; &#39;</span><span class="p">)</span><span class="n">Console</span><span class="p">.</span><span class="n">Write</span><span class="p">(</span><span class="n">b</span><span class="p">);</span><span class="k">else</span> <span class="n">Console</span><span class="p">.</span><span class="n">Write</span><span class="p">(</span><span class="n">b</span><span class="p">.</span><span class="n">ToString</span><span class="p">().</span><span class="n">ToUpper</span><span class="p">());</span><span class="n">c</span><span class="p">++;</span><span class="k">}</span><span class="p">);</span><span class="k">}}</span>
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
