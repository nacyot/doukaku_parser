<span class="nv">a</span><span class="o">=</span><span class="nv">unwords</span><span class="o">$</span><span class="nv">map</span><span class="p">(</span><span class="o">\</span><span class="p">(</span><span class="nv">x</span><span class="o">:</span><span class="nv">xs</span><span class="p">)</span><span class="o">-&gt;</span><span class="nv">toUpper</span> <span class="nv">x</span><span class="o">:</span><span class="nv">xs</span><span class="p">)</span><span class="o">$</span><span class="nv">words</span><span class="s">&quot;LL day and night&quot;</span>


