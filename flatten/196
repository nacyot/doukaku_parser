      <span class="k">if</span> <span class="o">[</span> <span class="nv">$c</span> <span class="o">=</span> <span class="k">${</span><span class="nv">l</span><span class="p">[$i]</span><span class="k">}</span> <span class="o">]</span>;then <span class="nv">c</span><span class="o">=</span><span class="k">${</span><span class="nv">u</span><span class="p">[$i]</span><span class="k">}</span>;break;fi
    <span class="k">done</span>
<span class="k">    </span><span class="nb">echo</span> -n <span class="s2">&quot;$c${w:1} &quot;</span>
  <span class="k">done</span>
<span class="k">  </span><span class="nb">echo</span>
<span class="k">done</span>
