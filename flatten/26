  <span class="p">{</span>
  <span class="nb">local</span> <span class="p">(</span><span class="nv">$totalSeatCount</span><span class="p">)</span> <span class="o">=</span> <span class="nb">shift</span> <span class="p">(</span><span class="nv">@_</span><span class="p">)</span> <span class="p">;</span>
  <span class="nb">local</span> <span class="p">(</span><span class="nv">@voteCounts</span><span class="p">)</span> <span class="o">=</span> <span class="nv">@_</span> <span class="p">;</span>
  <span class="nb">local</span> <span class="p">(</span><span class="nv">$seatCounts</span><span class="p">)</span> <span class="o">=</span> <span class="o">[]</span> <span class="p">;</span>

  <span class="nb">local</span> <span class="p">(</span><span class="nv">$voteCount</span><span class="p">)</span> <span class="p">;</span>
  <span class="k">foreach</span> <span class="nv">$voteCount</span> <span class="p">(</span><span class="nv">@voteCounts</span><span class="p">)</span>
    <span class="p">{</span><span class="nb">push</span> <span class="p">(</span><span class="nv">@</span><span class="p">{</span><span class="nv">$seatCounts</span><span class="p">},</span> <span class="mi">0</span><span class="p">)</span> <span class="p">;}</span>

  <span class="c1"># 「定数」回だけ繰り返す</span>
  <span class="k">while</span> <span class="p">(</span><span class="mi">0</span><span class="o">&lt;</span> <span class="nv">$totalSeatCount</span><span class="p">)</span>
    <span class="p">{</span>
    <span class="nb">local</span> <span class="p">(</span><span class="nv">$max</span><span class="p">,</span> <span class="nv">$maxIndex</span><span class="p">)</span> <span class="o">=</span> <span class="p">(</span><span class="mi">0</span><span class="p">,</span> <span class="nb">undef</span><span class="p">)</span> <span class="p">;</span>
    <span class="c1"># 最大の党を探す</span>
    <span class="nb">local</span> <span class="p">(</span><span class="nv">$i</span><span class="p">)</span> <span class="p">;</span>
    <span class="k">for</span> <span class="p">(</span><span class="nv">$i</span><span class="o">=</span><span class="mi">0</span> <span class="p">;</span> <span class="nv">$i</span><span class="o">&lt;</span><span class="nv">@voteCounts</span><span class="p">;</span> <span class="o">++</span><span class="nv">$i</span><span class="p">)</span>
      <span class="p">{</span>
      <span class="k">if</span> <span class="p">(</span><span class="nv">$max</span> <span class="o">&lt;</span> <span class="nv">$voteCounts</span><span class="p">[</span><span class="nv">$i</span><span class="p">]</span><span class="o">/</span><span class="p">(</span><span class="nv">$seatCounts</span><span class="o">-&gt;</span><span class="p">[</span><span class="nv">$i</span><span class="p">]</span> <span class="o">+</span> <span class="mi">1</span><span class="o">.</span><span class="mi">0</span><span class="p">))</span>
        <span class="p">{</span>
        <span class="nv">$max</span> <span class="o">=</span> <span class="nv">$voteCounts</span><span class="p">[</span><span class="nv">$i</span><span class="p">]</span><span class="o">/</span><span class="p">(</span><span class="nv">$seatCounts</span><span class="o">-&gt;</span><span class="p">[</span><span class="nv">$i</span><span class="p">]</span> <span class="o">+</span> <span class="mi">1</span><span class="o">.</span><span class="mi">0</span><span class="p">)</span> <span class="p">;</span>
        <span class="nv">$maxIndex</span> <span class="o">=</span> <span class="nv">$i</span> <span class="p">;</span>
        <span class="p">}</span>
      <span class="p">}</span>

    <span class="c1"># 最大の党の議席を1増やす</span>
    <span class="o">++</span><span class="nv">$seatCounts</span><span class="o">-&gt;</span><span class="p">[</span><span class="nv">$maxIndex</span><span class="p">]</span> <span class="p">;</span>
    <span class="p">}</span>
  <span class="k">continue</span>
    <span class="p">{</span><span class="o">--</span><span class="nv">$totalSeatCount</span> <span class="p">;}</span>

  <span class="k">return</span> <span class="nv">$seatCounts</span> <span class="p">;</span>
  <span class="p">}</span>

<span class="c1"># お試しコード</span>

<span class="nb">local</span> <span class="p">(</span><span class="nv">@seats</span><span class="p">)</span> <span class="o">=</span> <span class="nv">@</span><span class="p">{</span><span class="o">&amp;</span><span class="n">dHondt</span> <span class="p">(</span><span class="mi">100</span><span class="p">,</span>  <span class="mi">123</span><span class="p">,</span><span class="mi">4</span><span class="p">,</span><span class="mi">56</span><span class="p">,</span><span class="mi">78</span><span class="p">)}</span> <span class="p">;</span>
<span class="nb">local</span> <span class="p">(</span><span class="nv">$i</span><span class="p">)</span> <span class="p">;</span>
<span class="k">for</span> <span class="p">(</span><span class="nv">$i</span><span class="o">=</span><span class="mi">0</span><span class="p">;</span> <span class="nv">$i</span><span class="o">&lt;</span><span class="nv">@seats</span><span class="p">;</span> <span class="o">++</span><span class="nv">$i</span><span class="p">)</span>
  <span class="p">{</span><span class="k">print</span> <span class="s">&quot;$seats[$i], &quot;</span> <span class="p">;}</span>
<span class="k">print</span> <span class="s">&quot;\n&quot;</span> <span class="p">;</span>
