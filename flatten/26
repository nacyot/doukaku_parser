    <span class="kt">int</span> <span class="err">定数</span> <span class="o">=</span> <span class="mi">100</span> <span class="o">;</span>
    <span class="kt">int</span> <span class="err">党数</span> <span class="o">=</span> <span class="mi">4</span> <span class="o">;</span>

