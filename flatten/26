hyosu &lt;- c(a=123, b=4, c=56, d=78);
n &lt;- sort(rep(hyosu, length=teisu*length(hyosu))/rep(1:teisu, length=teisu*length(hyosu), 
          each=length(hyosu)), dec=TRUE)[1:teisu]
for(i in names(hyosu)) cat(paste(i, &quot;:&quot;, length(names(n)[names(n)==i])), sep=&quot;\n&quot;)
