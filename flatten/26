35</pre></td><td class="code"><div class="highlight"><pre><span class="k">function </span>dont<span class="o">()</span> <span class="o">{</span>
    <span class="nb">cd</span> /tmp
    <span class="nv">n</span><span class="o">=</span><span class="nv">$1</span>
    <span class="nb">shift</span>
<span class="nb">    </span><span class="nv">i</span><span class="o">=</span>0
    <span class="k">while</span> <span class="o">[</span> <span class="s2">&quot;$1&quot;</span> !<span class="o">=</span> <span class="s2">&quot;&quot;</span> <span class="o">]</span>;do
        <span class="nb">echo</span> <span class="nv">$1</span> &gt; d.v.<span class="nv">$i</span>
        <span class="nb">echo </span><span class="m">0</span>  &gt; d.ret.<span class="nv">$i</span>
        <span class="nb">shift</span>
<span class="nb">        </span><span class="nv">i</span><span class="o">=</span><span class="s">`expr $i + 1`</span>
    <span class="k">done</span>
<span class="k">    </span><span class="nv">len</span><span class="o">=</span><span class="nv">$i</span>

    <span class="nv">i</span><span class="o">=</span>0
    <span class="k">while</span> <span class="o">[</span> -f d.v.<span class="nv">$i</span> <span class="o">]</span>; <span class="k">do</span>
<span class="k">        </span><span class="nv">vv</span><span class="o">=</span><span class="s">`cat d.v.$i`</span>
        <span class="k">for</span> <span class="o">((</span><span class="nv">j</span><span class="o">=</span>1; j&lt;<span class="o">=</span>n; j++<span class="o">))</span>; <span class="k">do</span>
<span class="k">            </span><span class="nb">echo</span> <span class="s">`echo $vv / $j | bc -l`</span> <span class="nv">$i</span> &gt; d.d.<span class="nv">$i</span>.<span class="nv">$j</span>
        <span class="k">done</span>
<span class="k">        </span><span class="nv">i</span><span class="o">=</span><span class="s">`expr $i + 1`</span>
    <span class="k">done</span>

<span class="k">    </span>find -name <span class="s1">&#39;d.d.*&#39;</span> | xargs cat | sort -n | tail -<span class="nv">$n</span> &gt; d.sum
    <span class="k">for</span> <span class="o">((</span><span class="nv">i</span><span class="o">=</span>0; i&lt;len; i++<span class="o">))</span>; <span class="k">do</span>
<span class="k">        </span>egrep <span class="s2">&quot; $i&quot;</span><span class="nv">$ </span>d.sum | wc -l &gt; d.ret.<span class="nv">$i</span>
        cat d.ret.<span class="nv">$i</span>
    <span class="k">done</span>

<span class="k">    </span>find -name <span class="s1">&#39;d.ret.*&#39;</span> -o -name <span class="s1">&#39;d.v.*&#39;</span> -o -name <span class="s1">&#39;d.d.*&#39;</span> -o -name <span class="s1">&#39;d.sum&#39;</span>| xargs rm
<span class="o">}</span>
dont <span class="m">100</span>   <span class="m">123</span> <span class="m">4</span> <span class="m">56</span> 78
<span class="c"># 48</span>
<span class="c"># 1</span>
<span class="c"># 21</span>
<span class="c"># 30</span>
</pre></div>
</td></tr></table>
