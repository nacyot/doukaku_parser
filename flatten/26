21
22</pre></td><td class="code"><div class="highlight"><pre><span class="kr">import</span> <span class="n">Data</span><span class="o">.</span><span class="n">List</span>
<span class="kr">import</span> <span class="n">Control</span><span class="o">.</span><span class="n">Monad</span><span class="o">.</span><span class="n">State</span>
<span class="kr">import</span> <span class="n">Control</span><span class="o">.</span><span class="n">Monad</span><span class="o">.</span><span class="n">Writer</span>

<span class="kr">data</span> <span class="n">Vote</span> <span class="o">=</span> <span class="n">V</span> <span class="p">{</span><span class="nv">idx</span><span class="o">::</span><span class="n">Int</span><span class="p">,</span> <span class="nv">num</span><span class="o">::</span><span class="n">Double</span><span class="p">,</span> <span class="nv">poll</span><span class="o">::</span><span class="n">Double</span><span class="p">}</span> <span class="kr">deriving</span> <span class="n">Show</span>
<span class="kr">type</span> <span class="n">Votes</span> <span class="o">=</span> <span class="p">[</span><span class="n">Vote</span><span class="p">]</span>
<span class="kr">type</span> <span class="n">Proc</span> <span class="nv">a</span> <span class="o">=</span> <span class="n">StateT</span> <span class="n">Votes</span> <span class="p">(</span><span class="n">Writer</span> <span class="p">[</span><span class="n">Int</span><span class="p">])</span> <span class="nv">a</span>

<span class="nv">runProc</span> <span class="o">::</span> <span class="n">Proc</span> <span class="p">()</span> <span class="o">-&gt;</span> <span class="n">Votes</span> <span class="o">-&gt;</span> <span class="p">[</span><span class="n">Int</span><span class="p">]</span>
<span class="nv">runProc</span> <span class="nv">proc</span> <span class="nv">vs</span> <span class="o">=</span> <span class="nv">fmt</span> <span class="o">$</span> <span class="nv">group</span> <span class="o">$</span> <span class="nv">sort</span> <span class="o">$</span> <span class="nv">snd</span> <span class="o">$</span> <span class="nv">runWriter</span> <span class="o">$</span> <span class="nv">evalStateT</span> <span class="nv">proc</span> <span class="nv">vs</span>
  <span class="kr">where</span> <span class="nv">fmt</span>  <span class="o">=</span> <span class="nv">map</span> <span class="nv">length</span> <span class="o">.</span> <span class="nv">fmt&#39;</span> <span class="mi">1</span> <span class="o">.</span> <span class="nv">map</span> <span class="p">(</span><span class="o">\</span><span class="nv">x</span> <span class="o">-&gt;</span> <span class="p">(</span><span class="nv">head</span> <span class="nv">x</span><span class="p">,</span> <span class="nv">x</span><span class="p">))</span>
        <span class="nv">fmt&#39;</span> <span class="kr">_</span> <span class="p">[]</span> <span class="o">=</span> <span class="p">[]</span>
        <span class="nv">fmt&#39;</span> <span class="nv">n</span> <span class="p">((</span><span class="nv">x</span><span class="p">,</span><span class="nv">y</span><span class="p">)</span><span class="o">:</span><span class="nv">xs</span><span class="p">)</span> <span class="o">=</span> <span class="kr">if</span> <span class="nv">n</span> <span class="o">==</span> <span class="nv">x</span> <span class="kr">then</span> <span class="nv">y</span><span class="o">:</span><span class="nv">fmt&#39;</span> <span class="p">(</span><span class="nv">n</span> <span class="o">+</span> <span class="mi">1</span><span class="p">)</span> <span class="nv">xs</span> <span class="kr">else</span> <span class="p">[]</span><span class="o">:</span><span class="nv">fmt&#39;</span> <span class="p">(</span><span class="nv">n</span> <span class="o">+</span> <span class="mi">1</span><span class="p">)</span> <span class="p">((</span><span class="nv">x</span><span class="p">,</span><span class="nv">y</span><span class="p">)</span><span class="o">:</span><span class="nv">xs</span><span class="p">)</span>

<span class="nv">mkProc</span> <span class="o">::</span> <span class="n">Int</span> <span class="o">-&gt;</span> <span class="n">Proc</span> <span class="p">()</span>
<span class="nv">mkProc</span> <span class="nv">seats</span> <span class="o">=</span> <span class="kr">if</span> <span class="nv">seats</span> <span class="o">==</span> <span class="mi">0</span> <span class="kr">then</span> <span class="nv">return</span> <span class="p">()</span>
               <span class="kr">else</span> <span class="kr">do</span> <span class="nv">vs</span> <span class="o">&lt;-</span> <span class="nv">get</span>
                       <span class="kr">let</span> <span class="p">(</span><span class="n">V</span> <span class="nv">i</span> <span class="nv">n</span> <span class="nv">p</span><span class="o">:</span><span class="nv">ts</span><span class="p">)</span> <span class="o">=</span> <span class="nv">sortBy</span> <span class="p">(</span><span class="o">\</span><span class="nv">x</span> <span class="nv">y</span> <span class="o">-&gt;</span> <span class="nv">f</span> <span class="nv">y</span> <span class="p">`</span><span class="nv">compare</span><span class="p">`</span> <span class="nv">f</span> <span class="nv">x</span><span class="p">)</span> <span class="nv">vs</span>
                         <span class="kr">in</span> <span class="nv">tell</span> <span class="p">[</span><span class="nv">i</span><span class="p">]</span> <span class="o">&gt;&gt;</span> <span class="nv">put</span> <span class="p">((</span><span class="n">V</span> <span class="nv">i</span> <span class="p">(</span><span class="nv">n</span> <span class="o">+</span> <span class="mi">1</span><span class="p">)</span> <span class="nv">p</span><span class="p">)</span><span class="o">:</span><span class="nv">ts</span><span class="p">)</span> <span class="o">&gt;&gt;</span> <span class="nv">mkProc</span> <span class="p">(</span><span class="nv">seats</span> <span class="o">-</span> <span class="mi">1</span><span class="p">)</span>
  <span class="kr">where</span> <span class="nv">f</span> <span class="nv">x</span> <span class="o">=</span> <span class="nv">poll</span> <span class="nv">x</span> <span class="o">/</span> <span class="p">(</span><span class="nv">num</span> <span class="nv">x</span> <span class="o">+</span> <span class="mi">1</span><span class="p">)</span>

