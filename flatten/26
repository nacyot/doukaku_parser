23</pre></td><td class="code"><div class="highlight"><pre><span class="k">use</span> <span class="nn">List::</span><span class="n">Util</span> <span class="sx">qw(max)</span><span class="p">;</span>
<span class="k">use</span> <span class="nn">List::</span><span class="n">MoreUtils</span> <span class="sx">qw(indexes)</span><span class="p">;</span>
<span class="k">use</span> <span class="nn">HOP::</span><span class="n">Stream</span> <span class="sx">qw(upfrom transform head tail)</span><span class="p">;</span>

<span class="k">sub </span><span class="nf">max_index</span> <span class="p">{</span> <span class="k">my</span> <span class="nv">@xs</span> <span class="o">=</span> <span class="nv">@_</span><span class="p">;</span> <span class="p">(</span><span class="n">indexes</span> <span class="p">{</span> <span class="nv">$_</span> <span class="o">==</span> <span class="n">max</span><span class="p">(</span><span class="nv">@xs</span><span class="p">)</span> <span class="p">}</span> <span class="nv">@xs</span><span class="p">)[</span><span class="mi">0</span><span class="p">]</span> <span class="p">}</span>

<span class="k">my</span> <span class="nv">$num_seats</span> <span class="o">=</span> <span class="nb">shift</span><span class="p">;</span>
<span class="k">my</span> <span class="nv">@votes</span> <span class="o">=</span> <span class="nv">@ARGV</span><span class="p">;</span>

<span class="k">my</span> <span class="nv">@seqs</span><span class="p">;</span>
<span class="k">foreach</span> <span class="k">my</span> <span class="nv">$party</span> <span class="p">(</span><span class="mi">0</span><span class="o">..</span><span class="nv">$#votes</span><span class="p">)</span> <span class="p">{</span>
  <span class="k">my</span> <span class="nv">$votes</span> <span class="o">=</span> <span class="nv">$votes</span><span class="p">[</span><span class="nv">$party</span><span class="p">];</span>
  <span class="nv">$seqs</span><span class="p">[</span><span class="nv">$party</span><span class="p">]</span> <span class="o">=</span> <span class="n">transform</span> <span class="p">{</span> <span class="nv">$votes</span> <span class="o">/</span> <span class="nv">$_</span><span class="p">[</span><span class="mi">0</span><span class="p">]</span> <span class="p">}</span> <span class="n">upfrom</span><span class="p">(</span><span class="mi">1</span><span class="p">);</span>
<span class="p">}</span>

<span class="k">my</span> <span class="nv">@seat</span> <span class="o">=</span> <span class="p">();</span>
<span class="k">foreach</span> <span class="k">my</span> <span class="nv">$i</span> <span class="p">(</span><span class="mi">1</span><span class="o">..</span><span class="nv">$num_seats</span><span class="p">)</span> <span class="p">{</span>
  <span class="k">my</span> <span class="nv">$party</span> <span class="o">=</span> <span class="n">max_index</span><span class="p">(</span><span class="nb">map</span> <span class="p">{</span> <span class="n">head</span><span class="p">(</span><span class="nv">$_</span><span class="p">)</span> <span class="p">}</span> <span class="nv">@seqs</span><span class="p">);</span>
  <span class="nv">$seat</span><span class="p">[</span><span class="nv">$party</span><span class="p">]</span><span class="o">++</span><span class="p">;</span>
  <span class="nv">$seqs</span><span class="p">[</span><span class="nv">$party</span><span class="p">]</span> <span class="o">=</span> <span class="n">tail</span><span class="p">(</span><span class="nv">$seqs</span><span class="p">[</span><span class="nv">$party</span><span class="p">]);</span>
<span class="p">}</span>

<span class="k">print</span> <span class="nb">join</span><span class="p">(</span><span class="s">&quot; &quot;</span><span class="p">,</span> <span class="nv">@seat</span><span class="p">),</span> <span class="s">&quot;\n&quot;</span><span class="p">;</span>
</pre></div>
</td></tr></table>
