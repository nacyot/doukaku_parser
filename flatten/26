	<span class="nx">tmp</span><span class="o">=</span><span class="p">[]</span><span class="o">,</span><span class="nx">giseki</span><span class="o">=</span><span class="p">[];</span>
	<span class="k">for</span><span class="p">(</span><span class="nx">seitou</span><span class="o">=</span><span class="mi">0</span><span class="o">;</span><span class="nx">seitou</span><span class="o">&lt;</span><span class="nx">hyous</span><span class="p">.</span><span class="nx">length</span><span class="o">;</span><span class="nx">seitou</span><span class="o">++</span><span class="p">){</span>
		<span class="k">for</span><span class="p">(</span><span class="nx">i</span><span class="o">=</span><span class="mi">0</span><span class="o">;</span><span class="nx">i</span><span class="o">&lt;</span><span class="nx">teisu</span><span class="o">;</span><span class="nx">i</span><span class="o">++</span><span class="p">){</span>
