<span class="kt">uint</span>[] <span class="n">dhondt</span>(<span class="kt">uint</span>[] <span class="n">votes</span>, <span class="kt">uint</span> <span class="n">limit</span>) {
    <span class="k">auto</span> <span class="n">seats</span> = <span class="k">new</span> <span class="kt">uint</span>[<span class="n">votes</span>.<span class="n">length</span>];
    <span class="k">foreach</span>(<span class="n">_</span>; <span class="mi">0</span> .. <span class="n">limit</span>) {
        <span class="k">auto</span> <span class="n">quotients</span> = <span class="k">new</span> <span class="kt">real</span>[<span class="n">votes</span>.<span class="n">length</span>];
        <span class="k">foreach</span>(<span class="n">i</span>, <span class="k">ref</span> <span class="n">e</span>; <span class="n">quotients</span>) {
            <span class="n">e</span> = <span class="n">votes</span>[<span class="n">i</span>] / (<span class="n">seats</span>[<span class="n">i</span>] + <span class="mf">1.0</span>);
        }
        <span class="n">find_largest</span>: <span class="k">foreach</span>(<span class="n">i</span>, <span class="n">e</span>; <span class="n">quotients</span>) {
            <span class="k">foreach</span>(<span class="n">f</span>; <span class="n">quotients</span>[<span class="n">i</span> .. $]) {
                <span class="k">if</span>(<span class="n">f</span> &gt; <span class="n">e</span>) <span class="k">continue</span> <span class="n">find_largest</span>;
            }
            <span class="n">seats</span>[<span class="n">i</span>]++;
            <span class="k">break</span>;
        }
    }
    <span class="k">return</span> <span class="n">seats</span>;
}

<span class="kt">void</span> <span class="n">main</span>() {
    <span class="n">writeln</span>([<span class="mi">123</span>, <span class="mi">4</span>, <span class="mi">56</span>, <span class="mi">78</span>].<span class="n">dhondt</span>(<span class="mi">100</span>)); <span class="c">// [48 1 21 30]</span>
}
