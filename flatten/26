	total_seats = ARGV[1]
	
	party_count = ARGC - 2
	for (i=1; i&lt;=party_count; i++) votes[i] = ARGV[1+i];

	# 複数の商が同一の数値になり、くじ引きが必要になる回数
	lottery = 0

	for (i=1; i&lt;=NF; i++) votes[i] = $i

	for (p=1; p&lt;=party_count; p++) { # p = 党番号 (1〜)
		seats_for[p] = 0;
		denom[p] = 1;
		quotient[p] = votes[p] # votes[p] / denom[p] = votes[p] / 1 = votes[p]
	}

	for (i=total_seats; i&gt;0; i--) {
		p = max_at(quotient, party_count)

		seats_for[p]++
		denom[p]++
		quotient[p] = votes[p] / denom[p]
	}

	printf(&quot;総議席数: %d¥n&quot;, total_seats)
	printf(&quot;　各党の得票数: &quot;)
	for (i=1; i&lt;=party_count; i++) printf(&quot;%d &quot;, votes[i]); printf &quot;¥n&quot;

	printf(&quot;　各党の得票議席数: &quot;)
	for (p=1; p&lt;=party_count; p++) printf(&quot;%d &quot;, seats_for[p]);
	printf &quot;¥n&quot;

	if (lottery &gt; 0) {
		print &quot;※二つ（またはそれ以上）の商が同一の数値になります。&quot;
		print &quot;※公職選挙法第95条3項2号の規定により、このケースではどの党が獲得するかくじ引きで決める議席が発生します。&quot;
		print &quot;※従って、上記の結果は実際の結果と異なる場合があります。&quot;
	}

}

function max_at(ar,cnt, at,max,check_again,i,max_count)
{
	at = 0
	max = -1

	check_again = 0 # 最大値をとる党が複数ある可能性があるか

	for (i=1; i&lt;=cnt; i++) {
		if (ar[i] &gt; max) { at = i; max = ar[i]; }
		else if (ar[i] == max) check_again = 1
	}

	if (check_again) {
		max_count = 0
		for (i=1; i&lt;=cnt; i++) if (ar[i] == max) max_count++
		if (max_count &gt; 0) lottery++;
	}

	return at
}
</pre></div>
