50</pre></td><td class="code"><div class="highlight"><pre><span class="cp">#include &lt;stdio.h&gt;</span>

<span class="cp">#define VOTE_NUM 4</span>

<span class="k">typedef</span> <span class="k">struct</span> <span class="p">{</span>
    <span class="kt">int</span>    <span class="n">original</span><span class="p">;</span>   <span class="c">/* 元の投票数 */</span> 
    <span class="kt">int</span>    <span class="n">divided</span><span class="p">;</span>    <span class="c">/* 割る数     */</span>
    <span class="kt">double</span> <span class="n">value</span><span class="p">;</span>      <span class="c">/* 割った数   */</span>
    <span class="kt">int</span>    <span class="n">parliament</span><span class="p">;</span> <span class="c">/* 議席数     */</span>
<span class="p">}</span> <span class="n">calc_data</span><span class="p">;</span>

<span class="kt">int</span> <span class="nf">main</span><span class="p">()</span>
<span class="p">{</span>
    <span class="kt">int</span>       <span class="n">constant</span>        <span class="o">=</span> <span class="mi">100</span><span class="p">;</span>              <span class="c">/* 定数 */</span>
    <span class="kt">int</span>       <span class="n">votes</span><span class="p">[</span><span class="n">VOTE_NUM</span><span class="p">]</span> <span class="o">=</span> <span class="p">{</span><span class="mi">123</span><span class="p">,</span> <span class="mi">4</span><span class="p">,</span> <span class="mi">56</span><span class="p">,</span> <span class="mi">78</span><span class="p">};</span> <span class="c">/* 投票数 */</span>
    <span class="n">calc_data</span> <span class="n">datas</span><span class="p">[</span><span class="n">VOTE_NUM</span><span class="p">];</span>
    <span class="kt">int</span>       <span class="n">i</span><span class="p">,</span> <span class="n">j</span><span class="p">;</span>
    <span class="kt">int</span>       <span class="n">max_index</span><span class="p">;</span>

    <span class="c">/* 初期化 */</span>
    <span class="k">for</span> <span class="p">(</span><span class="n">i</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span> <span class="n">i</span> <span class="o">&lt;</span> <span class="n">VOTE_NUM</span><span class="p">;</span> <span class="n">i</span><span class="o">++</span><span class="p">)</span> <span class="p">{</span>
        <span class="n">datas</span><span class="p">[</span><span class="n">i</span><span class="p">].</span><span class="n">original</span>   <span class="o">=</span> <span class="n">votes</span><span class="p">[</span><span class="n">i</span><span class="p">];</span>
        <span class="n">datas</span><span class="p">[</span><span class="n">i</span><span class="p">].</span><span class="n">divided</span>    <span class="o">=</span> <span class="mi">1</span><span class="p">;</span>
        <span class="n">datas</span><span class="p">[</span><span class="n">i</span><span class="p">].</span><span class="n">value</span>      <span class="o">=</span> <span class="n">votes</span><span class="p">[</span><span class="n">i</span><span class="p">];</span>
        <span class="n">datas</span><span class="p">[</span><span class="n">i</span><span class="p">].</span><span class="n">parliament</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
    <span class="p">}</span>

    <span class="k">for</span> <span class="p">(</span><span class="n">i</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span> <span class="n">i</span> <span class="o">&lt;</span> <span class="n">constant</span><span class="p">;</span> <span class="n">i</span><span class="o">++</span><span class="p">)</span> <span class="p">{</span>
        <span class="c">/* 大きい値を探す */</span>
        <span class="n">max_index</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
        <span class="k">for</span> <span class="p">(</span><span class="n">j</span> <span class="o">=</span> <span class="mi">1</span><span class="p">;</span> <span class="n">j</span> <span class="o">&lt;</span> <span class="n">VOTE_NUM</span><span class="p">;</span> <span class="n">j</span><span class="o">++</span><span class="p">)</span> <span class="p">{</span>
            <span class="k">if</span> <span class="p">(</span><span class="n">datas</span><span class="p">[</span><span class="n">max_index</span><span class="p">].</span><span class="n">value</span> <span class="o">&lt;</span> <span class="n">datas</span><span class="p">[</span><span class="n">j</span><span class="p">].</span><span class="n">value</span><span class="p">)</span> <span class="p">{</span>
                <span class="n">max_index</span> <span class="o">=</span> <span class="n">j</span><span class="p">;</span>
            <span class="p">}</span>
        <span class="p">}</span>

        <span class="c">/* 更新 */</span>
        <span class="n">datas</span><span class="p">[</span><span class="n">max_index</span><span class="p">].</span><span class="n">parliament</span><span class="o">++</span><span class="p">;</span>
        <span class="n">datas</span><span class="p">[</span><span class="n">max_index</span><span class="p">].</span><span class="n">divided</span><span class="o">++</span><span class="p">;</span>
        <span class="n">datas</span><span class="p">[</span><span class="n">max_index</span><span class="p">].</span><span class="n">value</span> <span class="o">=</span>  <span class="p">(</span><span class="kt">double</span><span class="p">)</span><span class="n">datas</span><span class="p">[</span><span class="n">max_index</span><span class="p">].</span><span class="n">original</span>
                                <span class="o">/</span> <span class="n">datas</span><span class="p">[</span><span class="n">max_index</span><span class="p">].</span><span class="n">divided</span><span class="p">;</span>
    <span class="p">}</span>

    <span class="c">/* 結果の表示 */</span>
    <span class="k">for</span> <span class="p">(</span><span class="n">i</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span> <span class="n">i</span> <span class="o">&lt;</span> <span class="n">VOTE_NUM</span><span class="p">;</span> <span class="n">i</span><span class="o">++</span><span class="p">)</span> <span class="p">{</span>
        <span class="n">printf</span><span class="p">(</span><span class="s">&quot;%d</span><span class="se">\n</span><span class="s">&quot;</span><span class="p">,</span> <span class="n">datas</span><span class="p">[</span><span class="n">i</span><span class="p">].</span><span class="n">parliament</span><span class="p">);</span>
    <span class="p">}</span>

    <span class="k">return</span> <span class="mi">0</span><span class="p">;</span>
<span class="p">}</span>
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
