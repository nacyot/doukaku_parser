<span class="k">import</span> <span class="nn">java.util.Arrays</span> <span class="o">;</span>

<span class="k">class</span> <span class="nc">dHondtMethod</span>
  <span class="o">{</span>
  <span class="k">static</span> <span class="k">class</span> <span class="nc">Vote</span> <span class="k">implements</span> <span class="n">Comparable</span>
    <span class="o">{</span>
    <span class="n">String</span> <span class="err">政党名</span> <span class="o">;</span> <span class="kt">int</span> <span class="err">得票数</span> <span class="o">;</span> <span class="kt">int</span> <span class="err">既得の議席数</span> <span class="o">;</span>
