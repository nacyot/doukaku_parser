<span class="k">class</span> <span class="nc">Array</span>
    <span class="k">def</span> <span class="nf">steep?</span>
        <span class="n">reverse</span><span class="o">.</span><span class="n">inject</span><span class="p">(</span><span class="mi">0</span><span class="p">)</span> <span class="k">do</span> <span class="o">|</span><span class="n">sum</span><span class="p">,</span> <span class="n">previous</span><span class="o">|</span>
            <span class="k">return</span> <span class="kp">false</span> <span class="k">unless</span> <span class="n">previous</span> <span class="o">&gt;</span> <span class="n">sum</span>
            <span class="n">sum</span> <span class="o">+=</span> <span class="n">previous</span>
        <span class="k">end</span>
        <span class="kp">true</span>
    <span class="k">end</span>
<span class="k">end</span>

<span class="nb">p</span> <span class="o">[</span><span class="mi">5</span><span class="p">,</span> <span class="mi">3</span><span class="p">,</span> <span class="mi">2</span><span class="o">].</span><span class="n">steep?</span>
