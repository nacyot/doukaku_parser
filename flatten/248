<span class="k">sub </span><span class="nf">is_steep_array</span><span class="p">{</span>
    <span class="k">my</span> <span class="nv">@arr</span> <span class="o">=</span> <span class="nb">reverse</span> <span class="nv">@_</span><span class="p">;</span>
    <span class="k">my</span> <span class="nv">$sum</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
