23</pre></td><td class="code"><div class="highlight"><pre>(setf pair-lst &#39;((a  b)
                 (b  c)
                 (c  d)
                 (c  e)
                 (a  f)
                 (d  x)
                 (y  z)
                 (z  c) ))

(defun gen-family-relation (list env)
  (let ( node result )
    (dolist (x list result)
        (setf node (assoc (car x) env))
        (if node
            (setf (cdr node) (append (cdr node) (cdr x)))
            (push x env) )
        (setf result env) )))

(defun query (s env)
  (let ((node (assoc s env)))
    (if node
        (cons (car node)(mapcar #&#39;(lambda (x)(query x env)) (cdr node)))
        s)))
</pre></div>
</td></tr></table>
    </div>
	

  
