  <span class="k">print</span> <span class="s">&#39;</span><span class="se">\n</span><span class="s">divid </span><span class="si">%d</span><span class="s"> sample =&gt;&#39;</span> <span class="o">%</span> <span class="n">n</span>
  <span class="k">while</span> <span class="n">l</span><span class="p">[</span><span class="mi">0</span><span class="p">]:</span>
    <span class="k">print</span> <span class="s">&#39; &quot;</span><span class="si">%s</span><span class="s">&quot;&#39;</span> <span class="o">%</span> <span class="s">&#39;&#39;</span><span class="o">.</span><span class="n">join</span><span class="p">([</span><span class="n">a</span><span class="o">.</span><span class="n">pop</span><span class="p">(</span><span class="mi">0</span><span class="p">)</span> <span class="ow">and</span> <span class="n">sl</span><span class="o">.</span><span class="n">pop</span><span class="p">(</span><span class="mi">0</span><span class="p">)</span> <span class="k">for</span> <span class="n">a</span> <span class="ow">in</span> <span class="n">l</span> <span class="k">if</span> <span class="n">a</span><span class="p">])</span>

