16
17
18
19
20
21
22</pre></td><td class="code"><div class="highlight"><pre><span class="nb">require</span> <span class="s1">&#39;pp&#39;</span>
<span class="vg">$KCODE</span><span class="o">=</span><span class="s1">&#39;EUC&#39;</span>

<span class="k">class</span> <span class="nc">String</span>
  <span class="k">def</span> <span class="nf">divid</span><span class="p">(</span><span class="n">lines</span><span class="p">)</span>
    <span class="n">out</span><span class="p">,</span> <span class="n">x</span><span class="p">,</span> <span class="n">y</span> <span class="o">=</span> <span class="o">[</span><span class="mi">0</span><span class="o">]</span> <span class="o">*</span> <span class="n">lines</span><span class="p">,</span> <span class="nb">split</span><span class="p">(</span><span class="s1">&#39;&#39;</span><span class="p">),</span> <span class="kp">nil</span>
    <span class="n">y</span><span class="o">.</span><span class="n">times</span> <span class="p">{</span><span class="o">|</span><span class="n">i</span><span class="o">|</span> <span class="n">out</span><span class="o">[</span><span class="n">i</span><span class="o">]</span> <span class="o">+=</span> <span class="mi">1</span> <span class="p">}</span> <span class="k">while</span> <span class="p">(</span><span class="n">y</span> <span class="o">=</span> <span class="n">x</span><span class="o">.</span><span class="n">slice!</span><span class="p">(</span><span class="mi">0</span><span class="p">,</span> <span class="n">lines</span><span class="p">)</span><span class="o">.</span><span class="n">size</span><span class="p">)</span> <span class="o">&gt;</span> <span class="mi">0</span>

    <span class="n">rest</span> <span class="o">=</span> <span class="nb">self</span>
    <span class="n">out</span><span class="o">.</span><span class="n">map!</span> <span class="k">do</span> <span class="o">|</span><span class="n">len</span><span class="o">|</span>
      <span class="n">_</span><span class="p">,</span> <span class="n">ret</span><span class="p">,</span> <span class="n">rest</span> <span class="o">=</span> <span class="o">*</span><span class="n">rest</span><span class="o">.</span><span class="n">match</span><span class="p">(</span><span class="sr">/(.{</span><span class="si">#{</span><span class="n">len</span><span class="si">}</span><span class="sr">})(.*)/</span><span class="p">)</span>
      <span class="n">ret</span>
    <span class="k">end</span>
  <span class="k">end</span>
<span class="k">end</span>

<span class="n">sample</span> <span class="o">=</span> <span class="s2">&quot;ゆめよりもはかなき世のなかをなげきわびつゝあかしくらすほどに四月十よひにもなりぬれば木のしたくらがりもてゆく&quot;</span>
<span class="p">(</span><span class="mi">4</span><span class="o">.</span><span class="n">.</span><span class="mi">6</span><span class="p">)</span><span class="o">.</span><span class="n">to_a</span><span class="o">.</span><span class="n">each</span> <span class="k">do</span> <span class="o">|</span><span class="n">i</span><span class="o">|</span>
  <span class="nb">puts</span> <span class="s2">&quot;divid </span><span class="si">#{</span><span class="n">i</span><span class="si">}</span><span class="s2">:&quot;</span>
  <span class="n">pp</span> <span class="n">sample</span><span class="o">.</span><span class="n">divid</span><span class="p">(</span><span class="n">i</span><span class="p">)</span>
  <span class="nb">puts</span>
<span class="k">end</span>
