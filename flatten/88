22
23
24</pre></td><td class="code"><div class="highlight"><pre>// 入力される値が2byte文字のみと仮定
#module
#deffunc divid str _target, int num
    if (num &lt;= 0) : return

    target = _target
    dim result, num
    count = 0
    repeat strlen(target) &gt;&gt; 1
        result(count) += 2
        count++
        if(count == num) : count = 0
    loop
    count = 0
    repeat num
        mes strmid(target, count, result(cnt))
        count += result(cnt)
    loop
    return
#global
    sample = &quot;ゆめよりもはかなき世のなかをなげきわびつゝあかしくらすほどに四月十よひにもなりぬれば木のしたくらがりもてゆく&quot;
    divid sample, 4
    divid sample, 5
    divid sample, 6
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
