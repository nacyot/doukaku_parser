15</pre></td><td class="code"><div class="highlight"><pre>{
	printf(&quot;%.16f\n&quot;, cube_root($1))
}

function cube_root(r,  a,b,d)
{
	a = r
	while (1) {
		b = (r / (a*a) + 2 * a) / 3
		d = b - a ; if (d &lt; 0) d = -d
		if (d &lt; 5e-13) break
		a = b
	}
	return a
}
</pre></div>
