28</pre></td><td class="code"><div class="highlight"><pre><span class="kr">import</span> <span class="n">Data</span><span class="o">.</span><span class="n">Number</span><span class="o">.</span><span class="n">Dif</span>

<span class="nv">newton</span> <span class="nv">f</span> <span class="nv">init</span> <span class="nv">eps</span> <span class="o">=</span>
  <span class="kr">let</span> <span class="nv">f&#39;</span> <span class="o">=</span> <span class="nv">deriv</span> <span class="nv">f</span>
      <span class="nv">next</span> <span class="o">=</span> <span class="nv">init</span> <span class="o">-</span> <span class="p">(</span><span class="nv">unDif</span> <span class="nv">f</span> <span class="nv">init</span><span class="p">)</span> <span class="o">/</span> <span class="p">(</span><span class="nv">f&#39;</span> <span class="nv">init</span><span class="p">)</span>
  <span class="kr">in</span> <span class="kr">if</span> <span class="nv">abs</span> <span class="p">(</span><span class="nv">next</span> <span class="o">-</span> <span class="nv">init</span><span class="p">)</span> <span class="o">&lt;</span> <span class="nv">eps</span>
    <span class="kr">then</span> <span class="nv">init</span>
    <span class="kr">else</span> <span class="nv">newton</span> <span class="nv">f</span> <span class="nv">next</span> <span class="nv">eps</span>


<span class="c1">-- aの立方根は x^3 - a = 0の解</span>
<span class="nv">cbrt</span> <span class="nv">a</span> <span class="o">=</span> <span class="nv">newton</span> <span class="p">(</span><span class="o">\</span><span class="nv">x</span> <span class="o">-&gt;</span> <span class="nv">x</span><span class="o">**</span><span class="mi">3</span> <span class="o">-</span> <span class="nv">a</span><span class="p">)</span> <span class="mi">1</span> <span class="mf">1e-13</span>

<span class="nv">main</span> <span class="o">=</span> <span class="kr">do</span>
  <span class="nv">putStrLn</span> <span class="o">$</span> <span class="s">&quot;cbrt 10   = &quot;</span> <span class="o">++</span> <span class="nv">show</span> <span class="p">(</span><span class="nv">cbrt</span> <span class="mi">10</span><span class="p">)</span>
  <span class="nv">putStrLn</span> <span class="o">$</span> <span class="s">&quot;cbrt 100  = &quot;</span> <span class="o">++</span> <span class="nv">show</span> <span class="p">(</span><span class="nv">cbrt</span> <span class="mi">100</span><span class="p">)</span>
  <span class="nv">putStrLn</span> <span class="o">$</span> <span class="s">&quot;cbrt 1000 = &quot;</span> <span class="o">++</span> <span class="nv">show</span> <span class="p">(</span><span class="nv">cbrt</span> <span class="mi">1000</span><span class="p">)</span>
  <span class="nv">putStrLn</span> <span class="o">$</span> <span class="s">&quot;pi        = &quot;</span> <span class="o">++</span> <span class="nv">show</span> <span class="p">(</span><span class="nv">newton</span> <span class="nv">sin</span> <span class="mi">3</span> <span class="mf">1e-13</span><span class="p">)</span>
  <span class="nv">putStrLn</span> <span class="o">$</span> <span class="s">&quot;e         = &quot;</span> <span class="o">++</span> <span class="nv">show</span> <span class="p">(</span><span class="nv">newton</span> <span class="p">(</span><span class="o">\</span><span class="nv">x</span> <span class="o">-&gt;</span> <span class="nv">log</span> <span class="nv">x</span> <span class="o">-</span> <span class="mi">1</span><span class="p">)</span> <span class="mi">2</span> <span class="mf">1e-13</span><span class="p">)</span>

