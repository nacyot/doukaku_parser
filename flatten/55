22</pre></td><td class="code"><div class="highlight"><pre><span class="cp">&lt;?php</span>

<span class="k">function</span> <span class="nf">cube_root</span><span class="p">(</span><span class="nv">$num</span><span class="p">)</span>
<span class="p">{</span>
    <span class="nv">$a</span> <span class="o">=</span> <span class="nv">$num</span><span class="p">;</span>
    
    <span class="k">while</span><span class="p">(</span><span class="k">true</span><span class="p">)</span>
    <span class="p">{</span>
        <span class="nv">$b</span> <span class="o">=</span> <span class="p">(</span><span class="nv">$num</span><span class="o">/</span><span class="p">(</span><span class="nv">$a</span><span class="o">*</span><span class="nv">$a</span><span class="p">)</span> <span class="o">+</span> <span class="nv">$a</span><span class="o">*</span><span class="m">2</span><span class="p">)</span> <span class="o">/</span> <span class="m">3</span><span class="p">;</span>
        <span class="o">++</span><span class="nv">$i</span><span class="p">;</span>
        <span class="nv">$diff</span> <span class="o">=</span> <span class="nf">abs</span><span class="p">(</span><span class="nv">$b</span> <span class="o">-</span> <span class="nv">$a</span><span class="p">);</span>
        <span class="k">if</span><span class="p">(</span><span class="nv">$diff</span> <span class="o">&lt;</span> <span class="m">1</span><span class="nx">e</span><span class="o">-</span><span class="m">12</span><span class="p">)</span><span class="k">break</span><span class="p">;</span>
        <span class="nv">$a</span> <span class="o">=</span> <span class="nv">$b</span><span class="p">;</span>
    <span class="p">}</span>
    <span class="k">return</span> <span class="nv">$b</span><span class="p">;</span>
<span class="p">}</span>

<span class="c">//test code</span>
<span class="nv">$ret</span> <span class="o">=</span> <span class="nx">cube_root</span><span class="p">(</span><span class="m">10</span><span class="p">);</span>
<span class="nf">printf</span><span class="p">(</span><span class="s2">&quot;%.16f</span><span class="se">\n</span><span class="s2">%.16f</span><span class="se">\n</span><span class="s2">&quot;</span><span class="p">,</span> <span class="nv">$ret</span><span class="p">,</span> <span class="nv">$ret</span><span class="o">*</span><span class="nv">$ret</span><span class="o">*</span><span class="nv">$ret</span><span class="p">);</span>
<span class="nv">$ret</span> <span class="o">=</span> <span class="nx">cube_root</span><span class="p">(</span><span class="m">100</span><span class="p">);</span>
<span class="nf">printf</span><span class="p">(</span><span class="s2">&quot;%.16f</span><span class="se">\n</span><span class="s2">%.16f</span><span class="se">\n</span><span class="s2">&quot;</span><span class="p">,</span> <span class="nv">$ret</span><span class="p">,</span> <span class="nv">$ret</span><span class="o">*</span><span class="nv">$ret</span><span class="o">*</span><span class="nv">$ret</span><span class="p">);</span>
</pre></div>
