<span class="k">struct</span> <span class="n">char_at</span>
<span class="p">{</span>
    <span class="k">const</span> <span class="kt">char</span><span class="o">*</span> <span class="n">s</span><span class="p">;</span>

    <span class="k">static</span> <span class="n">size_t</span> <span class="n">i</span><span class="p">;</span>

