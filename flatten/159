<span class="p">{</span>
    <span class="k">while</span> <span class="p">(</span><span class="n">beg</span> <span class="o">!=</span> <span class="n">end</span> <span class="o">&amp;&amp;</span> <span class="p">(</span><span class="o">*</span><span class="n">beg</span><span class="p">)[</span><span class="n">depth</span><span class="p">]</span> <span class="o">&lt;</span> <span class="n">c</span><span class="p">)</span>
