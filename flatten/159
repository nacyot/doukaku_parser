s1 = reader.readLine()
s2 = reader.readLine()
prev = new int[s2.length() + 1]
max = 0
s1.eachWithIndex({ c1, i -&gt;
    next = new int[s2.length() + 1]
    s2.eachWithIndex({ c2, j -&gt; 
        if(c1 == c2) {
            next[j + 1] = prev[j] + 1
            max = Math.max(max, next[j + 1])
        }
    })
    prev = next
})
println(max)
</pre></div>
