<span class="gu">@@ -5,8 +5,7 @@</span>
     my $r;
     for( my $n = $x; $n&gt;0; $n --)
     {
<span class="gd">-        $r = rand; $r *= 1000;</span>
<span class="gd">-        push(@num, splice(@num, $r % $n, 1) );</span>
<span class="gi">+        push(@num, splice(@num, rand() * $n, 1) );</span>
     }
