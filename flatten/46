	<a href="/web/20100606194523/http://ja.doukaku.org/comment/3625/">#3625</a>(<script language="JavaScript">print_time(new Date("2007/10/31 09:42 GMT"));</script><noscript>2007/10/31 09:42 GMT</noscript>)
	
	
	
