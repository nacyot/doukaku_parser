    <span class="p">{</span>
        <span class="nb">push</span><span class="p">(</span><span class="nv">@out</span><span class="p">,</span> <span class="nv">$1</span><span class="p">)</span> <span class="k">if</span><span class="p">(</span> <span class="nv">$a</span> <span class="o">=~</span> <span class="sr">m/ ($b) /</span> <span class="p">);</span>
