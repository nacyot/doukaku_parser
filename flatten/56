      <span class="p">[</span><span class="n">dic</span> <span class="nl">setObject:</span><span class="p">[</span><span class="n">NSNumber</span> <span class="nl">numberWithInteger:</span><span class="p">[</span><span class="n">n</span> <span class="n">intValue</span><span class="p">]</span> <span class="o">+</span> <span class="mi">1</span><span class="p">]</span> <span class="nl">forKey:</span><span class="n">obj</span><span class="p">];</span>
    <span class="p">}</span> <span class="k">else</span> <span class="p">{</span>
      <span class="p">[</span><span class="n">dic</span> <span class="nl">setObject:</span><span class="p">[</span><span class="n">NSNumber</span> <span class="nl">numberWithInteger:</span><span class="mi">1</span><span class="p">]</span> <span class="nl">forKey:</span><span class="n">obj</span><span class="p">];</span>
