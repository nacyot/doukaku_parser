<span class="nv">uniqOnly</span> <span class="o">=</span> <span class="nv">uniq</span> <span class="p">[]</span>
  <span class="kr">where</span> <span class="nv">uniq</span> <span class="nv">xs</span> <span class="p">[]</span> <span class="o">=</span> <span class="nv">reverse</span> <span class="nv">xs</span>
        <span class="nv">uniq</span> <span class="nv">xs</span> <span class="p">(</span><span class="nv">y</span><span class="o">:</span><span class="nv">ys</span><span class="p">)</span> <span class="o">=</span> <span class="kr">case</span> <span class="nv">break</span> <span class="p">(</span><span class="nv">y</span> <span class="o">==</span><span class="p">)</span> <span class="nv">xs</span> <span class="kr">of</span>
