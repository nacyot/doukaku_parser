  <span class="k">if</span> <span class="n">duplicated</span> <span class="ow">is</span> <span class="bp">None</span><span class="p">:</span>
    <span class="n">duplicated</span> <span class="o">=</span> <span class="p">[]</span>
  <span class="k">if</span> <span class="nb">len</span><span class="p">(</span><span class="n">xs</span><span class="p">)</span> <span class="o">&lt;</span> <span class="mi">2</span><span class="p">:</span>
