<span class="c1">-- Test data</span>

<span class="kr">data</span> <span class="n">RGB</span> <span class="o">=</span> <span class="n">Red</span> <span class="o">|</span> <span class="n">Green</span> <span class="o">|</span> <span class="n">Blue</span>  <span class="kr">deriving</span> <span class="p">(</span><span class="n">Typeable</span><span class="p">,</span><span class="n">Bounded</span><span class="p">,</span><span class="n">Enum</span><span class="p">,</span><span class="n">Show</span><span class="p">)</span>
<span class="kr">data</span> <span class="n">ENWS</span> <span class="o">=</span> <span class="n">East</span> <span class="o">|</span> <span class="n">North</span> <span class="o">|</span> <span class="n">West</span> <span class="o">|</span> <span class="n">South</span> <span class="kr">deriving</span> <span class="p">(</span><span class="n">Typeable</span><span class="p">,</span><span class="n">Bounded</span><span class="p">,</span><span class="n">Enum</span><span class="p">,</span><span class="n">Show</span><span class="p">)</span>

<span class="nv">allItem</span> <span class="o">::</span> <span class="p">(</span><span class="n">Bounded</span> <span class="nv">a</span><span class="p">,</span> <span class="n">Enum</span> <span class="nv">a</span><span class="p">)</span> <span class="o">=&gt;</span> <span class="p">[</span><span class="nv">a</span><span class="p">]</span>
<span class="nv">allItem</span> <span class="o">=</span> <span class="p">[</span><span class="nv">minBound</span><span class="o">..</span><span class="nv">maxBound</span><span class="p">]</span>

<span class="nv">test</span> <span class="o">=</span> <span class="nv">crossProduct4</span> <span class="p">(</span><span class="nv">allItem</span><span class="o">::</span><span class="p">[()])</span> <span class="p">(</span><span class="nv">allItem</span><span class="o">::</span><span class="p">[</span><span class="n">Bool</span><span class="p">])</span> <span class="p">(</span><span class="nv">allItem</span><span class="o">::</span><span class="p">[</span><span class="n">RGB</span><span class="p">])</span> <span class="p">(</span><span class="nv">allItem</span><span class="o">::</span><span class="p">[</span><span class="n">ENWS</span><span class="p">])</span>

<span class="cm">{-</span>
<span class="cm">*Main&gt; putStr $ unlines $ map show $ test</span>
<span class="cm">((),False,Red,East)</span>
<span class="cm">((),False,Red,North)</span>
<span class="cm">((),False,Red,West)</span>
<span class="cm">((),False,Red,South)</span>
<span class="cm">((),False,Green,East)</span>
<span class="cm">((),False,Green,North)</span>
<span class="cm">((),False,Green,West)</span>
<span class="cm">((),False,Green,South)</span>
<span class="cm">((),False,Blue,East)</span>
<span class="cm">((),False,Blue,North)</span>
<span class="cm">((),False,Blue,West)</span>
<span class="cm">((),False,Blue,South)</span>
<span class="cm">((),True,Red,East)</span>
<span class="cm">((),True,Red,North)</span>
<span class="cm">((),True,Red,West)</span>
<span class="cm">((),True,Red,South)</span>
<span class="cm">((),True,Green,East)</span>
<span class="cm">((),True,Green,North)</span>
<span class="cm">((),True,Green,West)</span>
<span class="cm">((),True,Green,South)</span>
<span class="cm">((),True,Blue,East)</span>
<span class="cm">((),True,Blue,North)</span>
<span class="cm">((),True,Blue,West)</span>
<span class="cm">((),True,Blue,South)</span>
<span class="cm">-}</span>
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
