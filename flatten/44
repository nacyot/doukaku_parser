  <span class="k">if</span> <span class="n">arrays</span><span class="o">.</span><span class="n">length</span> <span class="o">==</span> <span class="mi">1</span>
    <span class="n">arrays</span><span class="o">.</span><span class="n">first</span><span class="o">.</span><span class="n">map</span><span class="p">{</span><span class="o">|</span><span class="n">x</span><span class="o">|</span> <span class="o">[</span><span class="n">x</span><span class="o">]</span><span class="p">}</span>
  <span class="k">else</span>
    <span class="n">cross_product</span><span class="p">(</span><span class="o">*</span><span class="n">arrays</span><span class="o">[</span><span class="mi">1</span><span class="o">.</span><span class="n">.</span><span class="o">-</span><span class="mi">1</span><span class="o">]</span><span class="p">)</span><span class="o">.</span><span class="n">inject</span><span class="p">(</span><span class="o">[]</span><span class="p">)</span> <span class="p">{</span><span class="o">|</span><span class="n">result</span><span class="p">,</span> <span class="n">x</span><span class="o">|</span>
      <span class="n">result</span><span class="o">.</span><span class="n">concat</span> <span class="n">arrays</span><span class="o">.</span><span class="n">first</span><span class="o">.</span><span class="n">map</span><span class="p">{</span><span class="o">|</span><span class="n">a</span><span class="o">|</span> <span class="o">[</span><span class="n">a</span><span class="p">,</span> <span class="o">*</span><span class="n">x</span><span class="o">]</span> <span class="p">}</span>
    <span class="p">}</span>
