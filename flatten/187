  <span class="p">(</span><span class="k">let </span><span class="p">((</span><span class="nf">bucket</span> <span class="p">(</span><span class="nb">make-vector </span><span class="p">(</span><span class="nb">+ </span><span class="mi">1</span> <span class="p">(</span><span class="nb">- </span><span class="nv">range-max</span> <span class="nv">range-min</span><span class="p">))</span> <span class="o">&#39;</span><span class="p">())))</span>
    <span class="p">(</span><span class="nf">for-each</span>
