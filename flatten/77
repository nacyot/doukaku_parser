<span class="k">use</span> <span class="n">warnings</span><span class="p">;</span>

<span class="nb">package</span> <span class="n">hoge</span><span class="p">;</span>

<span class="k">sub </span><span class="nf">fizzbuzz</span> <span class="p">{</span>
    <span class="k">my</span> <span class="p">(</span> <span class="nv">$self</span><span class="p">,</span> <span class="nv">$num</span><span class="p">,</span> <span class="nv">$hoge</span> <span class="p">)</span> <span class="o">=</span> <span class="nv">@_</span><span class="p">;</span>
    <span class="nv">$hoge</span> <span class="o">||</span> <span class="n">__PACKAGE__</span><span class="p">;</span>
<span class="p">}</span>

<span class="nb">package</span> <span class="n">Fizz</span><span class="p">;</span>
<span class="k">use</span> <span class="n">base</span> <span class="s">&#39;hoge&#39;</span><span class="p">;</span>

<span class="k">sub </span><span class="nf">fizzbuzz</span> <span class="p">{</span>
    <span class="k">my</span> <span class="p">(</span> <span class="nv">$self</span><span class="p">,</span> <span class="nv">$num</span><span class="p">,</span> <span class="nv">$hoge</span> <span class="p">)</span> <span class="o">=</span> <span class="nv">@_</span><span class="p">;</span>
    <span class="nv">$self</span><span class="o">-&gt;</span><span class="nn">SUPER::</span><span class="n">fizzbuzz</span><span class="p">(</span> <span class="nv">$num</span><span class="p">,</span> <span class="p">(</span> <span class="nv">$num</span> <span class="nv">%</span> <span class="nv">3</span> <span class="p">?</span> <span class="s">&quot;&quot;</span> <span class="p">:</span> <span class="n">__PACKAGE__</span> <span class="p">)</span> <span class="o">.</span> <span class="nv">$hoge</span> <span class="p">);</span>
<span class="p">}</span>

<span class="nb">package</span> <span class="n">Buzz</span><span class="p">;</span>
<span class="k">use</span> <span class="n">base</span> <span class="s">&#39;Fizz&#39;</span><span class="p">;</span>

<span class="k">sub </span><span class="nf">fizzbuzz</span> <span class="p">{</span>
    <span class="k">my</span> <span class="p">(</span> <span class="nv">$self</span><span class="p">,</span> <span class="nv">$num</span> <span class="p">)</span> <span class="o">=</span> <span class="nv">@_</span><span class="p">;</span>
    <span class="nv">$self</span><span class="o">-&gt;</span><span class="nn">SUPER::</span><span class="n">fizzbuzz</span><span class="p">(</span> <span class="nv">$num</span><span class="p">,</span> <span class="p">(</span> <span class="nv">$num</span> <span class="nv">%</span> <span class="nv">5</span> <span class="p">?</span> <span class="s">&quot;&quot;</span> <span class="p">:</span> <span class="n">__PACKAGE__</span> <span class="p">)</span> <span class="p">);</span>
<span class="p">}</span>

<span class="nb">package</span> <span class="nn">Fizz::</span><span class="n">Buzz</span><span class="p">;</span>
<span class="k">use</span> <span class="n">base</span> <span class="s">&#39;Buzz&#39;</span><span class="p">;</span>

<span class="k">sub </span><span class="nf">fizzbuzz</span> <span class="p">{</span>
    <span class="k">my</span> <span class="p">(</span> <span class="nv">$self</span><span class="p">,</span> <span class="nv">$num</span> <span class="p">)</span> <span class="o">=</span> <span class="nv">@_</span><span class="p">;</span>
    <span class="s">&quot;$num:&quot;</span> <span class="o">.</span> <span class="nv">$self</span><span class="o">-&gt;</span><span class="nn">SUPER::</span><span class="n">fizzbuzz</span><span class="p">(</span><span class="nv">$num</span><span class="p">)</span> <span class="o">.</span> <span class="s">&quot;\n&quot;</span><span class="p">;</span>
<span class="p">}</span>
<span class="k">sub </span><span class="nf">new</span> <span class="p">{</span> <span class="nb">bless</span> <span class="p">{}</span> <span class="o">=&gt;</span> <span class="nb">shift</span> <span class="p">}</span>

<span class="nb">package</span> <span class="n">main</span><span class="p">;</span>
<span class="k">my</span> <span class="nv">$fizzbuzz</span> <span class="o">=</span> <span class="nn">Fizz::</span><span class="n">Buzz</span><span class="o">-&gt;</span><span class="k">new</span><span class="p">;</span>
<span class="k">print</span> <span class="nv">$fizzbuzz</span><span class="o">-&gt;</span><span class="n">fizzbuzz</span><span class="p">(</span><span class="nv">$_</span><span class="p">)</span> <span class="k">for</span> <span class="p">(</span> <span class="mi">1</span> <span class="o">..</span> <span class="p">(</span> <span class="nb">shift</span> <span class="o">||</span> <span class="mi">100</span> <span class="p">)</span> <span class="p">);</span>
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
	<a href="/web/20090607072941/http://ja.doukaku.org/comment/3914/addtag/" class="addtag" id="addtag_3914">タグ</a>:
	
	タグなし
	&nbsp;

	
