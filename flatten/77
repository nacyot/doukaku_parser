setlocal enabledelayedexpansion
set /a n=%1+19
for /l %%i in (%1,1,%n%) do (
  set /a a=%%i%%3
  set /a b=%%i%%5
  set d=
  if !a!==0 set d=Fizz
  if !b!==0 set d=!d!Buzz
  if &quot;!d!&quot;==&quot;&quot; set d=hoge
  echo %%i:!d!
)
</pre></div>
