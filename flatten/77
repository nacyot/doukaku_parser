     if( (N%3) == 0 ) printf(&quot;%2d:%-s&quot;, N, &quot;Fizz&quot;);
     if( (N%5) == 0 )
     {
<span class="gd">-      if( (N%3) != 0 ) printf(&quot;%2d:&quot;, N);</span>
<span class="gi">+      if( (N*N%3) == 1 ) printf(&quot;%2d:&quot;, N);</span>
       printf(&quot;%-s&quot;,&quot;Buzz&quot;);
     }
     printf(&quot;\n&quot;);
