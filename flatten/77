    <span class="k">for</span> <span class="n">x</span> <span class="ow">in</span> <span class="nb">xrange</span><span class="p">(</span><span class="mi">1</span><span class="p">,</span><span class="n">num</span><span class="o">+</span><span class="mi">1</span><span class="p">):</span>
        <span class="k">if</span> <span class="n">x</span> <span class="o">%</span> <span class="mi">15</span> <span class="o">==</span> <span class="mi">0</span><span class="p">:</span>
            <span class="k">print</span> <span class="s">&quot;</span><span class="si">%2d</span><span class="s">:FizzBuzz&quot;</span> <span class="o">%</span> <span class="n">x</span>
            <span class="k">continue</span>
        <span class="k">if</span> <span class="n">x</span> <span class="o">%</span> <span class="mi">5</span> <span class="o">==</span> <span class="mi">0</span><span class="p">:</span>
            <span class="k">print</span> <span class="s">&quot;</span><span class="si">%2d</span><span class="s">:Buzz&quot;</span> <span class="o">%</span> <span class="n">x</span>
            <span class="k">continue</span>
        <span class="k">if</span> <span class="n">x</span> <span class="o">%</span> <span class="mi">3</span> <span class="o">==</span> <span class="mi">0</span><span class="p">:</span>
            <span class="k">print</span> <span class="s">&quot;</span><span class="si">%2d</span><span class="s">:Fizz&quot;</span> <span class="o">%</span> <span class="n">x</span>
            <span class="k">continue</span>
        <span class="k">print</span> <span class="s">&quot;</span><span class="si">%2d</span><span class="s">:hoge&quot;</span> <span class="o">%</span> <span class="n">x</span>
        
<span class="n">FizzBuzz</span><span class="p">(</span><span class="mi">20</span><span class="p">)</span>
