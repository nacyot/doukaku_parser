	<a href="/web/20090607072941/http://ja.doukaku.org/comment/6026/">#6026</a>(<script language="JavaScript">print_time(new Date("2008/03/21 01:43 GMT"));</script><noscript>2008/03/21 01:43 GMT</noscript>)
	
	
	
