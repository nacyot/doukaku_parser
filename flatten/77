<span class="nv">buzz</span> <span class="o">=</span> <span class="nv">cycle</span> <span class="o">$</span> <span class="nv">replicate</span> <span class="mi">4</span> <span class="s">&quot;&quot;</span> <span class="o">++</span> <span class="p">[</span><span class="s">&quot;Buzz&quot;</span><span class="p">]</span>
<span class="nv">fizzbuzz</span> <span class="o">=</span> <span class="nv">zipWith</span> <span class="p">(</span><span class="o">++</span><span class="p">)</span> <span class="nv">fizz</span> <span class="nv">buzz</span>
