<span class="kt">void</span> <span class="n">main</span>(){
    <span class="k">for</span>(<span class="kt">int</span> <span class="n">i</span> = <span class="mi">1</span>; <span class="n">i</span> &lt;= <span class="mi">20</span>; <span class="n">i</span>++){
        <span class="n">writef</span>(<span class="s">&quot;%2d:&quot;</span>, <span class="n">i</span>);
        (<span class="n">i</span> % <span class="mi">3</span>) == <span class="mi">0</span> &amp;&amp; (<span class="n">writef</span>(<span class="s">&quot;Fizz&quot;</span>), <span class="n">i</span> % <span class="mi">5</span>) ||
        (<span class="n">i</span> % <span class="mi">5</span>) == <span class="mi">0</span> &amp;&amp; (<span class="n">writef</span>(<span class="s">&quot;Buzz&quot;</span>), <span class="kc">true</span>) ||
        <span class="n">writef</span>(<span class="s">&quot;hoge&quot;</span>);
        <span class="n">writefln</span>();
    }
}
</pre></div>
</td></tr></table>
