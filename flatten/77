Dim s As String
Dim i As Long
For i = 1 To 20
    s = &quot;&quot;
    If i Mod 3 = 0 Then
        s = &quot;Fizz&quot;
    End If

    If i Mod 5 = 0 Then
        s = s + &quot;Buzz&quot;
    End If

    If s = &quot;&quot; Then
        s = &quot;hoge&quot;
    End If

    Print Str$(i) +  &quot;:&quot; + s
Next
</pre></div>
