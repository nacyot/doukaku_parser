    v &lt;- rep(c(&quot;hoge&quot;, &quot;hoge&quot;, &quot;Fizz&quot;, &quot;hoge&quot;, &quot;Buzz&quot;, &quot;Fizz&quot;, &quot;hoge&quot;, &quot;hoge&quot;, &quot;Fizz&quot;, &quot;Buzz&quot;, &quot;hoge&quot;, &quot;Fizz&quot;, &quot;hoge&quot;, &quot;hoge&quot;, &quot;FizzBuzz&quot;), n%/%15+1)[1:n]
