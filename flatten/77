<span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">fizzbuzz</span> <span class="nv">n</span><span class="p">)</span>
  <span class="p">(</span><span class="nb">for-each </span><span class="p">(</span><span class="k">lambda </span><span class="p">(</span><span class="nf">a</span> <span class="nv">b</span><span class="p">)</span> <span class="p">(</span><span class="nf">print</span> <span class="nv">a</span> <span class="s">&quot;:&quot;</span> <span class="nv">b</span><span class="p">))</span>
