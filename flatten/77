<span class="k">    </span><span class="nb">printf</span> <span class="s2">&quot;%2s:Fizz\n&quot;</span> <span class="nv">$CNT</span>
    <span class="k">continue</span>
<span class="k">  </span>elif <span class="o">[</span> <span class="k">$(</span><span class="o">(</span><span class="nv">$CNT</span> % <span class="m">5</span><span class="k">)</span><span class="o">)</span> <span class="o">=</span> <span class="m">0</span> <span class="o">]</span>; <span class="k">then</span>
<span class="k">    </span><span class="nb">printf</span> <span class="s2">&quot;%2s:Buzz\n&quot;</span> <span class="nv">$CNT</span>
    <span class="k">continue</span>
<span class="k">  fi</span>
<span class="k">  </span><span class="nb">printf</span> <span class="s2">&quot;%2s:hoge\n&quot;</span> <span class="nv">$CNT</span>
<span class="k">done</span>
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
	<a href="/web/20090607072941/http://ja.doukaku.org/comment/4065/addtag/" class="addtag" id="addtag_4065">タグ</a>:
	
