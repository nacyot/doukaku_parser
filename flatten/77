  <span class="k">if</span> <span class="o">[</span> <span class="k">$(</span><span class="o">(</span><span class="nv">$CNT</span> % <span class="m">15</span><span class="k">)</span><span class="o">)</span> <span class="o">=</span> <span class="m">0</span> <span class="o">]</span>; <span class="k">then</span>
