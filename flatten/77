<span class="nv">hogefizzbuzz</span> <span class="o">=</span> <span class="nv">zipWith</span><span class="p">(</span><span class="o">\</span><span class="nv">h</span> <span class="nv">f</span><span class="o">-&gt;</span> <span class="nv">drop</span> <span class="p">(</span><span class="nv">length</span> <span class="nv">f</span><span class="p">)</span> <span class="nv">h</span> <span class="o">++</span> <span class="nv">f</span><span class="p">)</span> <span class="nv">hoge</span> <span class="nv">fizzbuzz</span>

