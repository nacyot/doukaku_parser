<span class="nv">putStrCross</span> <span class="nv">s</span> <span class="o">=</span> <span class="nv">mapM_</span> <span class="nv">putStrLn</span> <span class="p">[[</span><span class="nv">fromMaybe</span> <span class="sc">&#39; &#39;</span> <span class="o">$</span> <span class="nv">lookup</span> <span class="p">(</span><span class="nv">i</span><span class="p">,</span><span class="nv">j</span><span class="p">)</span> <span class="nv">cs</span><span class="o">|</span> <span class="nv">i</span> <span class="o">&lt;-</span> <span class="nv">rng</span><span class="p">]</span><span class="o">|</span> <span class="nv">j</span> <span class="o">&lt;-</span> <span class="nv">rng</span><span class="p">]</span> <span class="kr">where</span>
  <span class="p">(</span><span class="nv">m</span><span class="p">,</span> <span class="nv">m3</span><span class="p">,</span> <span class="nv">rng</span><span class="p">)</span> <span class="o">=</span> <span class="p">(</span><span class="nv">length</span> <span class="nv">s</span><span class="p">,</span> <span class="mi">3</span><span class="o">*</span><span class="nv">m</span><span class="p">,</span> <span class="p">[</span><span class="mi">0</span><span class="o">..</span><span class="nv">m3</span><span class="p">])</span>
  <span class="nv">cs</span> <span class="o">=</span> <span class="nv">zip</span> <span class="p">(</span><span class="nv">cross</span> <span class="p">(</span><span class="nv">m</span><span class="p">,</span><span class="nv">m</span><span class="p">)</span> <span class="p">(</span><span class="mi">1</span><span class="p">,</span><span class="mi">0</span><span class="p">))</span> <span class="o">$</span> <span class="nv">concat</span> <span class="o">$</span> <span class="nv">replicate</span> <span class="mi">12</span> <span class="nv">s</span>
