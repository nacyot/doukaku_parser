64
65
66
67</pre></td><td class="code"><div class="highlight"><pre>#module

// 移動量を取得する
#deffunc GetStep array step, double arg, int r
    step(0) = int( cos(arg) * r )
    step(1) = int( sin(arg) * r )
    return
    
    
// 文字マップへ文字列を書き込む
#deffunc WriteStrToCharMap_v \
    var dst, int lenX, int lenY, var src, array pt, array step,  \
    local idx
    
    repeat strlen(src)
        idx = pt(0) + (pt(1) * (lenX + 2))
        poke dst, idx, peek(src, cnt)
        pt(0) += step(0)
        pt(1) += step(1)
    loop
    return
    
    
// 白抜き形の文字列を生成する
#defcfunc makePlusString str src_,    \
    local src, local len, local root, local buf,     \
    local arg, local arg_r, local pt, local stepForward, local stepRight
    
    src   = src_
    len   = strlen(src)
    root  = (len * 3 + 1)
    
    if ( len == 0 ) { return &quot;&quot; }
    if ( len == 1 ) { return src }
    
    // 結果のマップ
    sdim   buf,       (root + 2) * root + 1
    memset buf, 0x20, (root + 2) * root
    
    // 文字列を書き加える
    arg   = 0.0
;    arg_r = 0.0
    pt    = len, 0
    repeat 4
        arg_r = arg + (M_PI / 2)
        
        GetStep stepForward, arg,   1
        GetStep stepRight,   arg_r, 1
        
        WriteStrToCharMap_v buf, root, root, src, pt, stepForward
        WriteStrToCharMap_v buf, root, root, src, pt, stepRight
        WriteStrToCharMap_v buf, root, root, src, pt, stepForward
        
        arg = arg_r
    loop
    
    // 改行を埋め込む
    repeat root
        wpoke buf, root + (cnt * (root + 2)), 0x0A0D
    loop
    
    return buf
    
#global

// test
    mes makePlusString( &quot;doukaku&quot; )
</pre></div>
</td></tr></table>
    </div>
	

  
