<span class="cp">#include &lt;stdlib.h&gt;</span>
<span class="cp">#include &lt;string.h&gt;</span>

<span class="kt">void</span> <span class="nf">outputs</span><span class="p">(</span><span class="kt">char</span> <span class="o">*</span><span class="n">input_word</span><span class="p">)</span>
<span class="p">{</span>
    <span class="kt">int</span> <span class="n">unit</span> <span class="o">=</span> <span class="n">strlen</span><span class="p">(</span><span class="n">input_word</span><span class="p">);</span>
