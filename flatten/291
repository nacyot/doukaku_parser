  <span class="k">def</span> <span class="nf">initialize</span><span class="p">(</span><span class="n">edge</span><span class="p">)</span>
    <span class="vi">@Screen</span><span class="p">,</span> <span class="vi">@x</span><span class="p">,</span> <span class="vi">@y</span><span class="p">,</span> <span class="vi">@edge</span><span class="p">,</span> <span class="vi">@dx</span><span class="p">,</span> <span class="vi">@dy</span> <span class="o">=</span> <span class="o">[[]]</span><span class="p">,</span> <span class="mi">0</span><span class="p">,</span> <span class="mi">0</span><span class="p">,</span> <span class="n">edge</span><span class="p">,</span> <span class="mi">1</span><span class="p">,</span><span class="mi">0</span>
