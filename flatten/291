    <span class="c">/*繋げる*/</span>
    <span class="k">return</span> <span class="nx">a</span><span class="p">.</span><span class="nx">map</span> <span class="p">(</span><span class="k">function</span> <span class="p">(</span><span class="nx">b</span><span class="p">)</span> <span class="p">{</span><span class="k">return</span> <span class="nx">b</span><span class="p">.</span><span class="nx">join</span> <span class="p">(</span><span class="s1">&#39;&#39;</span><span class="p">).</span><span class="nx">replace</span><span class="p">(</span><span class="sr">/ +$/</span><span class="o">,</span> <span class="s1">&#39;&#39;</span><span class="p">)}).</span><span class="nx">join</span> <span class="p">(</span><span class="s2">&quot;\n&quot;</span><span class="p">);</span>
<span class="p">}</span>
