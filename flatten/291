len=str.size()-1

[&#39; &#39;*len+str+&#39; &#39;*len,
 *(str[1..-1].collect { &#39; &#39;*len+it+&#39; &#39;*(len-1)+it }),
 str+&#39; &#39;*(len-1)+str,
 *(str[1..-1].collect { it+&#39; &#39;*(((len)*3)-1)+it }),
 str+&#39; &#39;*(len-1)+str,
 *(str[1..-1].collect { &#39; &#39;*len+it+&#39; &#39;*(len-1)+it }),
 &#39; &#39;*len+str+&#39; &#39;*len,
].each {println it}
