    <span class="nv">quartar</span>   <span class="o">=</span> <span class="p">[(</span><span class="nv">ss</span> <span class="nv">len</span><span class="p">)</span> <span class="o">++</span> <span class="nv">s</span> <span class="o">++</span> <span class="p">(</span><span class="nv">ss</span> <span class="nv">len</span><span class="p">)</span> <span class="o">++</span> <span class="s">&quot; &quot;</span><span class="p">]</span> <span class="o">++</span>
