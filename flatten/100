<span class="k">import</span> <span class="n">std</span>.<span class="n">math</span>;
<span class="k">import</span> <span class="n">std</span>.<span class="n">string</span>;

<span class="kt">uint</span> <span class="n">goedel</span>(<span class="kt">uint</span> <span class="n">n</span>) {
        <span class="k">static</span> <span class="kt">int</span>[] <span class="n">primes</span> = [ <span class="mi">2</span>, <span class="mi">3</span> , <span class="mi">5</span>, <span class="mi">7</span>, <span class="mi">11</span>, <span class="mi">13</span>, <span class="mi">17</span>, <span class="mi">19</span>, <span class="mi">23</span>, <span class="mi">29</span> ];
        <span class="kt">real</span> <span class="n">rslt</span> = <span class="mi">1</span>;
        <span class="n">string</span> <span class="n">str</span> = <span class="n">toString</span>(<span class="n">n</span>);
        <span class="k">for</span> (<span class="kt">int</span> <span class="n">i</span> = <span class="mi">0</span>; <span class="n">i</span> &lt; <span class="n">str</span>.<span class="n">length</span>; ++<span class="n">i</span>) {
                <span class="n">rslt</span> *= <span class="n">pow</span>(<span class="k">cast</span>(<span class="kt">real</span>)(<span class="n">primes</span>[<span class="n">i</span>]), <span class="n">toUint</span>([<span class="n">str</span>[<span class="n">i</span>]]));
        }
        <span class="k">return</span> <span class="k">cast</span>(<span class="kt">uint</span>)(<span class="n">rslt</span>);
}

<span class="c">/*</span>
<span class="c">import std.stdio;</span>

<span class="c">void main() {</span>
<span class="c">        writefln(&quot;%d, %d, %d&quot;, 9, 512, goedel(9));</span>
<span class="c">        writefln(&quot;%d, %d, %d&quot;, 81, 768, goedel(81));</span>
<span class="c">        writefln(&quot;%d, %d, %d&quot;, 230, 108, goedel(230));</span>
<span class="c">}</span>
