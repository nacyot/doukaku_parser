   <span class="kr">let</span> <span class="n">now</span> <span class="k">=</span> <span class="n">ref</span> <span class="mi">2</span> <span class="kr">in</span>
   <span class="kr">let</span> <span class="n">buf</span> <span class="k">=</span> <span class="n">ref</span> <span class="k">[</span><span class="mi">2</span><span class="k">]</span> <span class="kr">in</span>
