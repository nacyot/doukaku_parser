27</pre></td><td class="code"><div class="highlight"><pre><span class="k">use</span> <span class="n">strict</span><span class="p">;</span>
<span class="k">use</span> <span class="n">warnings</span><span class="p">;</span>
<span class="k">use</span> <span class="nn">Math::Prime::</span><span class="n">XS</span> <span class="sx">qw(is_prime)</span><span class="p">;</span>
<span class="k">use</span> <span class="nn">List::</span><span class="n">Util</span> <span class="sx">qw(reduce)</span><span class="p">;</span>
<span class="k">use</span> <span class="nn">List::</span><span class="n">MoreUtils</span> <span class="sx">qw(pairwise)</span><span class="p">;</span>

<span class="k">sub </span><span class="nf">primes</span> <span class="p">{</span>
    <span class="k">my</span> <span class="nv">$number</span> <span class="o">=</span> <span class="nb">shift</span><span class="p">;</span>
    <span class="k">my</span> <span class="nv">@primes</span> <span class="o">=</span> <span class="p">(</span><span class="mi">2</span><span class="p">);</span>
    <span class="k">my</span> <span class="nv">$i</span>      <span class="o">=</span> <span class="mi">3</span><span class="p">;</span>
    <span class="k">while</span> <span class="p">(</span> <span class="nv">@primes</span> <span class="o">&lt;</span> <span class="nv">$number</span> <span class="p">)</span> <span class="p">{</span>
        <span class="nb">push</span><span class="p">(</span> <span class="nv">@primes</span><span class="p">,</span> <span class="nv">$i</span> <span class="p">)</span> <span class="k">if</span> <span class="n">is_prime</span><span class="p">(</span><span class="nv">$i</span><span class="p">);</span>
        <span class="nv">$i</span><span class="o">++</span><span class="p">;</span>
    <span class="p">}</span>
    <span class="k">return</span> <span class="nv">@primes</span><span class="p">;</span>
<span class="p">}</span>

<span class="k">sub </span><span class="nf">goedel</span> <span class="p">{</span>
    <span class="k">my</span> <span class="nv">$number</span> <span class="o">=</span> <span class="nb">shift</span><span class="p">;</span>
    <span class="k">my</span> <span class="nv">@num</span>    <span class="o">=</span> <span class="nb">split</span> <span class="sr">//</span><span class="p">,</span> <span class="nv">$number</span><span class="p">;</span>
    <span class="k">my</span> <span class="nv">@primes</span> <span class="o">=</span> <span class="n">primes</span><span class="p">(</span> <span class="nb">scalar</span><span class="p">(</span><span class="nv">@num</span><span class="p">)</span> <span class="p">);</span>
    <span class="n">reduce</span> <span class="p">{</span> <span class="nv">$a</span> <span class="o">*</span> <span class="nv">$b</span> <span class="p">}</span> <span class="n">pairwise</span> <span class="p">{</span> <span class="nv">$a</span><span class="o">**</span><span class="nv">$b</span> <span class="p">}</span> <span class="nv">@primes</span><span class="p">,</span> <span class="nv">@num</span><span class="p">;</span>
<span class="p">}</span>

<span class="k">print</span> <span class="n">goedel</span><span class="p">(</span><span class="mi">9</span><span class="p">);</span>
<span class="k">print</span> <span class="n">goedel</span><span class="p">(</span><span class="mi">81</span><span class="p">);</span>
<span class="k">print</span> <span class="n">goedel</span><span class="p">(</span><span class="mi">230</span><span class="p">);</span>
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
