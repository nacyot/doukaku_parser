   <span class="kr">in</span>
   <span class="kr">let</span> <span class="n">s</span>      <span class="k">=</span> <span class="n">string_of_int</span> <span class="n">n</span> <span class="kr">in</span>
   <span class="kr">let</span> <span class="n">buf</span>    <span class="k">=</span> <span class="n">Buffer</span><span class="k">.</span><span class="n">create</span> <span class="mi">10</span> <span class="kr">in</span>
   <span class="kr">let</span> <span class="n">add</span>    <span class="k">=</span> <span class="n">Buffer</span><span class="k">.</span><span class="n">add_string</span> <span class="n">buf</span> <span class="kr">in</span>
   <span class="kr">let</span> <span class="n">result</span> <span class="k">=</span> <span class="n">ref</span> <span class="mi">1</span> <span class="kr">in</span>
   <span class="kr">let</span> <span class="n">primes</span> <span class="k">=</span> <span class="n">gen_prime</span> <span class="k">()</span> <span class="kr">in</span>
   <span class="kr">for</span> <span class="n">i</span> <span class="k">=</span> <span class="mi">0</span> <span class="kr">to</span> <span class="k">(</span><span class="n">String</span><span class="k">.</span><span class="n">length</span> <span class="n">s</span><span class="k">)</span> <span class="k">-</span> <span class="mi">1</span> <span class="kr">do</span>
      <span class="kr">let</span> <span class="n">m</span> <span class="k">=</span> <span class="n">int_of_string</span> <span class="k">(</span><span class="n">String</span><span class="k">.</span><span class="n">make</span> <span class="mi">1</span> <span class="n">s</span><span class="k">.[</span><span class="n">i</span><span class="k">])</span> <span class="kr">in</span>
      <span class="kr">let</span> <span class="n">n</span> <span class="k">=</span> <span class="n">Stream</span><span class="k">.</span><span class="n">next</span> <span class="n">primes</span> <span class="kr">in</span>
      <span class="kr">if</span> <span class="n">Buffer</span><span class="k">.</span><span class="n">length</span> <span class="n">buf</span> <span class="k">&gt;</span> <span class="mi">0</span> <span class="kr">then</span> <span class="n">add</span> <span class="s2">&quot; * &quot;</span><span class="k">;</span>
      <span class="n">List</span><span class="k">.</span><span class="n">iter</span> <span class="n">add</span> <span class="k">[</span><span class="n">string_of_int</span> <span class="n">n</span><span class="k">;</span> <span class="s2">&quot;^&quot;</span><span class="k">;</span> <span class="n">string_of_int</span> <span class="n">m</span><span class="k">];</span>
      <span class="n">result</span> <span class="k">:=</span> <span class="o">!</span><span class="n">result</span> <span class="k">*</span> <span class="k">(</span><span class="n">pow</span> <span class="n">n</span> <span class="n">m</span><span class="k">)</span>
   <span class="kr">done</span><span class="k">;</span>
   <span class="k">(</span><span class="n">Buffer</span><span class="k">.</span><span class="n">contents</span> <span class="n">buf</span><span class="k">,</span> <span class="o">!</span><span class="n">result</span><span class="k">)</span>

<span class="kr">let</span> <span class="n">test</span> <span class="k">()</span> <span class="k">=</span>
   <span class="kr">let</span> <span class="n">samples</span> <span class="k">=</span> <span class="k">[</span><span class="mi">9</span><span class="k">;</span> <span class="mi">81</span><span class="k">;</span> <span class="mi">230</span><span class="k">]</span> <span class="kr">in</span>
   <span class="n">List</span><span class="k">.</span><span class="n">iter</span> <span class="kr">begin</span> <span class="kr">fun</span> <span class="n">n</span> <span class="k">-&gt;</span>
      <span class="kr">let</span> <span class="n">s</span><span class="k">,</span> <span class="n">r</span> <span class="k">=</span> <span class="n">goedel</span> <span class="n">n</span> <span class="kr">in</span>
      <span class="n">Printf</span><span class="k">.</span><span class="n">printf</span> <span class="s2">&quot;goedel %d =&gt; %s =&gt; %d</span><span class="se">\n</span><span class="s2">&quot;</span> <span class="n">n</span> <span class="n">s</span> <span class="n">r</span>
   <span class="kr">end</span> <span class="n">samples</span>

<span class="kr">let</span> <span class="k">()</span> <span class="k">=</span> <span class="kr">if</span> <span class="n">not</span> <span class="o">!</span><span class="n">Sys</span><span class="k">.</span><span class="n">interactive</span> <span class="kr">then</span> <span class="n">test</span> <span class="k">()</span>
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
