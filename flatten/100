      <span class="k">|</span> <span class="n">m</span> <span class="k">-&gt;</span>
           <span class="kr">let</span> <span class="kr">rec</span> <span class="n">loop</span> <span class="n">acc</span> <span class="k">=</span> <span class="kr">function</span>
