<span class="k">private</span> <span class="k">import</span> <span class="n">std</span>.<span class="n">c</span>.<span class="n">string</span>;
<span class="k">private</span> <span class="k">import</span> <span class="n">std</span>.<span class="n">string</span>;
<span class="k">private</span> <span class="k">import</span> <span class="n">std</span>.<span class="n">stdio</span>;

<span class="kt">char</span>[] <span class="kt">cent</span><span class="n">er</span>(<span class="kt">char</span>[] <span class="n">str</span>, <span class="k">in</span><span class="n">t</span> <span class="n">n</span>) {
    <span class="kt">char</span>[] <span class="n">padchar</span> = <span class="s">&quot; &quot;</span>;
    <span class="kt">char</span>[] <span class="n">pad</span> = <span class="n">repeat</span>(<span class="n">padchar</span>, (<span class="n">n</span> - <span class="n">str</span>.<span class="n">length</span>)/<span class="mi">2</span>);
    <span class="k">return</span> <span class="n">pad</span> ~ <span class="n">str</span> ~ <span class="n">pad</span>;
