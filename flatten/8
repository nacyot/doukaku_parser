<span class="kt">void</span> <span class="n">pyramid</span>(<span class="k">in</span><span class="n">t</span> <span class="n">n</span>) {
    <span class="k">for</span> (<span class="k">in</span><span class="n">t</span> <span class="n">i</span> = <span class="mi">1</span>; <span class="n">i</span>&lt;=<span class="n">n</span>; <span class="n">i</span>++) 
