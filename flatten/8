  <span class="n">padchar</span> <span class="o">=</span> <span class="s1">&#39;</span><span class="s"> &#39;</span>
  <span class="n">pad</span> <span class="o">=</span> <span class="nf">string.rep</span><span class="p">(</span><span class="n">padchar</span><span class="p">,</span> <span class="p">(</span><span class="n">n</span> <span class="o">-</span> <span class="nf">string.len</span><span class="p">(</span><span class="n">str</span><span class="p">))</span><span class="o">/</span><span class="mi">2</span><span class="p">)</span>
