<span class="nv">n</span><span class="o">=</span><span class="k">${</span><span class="nv">1</span><span class="k">:-</span><span class="nv">4</span><span class="k">}</span>    <span class="c"># default=4</span>
<span class="nv">block</span><span class="o">=</span><span class="s1">&#39;*&#39;</span>
<span class="nv">space</span><span class="o">=</span><span class="k">$(</span><span class="nb">printf</span> <span class="s2">&quot;%${n}s&quot;</span><span class="k">)</span>

<span class="k">for</span><span class="o">((</span>; n; n--<span class="o">))</span>; <span class="k">do</span>
<span class="k">    </span><span class="nv">space</span><span class="o">=</span><span class="k">${</span><span class="nv">space</span><span class="p">:</span><span class="nv">0</span><span class="p">:</span><span class="nv">n</span><span class="p">-1</span><span class="k">}</span>
    <span class="nb">echo</span> <span class="s2">&quot;$space$block&quot;</span>
    <span class="nv">block</span><span class="o">=</span><span class="s2">&quot;${block}**&quot;</span>
<span class="k">done</span>
</pre></div>
