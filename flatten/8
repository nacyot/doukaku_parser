<span class="nv">pyramid</span> <span class="o">::</span> <span class="n">Int</span> <span class="o">-&gt;</span> <span class="n">IO</span> <span class="p">()</span>
<span class="nv">pyramid</span> <span class="nv">n</span> <span class="o">=</span>
    <span class="nv">for</span> <span class="p">(</span><span class="nv">reverse</span> <span class="p">[</span><span class="mi">0</span><span class="o">..</span><span class="p">(</span><span class="nv">n</span><span class="o">-</span><span class="mi">1</span><span class="p">)])</span> <span class="p">(</span><span class="o">\</span><span class="nv">y</span> <span class="o">-&gt;</span>
        <span class="nv">for</span> <span class="p">[</span><span class="mi">0</span><span class="o">..</span><span class="p">(</span><span class="nv">n</span><span class="o">-</span><span class="mi">1</span><span class="p">)</span><span class="o">*</span><span class="mi">2</span><span class="p">]</span> <span class="p">(</span><span class="o">\</span><span class="nv">x</span> <span class="o">-&gt;</span>
            <span class="kr">if</span> <span class="nv">y</span> <span class="o">&lt;=</span> <span class="nv">x</span> <span class="o">&amp;&amp;</span> <span class="nv">x</span> <span class="o">&lt;=</span> <span class="p">(</span><span class="nv">n</span><span class="o">-</span><span class="mi">1</span><span class="p">)</span><span class="o">*</span><span class="mi">2</span>  <span class="o">-</span> <span class="nv">y</span>
               <span class="kr">then</span> <span class="nv">putChar</span> <span class="sc">&#39;*&#39;</span>
               <span class="kr">else</span> <span class="nv">putChar</span> <span class="sc">&#39; &#39;</span>
        <span class="p">)</span>
     <span class="o">&gt;&gt;</span> <span class="nv">putChar</span> <span class="sc">&#39;\n&#39;</span>
    <span class="p">)</span>
 <span class="o">&gt;&gt;</span> <span class="nv">return</span> <span class="p">()</span>
