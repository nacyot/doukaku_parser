dnl 
define(`forloop&#39;,
`pushdef(`$1&#39;, `$2&#39;)dnl
_forloop(`$1&#39;, `$2&#39;, `$3&#39;, `$4&#39;)popdef(`$1&#39;)&#39;)dnl
define(`_forloop&#39;,dnl
`$4`&#39;ifelse($1, `$3&#39;, ,dnl
`define(`$1&#39;, incr($1))_forloop(`$1&#39;, `$2&#39;, `$3&#39;, `$4&#39;)&#39;)&#39;)dnl
define(`pyramid&#39;, dnl
`ifelse($1, 0, , $#, 0, ,dnl
`forloop(`i&#39;, 1, `$1&#39;,`forloop(`sp&#39;, 0, eval(`$1&#39; - i),dnl
`ifelse(`sp&#39;, 0, ,` &#39;)&#39;)forloop(`as&#39;, 1, eval(i * 2 - 1), `*&#39;)
&#39;)&#39;)&#39;)dnl
</pre></div>
</td></tr></table>
