	<a href="/web/20100606060645/http://ja.doukaku.org/comment/9289/">#9289</a>(<script language="JavaScript">print_time(new Date("2009/07/05 20:49 GMT"));</script><noscript>2009/07/05 20:49 GMT</noscript>)
	
	
	
	[<a href="/web/20100606060645/http://ja.doukaku.org/lang/php/">
	  PHP
	</a>]
	
	
	
	&nbsp;

<span class="rating">評価<span style="background-color: #ffffff;">0/0=0.00</span></span>
