	<a href="/web/20100606060645/http://ja.doukaku.org/comment/2111/">#2111</a>(<script language="JavaScript">print_time(new Date("2007/08/13 13:51 GMT"));</script><noscript>2007/08/13 13:51 GMT</noscript>)
	
	
	
	[<a href="/web/20100606060645/http://ja.doukaku.org/lang/c/">
