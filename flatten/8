<span class="k">function</span> <span class="nf">py</span><span class="p">(</span><span class="nv">$n</span><span class="p">,</span><span class="nv">$m</span><span class="o">=</span><span class="s2">&quot;&quot;</span><span class="p">,</span><span class="nv">$result</span><span class="o">=</span><span class="k">array</span><span class="p">()){</span>
	<span class="k">if</span><span class="p">(</span><span class="nv">$m</span><span class="o">===</span><span class="m">0</span><span class="p">)</span> <span class="k">return</span> <span class="nf">implode</span><span class="p">(</span><span class="s2">&quot;</span><span class="se">\n</span><span class="s2">&quot;</span><span class="p">,</span><span class="nf">array_reverse</span><span class="p">(</span><span class="nv">$result</span><span class="p">));</span>
