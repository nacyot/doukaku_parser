15</pre></td><td class="code"><div class="highlight"><pre><span class="k">program</span> <span class="n">Hoge</span><span class="o">;</span>

<span class="cm">{$APPTYPE CONSOLE}</span>

<span class="k">procedure</span> <span class="nf">Pyramid</span><span class="p">(</span><span class="n">n</span><span class="o">:</span> <span class="kt">Integer</span><span class="p">)</span><span class="o">;</span>
<span class="k">var</span>
    <span class="n">i</span><span class="o">:</span> <span class="kt">Integer</span><span class="o">;</span>
<span class="k">begin</span>
	<span class="k">for</span> <span class="n">i</span> <span class="o">:=</span> <span class="mi">1</span> <span class="k">to</span> <span class="n">n</span> <span class="k">do</span>
		<span class="nb">Writeln</span><span class="p">(</span><span class="nb">StringOfChar</span><span class="p">(</span><span class="s">&#39; &#39;</span><span class="o">,</span> <span class="n">n</span><span class="o">-</span><span class="n">i</span><span class="p">)</span><span class="o">,</span> <span class="nb">StringOfChar</span><span class="p">(</span><span class="s">&#39;*&#39;</span><span class="o">,</span> <span class="n">i</span><span class="o">*</span><span class="mi">2</span><span class="o">-</span><span class="mi">1</span><span class="p">))</span><span class="o">;</span>
<span class="k">end</span><span class="o">;</span>

<span class="k">begin</span>
	<span class="n">Pyramid</span><span class="p">(</span><span class="mi">5</span><span class="p">)</span><span class="o">;</span>
<span class="k">end</span><span class="o">.</span>
</pre></div>
</td></tr></table>
    </div>
	

  
