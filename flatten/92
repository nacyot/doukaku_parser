15</pre></td><td class="code"><div class="highlight"><pre>#include &lt;cstdlib&gt;
#include &lt;iostream&gt;
#include &lt;curlpp/cURLpp.hpp&gt;

using namespace std;

int main ( int argc, char *argv[] ){
   if( argc &lt; 2 ){
      return EXIT_FAILURE;
   }
   string escaped, unescaped;
   cout &lt;&lt; (escaped = cURLpp::escape( argv[1] )) &lt;&lt; endl;
   cout &lt;&lt; (unescaped = cURLpp::unescape( escaped )) &lt;&lt; endl;
   return EXIT_SUCCESS;
}
</pre></div>
</td></tr></table>
