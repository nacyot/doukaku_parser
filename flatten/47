   <span class="k">for</span> <span class="k">my</span> <span class="nv">$num</span> <span class="p">(</span><span class="nv">@sublist</span><span class="p">){</span><span class="nb">printf</span> <span class="s">&quot;%3d&quot;</span><span class="p">,</span> <span class="nv">$num</span><span class="p">;};</span>
