<span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">show-bingo</span> <span class="nv">lis</span><span class="p">)</span>
  <span class="p">(</span><span class="nf">do-ec</span> <span class="p">(</span><span class="nf">:</span> <span class="nv">row</span> <span class="p">(</span><span class="nf">slices</span> <span class="p">(</span><span class="nf">zip</span> <span class="p">(</span><span class="nf">iota</span> <span class="p">(</span><span class="nb">length </span><span class="nv">lis</span><span class="p">)</span> <span class="mi">1</span><span class="p">)</span> <span class="nv">lis</span><span class="p">)</span> <span class="mi">10</span><span class="p">))</span>
