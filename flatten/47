<span class="k">sub </span><span class="nf">bingo</span> <span class="p">{</span><span class="n">p</span><span class="p">(</span><span class="n">shuffle</span><span class="p">(</span><span class="mi">1</span><span class="o">..</span><span class="nv">$_</span><span class="p">[</span><span class="mi">0</span><span class="p">]));}</span>

<span class="k">sub </span><span class="nf">p</span> <span class="p">{</span>
 <span class="k">my</span> <span class="nv">$i</span> <span class="o">=</span> <span class="mi">1</span><span class="p">;</span>
