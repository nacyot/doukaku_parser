<span class="c"># read LINE</span>
<span class="c"># echo &quot;$LINE&quot; | grep -v &quot;^#&quot;</span>

<span class="k">while </span><span class="nb">read</span> -r; <span class="k">do</span>
<span class="k">    </span><span class="nb">echo</span> <span class="s2">&quot;$REPLY&quot;</span> | grep -v <span class="s2">&quot;^#&quot;</span>
