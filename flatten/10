	<a href="/web/20101123175840/http://ja.doukaku.org/comment/587/">#587</a>(<script language="JavaScript">print_time(new Date("2007/07/11 16:01 GMT"));</script><noscript>2007/07/11 16:01 GMT</noscript>)
	
	
	
