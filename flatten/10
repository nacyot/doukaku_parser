  <span class="n">out_f</span> <span class="o">=</span> <span class="nf">io.output</span><span class="p">(</span><span class="n">out_filename</span><span class="p">)</span>
  <span class="k">for</span> <span class="n">line</span> <span class="k">in</span> <span class="nf">io.lines</span><span class="p">(</span><span class="n">in_filename</span><span class="p">)</span> <span class="k">do</span>
    <span class="k">if</span> <span class="nf">string.sub</span><span class="p">(</span><span class="n">line</span><span class="p">,</span> <span class="mi">1</span><span class="p">,</span> <span class="mi">1</span><span class="p">)</span> <span class="o">~=</span> <span class="s2">&quot;</span><span class="s">#&quot;</span> <span class="k">then</span>
      <span class="n">out_f</span><span class="p">:</span><span class="n">write</span><span class="p">(</span><span class="n">line</span><span class="p">,</span> <span class="s2">&quot;</span><span class="se">\n</span><span class="s">&quot;</span><span class="p">)</span>
    <span class="k">end</span>
  <span class="k">end</span>
  <span class="n">out_f</span><span class="p">:</span><span class="n">close</span><span class="p">()</span>
<span class="k">end</span>

<span class="n">remove_comments</span><span class="p">(</span><span class="s2">&quot;</span><span class="s">10.input&quot;</span><span class="p">,</span> <span class="s2">&quot;</span><span class="s">10.output&quot;</span><span class="p">)</span>
