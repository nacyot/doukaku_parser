 8
 9
10
11</pre></td><td class="code"><div class="highlight"><pre><span class="kr">let</span> <span class="n">rm_line</span> <span class="k">?(</span><span class="n">c</span><span class="k">=</span><span class="sc">&#39;#&#39;</span><span class="k">)</span> <span class="n">fname</span> <span class="k">=</span>
  <span class="kr">let</span> <span class="kr">rec</span> <span class="n">loop</span> <span class="n">r</span> <span class="n">w</span> <span class="k">=</span>
    <span class="kr">try</span>
      <span class="kr">let</span> <span class="n">l</span> <span class="k">=</span> <span class="n">input_line</span> <span class="n">r</span> <span class="o">^</span> <span class="s2">&quot;</span><span class="se">\n</span><span class="s2">&quot;</span> <span class="kr">in</span>
      <span class="kr">if</span> <span class="k">(</span><span class="n">l</span><span class="k">.[</span><span class="mi">0</span><span class="k">]</span> <span class="k">&lt;&gt;</span> <span class="n">c</span><span class="k">)</span> <span class="kr">then</span> <span class="k">(</span><span class="n">output_string</span> <span class="n">w</span> <span class="n">l</span><span class="k">);</span>
      <span class="n">loop</span> <span class="n">r</span> <span class="n">w</span>
    <span class="kr">with</span> <span class="n">err</span> <span class="k">-&gt;</span> 
      <span class="n">close_in</span> <span class="n">r</span><span class="k">;</span> <span class="n">close_out</span> <span class="n">w</span><span class="k">;</span>
      <span class="kr">if</span> <span class="n">err</span> <span class="k">=</span> <span class="n">End_of_file</span>
      <span class="kr">then</span> <span class="n">Sys</span><span class="k">.</span><span class="n">rename</span> <span class="k">(</span><span class="n">fname</span><span class="o">^</span><span class="s2">&quot;.tmp&quot;</span><span class="k">)</span> <span class="n">fname</span><span class="k">;</span>
  <span class="kr">in</span> <span class="n">loop</span> <span class="k">(</span><span class="n">open_in</span> <span class="n">fname</span><span class="k">)</span> <span class="k">(</span><span class="n">open_out</span> <span class="k">(</span><span class="n">fname</span><span class="o">^</span><span class="s2">&quot;.tmp&quot;</span><span class="k">));;</span>
</pre></div>
</td></tr></table>
    </div>
	

  
