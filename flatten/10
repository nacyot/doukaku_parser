  <span class="kr">let</span> <span class="n">line</span> <span class="k">=</span>
    <span class="kr">try</span> 
      <span class="n">Some</span> <span class="k">(</span><span class="n">input_line</span> <span class="n">file</span><span class="k">)</span>
