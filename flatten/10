    <span class="kt">int</span> <span class="n">c</span><span class="p">,</span> <span class="n">pc</span><span class="p">;</span>
    <span class="k">for</span> <span class="p">(</span><span class="n">pc</span> <span class="o">=</span> <span class="sc">&#39;\n&#39;</span><span class="p">;</span> <span class="p">(</span><span class="n">c</span> <span class="o">=</span> <span class="n">getchar</span><span class="p">())</span> <span class="o">!=</span> <span class="n">EOF</span><span class="p">;</span> <span class="n">pc</span> <span class="o">=</span> <span class="n">c</span><span class="p">)</span>
