    <span class="nx">contents</span> <span class="o">=</span> <span class="s2">&quot;&quot;</span><span class="o">;</span>
    <span class="k">if</span><span class="p">(</span> <span class="nx">objFSO</span><span class="p">.</span><span class="nx">FileExists</span><span class="p">(</span> <span class="nx">ifname</span> <span class="p">)</span> <span class="p">)</span> <span class="p">{</span>
