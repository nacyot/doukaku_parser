<span class="kr">let</span> <span class="n">out_file</span> <span class="k">=</span> <span class="n">open_out</span> <span class="s2">&quot;./newhellow.txt&quot;</span><span class="k">;;</span>
<span class="n">output_string</span> <span class="n">out_file</span> <span class="k">(</span><span class="n">work</span> <span class="k">(</span><span class="n">in_file</span><span class="k">,</span><span class="s2">&quot;&quot;</span><span class="k">));;</span>
