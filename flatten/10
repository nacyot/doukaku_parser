	<a href="/web/20101123175840/http://ja.doukaku.org/comment/412/">#412</a>(<script language="JavaScript">print_time(new Date("2007/07/10 00:01 GMT"));</script><noscript>2007/07/10 00:01 GMT</noscript>)
	
	
	
