<span class="nv">isMultipleOf</span> <span class="mi">0</span> <span class="nv">d</span> <span class="o">=</span> <span class="n">True</span>
<span class="nv">isMultipleOf</span> <span class="nv">n</span> <span class="nv">d</span> 
  <span class="o">|</span> <span class="nv">n</span> <span class="o">&gt;</span> <span class="mi">0</span>        <span class="o">=</span> <span class="nv">isMultipleOf</span> <span class="p">(</span><span class="nv">n</span><span class="o">-</span><span class="nv">d</span><span class="p">)</span> <span class="nv">d</span>
  <span class="o">|</span> <span class="nv">otherwise</span>    <span class="o">=</span> <span class="n">False</span> 

<span class="nv">isLeapYear</span> <span class="nv">year</span> <span class="o">=</span>  <span class="nv">year</span> <span class="p">`</span><span class="nv">isMultipleOf</span><span class="p">`</span> <span class="mi">400</span> <span class="o">||</span>
                  <span class="p">(</span><span class="nv">year</span> <span class="p">`</span><span class="nv">isMultipleOf</span><span class="p">`</span> <span class="mi">4</span>   <span class="o">&amp;&amp;</span> 
                   <span class="nv">not</span> <span class="p">(</span><span class="nv">year</span> <span class="p">`</span><span class="nv">isMultipleOf</span><span class="p">`</span> <span class="mi">100</span><span class="p">))</span>
</pre></div>
