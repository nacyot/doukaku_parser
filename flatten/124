<span class="k">use</span> <span class="n">strict</span><span class="p">;</span>
<span class="k">my</span> <span class="nv">$flag</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
<span class="k">if</span><span class="p">(</span><span class="nv">$#ARGV</span><span class="p">){</span>
    <span class="k">print</span> <span class="s">&quot;引数を一つ指定してください&quot;</span><span class="p">;</span>
    <span class="nb">exit</span><span class="p">;</span>
<span class="p">}</span>
<span class="k">while</span><span class="p">(</span><span class="o">&lt;</span><span class="nv">@ARGV</span><span class="o">&gt;</span><span class="p">){</span>
    <span class="k">my</span> <span class="nv">$i</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
    <span class="k">my</span> <span class="nv">$year</span> <span class="o">=</span> <span class="nb">shift</span><span class="p">;</span>
    <span class="k">unless</span><span class="p">(</span><span class="nv">$year</span> <span class="o">=~</span><span class="sr"> /^\d{1,4}$/</span><span class="p">){</span>
        <span class="k">print</span> <span class="s">&quot;不正な引数です。&quot;</span><span class="p">;</span>
        <span class="nb">exit</span><span class="p">;</span>
    <span class="p">}</span>
    <span class="k">while</span><span class="p">(</span><span class="nv">$i</span> <span class="o">&lt;=</span> <span class="nv">$year</span><span class="p">){</span>
        <span class="nv">$i</span> <span class="o">+=</span> <span class="mi">4</span><span class="p">;</span>
        <span class="k">if</span><span class="p">(</span><span class="nv">$i</span> <span class="o">==</span> <span class="nv">$year</span><span class="p">){</span>
            <span class="nv">$i</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
            <span class="k">while</span><span class="p">(</span><span class="nv">$i</span> <span class="o">&lt;=</span> <span class="nv">$year</span><span class="p">){</span>
                <span class="nv">$i</span> <span class="o">+=</span> <span class="mi">400</span><span class="p">;</span>
                <span class="k">if</span><span class="p">(</span><span class="nv">$i</span> <span class="o">==</span> <span class="nv">$year</span><span class="p">){</span>
                    <span class="nv">$flag</span> <span class="o">=</span> <span class="mi">1</span><span class="p">;</span>
                <span class="p">}</span>
            <span class="p">}</span>
            <span class="k">unless</span><span class="p">(</span><span class="nv">$flag</span><span class="p">){</span>
                <span class="nv">$i</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
                <span class="nv">$flag</span> <span class="o">=</span> <span class="mi">1</span><span class="p">;</span>
                <span class="k">while</span><span class="p">(</span><span class="nv">$i</span> <span class="o">&lt;=</span> <span class="nv">$year</span><span class="p">){</span>
                    <span class="nv">$i</span> <span class="o">+=</span> <span class="mi">100</span><span class="p">;</span>
                    <span class="k">if</span><span class="p">(</span><span class="nv">$i</span> <span class="o">==</span> <span class="nv">$year</span><span class="p">){</span>
                        <span class="nv">$flag</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
                    <span class="p">}</span>
                <span class="p">}</span>
            <span class="p">}</span>
        <span class="p">}</span>
    <span class="p">}</span>
<span class="p">}</span>
<span class="k">if</span><span class="p">(</span><span class="nv">$flag</span><span class="p">){</span>
    <span class="k">print</span> <span class="s">&quot;閏年です！&quot;</span><span class="p">;</span>
<span class="p">}</span><span class="k">else</span><span class="p">{</span>
    <span class="k">print</span> <span class="s">&quot;残念！&quot;</span><span class="p">;</span>
<span class="p">}</span>
