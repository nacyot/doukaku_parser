20</pre></td><td class="code"><div class="highlight"><pre><span class="k">public</span> <span class="k">class</span> <span class="nc">Sample124</span> <span class="o">{</span>
    <span class="k">public</span> <span class="k">static</span> <span class="kt">boolean</span> <span class="n">isLeepYear</span><span class="o">(</span><span class="kt">int</span> <span class="n">year</span><span class="o">)</span> <span class="o">{</span>
        <span class="k">if</span> <span class="o">(!</span><span class="n">isDividedBy4</span><span class="o">(</span><span class="n">year</span><span class="o">))</span> <span class="k">return</span> <span class="kc">false</span><span class="o">;</span>
        <span class="n">String</span> <span class="n">sYear</span> <span class="o">=</span> <span class="n">String</span><span class="o">.</span><span class="na">valueOf</span><span class="o">(</span><span class="n">year</span><span class="o">);</span>
        <span class="k">if</span> <span class="o">(</span><span class="n">sYear</span><span class="o">.</span><span class="na">endsWith</span><span class="o">(</span><span class="s">&quot;00&quot;</span><span class="o">))</span> <span class="o">{</span>
            <span class="k">return</span> <span class="n">isDividedBy4</span><span class="o">(</span><span class="n">Integer</span><span class="o">.</span><span class="na">parseInt</span><span class="o">(</span><span class="n">sYear</span><span class="o">.</span><span class="na">substring</span><span class="o">(</span><span class="mi">0</span><span class="o">,</span> <span class="n">sYear</span><span class="o">.</span><span class="na">length</span><span class="o">()</span> <span class="o">-</span> <span class="mi">2</span><span class="o">)));</span>
        <span class="o">}</span>
        <span class="k">return</span> <span class="kc">true</span><span class="o">;</span>
    <span class="o">}</span>
    <span class="k">private</span> <span class="k">static</span> <span class="kt">boolean</span> <span class="n">isDividedBy4</span><span class="o">(</span><span class="kt">int</span> <span class="n">num</span><span class="o">)</span> <span class="o">{</span>
        <span class="k">return</span> <span class="o">(</span><span class="n">num</span> <span class="o">&amp;</span> <span class="mi">3</span><span class="o">)</span> <span class="o">==</span> <span class="mi">0</span><span class="o">;</span>
    <span class="o">}</span>

    <span class="k">public</span> <span class="k">static</span> <span class="kt">void</span> <span class="n">main</span><span class="o">(</span><span class="n">String</span><span class="o">[]</span> <span class="n">args</span><span class="o">)</span> <span class="o">{</span>
        <span class="n">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">&quot;1900:&quot;</span> <span class="o">+</span> <span class="n">isLeepYear</span><span class="o">(</span><span class="mi">1900</span><span class="o">));</span>
        <span class="n">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">&quot;2000:&quot;</span> <span class="o">+</span> <span class="n">isLeepYear</span><span class="o">(</span><span class="mi">2000</span><span class="o">));</span>
        <span class="n">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">&quot;2008:&quot;</span> <span class="o">+</span> <span class="n">isLeepYear</span><span class="o">(</span><span class="mi">2008</span><span class="o">));</span>
        <span class="n">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">&quot;2100:&quot;</span> <span class="o">+</span> <span class="n">isLeepYear</span><span class="o">(</span><span class="mi">2100</span><span class="o">));</span>
    <span class="o">}</span>
<span class="o">}</span>
</pre></div>
</td></tr></table>
