    <span class="o">}</span>

    <span class="k">public</span> <span class="k">static</span> <span class="kt">boolean</span> <span class="n">isDivisible</span><span class="o">(</span><span class="kt">int</span> <span class="n">aLeft</span><span class="o">,</span> <span class="kt">int</span> <span class="n">aRight</span><span class="o">)</span> <span class="o">{</span>
        <span class="k">while</span> <span class="o">(</span><span class="n">aLeft</span> <span class="o">&gt;=</span> <span class="n">aRight</span><span class="o">)</span> <span class="o">{</span>
            <span class="n">aLeft</span> <span class="o">-=</span> <span class="n">aRight</span><span class="o">;</span>
