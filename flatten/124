           <span class="k">THEN</span> <span class="s1">&#39;平年&#39;</span>
            <span class="k">ELSE</span> <span class="k">CASE</span> <span class="k">WHEN</span> <span class="n">TB_FOREHANDRED</span><span class="p">.</span><span class="n">Y</span> <span class="k">IS</span> <span class="k">NOT</span> <span class="k">NULL</span>
                      <span class="k">THEN</span> <span class="s1">&#39;閏年&#39;</span>
                      <span class="k">WHEN</span> <span class="n">TB_HANDRED</span><span class="p">.</span><span class="n">Y</span> <span class="k">IS</span> <span class="k">NOT</span> <span class="k">NULL</span>
                      <span class="k">THEN</span> <span class="s1">&#39;平年&#39;</span>
                 <span class="k">ELSE</span> <span class="s1">&#39;閏年&#39;</span>
            <span class="k">END</span>
       <span class="k">END</span> <span class="k">AS</span> <span class="k">RESULT</span> 
