	<a href="/web/20090329074204/http://ja.doukaku.org/comment/6417/">#6417</a>(<script language="JavaScript">print_time(new Date("2008/06/03 10:35 GMT"));</script><noscript>2008/06/03 10:35 GMT</noscript>)
	
	
	
