    <span class="k">public</span> <span class="k">static</span> <span class="kt">boolean</span> <span class="n">isLeapYear</span><span class="o">(</span><span class="kt">int</span> <span class="n">aYear</span><span class="o">)</span> <span class="o">{</span>
