    <span class="kr">let</span> <span class="n">intToBigInt</span> <span class="k">(</span><span class="n">x</span><span class="k">:</span><span class="n">int</span><span class="k">)</span> <span class="k">=</span> <span class="n">Math</span><span class="k">.</span><span class="n">BigInt</span><span class="k">(</span><span class="n">x</span><span class="k">)</span>
