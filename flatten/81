          <span class="p">;</span> <span class="kr">let</span> <span class="nv">tbl</span> <span class="o">=</span> <span class="nv">mktbl</span> <span class="nv">rs</span>
          <span class="p">;</span> <span class="kr">let</span> <span class="nv">ls</span>  <span class="o">=</span> <span class="nv">map</span> <span class="nv">fst</span> <span class="nv">tbl</span>
          <span class="p">;</span> <span class="nv">mapM_</span> <span class="p">(</span><span class="nv">uncurry</span> <span class="nv">printing</span><span class="p">)</span>
          <span class="o">$</span> <span class="nv">take</span> <span class="p">(</span><span class="nv">to</span><span class="o">-</span><span class="nv">from</span><span class="o">+</span><span class="mi">1</span><span class="p">)</span>
          <span class="o">$</span> <span class="nv">zip</span> <span class="p">[</span><span class="nv">from</span> <span class="o">..</span> <span class="nv">to</span><span class="p">]</span>
          <span class="o">$</span> <span class="nv">drop</span> <span class="p">(</span><span class="nv">from</span> <span class="p">`</span><span class="nv">mod</span><span class="p">`</span> <span class="nv">foldr</span> <span class="nv">lcm</span> <span class="mi">1</span> <span class="nv">ls</span><span class="p">)</span>
          <span class="o">$</span> <span class="nv">fbs</span> <span class="nv">tbl</span>
          <span class="p">}</span>

<span class="nv">printing</span> <span class="o">::</span> <span class="n">Int</span> <span class="o">-&gt;</span> <span class="n">String</span> <span class="o">-&gt;</span> <span class="n">IO</span> <span class="p">()</span>
<span class="nv">printing</span> <span class="nv">n</span> <span class="nv">s</span> <span class="o">=</span> <span class="nv">printf</span> <span class="s">&quot;%4d: &quot;</span> <span class="nv">n</span> <span class="o">&gt;&gt;</span> <span class="nv">when</span> <span class="p">(</span><span class="nv">s</span> <span class="o">==</span> <span class="s">&quot;&quot;</span><span class="p">)</span> <span class="p">(</span><span class="nv">putStr</span> <span class="s">&quot;hoge&quot;</span><span class="p">)</span> <span class="o">&gt;&gt;</span> <span class="nv">putStrLn</span> <span class="nv">s</span>
</pre></div>
</td></tr></table>
    </div>
	

  
