42</pre></td><td class="code"><div class="highlight"><pre><span class="k">static</span> <span class="k">void</span> <span class="nf">Main</span><span class="p">(</span><span class="kt">string</span><span class="p">[]</span> <span class="n">args</span><span class="p">)</span>
<span class="k">{</span>
    <span class="n">FizzBuzz</span><span class="p">(</span> <span class="m">101</span><span class="p">,</span><span class="m">120</span><span class="p">,</span> <span class="s">&quot;3&quot;</span><span class="p">,</span><span class="s">&quot;Pizz&quot;</span><span class="p">,</span><span class="s">&quot;5&quot;</span><span class="p">,</span><span class="s">&quot;Quzz&quot;</span><span class="p">,</span><span class="s">&quot;7&quot;</span><span class="p">,</span><span class="s">&quot;Razz&quot;</span> <span class="p">);</span>
    <span class="n">FizzBuzz</span><span class="p">(</span>   <span class="m">1</span><span class="p">,</span> <span class="m">20</span><span class="p">,</span> <span class="s">&quot;3&quot;</span><span class="p">,</span><span class="s">&quot;Fizz&quot;</span><span class="p">,</span><span class="s">&quot;5&quot;</span><span class="p">,</span><span class="s">&quot;Buzz&quot;</span> <span class="p">);</span>
<span class="k">}</span>

<span class="k">static</span> <span class="k">public</span> <span class="k">void</span> <span class="nf">FizzBuzz</span><span class="p">(</span><span class="kt">int</span> <span class="n">from</span><span class="p">,</span> <span class="kt">int</span> <span class="n">to</span><span class="p">,</span> <span class="k">params</span> <span class="kt">string</span><span class="p">[]</span> <span class="n">kv</span><span class="p">)</span>
<span class="k">{</span>
    <span class="k">if</span> <span class="p">(</span><span class="n">kv</span><span class="p">.</span><span class="n">Length</span> <span class="p">%</span> <span class="m">2</span> <span class="p">!=</span> <span class="m">0</span> <span class="p">||</span> <span class="n">from</span> <span class="p">&gt;</span> <span class="n">to</span> <span class="p">)</span>
        <span class="k">return</span><span class="p">;</span>

    <span class="n">Dictionary</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">,</span> <span class="kt">string</span><span class="p">&gt;</span> <span class="n">fbPair</span> <span class="p">=</span> <span class="k">new</span> <span class="n">Dictionary</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">,</span> <span class="kt">string</span><span class="p">&gt;();</span>

    <span class="k">try</span>
    <span class="k">{</span>
        <span class="k">for</span> <span class="p">(</span><span class="kt">int</span> <span class="n">i</span> <span class="p">=</span> <span class="m">0</span><span class="p">;</span> <span class="n">i</span> <span class="p">&lt;</span> <span class="n">kv</span><span class="p">.</span><span class="n">Length</span><span class="p">;</span> <span class="n">i</span> <span class="p">+=</span> <span class="m">2</span><span class="p">)</span>
        <span class="k">{</span>
            <span class="n">fbPair</span><span class="p">.</span><span class="n">Add</span><span class="p">(</span><span class="kt">int</span><span class="p">.</span><span class="n">Parse</span><span class="p">(</span><span class="n">kv</span><span class="p">[</span><span class="n">i</span><span class="p">]),</span> <span class="n">kv</span><span class="p">[</span><span class="n">i</span> <span class="p">+</span> <span class="m">1</span><span class="p">]);</span>
        <span class="k">}</span>
    <span class="k">}</span>
    <span class="k">catch</span> <span class="p">(</span><span class="n">Exception</span><span class="p">)</span>
    <span class="k">{</span>
        <span class="k">return</span><span class="p">;</span>
    <span class="k">}</span>

    <span class="k">for</span> <span class="p">(</span><span class="kt">int</span> <span class="n">i</span> <span class="p">=</span> <span class="n">from</span><span class="p">;</span> <span class="n">i</span> <span class="p">&lt;=</span> <span class="n">to</span><span class="p">;</span> <span class="p">++</span><span class="n">i</span><span class="p">)</span>
    <span class="k">{</span>
        <span class="n">var</span> <span class="n">x</span> <span class="p">=</span> <span class="n">from</span> <span class="n">y</span> <span class="k">in</span> <span class="n">fbPair</span>
                <span class="n">where</span> <span class="n">i</span> <span class="p">%</span> <span class="n">y</span><span class="p">.</span><span class="n">Key</span> <span class="p">==</span> <span class="m">0</span>
                <span class="n">orderby</span> <span class="n">y</span><span class="p">.</span><span class="n">Key</span> <span class="n">ascending</span>
                <span class="n">select</span> <span class="n">y</span><span class="p">.</span><span class="n">Value</span><span class="p">;</span>

        <span class="n">StringBuilder</span>    <span class="n">outStr</span> <span class="p">=</span> <span class="k">new</span> <span class="n">StringBuilder</span><span class="p">();</span>

        <span class="k">foreach</span><span class="p">(</span> <span class="kt">string</span> <span class="n">v</span> <span class="k">in</span> <span class="n">x</span> <span class="p">)</span>
        <span class="k">{</span>
            <span class="n">outStr</span><span class="p">.</span><span class="n">Append</span><span class="p">(</span> <span class="n">v</span> <span class="p">);</span>
        <span class="k">}</span>

        <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">(</span><span class="s">&quot;{0,&quot;</span> <span class="p">+</span> <span class="n">to</span><span class="p">.</span><span class="n">ToString</span><span class="p">().</span><span class="n">Length</span> <span class="p">+</span> <span class="s">&quot;}:{1}&quot;</span><span class="p">,</span> <span class="n">i</span><span class="p">,</span> <span class="n">outStr</span><span class="p">.</span><span class="n">Length</span> <span class="p">!=</span> <span class="m">0</span> <span class="p">?</span> <span class="n">outStr</span><span class="p">.</span><span class="n">ToString</span><span class="p">()</span> <span class="p">:</span> <span class="s">&quot;hoge&quot;</span> <span class="p">);</span>
    <span class="k">}</span>
<span class="k">}</span>
</pre></div>
</td></tr></table>
