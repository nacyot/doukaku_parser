    <span class="k">my</span> <span class="nv">%str_of</span> <span class="o">=</span> <span class="nv">@_</span><span class="p">;</span>
    <span class="k">my</span> <span class="nv">$default_str</span> <span class="o">=</span> <span class="s">&quot;hoge&quot;</span><span class="p">;</span>
    
    <span class="k">for</span> <span class="k">my</span> <span class="nv">$i</span> <span class="p">(</span><span class="nv">$start</span><span class="o">..</span><span class="nv">$end</span><span class="p">)</span> <span class="p">{</span>
        <span class="k">my</span> <span class="nv">$str</span><span class="p">;</span>
        <span class="k">for</span> <span class="k">my</span> <span class="nv">$num</span> <span class="p">(</span><span class="nb">sort</span> <span class="nb">keys</span> <span class="nv">%str_of</span><span class="p">)</span> <span class="p">{</span>
            <span class="k">if</span> <span class="p">(</span><span class="nv">$i</span> <span class="nv">%</span> <span class="err">$</span><span class="nv">num</span> <span class="o">==</span> <span class="mi">0</span><span class="p">)</span> <span class="p">{</span>
