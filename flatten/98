16</pre></td><td class="code"><div class="highlight"><pre><span class="k">using</span> <span class="nn">System</span><span class="p">;</span>
<span class="k">static</span> <span class="k">class</span> <span class="nc">Program</span> <span class="k">{</span>
    <span class="k">static</span> <span class="k">void</span> <span class="nf">Main</span><span class="p">()</span> <span class="k">{</span>
        <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">(</span><span class="n">Bin</span><span class="p">(</span><span class="m">01001001</span><span class="p">));</span>   <span class="c">// 73</span>
        <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">(</span><span class="n">Bin</span><span class="p">(</span><span class="m">01101001</span><span class="p">));</span>   <span class="c">// 105</span>
    <span class="k">}</span>
    <span class="k">static</span> <span class="kt">int</span> <span class="nf">Bin</span><span class="p">(</span><span class="kt">uint</span> <span class="n">octet</span><span class="p">)</span> <span class="k">{</span>
        <span class="kt">int</span> <span class="n">digit</span> <span class="p">=</span> <span class="m">1</span><span class="p">,</span> <span class="n">bin</span> <span class="p">=</span> <span class="m">0</span><span class="p">;</span>
        <span class="k">while</span><span class="p">(</span><span class="n">octet</span> <span class="p">!=</span> <span class="m">0</span><span class="p">)</span> <span class="k">{</span>
            <span class="k">if</span> <span class="p">((</span><span class="n">octet</span> <span class="p">%</span> <span class="m">10</span><span class="p">)</span> <span class="p">!=</span> <span class="m">0</span><span class="p">)</span> <span class="n">bin</span> <span class="p">|=</span> <span class="n">digit</span><span class="p">;</span>
            <span class="n">digit</span> <span class="p">&lt;&lt;=</span> <span class="m">1</span><span class="p">;</span>
            <span class="n">octet</span> <span class="p">/=</span> <span class="m">10</span><span class="p">;</span>
        <span class="k">}</span>
        <span class="k">return</span> <span class="n">bin</span><span class="p">;</span>
    <span class="k">}</span>
<span class="k">}</span>
</pre></div>
