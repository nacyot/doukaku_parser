26</pre></td><td class="code"><div class="highlight"><pre><span class="cm">{- binmain.hs -}</span>

<span class="kr">import</span> <span class="n">Bin</span>

<span class="nv">main</span> <span class="o">=</span> <span class="nv">print</span> <span class="o">$</span><span class="p">(</span><span class="nv">b</span> <span class="mi">01101001</span><span class="p">)</span>



<span class="cm">{- bin.hs -}</span>

<span class="kr">module</span> <span class="n">Bin</span> <span class="p">(</span><span class="nv">b</span><span class="p">)</span> <span class="kr">where</span>

<span class="kr">import</span> <span class="n">Language</span><span class="o">.</span><span class="n">Haskell</span><span class="o">.</span><span class="n">TH</span>

<span class="nv">bin</span> <span class="o">::</span> <span class="n">Integer</span> <span class="o">-&gt;</span> <span class="n">Integer</span>
<span class="nv">bin</span> <span class="mi">0</span> <span class="o">=</span> <span class="mi">0</span>
<span class="nv">bin</span> <span class="nv">x</span> <span class="o">=</span> <span class="nv">bin&#39;</span> <span class="nv">x</span> <span class="o">+</span> <span class="mi">2</span> <span class="o">*</span> <span class="nv">bin</span> <span class="p">(</span><span class="nv">div</span> <span class="nv">x</span> <span class="mi">10</span><span class="p">)</span>
    <span class="kr">where</span> <span class="nv">bin&#39;</span> <span class="nv">x</span> <span class="o">=</span> <span class="kr">if</span> <span class="nv">odd</span> <span class="nv">x</span> <span class="kr">then</span> <span class="mi">1</span> <span class="kr">else</span> <span class="mi">0</span>

<span class="nv">b</span> <span class="nv">x</span> <span class="o">=</span> <span class="nv">litE</span> <span class="o">$</span> <span class="n">IntegerL</span> <span class="o">$</span> <span class="nv">bin</span> <span class="nv">x</span>

<span class="cm">{-</span>
<span class="cm">&gt;&gt; ghc --make -fth binmain.hs </span>
<span class="cm">&gt;&gt; ./binmain</span>
<span class="cm">105</span>
<span class="cm">-</span>
</pre></div>
