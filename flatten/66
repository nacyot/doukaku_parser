<span class="cp">#include &lt;string.h&gt;</span>
<span class="cp">#define BLOCK_SIZE 1024</span>

<span class="kt">void</span> <span class="n">read_line</span><span class="p">(</span><span class="kt">char</span> <span class="o">**</span><span class="n">out</span><span class="p">){</span>
