<span class="k">using</span> <span class="nn">System.Collections.Generic</span><span class="p">;</span>

<span class="k">class</span> <span class="nc">Matrix</span> <span class="k">{</span>
    <span class="k">static</span> <span class="k">public</span> <span class="kt">float</span> <span class="nf">Det</span><span class="p">(</span><span class="kt">float</span><span class="p">[,]</span> <span class="n">data</span><span class="p">)</span> <span class="k">{</span>
