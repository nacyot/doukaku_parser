            <span class="k">default</span><span class="p">:</span>
                <span class="kt">int</span> <span class="n">sign</span> <span class="p">=</span> <span class="m">1</span><span class="p">;</span>
                <span class="kt">float</span> <span class="n">ret</span> <span class="p">=</span> <span class="m">0</span><span class="p">;</span>
