        <span class="n">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="n">Sample218</span><span class="o">.</span><span class="na">determinant</span><span class="o">(</span><span class="k">new</span> <span class="kt">double</span><span class="o">[][]</span> <span class="o">{</span>
