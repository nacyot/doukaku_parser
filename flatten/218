        <span class="k">if</span> <span class="o">(</span><span class="n">matrix</span><span class="o">.</span><span class="na">length</span> <span class="o">==</span> <span class="n">except</span><span class="o">.</span><span class="na">size</span><span class="o">())</span> <span class="o">{</span>
