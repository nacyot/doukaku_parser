<span class="k">using</span> <span class="nn">MeCab</span><span class="p">;</span>

<span class="c">//libmecab.libだけだと色々と大変そうなので、</span>
<span class="c">//配布されてるライブラリを使いました</span>
<span class="c">//MeCabSharp.dll</span>
<span class="c">//http://mf2.dotpp.net/software/mecabsharp.xml</span>

<span class="k">class</span> <span class="nc">Program</span>
<span class="k">{</span>
    <span class="k">static</span> <span class="k">void</span> <span class="nf">Main</span><span class="p">(</span><span class="kt">string</span><span class="p">[]</span> <span class="n">args</span><span class="p">)</span>
    <span class="k">{</span>
        <span class="n">Tagger</span> <span class="n">mecab</span> <span class="p">=</span> <span class="k">new</span> <span class="n">Tagger</span><span class="p">();</span>
        <span class="kt">string</span> <span class="n">result</span> <span class="p">=</span> <span class="n">mecab</span><span class="p">.</span><span class="n">parse</span><span class="p">(</span>
            <span class="s">&quot;「どう書く？org」へようこそ！ このサイトは出されたお題をいかに解くか競い合う、&quot;</span> <span class="p">+</span>
            <span class="s">&quot; プログラマのためのコロシアムです。 投稿を試してみたい方はテスト、&quot;</span> <span class="p">+</span>
            <span class="s">&quot; とりあえず眺めてみたい方は言語の一覧 がおすすめです。&quot;</span>
        <span class="p">);</span>
        <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">(</span><span class="n">result</span><span class="p">);</span>
    <span class="k">}</span>
<span class="k">}</span>
</pre></div>
