          <span class="nv">print</span> <span class="o">.</span> <span class="nv">snd</span> <span class="o">$</span> <span class="nv">foldl</span> <span class="nv">f</span> <span class="p">(</span><span class="mi">1000</span><span class="o">-</span><span class="nv">n</span><span class="p">,</span> <span class="mi">0</span><span class="p">)</span> <span class="p">[</span><span class="mi">500</span><span class="p">,</span> <span class="mi">100</span><span class="p">,</span> <span class="mi">50</span><span class="p">,</span> <span class="mi">10</span><span class="p">,</span> <span class="mi">5</span><span class="p">,</span> <span class="mi">1</span><span class="p">]</span>
