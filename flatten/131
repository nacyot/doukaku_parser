          <span class="err">[</span><span class="p">(</span><span class="nb">&gt;= </span><span class="nv">t</span> <span class="mi">10</span><span class="p">)</span>  <span class="p">(</span><span class="nf">n-o-c</span> <span class="p">(</span><span class="nb">- </span><span class="nv">t</span> <span class="mi">10</span><span class="p">)</span> <span class="p">(</span><span class="nb">+ </span><span class="nv">c</span> <span class="mi">1</span><span class="p">))</span><span class="err">]</span>
