  def main(args: Array[String]) = {
    Console.setIn(new java.io.FileReader(args(0))) 
    Console.setOut(new java.io.FileOutputStream(args(1)));
    var sum = 1000 - Console.readInt
        
    val coins = Array(500, 100, 50, 10, 5, 1)
    var count = 0;
    for(c &lt;- coins) {
      count += sum / c
      sum %= c
    }
    println(count);
  }
}
