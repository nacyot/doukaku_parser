20
21
22</pre></td><td class="code"><div class="highlight"><pre><span class="kr">import</span> <span class="n">Prelude</span> <span class="nv">hiding</span> <span class="p">(</span><span class="nv">gcd</span><span class="p">)</span>

<span class="nv">fib</span> <span class="mi">0</span> <span class="o">=</span> <span class="p">(</span><span class="mi">0</span><span class="p">,</span><span class="mi">1</span><span class="p">)</span>
<span class="nv">fib</span> <span class="nv">n</span> <span class="o">|</span> <span class="nv">even</span> <span class="nv">n</span> <span class="o">=</span> <span class="kr">let</span> <span class="p">(</span><span class="nv">a</span><span class="p">,</span><span class="nv">b</span><span class="p">)</span> <span class="o">=</span> <span class="nv">fib</span> <span class="p">(</span><span class="nv">n</span><span class="o">-</span><span class="mi">1</span><span class="p">)</span>         <span class="kr">in</span> <span class="p">(</span><span class="nv">b</span><span class="p">,</span> <span class="nv">a</span><span class="o">+</span><span class="nv">b</span><span class="p">)</span>             
      <span class="o">|</span> <span class="nv">odd</span>  <span class="nv">n</span> <span class="o">=</span> <span class="kr">let</span> <span class="p">(</span><span class="nv">a</span><span class="p">,</span><span class="nv">b</span><span class="p">)</span> <span class="o">=</span> <span class="nv">fib</span> <span class="p">(</span><span class="nv">halve</span> <span class="p">(</span><span class="nv">n</span><span class="o">-</span><span class="mi">1</span><span class="p">))</span> <span class="kr">in</span> <span class="p">(</span><span class="nv">b</span><span class="o">*</span><span class="nv">b</span><span class="o">+</span><span class="nv">a</span><span class="o">*</span><span class="nv">a</span><span class="p">,</span> <span class="nv">b</span><span class="o">*</span><span class="nv">b</span><span class="o">+</span><span class="mi">2</span><span class="o">*</span><span class="nv">a</span><span class="o">*</span><span class="nv">b</span><span class="p">)</span>       

<span class="nv">halve</span> <span class="o">=</span> <span class="p">(`</span><span class="nv">div</span><span class="p">`</span> <span class="mi">2</span><span class="p">)</span>

<span class="nv">gcd</span> <span class="mi">1</span> <span class="kr">_</span> <span class="o">=</span> <span class="mi">1</span> 
<span class="nv">gcd</span> <span class="nv">a</span> <span class="nv">b</span> <span class="o">|</span> <span class="nv">a</span> <span class="o">==</span> <span class="nv">b</span> <span class="o">=</span> <span class="nv">a</span>
        <span class="o">|</span> <span class="nv">even</span> <span class="nv">a</span> <span class="o">=</span> <span class="kr">if</span> <span class="nv">even</span> <span class="nv">b</span> <span class="kr">then</span> <span class="mi">2</span><span class="o">*</span><span class="nv">gcd</span> <span class="p">(</span><span class="nv">halve</span> <span class="nv">a</span><span class="p">)</span> <span class="p">(</span><span class="nv">halve</span> <span class="nv">b</span><span class="p">)</span>           <span class="kr">else</span> <span class="nv">gcd</span> <span class="nv">b</span> <span class="p">(</span><span class="nv">halve</span> <span class="nv">a</span><span class="p">)</span>
        <span class="o">|</span> <span class="nv">odd</span>  <span class="nv">a</span> <span class="o">=</span> <span class="kr">if</span> <span class="nv">odd</span>  <span class="nv">b</span> <span class="kr">then</span> <span class="nv">gcd</span> <span class="p">(</span><span class="nv">min</span> <span class="nv">a</span> <span class="nv">b</span><span class="p">)</span> <span class="p">(</span><span class="nv">abs</span> <span class="p">(</span><span class="nv">halve</span> <span class="p">(</span><span class="nv">a</span> <span class="o">-</span> <span class="nv">b</span><span class="p">)))</span> <span class="kr">else</span> <span class="nv">gcd</span> <span class="nv">a</span> <span class="p">(</span><span class="nv">halve</span> <span class="nv">b</span><span class="p">)</span>
        

<span class="nv">gcd&#39;</span> <span class="mi">1</span> <span class="kr">_</span> <span class="nv">c</span> <span class="o">=</span> <span class="p">(</span><span class="mi">1</span><span class="p">,</span><span class="nv">c</span><span class="o">+</span><span class="mi">1</span><span class="p">)</span>
<span class="nv">gcd&#39;</span> <span class="nv">a</span> <span class="nv">b</span> <span class="nv">c</span> <span class="o">|</span> <span class="nv">a</span> <span class="o">==</span> <span class="nv">b</span> <span class="o">=</span> <span class="p">(</span><span class="nv">a</span><span class="p">,</span><span class="nv">c</span><span class="o">+</span><span class="mi">1</span><span class="p">)</span>
           <span class="o">|</span> <span class="nv">even</span> <span class="nv">a</span> <span class="o">=</span> <span class="kr">if</span> <span class="nv">even</span> <span class="nv">b</span> <span class="kr">then</span> <span class="nv">fstapp</span> <span class="p">(</span><span class="mi">2</span><span class="o">*</span><span class="p">)</span> <span class="p">(</span><span class="nv">gcd&#39;</span> <span class="p">(</span><span class="nv">halve</span> <span class="nv">a</span><span class="p">)</span> <span class="p">(</span><span class="nv">halve</span> <span class="nv">b</span><span class="p">)</span> <span class="p">(</span><span class="nv">c</span><span class="o">+</span><span class="mi">1</span><span class="p">))</span>
                      <span class="kr">else</span> <span class="nv">gcd&#39;</span> <span class="nv">a</span> <span class="p">(</span><span class="nv">halve</span> <span class="nv">b</span><span class="p">)</span> <span class="p">(</span><span class="nv">c</span><span class="o">+</span><span class="mi">1</span><span class="p">)</span>
           <span class="o">|</span> <span class="nv">odd</span>  <span class="nv">a</span> <span class="o">=</span> <span class="kr">if</span> <span class="nv">odd</span>  <span class="nv">b</span> <span class="kr">then</span> <span class="nv">gcd&#39;</span> <span class="p">(</span><span class="nv">min</span> <span class="nv">a</span> <span class="nv">b</span><span class="p">)</span> <span class="p">(</span><span class="nv">abs</span> <span class="p">(</span><span class="nv">halve</span> <span class="p">(</span><span class="nv">a</span> <span class="o">-</span> <span class="nv">b</span><span class="p">)))</span> <span class="p">(</span><span class="nv">c</span><span class="o">+</span><span class="mi">1</span><span class="p">)</span>
                      <span class="kr">else</span> <span class="nv">gcd&#39;</span> <span class="nv">a</span> <span class="p">(</span><span class="nv">halve</span> <span class="nv">b</span><span class="p">)</span> <span class="p">(</span><span class="nv">c</span><span class="o">+</span><span class="mi">1</span><span class="p">)</span>

<span class="nv">fstapp</span> <span class="nv">f</span> <span class="p">(</span><span class="nv">x</span><span class="p">,</span><span class="nv">y</span><span class="p">)</span> <span class="o">=</span> <span class="p">(</span><span class="nv">f</span> <span class="nv">x</span><span class="p">,</span><span class="nv">y</span><span class="p">)</span>
</pre></div>
</td></tr></table>
