31</pre></td><td class="code"><div class="highlight"><pre><span class="nb">require</span> <span class="s2">&quot;mathn&quot;</span>

<span class="k">class</span> <span class="nc">Fib</span>
  <span class="k">def</span> <span class="nf">initialize</span>
    <span class="vi">@fib_a</span><span class="o">=[</span><span class="mi">1</span><span class="p">,</span><span class="mi">1</span><span class="o">]</span>
  <span class="k">end</span>
  <span class="k">def</span> <span class="nf">[]</span><span class="p">(</span><span class="n">n</span><span class="p">)</span>
    <span class="p">(</span><span class="vi">@fib_a</span><span class="o">.</span><span class="n">size</span><span class="o">.</span><span class="n">.n</span><span class="p">)</span><span class="o">.</span><span class="n">each</span><span class="p">{</span><span class="o">|</span><span class="n">i</span><span class="o">|</span> <span class="vi">@fib_a</span><span class="o">&lt;&lt;=</span><span class="vi">@fib_a</span><span class="o">[</span><span class="n">i</span><span class="o">-</span><span class="mi">2</span><span class="o">]+</span><span class="vi">@fib_a</span><span class="o">[</span><span class="n">i</span><span class="o">-</span><span class="mi">1</span><span class="o">]</span><span class="p">}</span>  
    <span class="vi">@fib_a</span><span class="o">[</span><span class="n">n</span><span class="o">-</span><span class="mi">1</span><span class="o">]</span>
  <span class="k">end</span>
<span class="k">end</span>

<span class="k">def</span> <span class="nf">gcd</span><span class="p">(</span><span class="n">n</span><span class="p">,</span><span class="n">m</span><span class="p">)</span>
  <span class="n">c</span><span class="o">=</span><span class="mi">0</span>
  <span class="k">until</span><span class="p">(</span><span class="n">n</span><span class="o">==</span><span class="n">m</span><span class="p">)</span>
    <span class="n">n</span><span class="p">,</span><span class="n">m</span><span class="o">=</span><span class="n">m</span><span class="p">,</span><span class="n">n</span> <span class="k">if</span> <span class="n">n</span><span class="o">&lt;</span><span class="n">m</span>
    <span class="k">return</span> <span class="o">[</span><span class="mi">1</span><span class="p">,</span><span class="n">c</span><span class="o">]</span> <span class="k">if</span> <span class="n">m</span><span class="o">==</span><span class="mi">1</span>
    <span class="n">m1</span><span class="o">=</span><span class="n">m</span>
    <span class="k">while</span><span class="p">(</span><span class="n">m1</span><span class="o">&lt;</span><span class="n">n</span><span class="p">)</span>
      <span class="n">m1</span><span class="o">&lt;&lt;=</span><span class="mi">1</span>
      <span class="n">c</span><span class="o">+=</span><span class="mi">1</span>       
    <span class="k">end</span>
    <span class="n">m1</span><span class="o">&gt;&gt;=</span><span class="mi">1</span>
    <span class="n">n</span><span class="o">-=</span><span class="n">m1</span>
    <span class="n">c</span><span class="o">+=</span><span class="mi">1</span>
  <span class="k">end</span>
  <span class="o">[</span><span class="n">n</span><span class="p">,</span><span class="n">c</span><span class="o">]</span>
<span class="k">end</span>

<span class="n">fib</span><span class="o">=</span><span class="no">Fib</span><span class="o">.</span><span class="n">new</span>
<span class="nb">p</span> <span class="n">gcd</span><span class="p">(</span><span class="n">fib</span><span class="o">[</span><span class="mi">2000</span><span class="o">]</span><span class="p">,</span><span class="n">fib</span><span class="o">[</span><span class="mi">1999</span><span class="o">]</span><span class="p">)</span>
</pre></div>
