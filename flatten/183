           <span class="p">(</span><span class="k">and </span><span class="p">(</span><span class="nb">not </span><span class="p">(</span><span class="nb">= </span><span class="nv">a</span> <span class="nv">b</span><span class="p">))</span>
                <span class="p">(</span><span class="nf">every</span> <span class="p">(</span><span class="nf">cute</span> <span class="p">(</span><span class="k">if </span><span class="p">(</span><span class="nb">&lt; </span><span class="nv">a</span> <span class="nv">b</span><span class="p">)</span> <span class="nv">&lt;</span> <span class="nv">&gt;</span><span class="p">)</span> <span class="nv">a</span> <span class="nv">&lt;&gt;</span><span class="p">)</span> <span class="p">(</span><span class="nb">cddr </span><span class="nv">xs</span><span class="p">))</span>
                <span class="p">(</span><span class="nf">loop</span> <span class="p">(</span><span class="nb">cdr </span><span class="nv">xs</span><span class="p">)))))))</span>
