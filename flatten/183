31</pre></td><td class="code"><div class="highlight"><pre><span class="k">using</span> <span class="nn">System</span><span class="p">;</span>
<span class="k">using</span> <span class="nn">System.Linq</span><span class="p">;</span>

<span class="k">class</span> <span class="nc">Program</span>
<span class="k">{</span>
    <span class="k">static</span> <span class="k">void</span> <span class="nf">Main</span><span class="p">()</span>
    <span class="k">{</span>
        <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">(</span><span class="n">check</span><span class="p">(</span><span class="k">new</span><span class="p">[]</span> <span class="k">{</span> <span class="m">825</span><span class="p">,</span> <span class="m">102</span><span class="p">,</span> <span class="m">811</span><span class="p">,</span> <span class="m">140</span><span class="p">,</span> <span class="m">812</span><span class="p">,</span> <span class="m">125</span><span class="p">,</span> <span class="m">263</span> <span class="k">}</span><span class="p">));</span>
        <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">(</span><span class="n">check</span><span class="p">(</span><span class="k">new</span><span class="p">[]</span> <span class="k">{</span> <span class="m">824</span><span class="p">,</span> <span class="m">102</span><span class="p">,</span> <span class="m">811</span><span class="p">,</span> <span class="m">140</span><span class="p">,</span> <span class="m">810</span><span class="p">,</span> <span class="m">155</span><span class="p">,</span> <span class="m">263</span> <span class="k">}</span><span class="p">));</span>
    <span class="k">}</span>

    <span class="k">static</span> <span class="kt">bool</span> <span class="nf">check</span><span class="p">(</span><span class="kt">int</span><span class="p">[]</span> <span class="n">ls</span><span class="p">)</span>
    <span class="k">{</span>
        <span class="k">if</span> <span class="p">(</span><span class="n">ls</span><span class="p">.</span><span class="n">Count</span><span class="p">()</span> <span class="p">&lt;=</span> <span class="m">1</span><span class="p">)</span>
        <span class="k">{</span>
            <span class="k">return</span> <span class="k">true</span><span class="p">;</span>
        <span class="k">}</span>
        <span class="k">else</span> <span class="nf">if</span> <span class="p">(</span><span class="n">ls</span><span class="p">.</span><span class="n">First</span><span class="p">()</span> <span class="p">&lt;</span> <span class="n">ls</span><span class="p">.</span><span class="n">Skip</span><span class="p">(</span><span class="m">1</span><span class="p">).</span><span class="n">First</span><span class="p">())</span>
        <span class="k">{</span>
            <span class="k">return</span> <span class="n">ls</span><span class="p">.</span><span class="n">Skip</span><span class="p">(</span><span class="m">1</span><span class="p">).</span><span class="n">All</span><span class="p">(</span><span class="n">n</span> <span class="p">=&gt;</span> <span class="n">ls</span><span class="p">.</span><span class="n">First</span><span class="p">()</span> <span class="p">&lt;</span> <span class="n">n</span><span class="p">)</span> <span class="p">&amp;&amp;</span> <span class="n">check</span><span class="p">(</span><span class="n">ls</span><span class="p">.</span><span class="n">Skip</span><span class="p">(</span><span class="m">1</span><span class="p">).</span><span class="n">ToArray</span><span class="p">());</span>
        <span class="k">}</span>
        <span class="k">else</span> <span class="nf">if</span> <span class="p">(</span><span class="n">ls</span><span class="p">.</span><span class="n">First</span><span class="p">()</span> <span class="p">&gt;</span> <span class="n">ls</span><span class="p">.</span><span class="n">Skip</span><span class="p">(</span><span class="m">1</span><span class="p">).</span><span class="n">First</span><span class="p">())</span>
        <span class="k">{</span>
            <span class="k">return</span> <span class="n">ls</span><span class="p">.</span><span class="n">Skip</span><span class="p">(</span><span class="m">1</span><span class="p">).</span><span class="n">All</span><span class="p">(</span><span class="n">n</span> <span class="p">=&gt;</span> <span class="n">ls</span><span class="p">.</span><span class="n">First</span><span class="p">()</span> <span class="p">&gt;</span> <span class="n">n</span><span class="p">)</span> <span class="p">&amp;&amp;</span> <span class="n">check</span><span class="p">(</span><span class="n">ls</span><span class="p">.</span><span class="n">Skip</span><span class="p">(</span><span class="m">1</span><span class="p">).</span><span class="n">ToArray</span><span class="p">());</span>
        <span class="k">}</span>
        <span class="k">else</span>
        <span class="k">{</span>
            <span class="k">return</span> <span class="k">false</span><span class="p">;</span>
        <span class="k">}</span>
    <span class="k">}</span>
<span class="k">}</span>
</pre></div>
</td></tr></table>
    </div>
	

  
