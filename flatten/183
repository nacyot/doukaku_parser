<span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">main</span> <span class="nv">args</span><span class="p">)</span>
  <span class="p">(</span><span class="nf">print</span>
   <span class="p">(</span><span class="k">let </span><span class="nv">loop</span> <span class="p">((</span><span class="nf">xs</span> <span class="p">(</span><span class="nb">map </span><span class="nv">string-&gt;number</span> <span class="p">(</span><span class="nb">cdr </span><span class="nv">args</span><span class="p">))))</span>
