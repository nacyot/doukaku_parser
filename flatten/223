  <span class="k">end</span>
  <span class="k">def</span> <span class="nf">play</span>
    <span class="n">cycle</span><span class="p">(</span><span class="kp">true</span><span class="p">){</span><span class="vi">@hi</span><span class="o">.</span><span class="n">size</span> <span class="o">&lt;=</span> <span class="no">Num</span><span class="p">}</span>
    <span class="vi">@players</span><span class="o">.</span><span class="n">each</span><span class="p">{</span><span class="o">|</span><span class="nb">p</span><span class="o">|</span> <span class="nb">puts</span> <span class="nb">p</span><span class="p">}</span>
  <span class="k">end</span>
  <span class="kp">private</span>
  <span class="k">def</span> <span class="nf">cycle</span><span class="p">(</span><span class="n">play</span><span class="o">=</span><span class="kp">nil</span><span class="p">)</span>
    <span class="vi">@players</span><span class="o">.</span><span class="n">cycle</span><span class="p">{</span><span class="o">|</span><span class="nb">p</span><span class="o">|</span>
      <span class="nb">p</span><span class="o">.</span><span class="n">get</span><span class="p">(</span><span class="vi">@hi</span><span class="o">.</span><span class="n">shift</span><span class="p">)</span> <span class="k">if</span> <span class="nb">p</span><span class="o">.</span><span class="n">size</span> <span class="o">&lt;</span> <span class="no">Num</span>
      <span class="nb">p</span><span class="o">.</span><span class="n">set</span>            <span class="k">if</span> <span class="n">play</span>
      <span class="nb">p</span><span class="o">.</span><span class="n">repai</span>
      <span class="k">break</span> <span class="k">if</span> <span class="k">yield</span><span class="p">}</span>
  <span class="k">end</span>
<span class="k">end</span>
<span class="k">if</span> <span class="bp">__FILE__</span> <span class="o">==</span> <span class="vg">$0</span>
  <span class="n">m</span> <span class="o">=</span> <span class="no">Mahjang</span><span class="o">.</span><span class="n">new</span>
  <span class="n">m</span><span class="o">.</span><span class="n">play</span>
<span class="k">end</span>
</pre></div>
</td></tr></table>
    </div>
	

  
