<span class="kr">import</span> <span class="n">List</span>

<span class="kr">data</span> <span class="n">Record</span> <span class="o">=</span> <span class="n">Record</span> <span class="p">{</span><span class="nv">familyName</span><span class="o">::</span><span class="n">String</span><span class="p">,</span>
                      <span class="nv">personalName</span><span class="o">::</span><span class="n">String</span><span class="p">,</span>
                      <span class="nv">sex</span><span class="o">::</span><span class="n">Char</span><span class="p">,</span>
                      <span class="nv">age</span><span class="o">::</span><span class="n">Integer</span><span class="p">,</span>
                      <span class="nv">year</span><span class="o">::</span><span class="n">Integer</span><span class="p">,</span>
                      <span class="nv">month</span><span class="o">::</span><span class="n">Integer</span><span class="p">,</span>
                      <span class="nv">dayRecord</span><span class="o">::</span><span class="p">[</span><span class="n">DayRecord</span><span class="p">]}</span>

<span class="kr">data</span> <span class="n">DayRecord</span> <span class="o">=</span> <span class="n">DayRecord</span> <span class="p">{</span><span class="nv">day</span><span class="o">::</span><span class="n">Integer</span><span class="p">,</span>
                            <span class="nv">breakfast</span><span class="o">::</span><span class="n">String</span><span class="p">,</span>
                            <span class="nv">lunch</span><span class="o">::</span><span class="n">String</span><span class="p">,</span>
                            <span class="nv">dinner</span><span class="o">::</span><span class="n">String</span><span class="p">}</span>

<span class="nv">formatOfDayRecord</span> <span class="o">=</span>  <span class="p">[</span><span class="mi">2</span><span class="p">,</span><span class="mi">500</span><span class="p">,</span><span class="mi">500</span><span class="p">,</span><span class="mi">500</span><span class="p">]</span>
<span class="nv">formatOfRecordHead</span> <span class="o">=</span>  <span class="p">[</span><span class="mi">12</span><span class="p">,</span><span class="mi">12</span><span class="p">,</span><span class="mi">1</span><span class="p">,</span><span class="mi">3</span><span class="p">,</span><span class="mi">4</span><span class="p">,</span><span class="mi">2</span><span class="p">]</span>

<span class="nv">sizeOfDayRecord</span> <span class="o">=</span> <span class="nv">sum</span> <span class="nv">formatOfDayRecord</span>
<span class="nv">sizeOfRecord</span>    <span class="o">=</span> <span class="nv">sum</span> <span class="nv">formatOfRecordHead</span> <span class="o">+</span> <span class="mi">31</span> <span class="o">*</span> <span class="nv">sizeOfDayRecord</span> 
<span class="nv">sizeOfDatabase</span>  <span class="o">=</span> <span class="mi">500</span> <span class="o">*</span> <span class="nv">sizeOfRecord</span>


<span class="nv">mkDatabase</span> <span class="o">::</span> <span class="n">String</span> <span class="o">-&gt;</span> <span class="p">[</span><span class="n">Record</span><span class="p">]</span>
<span class="nv">mkDatabase</span> <span class="nv">str</span> <span class="o">=</span> <span class="nv">map</span> <span class="nv">mkRecord</span> <span class="p">(</span><span class="nv">every</span> <span class="nv">sizeOfRecord</span> <span class="nv">str</span><span class="p">)</span>

<span class="nv">mkRecord</span>   <span class="o">::</span> <span class="n">String</span> <span class="o">-&gt;</span> <span class="n">Record</span>
<span class="nv">mkRecord</span> <span class="nv">str</span> <span class="o">=</span> <span class="kr">let</span> <span class="p">(</span><span class="nv">str&#39;</span><span class="p">,</span><span class="nv">recordHead</span><span class="p">)</span> <span class="o">=</span> <span class="nv">formatSplit</span> <span class="nv">str</span> <span class="nv">formatOfRecordHead</span>
               <span class="kr">in</span>  <span class="p">(</span><span class="n">Record</span> <span class="p">(</span><span class="nv">recordHead</span> <span class="o">!!</span> <span class="mi">0</span><span class="p">)</span>       <span class="c1">--family name</span>
                           <span class="p">(</span><span class="nv">recordHead</span> <span class="o">!!</span> <span class="mi">1</span><span class="p">)</span>       <span class="c1">--personal name</span>
                           <span class="p">(</span><span class="nv">head</span> <span class="p">(</span><span class="nv">recordHead</span> <span class="o">!!</span> <span class="mi">2</span><span class="p">))</span>  <span class="c1">--sex</span>
                           <span class="p">(</span><span class="nv">read</span> <span class="p">(</span><span class="nv">recordHead</span> <span class="o">!!</span> <span class="mi">3</span><span class="p">))</span>  <span class="c1">--age</span>
                           <span class="p">(</span><span class="nv">read</span> <span class="p">(</span><span class="nv">recordHead</span> <span class="o">!!</span> <span class="mi">4</span><span class="p">))</span>  <span class="c1">--year</span>
                           <span class="p">(</span><span class="nv">read</span> <span class="p">(</span><span class="nv">recordHead</span> <span class="o">!!</span> <span class="mi">5</span><span class="p">))</span>  <span class="c1">--month</span>
                           <span class="p">(</span><span class="nv">mkDayRecords</span> <span class="nv">str&#39;</span><span class="p">))</span> <span class="c1">--dayRecord</span>

<span class="nv">mkDayRecords</span> <span class="o">::</span> <span class="n">String</span> <span class="o">-&gt;</span> <span class="p">[</span><span class="n">DayRecord</span><span class="p">]</span>
<span class="nv">mkDayRecords</span> <span class="nv">str</span> <span class="o">=</span> <span class="nv">map</span> <span class="nv">mkDayRecord</span> <span class="p">(</span><span class="nv">every</span> <span class="nv">sizeOfDayRecord</span> <span class="nv">str</span><span class="p">)</span>

<span class="nv">mkDayRecord</span>  <span class="o">::</span> <span class="n">String</span> <span class="o">-&gt;</span> <span class="n">DayRecord</span>
<span class="nv">mkDayRecord</span> <span class="nv">str</span> <span class="o">=</span> <span class="kr">let</span> <span class="p">(</span><span class="kr">_</span><span class="p">,</span><span class="nv">dayRecord</span><span class="p">)</span> <span class="o">=</span> <span class="nv">formatSplit</span> <span class="nv">str</span> <span class="nv">formatOfDayRecord</span>
                  <span class="kr">in</span> <span class="p">(</span><span class="n">DayRecord</span> <span class="p">(</span><span class="nv">read</span> <span class="p">(</span><span class="nv">dayRecord</span> <span class="o">!!</span> <span class="mi">0</span><span class="p">))</span> <span class="c1">--day</span>
                                <span class="p">(</span><span class="nv">dayRecord</span> <span class="o">!!</span> <span class="mi">1</span><span class="p">)</span>  <span class="c1">--breakfast</span>
                                <span class="p">(</span><span class="nv">dayRecord</span> <span class="o">!!</span> <span class="mi">2</span><span class="p">)</span>  <span class="c1">--lunch</span>
                                <span class="p">(</span><span class="nv">dayRecord</span> <span class="o">!!</span> <span class="mi">3</span><span class="p">))</span> <span class="c1">--dinner</span>



<span class="nv">formatSplit</span> <span class="nv">content</span> <span class="nv">format</span> <span class="o">=</span> <span class="nv">mapAccumL</span> <span class="nv">f</span> <span class="nv">content</span> <span class="nv">format</span>
   <span class="kr">where</span> <span class="nv">f</span> <span class="nv">content</span> <span class="nv">size</span>  <span class="o">=</span> <span class="kr">let</span> <span class="p">(</span><span class="nv">a</span><span class="p">,</span><span class="nv">b</span><span class="p">)</span> <span class="o">=</span> <span class="nv">splitAt</span> <span class="nv">size</span> <span class="nv">content</span>
                           <span class="kr">in</span>  <span class="p">(</span><span class="nv">b</span><span class="p">,</span><span class="nv">a</span><span class="p">)</span>

<span class="nv">every</span> <span class="nv">n</span> <span class="nv">xs</span> <span class="o">=</span> <span class="nv">unfoldr</span> <span class="nv">f</span> <span class="nv">xs</span> 
    <span class="kr">where</span>  
      <span class="nv">f</span> <span class="p">[]</span> <span class="o">=</span> <span class="n">Nothing</span>
      <span class="nv">f</span> <span class="nv">cs</span> <span class="o">=</span> <span class="n">Just</span> <span class="p">(</span><span class="nv">splitAt</span> <span class="nv">n</span> <span class="nv">cs</span><span class="p">)</span>


<span class="nv">filePath</span> <span class="o">=</span> <span class="s">&quot;&quot;</span>

<span class="nv">main</span> <span class="o">=</span> <span class="nv">bracket</span> <span class="p">(</span><span class="nv">openFile</span> <span class="nv">filePath</span> <span class="n">ReadMode</span><span class="p">)</span>
               <span class="nv">hClose</span>
               <span class="p">(</span><span class="o">\</span><span class="nv">h</span><span class="o">-&gt;</span> 
                 <span class="kr">do</span> <span class="nv">contents</span> <span class="o">&lt;-</span> <span class="nv">hGetContents</span> <span class="nv">h</span>
                    <span class="kr">let</span> <span class="nv">database</span> <span class="o">=</span> <span class="nv">mkDatabase</span> <span class="o">$</span> <span class="nv">take</span> <span class="nv">sizeOfDatabase</span> <span class="nv">contents</span>
                    <span class="nv">putStrLn</span> <span class="s">&quot;end&quot;</span>
               <span class="p">)</span>
</pre></div>
