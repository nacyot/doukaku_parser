    <span class="n">rec_struct</span> <span class="o">=</span> <span class="no">Struct</span><span class="o">.</span><span class="n">new</span><span class="p">(</span><span class="ss">:family_name</span><span class="p">,</span> <span class="ss">:last_name</span><span class="p">,</span> <span class="ss">:sex</span><span class="p">,</span> <span class="ss">:age</span><span class="p">,</span>
                            <span class="ss">:year</span><span class="p">,</span> <span class="ss">:month</span><span class="p">,</span> <span class="ss">:days</span><span class="p">)</span>
