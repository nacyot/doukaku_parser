<span class="nb">declare</span> -i age

<span class="k">function </span>readRecord<span class="o">()</span> <span class="o">{</span>
    <span class="nb">local </span><span class="nv">n</span><span class="o">=</span><span class="nv">$1</span>
    <span class="nb">local </span>s d

    <span class="nb">read</span> -n <span class="m">12</span> s
    family_name<span class="o">[</span><span class="nv">$n</span><span class="o">]=</span><span class="k">${</span><span class="nv">s</span><span class="p">%%+( )</span><span class="k">}</span>

    <span class="nb">read</span> -n <span class="m">12</span> s
    first_name<span class="o">[</span><span class="nv">$n</span><span class="o">]=</span><span class="k">${</span><span class="nv">s</span><span class="p">%%+( )</span><span class="k">}</span>

    <span class="nb">read</span> -n <span class="m">1</span> sex<span class="o">[</span><span class="nv">$n</span><span class="o">]</span>
    <span class="nb">read</span> -n <span class="m">3</span> age<span class="o">[</span><span class="nv">$n</span><span class="o">]</span>
    <span class="nb">read</span> -n <span class="m">4</span> year<span class="o">[</span><span class="nv">$n</span><span class="o">]</span>
    <span class="nb">read</span> -n <span class="m">2</span> mon<span class="o">[</span><span class="nv">$n</span><span class="o">]</span>

    <span class="k">for</span> <span class="o">((</span><span class="nv">d</span> <span class="o">=</span> 0; d &lt; 31; d++<span class="o">))</span>;do
        <span class="nb">eval read</span> -n <span class="m">2</span> date_<span class="nv">$d</span><span class="o">[</span><span class="nv">$n</span><span class="o">]</span>

        <span class="nb">read</span> -n <span class="m">500</span> s
        <span class="nb">eval</span> <span class="s2">&quot;breakfast_$d[$n]=&#39;${s%%+( )}&#39;&quot;</span>

        <span class="nb">read</span> -n <span class="m">500</span> s
        <span class="nb">eval</span> <span class="s2">&quot;lunch$d[$n]=&#39;${s%%+( )}&#39;&quot;</span>

        <span class="nb">read</span> -n <span class="m">500</span> s
        <span class="nb">eval</span> <span class="s2">&quot;dinner_$d[$n]=&#39;${s%%+( )}&#39;&quot;</span>
    <span class="k">done</span>
<span class="o">}</span>

<span class="k">for</span> <span class="o">((</span><span class="nv">i</span> <span class="o">=</span> 0; i &lt; 500; i++<span class="o">))</span>;do
    readRecord <span class="nv">$i</span>
<span class="k">done</span> &lt; data.txt
</pre></div>
