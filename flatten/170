<span class="nv">$path</span> <span class="o">=</span> <span class="s2">&quot;./&quot;</span><span class="p">;</span>
<span class="nv">$file</span> <span class="o">=</span> <span class="s2">&quot;data.txt&quot;</span><span class="p">;</span>
<span class="nv">$rsz</span>  <span class="o">=</span> <span class="m">46596</span><span class="p">;</span>

<span class="nv">$ary</span><span class="p">[</span><span class="s2">&quot;SEI&quot;</span><span class="p">]</span>      <span class="o">=</span>    <span class="m">12</span><span class="p">;</span>
<span class="nv">$ary</span><span class="p">[</span><span class="s2">&quot;MEI&quot;</span><span class="p">]</span>      <span class="o">=</span>    <span class="m">12</span><span class="p">;</span>
<span class="nv">$ary</span><span class="p">[</span><span class="s2">&quot;SEIBETSU&quot;</span><span class="p">]</span> <span class="o">=</span>     <span class="m">1</span><span class="p">;</span>
<span class="nv">$ary</span><span class="p">[</span><span class="s2">&quot;NENREI&quot;</span><span class="p">]</span>   <span class="o">=</span>     <span class="m">3</span><span class="p">;</span>
<span class="nv">$ary</span><span class="p">[</span><span class="s2">&quot;NEN&quot;</span><span class="p">]</span>      <span class="o">=</span>     <span class="m">4</span><span class="p">;</span>
<span class="nv">$ary</span><span class="p">[</span><span class="s2">&quot;TSUKI&quot;</span><span class="p">]</span>    <span class="o">=</span>     <span class="m">2</span><span class="p">;</span>
<span class="nv">$ary</span><span class="p">{</span><span class="s2">&quot;DATA&quot;</span><span class="p">]</span>     <span class="o">=</span> <span class="m">46562</span><span class="p">;</span>

<span class="nv">$fp</span> <span class="o">=</span> <span class="nf">fopen</span><span class="p">(</span><span class="nv">$path</span><span class="o">.</span><span class="nv">$file</span><span class="p">,</span><span class="s2">&quot;rb&quot;</span><span class="p">);</span>
<span class="k">while</span> <span class="p">(</span> <span class="o">!</span><span class="nf">feof</span><span class="p">(</span><span class="nv">$fp</span><span class="p">)</span> <span class="p">)</span> <span class="p">{</span>
    <span class="nv">$data</span> <span class="o">=</span> <span class="k">array</span><span class="p">();</span>
    <span class="nv">$stcm</span> <span class="o">=</span> <span class="m">0</span><span class="p">;</span>
    <span class="nv">$line</span> <span class="o">=</span> <span class="nf">fread</span><span class="p">(</span><span class="nv">$fp</span><span class="p">,</span><span class="nv">$rsz</span><span class="p">);</span>
    <span class="k">foreach</span> <span class="p">(</span> <span class="nv">$ary</span> <span class="k">as</span> <span class="nv">$key</span> <span class="o">=&gt;</span> <span class="nv">$val</span> <span class="p">)</span> <span class="p">{</span>
        <span class="nv">$data</span><span class="p">[</span><span class="nv">$key</span><span class="p">]</span> <span class="o">=</span> <span class="nf">substr</span><span class="p">(</span><span class="nv">$line</span><span class="p">,</span><span class="nv">$stcm</span><span class="p">,</span><span class="nv">$val</span><span class="p">);</span>
        <span class="nv">$stcm</span> <span class="o">+=</span> <span class="nv">$val</span><span class="p">;</span>
        <span class="k">if</span> <span class="p">(</span> <span class="nv">$key</span> <span class="o">==</span> <span class="s2">&quot;DATA&quot;</span> <span class="p">)</span> <span class="p">{</span>
            <span class="k">for</span> <span class="p">(</span> <span class="nv">$i</span> <span class="o">=</span> <span class="m">0</span><span class="p">;</span> <span class="nv">$i</span> <span class="o">&lt;=</span> <span class="nv">$val</span><span class="p">;</span> <span class="nv">$i</span> <span class="o">+=</span> <span class="m">1502</span> <span class="p">)</span> <span class="p">{</span>
                <span class="nv">$day</span> <span class="o">=</span> <span class="nf">substr</span><span class="p">(</span><span class="nv">$data</span><span class="p">[</span><span class="nv">$key</span><span class="p">],</span><span class="nv">$i</span><span class="p">,</span><span class="m">2</span><span class="p">);</span>
                <span class="nv">$data</span><span class="p">[</span><span class="nv">$day</span><span class="p">][</span><span class="s2">&quot;ASA&quot;</span><span class="p">]</span>  <span class="o">=</span> <span class="nf">substr</span><span class="p">(</span><span class="nv">$data</span><span class="p">[</span><span class="nv">$key</span><span class="p">],</span><span class="nv">$i</span><span class="o">+</span><span class="m">2</span>   <span class="p">,</span><span class="m">500</span><span class="p">);</span>
                <span class="nv">$data</span><span class="p">[</span><span class="nv">$day</span><span class="p">][</span><span class="s2">&quot;HIRU&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="nf">substr</span><span class="p">(</span><span class="nv">$data</span><span class="p">[</span><span class="nv">$key</span><span class="p">],</span><span class="nv">$i</span><span class="o">+</span><span class="m">502</span> <span class="p">,</span><span class="m">500</span><span class="p">);</span>
                <span class="nv">$data</span><span class="p">[</span><span class="nv">$day</span><span class="p">][</span><span class="s2">&quot;YORU&quot;</span><span class="p">]</span> <span class="o">=</span> <span class="nf">substr</span><span class="p">(</span><span class="nv">$data</span><span class="p">[</span><span class="nv">$key</span><span class="p">],</span><span class="nv">$i</span><span class="o">+</span><span class="m">1002</span><span class="p">,</span><span class="m">500</span><span class="p">);</span>
            <span class="p">}</span>
        <span class="p">}</span>
    <span class="p">}</span>
<span class="p">}</span>
<span class="nf">fclose</span><span class="p">(</span><span class="nv">$fp</span><span class="p">);</span>
<span class="cp">?&gt;</span><span class="x"></span>
</pre></div>
</td></tr></table>
