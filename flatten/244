<span class="k">using</span> <span class="nn">System.Security.AccessControl</span><span class="p">;</span>
<span class="k">using</span> <span class="nn">System.Security.Principal</span><span class="p">;</span>

<span class="k">class</span> <span class="nc">P</span>
<span class="k">{</span>
  <span class="k">static</span> <span class="k">void</span> <span class="nf">Main</span><span class="p">(</span><span class="kt">string</span><span class="p">[]</span> <span class="n">args</span><span class="p">)</span>
  <span class="k">{</span>
    <span class="k">if</span> <span class="p">(</span><span class="n">args</span><span class="p">.</span><span class="n">Length</span> <span class="p">==</span> <span class="m">0</span><span class="p">)</span> <span class="k">return</span><span class="p">;</span>
    <span class="k">using</span> <span class="p">(</span><span class="n">Stream</span> <span class="n">s</span> <span class="p">=</span> <span class="n">File</span><span class="p">.</span><span class="n">Open</span>
      <span class="p">(</span><span class="n">args</span><span class="p">[</span><span class="m">0</span><span class="p">],</span>
      <span class="n">FileMode</span><span class="p">.</span><span class="n">CreateNew</span><span class="p">,</span>
      <span class="n">FileAccess</span><span class="p">.</span><span class="n">Write</span><span class="p">,</span>
      <span class="n">FileShare</span><span class="p">.</span><span class="n">ReadWrite</span><span class="p">))</span>
    <span class="k">{</span>
      <span class="n">FileSecurity</span> <span class="n">sec</span> <span class="p">=</span> <span class="n">File</span><span class="p">.</span><span class="n">GetAccessControl</span><span class="p">(</span><span class="n">args</span><span class="p">[</span><span class="m">0</span><span class="p">]);</span>
      <span class="n">IdentityReference</span> <span class="n">owner</span> <span class="p">=</span> <span class="n">sec</span><span class="p">.</span><span class="n">GetOwner</span><span class="p">(</span><span class="k">typeof</span><span class="p">(</span><span class="n">NTAccount</span><span class="p">));</span>
      <span class="n">sec</span><span class="p">.</span><span class="n">SetAccessRuleProtection</span><span class="p">(</span><span class="k">true</span><span class="p">,</span> <span class="k">false</span><span class="p">);</span>
      <span class="k">foreach</span> <span class="p">(</span><span class="n">AuthorizationRule</span> <span class="n">r</span>
        <span class="k">in</span> <span class="n">sec</span><span class="p">.</span><span class="n">GetAccessRules</span><span class="p">(</span><span class="k">true</span><span class="p">,</span> <span class="k">true</span><span class="p">,</span> <span class="k">typeof</span><span class="p">(</span><span class="n">NTAccount</span><span class="p">)))</span>
      <span class="k">{</span>
        <span class="n">sec</span><span class="p">.</span><span class="n">RemoveAccessRule</span><span class="p">((</span><span class="n">FileSystemAccessRule</span><span class="p">)</span><span class="n">r</span><span class="p">);</span>
      <span class="k">}</span>
      <span class="n">sec</span><span class="p">.</span><span class="n">AddAccessRule</span>
        <span class="p">(</span><span class="k">new</span> <span class="n">FileSystemAccessRule</span>
          <span class="p">(</span><span class="n">owner</span><span class="p">,</span>
          <span class="n">FileSystemRights</span><span class="p">.</span><span class="n">Read</span> <span class="p">|</span> <span class="n">FileSystemRights</span><span class="p">.</span><span class="n">Write</span><span class="p">,</span>
          <span class="n">AccessControlType</span><span class="p">.</span><span class="n">Allow</span><span class="p">));</span>
