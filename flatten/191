    <span class="k">echo</span> <span class="nv">$key</span><span class="o">.</span><span class="s1">&#39;: &#39;</span><span class="o">.</span><span class="nv">$_ENV</span><span class="p">[</span><span class="nv">$key</span><span class="p">]</span><span class="o">.</span><span class="nx">PHP_EOL</span><span class="p">;</span>
<span class="p">}</span>
</pre></div>
