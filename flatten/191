<span class="nx">stdIn</span> <span class="o">=</span> <span class="nx">WScript</span><span class="p">.</span><span class="nx">StdIn</span><span class="o">;</span>
<span class="nx">stdOut</span> <span class="o">=</span> <span class="nx">WScript</span><span class="p">.</span><span class="nx">StdOut</span><span class="o">;</span>
<span class="nx">stdOut</span><span class="p">.</span><span class="nx">WriteLine</span><span class="p">(</span><span class="s1">&#39;キー名を指定してください&#39;</span><span class="p">);</span>
<span class="nx">key</span> <span class="o">=</span> <span class="nx">stdIn</span><span class="p">.</span><span class="nx">ReadLine</span><span class="p">();</span>
<span class="nx">stdOut</span><span class="p">.</span><span class="nx">WriteLine</span><span class="p">(</span><span class="s1">&#39;%&#39;</span> <span class="o">+</span> <span class="nx">key</span> <span class="o">+</span> <span class="s1">&#39;% == &#39;</span> <span class="o">+</span> <span class="nx">env</span><span class="p">(</span><span class="nx">key</span><span class="p">));</span>
