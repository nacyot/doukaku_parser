    <span class="k">yield</span> <span class="n">x</span>
    <span class="n">x</span> <span class="o">=</span> <span class="n">succ</span><span class="p">(</span><span class="n">x</span><span class="p">,</span> <span class="n">n</span><span class="p">)</span>

