<span class="x">	function hello_world(){</span>
<span class="x">	}</span>

<span class="x">	function print_word(){</span>
<span class="x">		$str = &quot;Hello, world!&quot;;</span>
<span class="x">		return $str;</span>
<span class="x">	}</span>
<span class="x">}</span>
</pre></div>
