<span class="k">private</span> <span class="k">import</span> <span class="n">std</span>.<span class="n">stdio</span>;

<span class="k">class</span> <span class="n">Hello</span>
{
  <span class="k">this</span>()
  {
    <span class="n">writef</span>(<span class="s">&quot;Hello&quot;</span>);
  }

  <span class="kt">void</span> <span class="n">puts</span>(<span class="kt">void</span> <span class="k">delegate</span>() <span class="n">dg</span>)
  {
    <span class="n">dg</span>();
  }

  ~<span class="k">this</span>()
  {
    <span class="n">writef</span>(<span class="s">&quot; world!\n&quot;</span>);
  }
}

<span class="k">in</span><span class="n">t</span> <span class="n">main</span>()
{
  (<span class="k">new</span> <span class="n">Hello</span>()).<span class="n">puts</span>(
  {
    <span class="n">writef</span>(<span class="s">&quot;,&quot;</span>);
  });
  <span class="k">return</span> <span class="mi">0</span>;
}
</pre></div>
