  <span class="n">opmap</span><span class="p">[</span><span class="s">&#39;PRINT_NEWLINE&#39;</span><span class="p">],</span>
  <span class="n">opmap</span><span class="p">[</span><span class="s">&#39;LOAD_CONST&#39;</span><span class="p">],</span> <span class="n">consts</span><span class="o">.</span><span class="n">index</span><span class="p">(</span><span class="bp">None</span><span class="p">),</span> <span class="mi">0</span><span class="p">,</span>
