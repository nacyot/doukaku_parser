    <span class="n">stringstream</span> <span class="n">ss</span><span class="p">(</span><span class="s">&quot;&quot;</span><span class="p">);</span>
    <span class="c">// 1 is special case</span>
    <span class="n">ss</span> <span class="o">&lt;&lt;</span> <span class="s">&quot;1-&quot;</span> <span class="o">&lt;&lt;</span> <span class="n">vertex</span><span class="p">[</span><span class="mi">0</span><span class="p">]</span> <span class="o">&lt;&lt;</span> <span class="sc">&#39; &#39;</span><span class="p">;</span>

