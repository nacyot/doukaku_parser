    <span class="kt">boolean</span><span class="o">[][]</span> <span class="n">match</span><span class="o">;</span>
    <span class="kt">int</span><span class="o">[][]</span> <span class="n">pair</span><span class="o">;</span>
    <span class="kt">int</span> <span class="n">num</span><span class="o">;</span>
