        <span class="k">}</span>

        <span class="c">//各組み合わせを生成</span>
        <span class="n">List</span><span class="p">&lt;</span><span class="n">List</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">[]&gt;&gt;</span> <span class="n">leageSchedule</span> <span class="p">=</span> <span class="k">new</span> <span class="n">List</span><span class="p">&lt;</span><span class="n">List</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">[]&gt;&gt;();</span>
