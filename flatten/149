            <span class="k">}</span>
            <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">();</span>
        <span class="k">}</span>
        <span class="n">Console</span><span class="p">.</span><span class="n">ReadLine</span><span class="p">();</span>
    <span class="k">}</span>

    <span class="k">static</span> <span class="n">List</span><span class="p">&lt;</span><span class="n">List</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">[]&gt;&gt;</span> <span class="n">Leage</span><span class="p">(</span><span class="kt">int</span> <span class="n">teamCount</span><span class="p">)</span> <span class="k">{</span>
        <span class="c">//チームリスト生成</span>
        <span class="n">List</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">&gt;</span> <span class="n">teamList</span> <span class="p">=</span> <span class="k">new</span> <span class="n">List</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">&gt;();</span>
        <span class="k">for</span><span class="p">(</span><span class="kt">int</span> <span class="n">teamNumber</span> <span class="p">=</span> <span class="m">1</span><span class="p">;</span> <span class="n">teamNumber</span> <span class="p">&lt;=</span> <span class="n">teamCount</span><span class="p">;</span> <span class="n">teamNumber</span><span class="p">++)</span> <span class="k">{</span>
