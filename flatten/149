39
40</pre></td><td class="code"><div class="highlight"><pre><span class="kr">module</span> <span class="n">Main</span>
    <span class="kr">where</span>

<span class="nv">n</span> <span class="o">=</span> <span class="mi">6</span>

<span class="nv">firsts</span> <span class="o">=</span> <span class="p">[(</span><span class="mi">1</span><span class="p">,</span> <span class="nv">x</span><span class="p">)</span> <span class="o">|</span> <span class="nv">x</span> <span class="o">&lt;-</span> <span class="p">[</span><span class="mi">2</span><span class="o">..</span><span class="nv">n</span><span class="p">]]</span>
<span class="nv">rests</span> <span class="o">=</span> <span class="p">[(</span><span class="nv">x</span><span class="p">,</span> <span class="nv">y</span><span class="p">)</span> <span class="o">|</span> <span class="nv">x</span> <span class="o">&lt;-</span> <span class="p">[</span><span class="mi">2</span><span class="o">..</span><span class="nv">n</span><span class="p">],</span> <span class="nv">y</span> <span class="o">&lt;-</span> <span class="p">[(</span><span class="nv">x</span><span class="o">+</span><span class="mi">1</span><span class="p">)</span><span class="o">..</span><span class="nv">n</span><span class="p">]]</span>

<span class="nv">cMatchesPerDay</span> <span class="o">=</span> <span class="nv">n</span> <span class="p">`</span><span class="nv">div</span><span class="p">`</span> <span class="mi">2</span>

<span class="nv">filterCollision</span> <span class="o">::</span> <span class="p">[(</span><span class="n">Int</span><span class="p">,</span> <span class="n">Int</span><span class="p">)]</span> <span class="o">-&gt;</span> <span class="p">(</span><span class="n">Int</span><span class="p">,</span> <span class="n">Int</span><span class="p">)</span> <span class="o">-&gt;</span> <span class="p">[(</span><span class="n">Int</span><span class="p">,</span> <span class="n">Int</span><span class="p">)]</span>
<span class="nv">filterCollision</span> <span class="nv">lst</span> <span class="p">(</span><span class="nv">x</span><span class="p">,</span> <span class="nv">y</span><span class="p">)</span> <span class="o">=</span> <span class="nv">filter</span> <span class="p">(</span><span class="nv">matchxory</span><span class="o">.</span><span class="nv">fst</span><span class="p">)</span> <span class="o">$</span> <span class="nv">filter</span> <span class="p">(</span><span class="nv">matchxory</span><span class="o">.</span><span class="nv">snd</span><span class="p">)</span> <span class="nv">lst</span>
    <span class="kr">where</span> 
        <span class="nv">matchxory</span> <span class="o">::</span> <span class="n">Int</span> <span class="o">-&gt;</span> <span class="n">Bool</span>
        <span class="nv">matchxory</span> <span class="nv">a</span> <span class="o">=</span> <span class="p">(</span><span class="nv">a</span> <span class="o">/=</span> <span class="nv">x</span><span class="p">)</span> <span class="o">&amp;&amp;</span> <span class="p">(</span><span class="nv">a</span> <span class="o">/=</span> <span class="nv">y</span><span class="p">)</span>

<span class="nv">dayMatches</span> <span class="o">::</span> <span class="p">[(</span><span class="n">Int</span><span class="p">,</span> <span class="n">Int</span><span class="p">)]</span> <span class="o">-&gt;</span> <span class="n">Int</span> <span class="o">-&gt;</span> <span class="p">[(</span><span class="n">Int</span><span class="p">,</span> <span class="n">Int</span><span class="p">)]</span> <span class="o">-&gt;</span> <span class="p">[(</span><span class="n">Int</span><span class="p">,</span> <span class="n">Int</span><span class="p">)]</span>
<span class="nv">dayMatches</span> <span class="p">(</span><span class="nv">x</span><span class="o">:</span><span class="nv">xs</span><span class="p">)</span> <span class="mi">1</span> <span class="nv">t</span> <span class="o">=</span> <span class="nv">t</span> <span class="o">++</span> <span class="p">[</span><span class="nv">x</span><span class="p">]</span>
<span class="nv">dayMatches</span> <span class="p">[]</span> <span class="kr">_</span> <span class="nv">t</span> <span class="o">=</span> <span class="nv">t</span>

<span class="nv">dayMatches</span> <span class="nv">restsFiltered</span><span class="o">@</span><span class="p">(</span><span class="nv">x</span><span class="o">:</span><span class="nv">xs</span><span class="p">)</span> <span class="nv">c</span> <span class="nv">daymatch</span>
    <span class="o">|</span> <span class="p">(</span><span class="nv">length</span> <span class="nv">next</span><span class="p">)</span> <span class="o">/=</span> <span class="nv">cMatchesPerDay</span> <span class="o">=</span> <span class="nv">dayMatches</span> <span class="nv">xs</span> <span class="nv">c</span> <span class="nv">daymatch</span>
    <span class="o">|</span> <span class="nv">otherwise</span> <span class="o">=</span> <span class="nv">next</span>
    <span class="kr">where</span>
        <span class="nv">next</span> <span class="o">=</span> <span class="nv">dayMatches</span> <span class="p">(</span><span class="nv">filterCollision</span> <span class="nv">xs</span> <span class="nv">x</span><span class="p">)</span> <span class="p">(</span><span class="nv">c</span> <span class="o">-</span> <span class="mi">1</span><span class="p">)</span> <span class="p">(</span><span class="nv">daymatch</span> <span class="o">++</span> <span class="p">[</span><span class="nv">x</span><span class="p">])</span>

<span class="nv">generateTournament</span> <span class="o">::</span> <span class="p">(</span><span class="n">Int</span><span class="p">,</span> <span class="n">Int</span><span class="p">)</span> <span class="o">-&gt;</span> <span class="p">[(</span><span class="n">Int</span><span class="p">,</span> <span class="n">Int</span><span class="p">)]</span> <span class="o">-&gt;</span> <span class="p">([(</span><span class="n">Int</span><span class="p">,</span> <span class="n">Int</span><span class="p">)],</span> <span class="p">[(</span><span class="n">Int</span><span class="p">,</span> <span class="n">Int</span><span class="p">)])</span>
<span class="nv">generateTournament</span> <span class="nv">firstMatch</span> <span class="nv">moves</span> <span class="o">=</span> <span class="p">(</span><span class="nv">tournament</span><span class="p">,</span> <span class="nv">moves&#39;</span><span class="p">)</span>
    <span class="kr">where</span>
        <span class="nv">tournament</span> <span class="o">=</span> <span class="nv">dayMatches</span> <span class="p">(</span><span class="nv">filterCollision</span> <span class="nv">moves</span> <span class="nv">firstMatch</span><span class="p">)</span> <span class="p">(</span><span class="nv">cMatchesPerDay</span> <span class="o">-</span> <span class="mi">1</span><span class="p">)</span> <span class="p">[</span><span class="nv">firstMatch</span><span class="p">]</span>
        <span class="nv">moves&#39;</span> <span class="o">=</span> <span class="nv">filter</span> <span class="p">(</span><span class="o">\</span><span class="nv">x</span> <span class="o">-&gt;</span> <span class="nv">not</span> <span class="o">$</span> <span class="nv">elem</span> <span class="nv">x</span> <span class="nv">tournament</span><span class="p">)</span> <span class="nv">moves</span>

<span class="nv">mapToFirsts</span> <span class="p">[]</span> <span class="kr">_</span> <span class="o">=</span> <span class="p">[]</span>
<span class="nv">mapToFirsts</span> <span class="p">(</span><span class="nv">x</span><span class="o">:</span><span class="nv">xs</span><span class="p">)</span> <span class="nv">moves</span> <span class="o">=</span> <span class="p">(</span><span class="nv">tournament</span> <span class="o">:</span> <span class="nv">mapToFirsts</span> <span class="nv">xs</span> <span class="nv">moves&#39;</span><span class="p">)</span>
    <span class="kr">where</span>
        <span class="p">(</span><span class="nv">tournament</span><span class="p">,</span> <span class="nv">moves&#39;</span><span class="p">)</span> <span class="o">=</span> <span class="nv">generateTournament</span> <span class="nv">x</span> <span class="nv">moves</span>

<span class="nv">showMatches</span> <span class="nv">lst</span> <span class="o">=</span> <span class="nv">mapM_</span> <span class="p">(</span><span class="nv">putStrLn</span><span class="o">.</span><span class="nv">show</span><span class="p">)</span> <span class="nv">lst</span>

<span class="nv">main</span> <span class="o">=</span> <span class="nv">showMatches</span> <span class="o">$</span> <span class="nv">mapToFirsts</span> <span class="nv">firsts</span> <span class="nv">rests</span>
</pre></div>
