            <span class="n">List</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">[]&gt;</span> <span class="n">daySchedule</span> <span class="p">=</span> <span class="k">new</span> <span class="n">List</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">[]&gt;();</span>
