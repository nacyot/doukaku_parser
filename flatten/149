                <span class="n">daySchedule</span><span class="p">.</span><span class="n">Add</span><span class="p">(</span><span class="k">new</span> <span class="kt">int</span><span class="p">[]</span> <span class="k">{</span> <span class="n">teamList</span><span class="p">[</span><span class="m">1</span> <span class="p">+</span> <span class="n">game</span><span class="p">],</span> <span class="n">teamList</span><span class="p">[</span><span class="n">teamList</span><span class="p">.</span><span class="n">Count</span> <span class="p">-</span> <span class="n">game</span><span class="p">]</span> <span class="k">}</span><span class="p">);</span>
