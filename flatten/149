<span class="k">using</span> <span class="nn">System</span><span class="p">;</span>
<span class="k">using</span> <span class="nn">System.Collections.Generic</span><span class="p">;</span>

<span class="k">class</span> <span class="nc">Program</span> <span class="k">{</span>
    <span class="k">static</span> <span class="k">void</span> <span class="nf">Main</span><span class="p">(</span><span class="kt">string</span><span class="p">[]</span> <span class="n">args</span><span class="p">)</span> <span class="k">{</span>
        <span class="kt">int</span> <span class="n">teamCount</span> <span class="p">=</span> <span class="kt">int</span><span class="p">.</span><span class="n">Parse</span><span class="p">(</span><span class="n">args</span><span class="p">[</span><span class="m">0</span><span class="p">]);</span>
        <span class="n">List</span><span class="p">&lt;</span><span class="n">List</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">[]&gt;&gt;</span> <span class="n">leageSchedule</span> <span class="p">=</span> <span class="n">Leage</span><span class="p">(</span><span class="n">teamCount</span><span class="p">);</span>

        <span class="c">//出力</span>
        <span class="k">for</span><span class="p">(</span><span class="kt">int</span> <span class="n">day</span> <span class="p">=</span> <span class="m">0</span><span class="p">;</span> <span class="n">day</span> <span class="p">&lt;</span> <span class="n">leageSchedule</span><span class="p">.</span><span class="n">Count</span><span class="p">;</span> <span class="n">day</span><span class="p">++)</span> <span class="k">{</span>
            <span class="k">for</span><span class="p">(</span><span class="kt">int</span> <span class="n">game</span> <span class="p">=</span> <span class="m">0</span><span class="p">;</span> <span class="n">game</span> <span class="p">&lt;</span> <span class="n">leageSchedule</span><span class="p">[</span><span class="n">day</span><span class="p">].</span><span class="n">Count</span><span class="p">;</span> <span class="n">game</span><span class="p">++)</span> <span class="k">{</span>
                <span class="kt">int</span> <span class="n">min</span> <span class="p">=</span> <span class="n">Math</span><span class="p">.</span><span class="n">Min</span><span class="p">(</span><span class="n">leageSchedule</span><span class="p">[</span><span class="n">day</span><span class="p">][</span><span class="n">game</span><span class="p">][</span><span class="m">0</span><span class="p">],</span> <span class="n">leageSchedule</span><span class="p">[</span><span class="n">day</span><span class="p">][</span><span class="n">game</span><span class="p">][</span><span class="m">1</span><span class="p">]);</span>
                <span class="kt">int</span> <span class="n">max</span> <span class="p">=</span> <span class="n">Math</span><span class="p">.</span><span class="n">Max</span><span class="p">(</span><span class="n">leageSchedule</span><span class="p">[</span><span class="n">day</span><span class="p">][</span><span class="n">game</span><span class="p">][</span><span class="m">0</span><span class="p">],</span> <span class="n">leageSchedule</span><span class="p">[</span><span class="n">day</span><span class="p">][</span><span class="n">game</span><span class="p">][</span><span class="m">1</span><span class="p">]);</span>
