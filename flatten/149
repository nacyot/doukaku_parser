<span class="kt">void</span> <span class="n">test_1</span><span class="p">()</span> <span class="p">{</span>
  <span class="n">string</span> <span class="n">ss</span><span class="p">[]</span> <span class="o">=</span> <span class="p">{</span>
    <span class="s">&quot;1-2 3-6 4-5&quot;</span><span class="p">,</span> <span class="s">&quot;1-3 4-2 5-6&quot;</span><span class="p">,</span> <span class="s">&quot;1-4 5-3 6-2&quot;</span><span class="p">,</span>
