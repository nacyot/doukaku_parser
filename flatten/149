  <span class="p">}</span>

  <span class="k">return</span> <span class="n">result</span><span class="p">;</span>
<span class="p">}</span>

<span class="kt">void</span> <span class="n">test</span><span class="p">(</span><span class="n">string</span> <span class="n">expected</span><span class="p">[],</span> <span class="kt">int</span> <span class="n">n</span><span class="p">)</span> <span class="p">{</span>
  <span class="n">VS</span> <span class="n">vs</span><span class="p">(</span><span class="n">expected</span><span class="p">,</span> <span class="n">expected</span><span class="o">+</span><span class="n">n</span><span class="o">-</span><span class="mi">1</span><span class="p">);</span>
