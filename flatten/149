  <span class="p">(</span><span class="mi">1</span><span class="o">.</span><span class="n">.anotherteams</span><span class="o">.</span><span class="n">length</span><span class="p">)</span><span class="o">.</span><span class="n">inject</span><span class="p">(</span><span class="o">[]</span><span class="p">)</span> <span class="p">{</span><span class="o">|</span><span class="n">result</span><span class="p">,</span> <span class="n">notuse</span><span class="o">|</span>
