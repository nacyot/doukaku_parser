    l &lt;- length(unlist(strsplit(as.character(abs(x)), &quot;&quot;)))
    cat(&quot;桁数 =&quot;, l, &quot;, 位 =&quot;, as.integer(10^l), &quot;\n&quot;)
}
</pre></div>
