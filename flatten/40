17</pre></td><td class="code"><div class="highlight"><pre>k: fun(n) {
  value: 1;
  (n - 1).times.each { value *= 10; }
  return value;
}

kk: fun(n) {
  len: math::abs(n).to_i.to_s.size;
  return len, k(len);
}

kk(123).p; //=&gt; [3,100]
kk(-123).p; //=&gt; [3,100]
kk(12345).p; //=&gt; [5,10000]
kk(1).p; //=&gt; [1,1]
kk(1.001).p; //=&gt; [1,1]
kk(-1.001).p; //=&gt; [1,1]
</pre></div>
</td></tr></table>
    </div>
	

  
