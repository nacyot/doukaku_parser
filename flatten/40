<span class="k">template</span> <span class="o">&lt;</span><span class="n">size_t</span> <span class="n">N</span><span class="o">&gt;</span>
<span class="k">struct</span> <span class="n">keta</span>
<span class="p">{</span>
    <span class="k">static</span> <span class="k">const</span> <span class="n">size_t</span> <span class="n">value</span> <span class="o">=</span> <span class="n">keta</span><span class="o">&lt;</span><span class="n">N</span> <span class="o">/</span> <span class="mi">10</span><span class="o">&gt;::</span><span class="n">value</span> <span class="o">+</span> <span class="mi">1</span><span class="p">;</span>
<span class="p">};</span>

<span class="k">template</span> <span class="o">&lt;&gt;</span>
<span class="k">struct</span> <span class="n">keta</span><span class="o">&lt;</span><span class="mi">0</span><span class="o">&gt;</span>
<span class="p">{</span>
    <span class="k">static</span> <span class="k">const</span> <span class="n">size_t</span> <span class="n">value</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
<span class="p">};</span>

<span class="k">template</span> <span class="o">&lt;</span><span class="n">size_t</span> <span class="n">N</span><span class="o">&gt;</span>
<span class="k">struct</span> <span class="n">kurai_impl</span>
<span class="p">{</span>
    <span class="k">static</span> <span class="k">const</span> <span class="n">size_t</span> <span class="n">value</span> <span class="o">=</span> <span class="n">kurai_impl</span><span class="o">&lt;</span><span class="n">N</span> <span class="o">/</span> <span class="mi">10</span><span class="o">&gt;::</span><span class="n">value</span> <span class="o">*</span> <span class="mi">10</span><span class="p">;</span>
<span class="p">};</span>

<span class="k">template</span> <span class="o">&lt;&gt;</span>
<span class="k">struct</span> <span class="n">kurai_impl</span><span class="o">&lt;</span><span class="mi">0</span><span class="o">&gt;</span>
<span class="p">{</span>
    <span class="k">static</span> <span class="k">const</span> <span class="n">size_t</span> <span class="n">value</span> <span class="o">=</span> <span class="mi">1</span><span class="p">;</span>
<span class="p">};</span>

<span class="k">template</span> <span class="o">&lt;</span><span class="n">size_t</span> <span class="n">N</span><span class="o">&gt;</span>
<span class="k">struct</span> <span class="n">kurai</span>
