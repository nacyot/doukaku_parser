  <span class="n">keta</span> <span class="o">=</span> <span class="n">num</span><span class="o">.</span><span class="n">abs</span><span class="o">.</span><span class="n">truncate</span><span class="o">.</span><span class="n">to_s</span><span class="o">.</span><span class="n">length</span>
  <span class="n">kurai</span> <span class="o">=</span> <span class="s2">&quot;1&quot;</span>
  <span class="n">kurai</span> <span class="o">+=</span> <span class="s2">&quot;0&quot;</span> <span class="o">*</span> <span class="p">(</span><span class="n">keta</span><span class="o">-</span><span class="mi">1</span><span class="p">)</span> <span class="k">if</span> <span class="n">keta</span> <span class="o">&gt;</span> <span class="mi">1</span>
  <span class="k">return</span> <span class="n">keta</span><span class="p">,</span> <span class="n">kurai</span>
