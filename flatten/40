	<a href="/web/20110320100046/http://ja.doukaku.org/comment/1887/">#1887</a>(<script language="JavaScript">print_time(new Date("2007/08/05 18:31 GMT"));</script><noscript>2007/08/05 18:31 GMT</noscript>)
	
	
	
	[<a href="/web/20110320100046/http://ja.doukaku.org/lang/matlab/">
