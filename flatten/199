    <span class="k">public</span> <span class="nf">Echo</span><span class="o">(</span><span class="n">String</span> <span class="n">host</span><span class="o">,</span><span class="kt">int</span> <span class="n">port</span><span class="o">)</span> <span class="k">throws</span> <span class="n">Exception</span> <span class="o">{</span>
        <span class="n">socket</span> <span class="o">=</span> <span class="k">new</span> <span class="n">Socket</span><span class="o">(</span><span class="n">host</span><span class="o">,</span><span class="n">port</span><span class="o">);</span>
