<span class="k">import</span>    <span class="nn">java.io.OutputStream</span><span class="o">;</span>
<span class="k">import</span>    <span class="nn">java.net.Socket</span><span class="o">;</span>

<span class="k">class</span> <span class="nc">Echo</span> <span class="o">{</span>
    <span class="n">Socket</span>    <span class="n">socket</span><span class="o">;</span>
    <span class="n">OutputStream</span>    <span class="n">out</span><span class="o">;</span>
