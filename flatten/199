11</pre></td><td class="code"><div class="highlight"><pre><span class="nb">require</span> <span class="s1">&#39;socket&#39;</span>

<span class="n">host</span> <span class="o">=</span> <span class="no">ARGV</span><span class="o">[</span><span class="mi">0</span><span class="o">]</span>
<span class="n">port</span> <span class="o">=</span> <span class="no">ARGV</span><span class="o">[</span><span class="mi">1</span><span class="o">].</span><span class="n">to_i</span>

<span class="no">TCPSocket</span><span class="o">.</span><span class="n">open</span><span class="p">(</span><span class="n">host</span><span class="p">,</span> <span class="n">port</span><span class="p">)</span> <span class="p">{</span> <span class="o">|</span><span class="n">s</span><span class="o">|</span>
  <span class="k">while</span> <span class="vg">$stdin</span><span class="o">.</span><span class="n">gets</span>
    <span class="n">s</span><span class="o">.</span><span class="n">write</span> <span class="vg">$_</span>
    <span class="nb">print</span> <span class="n">s</span><span class="o">.</span><span class="n">gets</span>
  <span class="k">end</span>
<span class="p">}</span>
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
