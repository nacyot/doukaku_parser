        <span class="n">buf</span> <span class="o">+=</span> <span class="s2">&quot; &quot;</span><span class="o">*</span><span class="n">space</span> <span class="o">+</span> <span class="s2">&quot;|&quot;</span>
        <span class="n">idx</span> <span class="o">+=</span> <span class="mi">1</span>
      <span class="k">end</span>
    <span class="k">end</span>
    <span class="k">break</span> <span class="k">if</span> <span class="n">flag</span>
    <span class="nb">puts</span> <span class="n">buf</span>
  <span class="k">end</span>
  <span class="n">output</span> <span class="n">list</span><span class="p">,</span> <span class="n">space</span>
