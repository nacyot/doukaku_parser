    <span class="k">static</span> <span class="k">void</span> <span class="nf">ls</span><span class="p">(</span><span class="n">IEnumerable</span><span class="p">&lt;</span><span class="kt">string</span><span class="p">&gt;</span> <span class="n">pathList</span><span class="p">,</span> <span class="kt">string</span> <span class="n">path</span><span class="p">)</span> <span class="k">{</span>
