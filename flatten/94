<span class="p">(</span><span class="nf">test*</span> <span class="s">&quot;case &#39;(ls path-list \&quot;aaa/\&quot;)&#39;&quot;</span>
       <span class="o">&#39;</span><span class="p">(</span><span class="s">&quot;bbb&quot;</span> <span class="s">&quot;ccc&quot;</span> <span class="s">&quot;ddd/&quot;</span><span class="p">)</span> <span class="p">(</span><span class="nf">ls</span> <span class="nv">path-list</span> <span class="s">&quot;aaa/&quot;</span><span class="p">))</span>
