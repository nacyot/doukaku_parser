<span class="gi">+++ pseudo-ls.rb        2007-11-27 21:20:16.000000000 +0900</span>
<span class="gu">@@ -30,7 +30,7 @@</span>

   def traversal(prefix = &#39;&#39;)
     @files.map do |name, file|
<span class="gd">-      file.traversal(prefix + self.name)</span>
<span class="gi">+      file.traversal(prefix + file.name)</span>
     end.flatten
   end

