       <span class="o">&#39;</span><span class="p">(</span><span class="s">&quot;eee&quot;</span><span class="p">)</span> <span class="p">(</span><span class="nf">ls</span> <span class="nv">path-list</span> <span class="s">&quot;aaa/ddd/&quot;</span><span class="p">))</span>
