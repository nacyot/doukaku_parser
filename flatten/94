<span class="k">static</span> <span class="k">class</span> <span class="nc">Program</span> <span class="k">{</span>
    <span class="k">static</span> <span class="k">void</span> <span class="nf">Main</span><span class="p">()</span> <span class="k">{</span>
        <span class="kt">string</span><span class="p">[]</span> <span class="n">pathList</span> <span class="p">=</span> <span class="k">{</span><span class="s">&quot;aaa/bbb&quot;</span><span class="p">,</span> <span class="s">&quot;aaa/ccc&quot;</span><span class="p">,</span> <span class="s">&quot;aaa/ddd/eee&quot;</span><span class="p">,</span> <span class="s">&quot;bbb/ddd/eee&quot;</span><span class="p">,</span> <span class="s">&quot;bbb/ddd/fff&quot;</span><span class="k">}</span><span class="p">;</span>
