25</pre></td><td class="code"><div class="highlight"><pre><span class="o">#</span><span class="nv">!/usr/bin/env</span> <span class="nv">gosh</span>

<span class="p">(</span><span class="nf">use</span> <span class="nv">gauche</span><span class="o">.</span><span class="nv">test</span><span class="p">)</span>
<span class="p">(</span><span class="nf">use</span> <span class="nv">srfi-1</span><span class="p">)</span>

<span class="p">(</span><span class="k">define </span><span class="nv">path-list</span> <span class="p">(</span><span class="nb">list </span><span class="s">&quot;aaa/bbb&quot;</span> <span class="s">&quot;aaa/ccc&quot;</span> <span class="s">&quot;aaa/ddd/eee&quot;</span> <span class="s">&quot;bbb/ddd/eee&quot;</span><span class="p">))</span>

<span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">ls</span> <span class="nv">path-list</span> <span class="nv">dir</span><span class="p">)</span>
  <span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">matcher</span> <span class="nv">x</span><span class="p">)</span>
    <span class="p">(</span><span class="nf">and-let*</span> <span class="p">((</span><span class="nf">rx</span> <span class="p">(</span><span class="nf">string-&gt;regexp</span> <span class="o">#`</span><span class="s">&quot;^,(regexp-quote dir)/?([^/]+/?)&quot;</span><span class="p">))</span>
               <span class="p">(</span><span class="nf">m</span> <span class="p">(</span><span class="nf">rx</span> <span class="nv">x</span><span class="p">))</span>
               <span class="p">(</span><span class="nf">r</span> <span class="p">(</span><span class="nf">m</span> <span class="mi">1</span><span class="p">)))</span>
      <span class="nv">r</span><span class="p">))</span>
  <span class="p">(</span><span class="nf">filter-map</span> <span class="nv">matcher</span> <span class="nv">path-list</span><span class="p">))</span>

