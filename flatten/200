      <table><tr><td class="linenos"><pre>1</pre></td><td class="code"><div class="highlight"><pre><span class="nb">puts</span> <span class="n">z</span><span class="o">=</span><span class="mi">0</span><span class="p">;</span><span class="k">while</span> <span class="mi">1</span><span class="o">.</span><span class="n">upto</span><span class="p">(</span><span class="mi">9</span><span class="p">){</span><span class="o">|</span><span class="n">i</span><span class="o">|</span><span class="nb">puts</span><span class="p">((</span><span class="n">t</span><span class="o">=</span><span class="n">i</span><span class="o">*</span><span class="mi">10</span><span class="o">**</span><span class="n">z</span><span class="p">)</span><span class="o">&lt;=</span><span class="mi">300</span><span class="o">?</span><span class="n">t</span><span class="ss">:exit</span><span class="p">)};</span><span class="n">z</span><span class="o">+=</span><span class="mi">1</span><span class="p">;</span><span class="k">end</span>
</pre></div>
</td></tr></table>
    </div>
	

  
