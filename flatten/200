  <span class="k">for</span> <span class="o">((</span><span class="nv">i</span><span class="o">=</span>1;i&lt;10;i++<span class="o">))</span>;do
    <span class="nb">echo</span> <span class="nv">$i$m</span>
    <span class="o">[</span> <span class="nv">$i$m</span> <span class="o">=</span> <span class="nv">$1</span> <span class="o">]</span>&amp;&amp;exit
  <span class="k">done</span>
