27
28
29
30
31
32</pre></td><td class="code"><div class="highlight"><pre>#module

#deffunc CountUpMaxPlace var vIo,  local c
    c = peek(vIo)
    if ( &#39;0&#39; &lt;= c &amp;&amp; c &lt;= &#39;8&#39; ) {
        poke vIo, , c + 1
        
    } else : if ( c == &#39;9&#39; ) {
        poke vIo,, &#39;1&#39;
        vIo += &quot;0&quot;
        
    } else {
        vIo = &quot;0&quot;
    }
    return
    
#global

    nMax = 300        // 数える最大値
    
    sdim sBuf, 3200
    sdim sNumber
    repeat
        CountUpMaxPlace sNumber
        sBuf += sNumber +&quot;\n&quot;
        if ( int(sNumber) &gt;= nMax ) {
            break
        }
    loop
    objmode 2
    mesbox sBuf, ginfo(12), ginfo(13)
    stop
</pre></div>
</td></tr></table>
    </div>
	

  
