def profile = { function, x, y -&gt;
    def start = java.util.Calendar.instance.timeInMillis
    function(x, y)
    println(java.util.Calendar.instance.timeInMillis - start)
}


def newFunc = profile.curry(func)
newFunc( 200, 250 )
