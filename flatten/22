20</pre></td><td class="code"><div class="highlight"><pre><span class="k">module</span> <span class="k">do</span><span class="n">ukaku</span>;
<span class="k">private</span> <span class="k">import</span> <span class="n">std</span>.<span class="n">stdio</span>;
<span class="k">private</span> <span class="k">import</span> <span class="n">std</span>.<span class="n">date</span>;

<span class="n">d_time</span> <span class="n">profile</span>(<span class="kt">void</span> <span class="k">delegate</span>() <span class="n">yield</span>) {
    <span class="n">d_time</span> <span class="n">s</span> = <span class="n">getUTCtime</span>(), <span class="n">diff</span>;
    <span class="n">yield</span>();
    <span class="n">diff</span> = <span class="n">getUTCtime</span>() - <span class="n">s</span>;
    <span class="n">writefln</span>(<span class="s">&quot;%f secs&quot;</span>, <span class="k">cast</span>(<span class="kt">real</span>)<span class="n">diff</span> / <span class="n">TicksPerSecond</span>);
    <span class="k">return</span> <span class="n">diff</span>;
}

<span class="kt">void</span> <span class="n">func</span>(<span class="k">in</span><span class="n">t</span> <span class="n">n</span>) {
    <span class="n">writefln</span>(<span class="s">&quot;output from func&quot;</span>);
    <span class="k">for</span> (<span class="k">in</span><span class="n">t</span> <span class="n">i</span>=<span class="mi">0</span>; <span class="n">i</span>&lt;<span class="n">n</span>; <span class="n">i</span>++){};
}

<span class="kt">void</span> <span class="n">main</span>() {
    <span class="n">profile</span>(<span class="k">delegate</span>() { <span class="n">func</span>(<span class="mi">1000000000</span>); });
}
</pre></div>
