    endmacro;

profile:
    ##start = tickcount;
    call func;
    ##end = tickcount;
    message str( ##end - ##start ) + &quot; ミリ秒&quot;;
    return;

func:
    ##i = 500000;
    while ( ##i &gt; 0 ) {
        ##i = ##i - 1;
    }
    return;
</pre></div>
