<span class="kr">let</span> <span class="n">time</span> <span class="n">f</span> <span class="k">=</span>
    <span class="kr">let</span> <span class="n">start</span> <span class="k">=</span> <span class="n">DateTime</span><span class="k">.</span><span class="n">Now</span> <span class="kr">in</span>
    <span class="kr">let</span> <span class="n">res</span> <span class="k">=</span> <span class="n">f</span> <span class="k">()</span> <span class="kr">in</span>
    <span class="kr">let</span> <span class="n">finish</span> <span class="k">=</span> <span class="n">DateTime</span><span class="k">.</span><span class="n">Now</span> <span class="kr">in</span>
    <span class="n">Console</span><span class="k">.</span><span class="n">WriteLine</span><span class="k">(</span> <span class="k">(</span><span class="n">finish</span><span class="k">-</span><span class="n">start</span><span class="k">)</span> <span class="k">);</span>
    <span class="n">res</span><span class="k">;;</span>

<span class="kr">let</span> <span class="kr">rec</span> <span class="n">fib</span> <span class="n">n</span> <span class="k">=</span>
    <span class="kr">if</span> <span class="n">n</span> <span class="k">=</span> <span class="mi">0</span>
    <span class="kr">then</span> <span class="mi">1</span>
    <span class="n">elif</span> <span class="n">n</span> <span class="k">=</span> <span class="mi">1</span>
    <span class="kr">then</span> <span class="mi">1</span>
    <span class="kr">else</span> <span class="n">fib</span> <span class="k">(</span><span class="n">n</span><span class="k">-</span><span class="mi">1</span><span class="k">)</span> <span class="k">+</span> <span class="n">fib</span> <span class="k">(</span><span class="n">n</span><span class="k">-</span><span class="mi">2</span><span class="k">);;</span>

<span class="kr">let</span> <span class="n">profile</span> <span class="n">n</span> <span class="k">=</span> <span class="n">time</span> <span class="k">(</span><span class="kr">fun</span> <span class="k">()</span> <span class="k">-&gt;</span> <span class="n">fib</span> <span class="n">n</span><span class="k">);;</span>
