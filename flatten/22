	<a href="/web/20100324085422/http://ja.doukaku.org/comment/892/">#892</a>(<script language="JavaScript">print_time(new Date("2007/07/15 03:40 GMT"));</script><noscript>2007/07/15 03:40 GMT</noscript>)
	
	
	
