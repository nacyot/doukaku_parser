    <span class="k">my</span> <span class="nv">$yield</span> <span class="o">=</span> <span class="nv">$_</span><span class="p">[</span><span class="mi">0</span><span class="p">];</span>
    <span class="nv">$start</span> <span class="o">=</span> <span class="nb">time</span><span class="p">();</span>
    <span class="o">&amp;</span><span class="nv">$yield</span><span class="p">();</span>
    <span class="nv">$end</span> <span class="o">=</span> <span class="nb">time</span><span class="p">();</span>
