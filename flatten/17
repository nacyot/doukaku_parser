  <span class="k">def</span> <span class="nf">off_comment</span><span class="p">(</span><span class="n">cs</span><span class="p">,</span> <span class="n">rs</span><span class="p">)</span>
    <span class="k">return</span> <span class="n">rs</span><span class="o">.</span><span class="n">join</span> <span class="k">unless</span><span class="p">(</span><span class="n">cs</span><span class="p">);</span>
    <span class="k">if</span> <span class="p">((</span><span class="n">cs</span><span class="o">[</span><span class="mi">0</span><span class="o">]</span> <span class="o">==</span> <span class="s2">&quot;/&quot;</span><span class="p">)</span> <span class="o">&amp;&amp;</span> <span class="p">(</span><span class="n">cs</span><span class="o">[</span><span class="mi">1</span><span class="o">]</span> <span class="o">==</span> <span class="s2">&quot;*&quot;</span><span class="p">))</span>
      <span class="n">on_comment</span><span class="p">(</span><span class="n">cs</span><span class="o">[</span><span class="mi">2</span><span class="o">.</span><span class="n">.</span><span class="p">(</span><span class="n">cs</span><span class="o">.</span><span class="n">length</span> <span class="o">-</span> <span class="mi">1</span><span class="p">)</span><span class="o">]</span><span class="p">,</span> <span class="n">rs</span><span class="p">);</span>
    <span class="k">else</span>    
      <span class="n">off_comment</span><span class="p">(</span><span class="n">cs</span><span class="o">[</span><span class="mi">1</span><span class="o">.</span><span class="n">.</span><span class="p">(</span><span class="n">cs</span><span class="o">.</span><span class="n">length</span> <span class="o">-</span> <span class="mi">1</span><span class="p">)</span><span class="o">]</span><span class="p">,</span> <span class="n">rs</span><span class="o">.</span><span class="n">push</span><span class="p">(</span><span class="n">cs</span><span class="o">[</span><span class="mi">0</span><span class="o">]</span><span class="p">));</span>
    <span class="k">end</span>
  <span class="k">end</span>

  <span class="k">def</span> <span class="nf">on_comment</span><span class="p">(</span><span class="n">cs</span><span class="p">,</span> <span class="n">rs</span><span class="p">)</span>
    <span class="k">return</span> <span class="n">rs</span><span class="o">.</span><span class="n">join</span> <span class="k">unless</span><span class="p">(</span><span class="n">cs</span><span class="p">);</span>
    <span class="k">if</span> <span class="p">((</span><span class="n">cs</span><span class="o">[</span><span class="mi">0</span><span class="o">]</span> <span class="o">==</span> <span class="s2">&quot;*&quot;</span><span class="p">)</span> <span class="o">&amp;&amp;</span> <span class="p">(</span><span class="n">cs</span><span class="o">[</span><span class="mi">1</span><span class="o">]</span> <span class="o">==</span> <span class="s2">&quot;/&quot;</span><span class="p">))</span>
      <span class="n">off_comment</span><span class="p">(</span><span class="n">cs</span><span class="o">[</span><span class="mi">2</span><span class="o">.</span><span class="n">.</span><span class="p">(</span><span class="n">cs</span><span class="o">.</span><span class="n">length</span> <span class="o">-</span> <span class="mi">1</span><span class="p">)</span><span class="o">]</span><span class="p">,</span> <span class="n">rs</span><span class="p">);</span>
    <span class="k">else</span>    
      <span class="n">on_comment</span><span class="p">(</span><span class="n">cs</span><span class="o">[</span><span class="mi">1</span><span class="o">.</span><span class="n">.</span><span class="p">(</span><span class="n">cs</span><span class="o">.</span><span class="n">length</span> <span class="o">-</span> <span class="mi">1</span><span class="p">)</span><span class="o">]</span><span class="p">,</span> <span class="n">rs</span><span class="p">);</span>
    <span class="k">end</span>
  <span class="k">end</span>

  <span class="n">off_comment</span><span class="p">(</span><span class="n">str</span><span class="o">.</span><span class="n">split</span><span class="p">(</span><span class="sr">//</span><span class="p">),</span> <span class="o">[]</span><span class="p">);</span>
