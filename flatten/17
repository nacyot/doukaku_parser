<span class="k">import</span> <span class="nn">re</span>

<span class="k">def</span> <span class="nf">remove_comment</span><span class="p">(</span><span class="n">comment</span><span class="p">):</span>
    <span class="k">print</span> <span class="n">re</span><span class="o">.</span><span class="n">sub</span><span class="p">(</span><span class="s">r&#39;/\*.*?((\*/)|($))&#39;</span><span class="p">,</span><span class="s">&#39;&#39;</span><span class="p">,</span><span class="n">comment</span><span class="p">,</span> <span class="n">re</span><span class="o">.</span><span class="n">M</span><span class="p">)</span>

