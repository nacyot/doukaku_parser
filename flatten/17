  <div class="comment_content">
	<div class="comment_body">
	  正規表現使わない
	</div>
	
	
	
    <div class="compact">
      <table><tr><td class="linenos"><pre>1
2
3
4
5
6
7
8</pre></td><td class="code"><div class="highlight"><pre><span class="nv">removeComment</span> <span class="p">[]</span>           <span class="o">=</span> <span class="p">[]</span>
<span class="nv">removeComment</span> <span class="p">(</span><span class="sc">&#39;/&#39;</span><span class="o">:</span><span class="sc">&#39;*&#39;</span><span class="o">:</span><span class="nv">xs</span><span class="p">)</span> <span class="o">=</span> <span class="nv">skip</span> <span class="nv">xs</span>
    <span class="kr">where</span> <span class="nv">skip</span> <span class="p">[]</span>           <span class="o">=</span> <span class="p">[]</span>
          <span class="nv">skip</span> <span class="p">(</span><span class="sc">&#39;*&#39;</span><span class="o">:</span><span class="sc">&#39;/&#39;</span><span class="o">:</span><span class="nv">xs</span><span class="p">)</span> <span class="o">=</span> <span class="nv">removeComment</span> <span class="nv">xs</span>
          <span class="nv">skip</span> <span class="p">(</span><span class="nv">x</span><span class="o">:</span><span class="nv">xs</span><span class="p">)</span>       <span class="o">=</span> <span class="nv">skip</span> <span class="nv">xs</span>  
<span class="nv">removeComment</span> <span class="p">(</span><span class="nv">x</span><span class="o">:</span><span class="nv">xs</span><span class="p">)</span>       <span class="o">=</span> <span class="nv">x</span> <span class="o">:</span> <span class="nv">removeComment</span> <span class="nv">xs</span> 

<span class="nv">main</span> <span class="o">=</span> <span class="nv">getContents</span> <span class="o">&gt;&gt;=</span> <span class="p">(</span><span class="nv">putStr</span> <span class="o">.</span> <span class="nv">removeComment</span><span class="p">)</span>
</pre></div>
</td></tr></table>
