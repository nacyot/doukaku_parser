<span class="p">(</span><span class="nf">print</span> <span class="p">(</span><span class="nf">remove-comment</span> <span class="s">&quot;AAA/a//*BB*B**/CCC&quot;</span><span class="p">))</span>      <span class="c1">; =&gt; &quot;AAA/a/CCC&quot;</span>
