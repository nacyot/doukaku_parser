<span class="k">private</span> <span class="k">import</span> <span class="n">std</span>.<span class="n">regexp</span>;
<span class="k">private</span> <span class="k">import</span> <span class="n">std</span>.<span class="n">stdio</span>;

<span class="kt">char</span>[] <span class="n">remove_comment</span>(<span class="kt">char</span>[] <span class="n">str</span>) {
    <span class="k">return</span> <span class="n">sub</span>(<span class="n">str</span>, <span class="s">r&quot;/\*(.+?)(\*/|$)&quot;</span>, <span class="s">&quot;&quot;</span>, <span class="s">&quot;g&quot;</span>);
}

<span class="k">in</span><span class="n">t</span> <span class="n">main</span> () {
    <span class="n">writefln</span>(<span class="n">remove_comment</span>(<span class="s">&quot;AAA&quot;</span>));
    <span class="n">writefln</span>(<span class="n">remove_comment</span>(<span class="s">&quot;AAA/*BBB*/&quot;</span>));
