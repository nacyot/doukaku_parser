    <span class="k">return</span> <span class="n">f</span><span class="p">(</span><span class="n">d</span><span class="p">,</span> <span class="n">base</span><span class="p">)</span> <span class="o">+</span> <span class="n">digit</span><span class="p">[</span><span class="n">m</span><span class="p">]</span>
  <span class="k">else</span><span class="p">:</span>
    <span class="k">return</span> <span class="n">digit</span><span class="p">[</span><span class="n">m</span><span class="p">]</span>

<span class="k">if</span> <span class="n">__name__</span> <span class="o">==</span> <span class="s">&#39;__main__&#39;</span><span class="p">:</span>
  <span class="k">import</span> <span class="nn">doctest</span>
