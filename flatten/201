<span class="k">def</span> <span class="nf">f</span><span class="p">(</span><span class="n">dec</span><span class="p">,</span> <span class="n">base</span><span class="p">):</span>
  <span class="n">digit</span> <span class="o">=</span> <span class="s">&#39;0123456789abcdefghijklmnopqrstuvwxyz&#39;</span>
  <span class="n">d</span><span class="p">,</span> <span class="n">m</span> <span class="o">=</span> <span class="nb">divmod</span><span class="p">(</span><span class="n">dec</span><span class="p">,</span> <span class="n">base</span><span class="p">)</span>
