16
17
18
19</pre></td><td class="code"><div class="highlight"><pre><span class="c">(* 数値からcharを返す *)</span>
<span class="kr">let</span> <span class="n">int2char</span> <span class="n">i</span> <span class="k">=</span> 
  <span class="kr">if</span> <span class="n">i</span> <span class="k">&gt;</span> <span class="mi">9</span> <span class="kr">then</span>
    <span class="n">Char</span><span class="k">.</span><span class="n">chr</span> <span class="k">(</span><span class="mi">87</span><span class="k">+</span><span class="n">i</span><span class="k">)</span>
  <span class="kr">else</span> 
    <span class="n">Char</span><span class="k">.</span><span class="n">chr</span> <span class="k">(</span><span class="mi">48</span><span class="k">+</span><span class="n">i</span><span class="k">)</span>
<span class="k">;;</span>

<span class="kr">let</span> <span class="kr">rec</span> <span class="n">char_aux</span> <span class="n">i</span> <span class="n">dec</span> <span class="n">str</span> <span class="k">=</span> 
  <span class="kr">match</span> <span class="n">i</span> <span class="kr">with</span>
  <span class="k">|</span> <span class="mi">0</span> <span class="k">-&gt;</span> <span class="n">str</span>
  <span class="k">|</span> <span class="k">_</span> <span class="k">-&gt;</span> <span class="kr">let</span> <span class="n">str_</span> <span class="k">=</span> <span class="k">(</span> <span class="n">Char</span><span class="k">.</span><span class="n">escaped</span> <span class="k">(</span><span class="n">int2char</span> <span class="k">(</span><span class="n">i</span> <span class="kr">mod</span> <span class="n">dec</span><span class="k">))</span> <span class="k">)</span> <span class="o">^</span> <span class="n">str</span> <span class="kr">in</span>
         <span class="n">char_aux</span> <span class="k">(</span> <span class="n">i</span> <span class="o">/</span> <span class="n">dec</span> <span class="k">)</span> <span class="n">dec</span> <span class="n">str_</span>
<span class="k">;;</span>

<span class="c">(* 10進を16進へ変換 *)</span>
<span class="kr">let</span> <span class="n">dechex</span> <span class="n">i</span> <span class="k">=</span>  <span class="n">char_aux</span> <span class="n">i</span> <span class="mi">16</span> <span class="s2">&quot;&quot;</span><span class="k">;;</span>
<span class="c">(* 10進を2進へ変換 *)</span>
<span class="kr">let</span> <span class="n">decbin</span> <span class="n">i</span> <span class="k">=</span>  <span class="n">char_aux</span> <span class="n">i</span> <span class="mi">2</span> <span class="s2">&quot;&quot;</span><span class="k">;;</span>
</pre></div>
