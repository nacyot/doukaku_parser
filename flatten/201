<span class="k">def</span> <span class="nf">fmt_iter</span><span class="p">(</span><span class="n">n</span><span class="p">,</span> <span class="n">base</span><span class="p">):</span>
    <span class="k">if</span> <span class="n">n</span> <span class="o">/</span> <span class="n">base</span><span class="p">:</span>
        <span class="k">for</span> <span class="n">x</span> <span class="ow">in</span> <span class="n">fmt</span><span class="p">(</span><span class="n">n</span> <span class="o">/</span> <span class="n">base</span><span class="p">,</span> <span class="n">base</span><span class="p">):</span>
            <span class="k">yield</span> <span class="n">x</span>
    <span class="k">yield</span> <span class="n">DIGITS</span><span class="p">[</span><span class="n">n</span> <span class="o">%</span> <span class="n">base</span><span class="p">]</span>

<span class="k">def</span> <span class="nf">fmt</span><span class="p">(</span><span class="n">n</span><span class="p">,</span> <span class="n">base</span><span class="p">):</span>
    <span class="k">return</span> <span class="s">&#39;&#39;</span><span class="o">.</span><span class="n">join</span><span class="p">(</span><span class="n">fmt_iter</span><span class="p">(</span><span class="n">n</span><span class="p">,</span> <span class="n">base</span><span class="p">))</span>

