<span class="nv">toStr</span> <span class="nv">n</span> <span class="nv">b</span> <span class="o">=</span> <span class="nv">toStr&#39;</span> <span class="nv">n</span> <span class="nv">b</span>
    <span class="kr">where</span>
        <span class="nv">toStr&#39;</span> <span class="mi">0</span> <span class="kr">_</span> <span class="o">=</span> <span class="s">&quot;&quot;</span>
        <span class="nv">toStr&#39;</span> <span class="nv">n</span> <span class="nv">b</span> <span class="o">=</span> <span class="nv">toStr&#39;</span> <span class="p">(</span><span class="nv">n</span> <span class="p">`</span><span class="nv">div</span><span class="p">`</span> <span class="nv">b</span><span class="p">)</span> <span class="nv">b</span> <span class="o">++</span> <span class="p">[</span><span class="nv">toChar</span> <span class="p">(</span><span class="nv">n</span> <span class="p">`</span><span class="nv">mod</span><span class="p">`</span> <span class="nv">b</span><span class="p">)]</span>

<span class="nv">main</span> <span class="o">=</span> <span class="kr">do</span>
    <span class="nv">putStrLn</span> <span class="o">$</span> <span class="nv">toStr</span> <span class="mi">0</span> <span class="mi">10</span>
    <span class="nv">putStrLn</span> <span class="o">$</span> <span class="nv">toStr</span> <span class="mi">255</span> <span class="mi">2</span>
    <span class="nv">putStrLn</span> <span class="o">$</span> <span class="nv">toStr</span> <span class="mi">255</span> <span class="mi">10</span>
    <span class="nv">putStrLn</span> <span class="o">$</span> <span class="nv">toStr</span> <span class="mi">255</span> <span class="mi">16</span>
    <span class="nv">putStrLn</span> <span class="o">$</span> <span class="nv">toStr</span> <span class="mi">255</span> <span class="mi">32</span>
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
