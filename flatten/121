55
56</pre></td><td class="code"><div class="highlight"><pre><span class="x">#!/usr/bin/php</span>
<span class="cp">&lt;?php</span>
<span class="nv">$suits</span> <span class="o">=</span> <span class="k">array</span><span class="p">();</span>
<span class="nv">$ranks</span> <span class="o">=</span> <span class="k">array</span><span class="p">();</span>
<span class="k">for</span> <span class="p">(</span><span class="nv">$i</span> <span class="o">=</span> <span class="m">0</span><span class="p">;</span> <span class="nv">$i</span> <span class="o">&lt;</span> <span class="m">10</span><span class="p">;</span> <span class="nv">$i</span> <span class="o">+=</span> <span class="m">2</span><span class="p">)</span> <span class="p">{</span>
    <span class="nv">$suits</span><span class="p">[]</span> <span class="o">=</span> <span class="nv">$argv</span><span class="p">[</span><span class="m">1</span><span class="p">][</span><span class="nv">$i</span><span class="p">];</span>
    <span class="nv">$ranks</span><span class="p">[]</span> <span class="o">=</span> <span class="nf">strtr</span><span class="p">(</span><span class="nv">$argv</span><span class="p">[</span><span class="m">1</span><span class="p">][</span><span class="nv">$i</span><span class="o">+</span><span class="m">1</span><span class="p">],</span> <span class="s2">&quot;TJQKA&quot;</span><span class="p">,</span> <span class="s2">&quot;ABCDE&quot;</span><span class="p">);</span>
<span class="p">}</span>
<span class="nf">sort</span><span class="p">(</span><span class="nv">$suits</span><span class="p">);</span>
<span class="nf">sort</span><span class="p">(</span><span class="nv">$ranks</span><span class="p">);</span>
<span class="nv">$suitCnt</span> <span class="o">=</span> <span class="nf">array_count_values</span><span class="p">(</span><span class="nv">$suits</span><span class="p">);</span>
<span class="nf">asort</span><span class="p">(</span><span class="nv">$suitCnt</span><span class="p">);</span>
<span class="nv">$rankCnt</span> <span class="o">=</span> <span class="nf">array_count_values</span><span class="p">(</span><span class="nv">$ranks</span><span class="p">);</span>
<span class="nf">arsort</span><span class="p">(</span><span class="nv">$rankCnt</span><span class="p">);</span>

<span class="nv">$straight</span> <span class="o">=</span> <span class="k">false</span><span class="p">;</span>
<span class="nv">$flush</span> <span class="o">=</span> <span class="k">false</span><span class="p">;</span>
<span class="nv">$suitHist</span> <span class="o">=</span> <span class="nf">array_values</span><span class="p">(</span><span class="nv">$suitCnt</span><span class="p">);</span>
<span class="k">if</span> <span class="p">(</span><span class="nv">$suitHist</span><span class="p">[</span><span class="m">0</span><span class="p">]</span> <span class="o">==</span> <span class="m">5</span><span class="p">)</span> <span class="p">{</span>
    <span class="nv">$flush</span> <span class="o">=</span> <span class="k">true</span><span class="p">;</span>
<span class="p">}</span>
<span class="nv">$rankHist</span> <span class="o">=</span> <span class="nf">array_values</span><span class="p">(</span><span class="nv">$rankCnt</span><span class="p">);</span>
<span class="k">if</span> <span class="p">(</span><span class="nf">strstr</span><span class="p">(</span><span class="s2">&quot;23456789ABCDE,2345E&quot;</span><span class="p">,</span> <span class="nf">implode</span><span class="p">(</span><span class="s1">&#39;&#39;</span><span class="p">,</span><span class="nv">$ranks</span><span class="p">)))</span> <span class="p">{</span>
    <span class="nv">$straight</span> <span class="o">=</span> <span class="k">true</span><span class="p">;</span>
<span class="p">}</span>

<span class="nv">$hand</span> <span class="o">=</span> <span class="s1">&#39;&#39;</span><span class="p">;</span>
<span class="k">if</span> <span class="p">(</span><span class="nv">$straight</span> <span class="o">&amp;&amp;</span> <span class="nv">$flush</span><span class="p">)</span> <span class="p">{</span>
    <span class="k">if</span> <span class="p">(</span><span class="nf">implode</span><span class="p">(</span><span class="s1">&#39;&#39;</span><span class="p">,</span> <span class="nv">$ranks</span><span class="p">)</span> <span class="o">==</span> <span class="s1">&#39;ABCDE&#39;</span><span class="p">)</span> <span class="p">{</span>
        <span class="nv">$hand</span> <span class="o">=</span> <span class="s2">&quot;Royal straight flush&quot;</span><span class="p">;</span>
    <span class="p">}</span> <span class="k">else</span> <span class="p">{</span>
        <span class="nv">$hand</span> <span class="o">=</span> <span class="s2">&quot;Straight flush&quot;</span><span class="p">;</span>
    <span class="p">}</span>
<span class="p">}</span> <span class="k">else</span> <span class="k">if</span> <span class="p">(</span><span class="nv">$rankHist</span><span class="p">[</span><span class="m">0</span><span class="p">]</span> <span class="o">==</span> <span class="m">4</span><span class="p">)</span> <span class="p">{</span>
    <span class="nv">$hand</span> <span class="o">=</span> <span class="s2">&quot;Four of a kind&quot;</span><span class="p">;</span>
<span class="p">}</span> <span class="k">else</span> <span class="k">if</span> <span class="p">(</span><span class="nv">$rankHist</span><span class="p">[</span><span class="m">0</span><span class="p">]</span> <span class="o">==</span> <span class="m">3</span> <span class="o">&amp;&amp;</span> <span class="nv">$rankHist</span><span class="p">[</span><span class="m">1</span><span class="p">]</span> <span class="o">==</span> <span class="m">2</span><span class="p">)</span> <span class="p">{</span>
    <span class="nv">$hand</span> <span class="o">=</span> <span class="s2">&quot;Full house&quot;</span><span class="p">;</span>
<span class="p">}</span>
<span class="k">if</span> <span class="p">(</span><span class="nv">$hand</span> <span class="o">==</span> <span class="s1">&#39;&#39;</span><span class="p">)</span> <span class="p">{</span>
    <span class="k">if</span> <span class="p">(</span><span class="nv">$flush</span><span class="p">)</span> <span class="p">{</span>
        <span class="nv">$hand</span> <span class="o">=</span> <span class="s2">&quot;Flush&quot;</span><span class="p">;</span>
    <span class="p">}</span> <span class="k">else</span> <span class="k">if</span> <span class="p">(</span><span class="nv">$straight</span><span class="p">)</span> <span class="p">{</span>
        <span class="nv">$hand</span> <span class="o">=</span> <span class="s2">&quot;Straight&quot;</span><span class="p">;</span>
    <span class="p">}</span> <span class="k">else</span> <span class="k">if</span> <span class="p">(</span><span class="nv">$rankHist</span><span class="p">[</span><span class="m">0</span><span class="p">]</span> <span class="o">==</span> <span class="m">3</span><span class="p">)</span> <span class="p">{</span>
        <span class="nv">$hand</span> <span class="o">=</span> <span class="s2">&quot;Three of a kind&quot;</span><span class="p">;</span>
    <span class="p">}</span> <span class="k">else</span> <span class="k">if</span> <span class="p">(</span><span class="nv">$rankHist</span><span class="p">[</span><span class="m">0</span><span class="p">]</span> <span class="o">==</span> <span class="m">2</span> <span class="o">&amp;&amp;</span> <span class="nv">$rankHist</span><span class="p">[</span><span class="m">1</span><span class="p">]</span> <span class="o">==</span> <span class="m">2</span><span class="p">)</span> <span class="p">{</span>
        <span class="nv">$hand</span> <span class="o">=</span> <span class="s2">&quot;Two pairs&quot;</span><span class="p">;</span>
    <span class="p">}</span> <span class="k">else</span> <span class="k">if</span> <span class="p">(</span><span class="nv">$rankHist</span><span class="p">[</span><span class="m">0</span><span class="p">]</span> <span class="o">==</span> <span class="m">2</span><span class="p">)</span> <span class="p">{</span>
        <span class="nv">$hand</span> <span class="o">=</span> <span class="s2">&quot;One pair&quot;</span><span class="p">;</span>
    <span class="p">}</span> <span class="k">else</span> <span class="p">{</span>
        <span class="nv">$hand</span> <span class="o">=</span> <span class="s2">&quot;No Pair&quot;</span><span class="p">;</span>
    <span class="p">}</span>
<span class="p">}</span>

<span class="k">echo</span> <span class="nv">$hand</span> <span class="o">.</span> <span class="s2">&quot;</span><span class="se">\n</span><span class="s2">&quot;</span><span class="p">;</span>
<span class="cp">?&gt;</span><span class="x"></span>
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
