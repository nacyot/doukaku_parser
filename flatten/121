<span class="k">using</span> <span class="nn">System.Collections.Generic</span><span class="p">;</span>
<span class="k">using</span> <span class="nn">System.Linq</span><span class="p">;</span>

<span class="k">namespace</span> <span class="nn">Doukaku</span>
<span class="k">{</span>
    <span class="k">class</span> <span class="nc">PorkerJudge</span>
    <span class="k">{</span>
        <span class="k">protected</span> <span class="k">static</span> <span class="kt">char</span><span class="p">[]</span> <span class="n">Suits</span> <span class="p">=</span> <span class="k">new</span> <span class="kt">char</span><span class="p">[]</span> <span class="k">{</span> <span class="sc">&#39;S&#39;</span><span class="p">,</span> <span class="sc">&#39;D&#39;</span><span class="p">,</span> <span class="sc">&#39;H&#39;</span><span class="p">,</span> <span class="sc">&#39;C&#39;</span> <span class="k">}</span><span class="p">;</span>
