<span class="k">from</span> <span class="nn">itertools</span> <span class="k">import</span> <span class="n">islice</span>
<span class="k">from</span> <span class="nn">math</span> <span class="k">import</span> <span class="n">sin</span><span class="p">,</span><span class="n">cos</span><span class="p">,</span><span class="n">pi</span>

<span class="k">def</span> <span class="nf">randomwalk</span><span class="p">(</span><span class="n">x</span> <span class="o">=</span> <span class="mi">0</span><span class="p">,</span><span class="n">y</span> <span class="o">=</span> <span class="mi">0</span><span class="p">):</span>
