  <span class="n">points</span> <span class="o">=&gt;</span> <span class="nb">join</span><span class="p">(</span><span class="n">q</span><span class="sr">/ /</span><span class="p">,</span> <span class="nb">map</span> <span class="p">{</span> <span class="nb">join</span> <span class="n">q</span><span class="sr">/,/</span><span class="p">,</span> <span class="nb">map</span> <span class="p">{</span> <span class="nv">$_</span> <span class="o">*</span> <span class="nv">$grid</span> <span class="o">+</span> <span class="nv">$grid</span><span class="o">/</span><span class="mi">2</span> <span class="p">}</span> <span class="nv">@$_</span> <span class="p">}</span>
                       <span class="nb">map</span> <span class="p">{</span> <span class="p">[</span><span class="nv">$_</span><span class="o">-&gt;</span><span class="p">[</span><span class="mi">0</span><span class="p">]</span><span class="o">-</span><span class="nv">$area</span><span class="p">[</span><span class="mi">0</span><span class="p">],</span><span class="nv">$_</span><span class="o">-&gt;</span><span class="p">[</span><span class="mi">1</span><span class="p">]</span><span class="o">-</span><span class="nv">$area</span><span class="p">[</span><span class="mi">1</span><span class="p">]]</span> <span class="p">}</span>
                       <span class="nv">@footmarks</span><span class="p">)</span>
