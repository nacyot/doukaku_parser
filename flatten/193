int d = 1;

void setup() {
  x = width / 2;
  y = height / 2;
  t = 0;
//  randomSeed(0);
}

void draw() {
  t += 1;
  float r = random(1);
  if (r &lt; 0.25) {
    x += d;
  } else if (r &lt; 0.5) {
    x -= d;
  } else if (r &lt; 0.75) {
    y += d;
  } else {
    y -= d;
  }
  point(x, y);
  println(t + &quot; &quot; + x + &quot; &quot; + y);
}
