<span class="k">use</span> <span class="n">warnings</span><span class="p">;</span>

<span class="k">use</span> <span class="nn">List::</span><span class="n">Util</span> <span class="n">qw</span><span class="sr">/min max/</span><span class="p">;</span>
<span class="k">use</span> <span class="nn">Image::</span><span class="n">Magick</span><span class="p">;</span>

<span class="c1"># 歩く回数</span>
<span class="k">my</span> <span class="nv">$count</span> <span class="o">=</span> <span class="mi">10000</span><span class="p">;</span>

<span class="k">sub </span><span class="nf">_walk</span> <span class="p">{</span> <span class="nb">int</span><span class="p">(</span><span class="nb">rand</span><span class="p">(</span><span class="mi">3</span><span class="p">))</span><span class="o">-</span><span class="mi">1</span><span class="p">;</span> <span class="p">}</span>

