<span class="c">//移動回数</span>
<span class="nf">define</span><span class="p">(</span><span class="s1">&#39;LOOP&#39;</span><span class="p">,</span> <span class="m">10</span><span class="p">);</span>

<span class="c">//初期位置</span>
<span class="nv">$pos</span><span class="p">[</span><span class="m">0</span><span class="p">]</span> <span class="o">=</span> <span class="k">array</span><span class="p">(</span><span class="s1">&#39;x&#39;</span> <span class="o">=&gt;</span> <span class="m">0</span><span class="p">,</span> <span class="s1">&#39;y&#39;</span> <span class="o">=&gt;</span> <span class="m">0</span><span class="p">);</span>

<span class="k">for</span><span class="p">(</span><span class="nv">$time</span> <span class="o">=</span> <span class="m">1</span><span class="p">;</span> <span class="nv">$time</span> <span class="o">&lt;=</span> <span class="nx">LOOP</span><span class="p">;</span> <span class="nv">$time</span><span class="o">++</span><span class="p">){</span>
    <span class="nv">$rad</span> <span class="o">=</span> <span class="nf">deg2rad</span><span class="p">(</span><span class="nf">mt_rand</span><span class="p">()</span><span class="o">%</span><span class="m">360</span><span class="p">);</span>
    <span class="nv">$x</span> <span class="o">=</span> <span class="nv">$pos</span><span class="p">[</span><span class="nv">$time</span><span class="o">-</span><span class="m">1</span><span class="p">][</span><span class="s1">&#39;x&#39;</span><span class="p">]</span> <span class="o">+</span> <span class="nf">cos</span><span class="p">(</span><span class="nv">$rad</span><span class="p">);</span>
    <span class="nv">$y</span> <span class="o">=</span> <span class="nv">$pos</span><span class="p">[</span><span class="nv">$time</span><span class="o">-</span><span class="m">1</span><span class="p">][</span><span class="s1">&#39;y&#39;</span><span class="p">]</span> <span class="o">+</span> <span class="nf">sin</span><span class="p">(</span><span class="nv">$rad</span><span class="p">);</span>
    <span class="nv">$pos</span><span class="p">[</span><span class="nv">$time</span><span class="p">]</span> <span class="o">=</span> <span class="k">array</span><span class="p">(</span><span class="s1">&#39;x&#39;</span> <span class="o">=&gt;</span> <span class="nv">$x</span><span class="p">,</span> <span class="s1">&#39;y&#39;</span> <span class="o">=&gt;</span> <span class="nv">$y</span><span class="p">);</span>
<span class="p">}</span>

<span class="k">foreach</span><span class="p">(</span><span class="nv">$pos</span> <span class="k">as</span> <span class="nv">$key</span> <span class="o">=&gt;</span> <span class="nv">$val</span><span class="p">){</span>
    <span class="k">echo</span> <span class="nv">$key</span> <span class="o">.</span> <span class="s2">&quot; &quot;</span> <span class="o">.</span> <span class="nv">$val</span><span class="p">[</span><span class="s1">&#39;x&#39;</span><span class="p">]</span> <span class="o">.</span> <span class="s2">&quot; &quot;</span> <span class="o">.</span> <span class="nv">$val</span><span class="p">[</span><span class="s1">&#39;y&#39;</span><span class="p">]</span> <span class="o">.</span> <span class="s2">&quot; &quot;</span> <span class="o">.</span> <span class="s2">&quot;</span><span class="se">\n</span><span class="s2">&quot;</span><span class="p">;</span>
<span class="p">}</span>
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
