  <span class="nv">$area</span><span class="p">[</span><span class="mi">0</span><span class="p">]</span> <span class="o">=</span> <span class="n">min</span> <span class="nv">$area</span><span class="p">[</span><span class="mi">0</span><span class="p">],</span> <span class="nv">$current</span><span class="p">[</span><span class="mi">0</span><span class="p">];</span>
  <span class="nv">$area</span><span class="p">[</span><span class="mi">1</span><span class="p">]</span> <span class="o">=</span> <span class="n">min</span> <span class="nv">$area</span><span class="p">[</span><span class="mi">1</span><span class="p">],</span> <span class="nv">$current</span><span class="p">[</span><span class="mi">1</span><span class="p">];</span>
