	<a href="/web/20110416061249/http://ja.doukaku.org/comment/1776/">#1776</a>(<script language="JavaScript">print_time(new Date("2007/08/03 12:01 GMT"));</script><noscript>2007/08/03 12:01 GMT</noscript>)
	
	
	
	
	
