	&nbsp;
	<a href="/web/20110416061249/http://ja.doukaku.org/comment/1800/">#1800</a>(<script language="JavaScript">print_time(new Date("2007/08/03 18:57 GMT"));</script><noscript>2007/08/03 18:57 GMT</noscript>)
	
	
	
