<span class="p">{</span>
    <span class="k">typedef</span>
        <span class="k">typename</span> <span class="n">Choice</span><span class="o">&lt;</span> <span class="n">n</span> <span class="o">&lt;</span> <span class="n">Kilo</span><span class="o">::</span><span class="n">base</span><span class="p">,</span>     <span class="n">None</span><span class="p">,</span>
