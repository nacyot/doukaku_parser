    <span class="k">enum</span> <span class="p">{</span> <span class="n">symbol</span> <span class="o">=</span> <span class="sc">&#39;T&#39;</span> <span class="p">};</span>
    <span class="k">static</span> <span class="kt">double</span> <span class="n">scaled</span><span class="p">(</span><span class="kt">long</span> <span class="kt">long</span> <span class="n">n</span><span class="p">)</span> <span class="k">throw</span><span class="p">()</span> 
      <span class="p">{</span> <span class="k">return</span> <span class="k">static_cast</span><span class="o">&lt;</span><span class="kt">double</span><span class="o">&gt;</span><span class="p">(</span><span class="n">n</span><span class="p">)</span> <span class="o">/</span> <span class="mi">1000000000000L</span><span class="n">L</span><span class="p">;</span> <span class="p">}</span>
