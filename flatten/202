	<a href="/web/20091213020908/http://ja.doukaku.org/comment/7326/">#7326</a>(<script language="JavaScript">print_time(new Date("2008/08/25 03:38 GMT"));</script><noscript>2008/08/25 03:38 GMT</noscript>)
	
	
	
	[<a href="/web/20091213020908/http://ja.doukaku.org/lang/r/">
