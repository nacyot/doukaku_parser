<span class="kt">void</span> <span class="n">conv_put</span><span class="p">(</span><span class="kt">char</span> <span class="o">*</span><span class="n">buf</span><span class="p">,</span><span class="n">QWORD</span> <span class="n">n</span><span class="p">){</span>
