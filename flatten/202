16</pre></td><td class="code"><div class="highlight"><pre><span class="kr">let</span> <span class="n">foo</span> <span class="n">n</span> <span class="k">=</span>
  <span class="kr">let</span> <span class="n">u</span> <span class="k">=</span> <span class="s2">&quot; kMGT&quot;</span> <span class="kr">in</span>
  <span class="kr">let</span> <span class="n">l</span> <span class="k">=</span> <span class="n">String</span><span class="k">.</span><span class="n">length</span> <span class="n">n</span> <span class="kr">in</span>
  <span class="kr">let</span> <span class="n">idx</span> <span class="k">=</span> <span class="kr">if</span> <span class="k">(</span><span class="n">l</span> <span class="kr">mod</span> <span class="mi">3</span><span class="k">)</span> <span class="k">==</span> <span class="mi">0</span> <span class="kr">then</span> <span class="k">(</span><span class="n">l</span> <span class="o">/</span> <span class="mi">3</span><span class="k">)</span> <span class="k">-</span> <span class="mi">1</span> <span class="kr">else</span> <span class="k">(</span><span class="n">l</span> <span class="o">/</span> <span class="mi">3</span><span class="k">)</span> <span class="kr">in</span>
    <span class="n">Printf</span><span class="k">.</span><span class="n">printf</span> <span class="s2">&quot;%s%s %cbytes</span><span class="se">\n</span><span class="s2">&quot;</span>
      <span class="k">(</span><span class="n">String</span><span class="k">.</span><span class="n">sub</span> <span class="n">n</span> <span class="mi">0</span> <span class="k">(</span><span class="n">l</span> <span class="k">-</span> <span class="k">(</span><span class="n">idx</span> <span class="k">*</span> <span class="mi">3</span><span class="k">)))</span>
      <span class="k">(</span><span class="kr">if</span> <span class="n">l</span> <span class="k">&gt;</span> <span class="mi">3</span> <span class="kr">then</span> <span class="s2">&quot;.&quot;</span> <span class="o">^</span> <span class="k">(</span><span class="n">String</span><span class="k">.</span><span class="n">sub</span> <span class="n">n</span> <span class="k">(</span><span class="n">l</span><span class="k">-(</span><span class="n">idx</span><span class="k">*</span><span class="mi">3</span><span class="k">))</span> <span class="mi">1</span><span class="k">)</span> <span class="kr">else</span> <span class="s2">&quot;&quot;</span><span class="k">)</span>
      <span class="n">u</span><span class="k">.[</span><span class="n">idx</span><span class="k">]</span>
<span class="k">;;</span>

<span class="c">(*</span>
<span class="c">短く</span>
<span class="c">let f n = let u = &quot; kMGT&quot; in let l = String.length n in let x = if (l mod 3) == 0 then (l / 3) - 1 else (l / 3) in Printf.printf &quot;%s%s %cbytes\n&quot; (String.sub n 0 (l - (x * 3))) (if l &gt; 3 then &quot;.&quot;^(String.sub n (l-(x*3)) 1) else &quot;&quot;) u.[x];;</span>
