  <span class="k">for</span> <span class="n">u</span> <span class="ow">in</span> <span class="nb">iter</span><span class="p">(</span><span class="s">&#39;BKMGT&#39;</span><span class="p">):</span>
    <span class="k">if</span> <span class="n">n</span> <span class="o">&lt;</span> <span class="mi">1024</span><span class="p">:</span>
      <span class="k">return</span> <span class="s">&quot;</span><span class="si">%.1f%s</span><span class="s">&quot;</span> <span class="o">%</span> <span class="p">(</span><span class="n">n</span><span class="p">,</span><span class="n">u</span><span class="p">)</span>
    <span class="n">n</span> <span class="o">/=</span> <span class="mf">1024.0</span>

<span class="k">print</span> <span class="n">f</span><span class="p">(</span><span class="nb">int</span><span class="p">(</span><span class="nb">raw_input</span><span class="p">()))</span>
