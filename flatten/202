      <span class="o">&gt;::</span><span class="n">Result</span><span class="o">&gt;::</span><span class="n">Result</span><span class="o">&gt;::</span><span class="n">Result</span><span class="o">&gt;::</span><span class="n">Result</span>  <span class="n">Result</span><span class="p">;</span>
