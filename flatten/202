<span class="k">template</span> <span class="o">&lt;</span><span class="kt">bool</span> <span class="n">b</span><span class="p">,</span> <span class="k">class</span> <span class="nc">T</span><span class="p">,</span> <span class="k">class</span> <span class="nc">S</span><span class="o">&gt;</span> <span class="k">struct</span> <span class="n">Choice</span> <span class="p">{</span> <span class="k">typedef</span> <span class="n">T</span> <span class="n">Result</span><span class="p">;</span> <span class="p">};</span>
