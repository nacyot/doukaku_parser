<span class="k">use</span> <span class="n">strict</span><span class="p">;</span>
<span class="k">use</span> <span class="n">warnings</span><span class="p">;</span>

<span class="k">use</span> <span class="nn">Data::</span><span class="n">Dumper</span><span class="p">;</span>

<span class="k">my</span> <span class="nv">$nmax</span> <span class="o">=</span> <span class="nb">shift</span><span class="p">;</span>
<span class="k">my</span> <span class="nv">$mmax</span> <span class="o">=</span> <span class="nb">shift</span><span class="p">;</span>

<span class="k">my</span> <span class="nv">@winner</span><span class="p">;</span>

<span class="c1">#warn &quot;selecting $nmax from $mmax\n&quot;;</span>
<span class="k">for</span> <span class="k">my</span> <span class="nv">$n</span> <span class="p">(</span> <span class="mi">0</span> <span class="o">..</span> <span class="nv">$nmax</span> <span class="o">-</span> <span class="mi">1</span> <span class="p">)</span> <span class="p">{</span>
