    <div class="compact">
      <table><tr><td class="linenos"><pre>1
2
3
4
5
6
7
8
9</pre></td><td class="code"><div class="highlight"><pre><span class="k">import</span> <span class="nn">random</span>
<span class="k">def</span> <span class="nf">lot</span><span class="p">(</span><span class="n">n</span><span class="p">,</span><span class="n">m</span><span class="p">):</span>
    <span class="k">if</span> <span class="n">n</span><span class="o">&lt;</span><span class="n">m</span><span class="p">:</span>
        <span class="k">print</span> <span class="s">&quot;Error&quot;</span>
        <span class="k">return</span>
    <span class="n">myset</span> <span class="o">=</span> <span class="n">set</span><span class="p">()</span>
    <span class="k">while</span> <span class="nb">len</span><span class="p">(</span><span class="n">myset</span><span class="p">)</span><span class="o">!=</span><span class="n">m</span><span class="p">:</span>
        <span class="n">myset</span><span class="o">.</span><span class="n">add</span><span class="p">(</span><span class="n">random</span><span class="o">.</span><span class="n">randint</span><span class="p">(</span><span class="mi">0</span><span class="p">,</span><span class="n">n</span><span class="o">-</span><span class="mi">1</span><span class="p">))</span>
    <span class="k">return</span> <span class="n">myset</span>
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
