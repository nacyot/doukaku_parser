        <span class="k">for</span> <span class="k">my</span> <span class="nv">$m</span> <span class="p">(</span> <span class="mi">1</span> <span class="o">..</span> <span class="nv">$mmax</span> <span class="p">)</span> <span class="p">{</span>
                <span class="k">next</span> <span class="k">if</span> <span class="nb">grep</span> <span class="p">{</span> <span class="nb">defined</span> <span class="nv">$_</span> <span class="ow">and</span> <span class="nv">$_</span> <span class="o">==</span> <span class="nv">$m</span> <span class="p">}</span> <span class="nv">@winner</span><span class="p">;</span>
                <span class="k">if</span> <span class="p">(</span> <span class="nb">rand</span><span class="p">(</span> <span class="nv">$count</span> <span class="p">)</span> <span class="o">&lt;</span> <span class="mi">1</span> <span class="p">)</span> <span class="p">{</span>
