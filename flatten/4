      <span class="k">let</span> <span class="k">r</span> <span class="p">=</span> libcallnr<span class="p">(</span><span class="s2">&quot;libc&quot;</span><span class="p">,</span> <span class="c">&quot;rand&quot;, 0)</span>
    <span class="k">endif</span>
    <span class="k">silent</span><span class="p">!</span> <span class="k">call</span> remove<span class="p">(</span>v<span class="p">,</span> <span class="p">(</span><span class="k">r</span> % len<span class="p">(</span>v<span class="p">)))</span>
  <span class="k">endwhile</span>
  <span class="k">return</span> v
endfun
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
