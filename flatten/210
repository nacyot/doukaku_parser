    Range(&quot;B2&quot;).FormulaR1C1 =  _
        &quot;=FACT(RC[-1]*2)/FACT(RC[-1])/FACT(RC[-1])&quot; &amp; _
        &quot;/POWER(2,4*RC[-1]+1)/(2*RC[-1]+1)&quot;
    Range(&quot;A2:B22&quot;).Select
    Selection.FillDown
