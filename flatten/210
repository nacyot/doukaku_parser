         nums &lt;- sequence(map sequence (replicate t (replicate 2 randomFIO)))
         putStrLn $ show $ (fromIntegral(length(filter f nums)  )/fromIntegral(t)) * 4
      where
         f :: [Float] -&gt; Bool
         f = (&lt; 1). sum.map(^2)   
         randomFIO :: IO Float
         randomFIO = randomIO
