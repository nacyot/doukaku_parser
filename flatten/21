18
19</pre></td><td class="code"><div class="highlight"><pre>    #i = 1;
    while( #i &lt;= 100 ) {
        call excelalpha #i;
        if ( #i != 1 ) {
            insert &quot;, &quot;;
        }
        insert $$return;
        #i = #i + 1;
    }
    endmacro;

excelalpha:
    $$result = &quot;&quot;;
    while ( ##1 != 0 ) {
        ##1 = ##1 - 1;
        $$result = char( ##1 % 26 + &#39;A&#39; ) + $$result;
        ##1 = ##1 / 26;
    }
    return $$result;
</pre></div>
</td></tr></table>
