22</pre></td><td class="code"><div class="highlight"><pre><span class="p">(</span><span class="nf">use</span> <span class="nv">util</span><span class="o">.</span><span class="nv">stream</span><span class="p">)</span>

<span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">stream-appendx</span> <span class="nv">x</span><span class="p">)</span>
  <span class="p">(</span><span class="k">cond </span><span class="p">((</span><span class="nf">stream-null?</span> <span class="nv">x</span><span class="p">)</span> <span class="p">(</span><span class="nf">stream-delay</span> <span class="p">()))</span>
        <span class="p">((</span><span class="nf">stream?</span> <span class="p">(</span><span class="nf">stream-car</span> <span class="nv">x</span><span class="p">))</span> <span class="p">(</span><span class="nf">stream-append</span> <span class="p">(</span><span class="nf">stream-car</span> <span class="nv">x</span><span class="p">)</span> <span class="p">(</span><span class="nf">stream-delay</span> <span class="p">(</span><span class="nf">stream-appendx</span> <span class="p">(</span><span class="nf">stream-cdr</span> <span class="nv">x</span><span class="p">)))))</span>
        <span class="p">(</span><span class="k">else </span><span class="nv">x</span><span class="p">)))</span>

<span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">stream-iota-n</span> <span class="o">.</span> <span class="nv">rest</span><span class="p">)</span>
  <span class="p">(</span><span class="nf">stream-appendx</span>
    <span class="p">(</span><span class="nf">stream-map</span>
      <span class="p">(</span><span class="k">if </span><span class="p">(</span><span class="nb">null? </span><span class="p">(</span><span class="nb">cdr </span><span class="nv">rest</span><span class="p">))</span>
        <span class="nv">list</span>
        <span class="p">(</span><span class="k">lambda </span><span class="p">(</span><span class="nf">x</span><span class="p">)</span>
          <span class="p">(</span><span class="nf">stream-map</span>
            <span class="p">(</span><span class="k">lambda </span><span class="p">(</span><span class="nf">y</span><span class="p">)</span> <span class="p">(</span><span class="nb">cons </span><span class="nv">x</span> <span class="nv">y</span><span class="p">))</span>
            <span class="p">(</span><span class="nb">apply </span><span class="nv">stream-iota-n</span> <span class="p">(</span><span class="nb">cdr </span><span class="nv">rest</span><span class="p">)))))</span>
      <span class="p">(</span><span class="nf">stream-iota</span> <span class="p">(</span><span class="nb">car </span><span class="nv">rest</span><span class="p">)))))</span>

<span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">i-&gt;alpha</span> <span class="nv">c</span><span class="p">)</span>
  <span class="p">(</span><span class="nb">apply </span><span class="nv">string</span> <span class="p">(</span><span class="nb">map </span><span class="p">(</span><span class="k">lambda </span><span class="p">(</span><span class="nf">x</span><span class="p">)</span> <span class="p">(</span><span class="nb">integer-&gt;char </span><span class="p">(</span><span class="nb">+ </span><span class="mi">65</span> <span class="nv">x</span><span class="p">)))</span> <span class="nv">c</span><span class="p">)))</span>

<span class="p">(</span><span class="nf">print</span> <span class="p">(</span><span class="nf">stream-&gt;list</span> <span class="p">(</span><span class="nf">stream-take</span> <span class="p">(</span><span class="nf">stream-map</span> <span class="nv">i-&gt;alpha</span> <span class="p">(</span><span class="nf">stream-iota-n</span> <span class="mi">26</span> <span class="mi">26</span><span class="p">))</span> <span class="mi">100</span><span class="p">)))</span>
