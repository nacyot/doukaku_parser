<span class="kr">let</span> <span class="n">int2alp</span> <span class="n">i</span> <span class="k">=</span>
  <span class="kr">let</span> <span class="n">code_base</span> <span class="k">=</span> <span class="n">code</span> <span class="sc">&#39;A&#39;</span> <span class="kr">in</span>
  <span class="n">escaped</span> <span class="k">(</span><span class="n">chr</span> <span class="k">(</span><span class="n">i</span> <span class="k">+</span> <span class="n">code_base</span><span class="k">))</span>

