12</pre></td><td class="code"><div class="highlight"><pre><span class="cp">&lt;?php</span>
<span class="k">function</span> <span class="nf">alphabetica</span><span class="p">(</span><span class="nv">$n</span><span class="p">)</span>
<span class="p">{</span>	<span class="nv">$r</span><span class="o">=</span><span class="s1">&#39;&#39;</span><span class="p">;</span>
	<span class="k">for</span><span class="p">(;</span><span class="nv">$n</span><span class="o">&gt;</span><span class="m">0</span><span class="p">;</span><span class="nv">$n</span><span class="o">=</span><span class="p">(</span><span class="nx">int</span><span class="p">)(</span><span class="nv">$n</span><span class="o">/</span><span class="m">26</span><span class="p">))</span>
		<span class="nv">$r</span><span class="o">=</span><span class="nf">chr</span><span class="p">(</span><span class="nf">ord</span><span class="p">(</span><span class="s1">&#39;A&#39;</span><span class="p">)</span> <span class="o">+</span> <span class="o">--</span><span class="nv">$n</span> <span class="o">%</span> <span class="m">26</span><span class="p">)</span><span class="o">.</span><span class="nv">$r</span><span class="p">;</span>
	<span class="k">return</span> <span class="nv">$r</span><span class="p">;</span>
<span class="p">}</span>

<span class="k">for</span><span class="p">(</span><span class="nv">$i</span><span class="o">=</span><span class="m">1</span><span class="p">;</span><span class="nv">$i</span><span class="o">&lt;=</span><span class="m">100</span><span class="p">;</span><span class="o">++</span><span class="nv">$i</span><span class="p">)</span>
	<span class="k">echo</span> <span class="nx">alphabetica</span><span class="p">(</span><span class="nv">$i</span><span class="p">),</span><span class="s2">&quot; &quot;</span><span class="p">;</span>
<span class="k">echo</span> <span class="s2">&quot;</span><span class="se">\n</span><span class="s2">&quot;</span><span class="p">;</span>
<span class="cp">?&gt;</span><span class="x"></span>
</pre></div>
</td></tr></table>
    </div>
	

  
