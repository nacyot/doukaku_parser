17</pre></td><td class="code"><div class="highlight"><pre><span class="k">module</span> <span class="k">do</span><span class="n">ukaku</span>;
<span class="k">private</span> <span class="k">import</span> <span class="n">std</span>.<span class="n">stdio</span>;
<span class="kt">char</span>[] <span class="n">alphabets</span>(<span class="k">in</span><span class="n">t</span> <span class="n">n</span>) {
    <span class="kt">char</span> <span class="n">alph</span>(<span class="k">in</span><span class="n">t</span> <span class="n">m</span>) { <span class="k">return</span> <span class="sc">&#39;A&#39;</span>+<span class="n">m</span>-<span class="mi">1</span>; }
    <span class="k">in</span><span class="n">t</span> <span class="n">a2</span> = (<span class="n">n</span>-<span class="mi">1</span>) / <span class="mi">26</span>;
    <span class="k">in</span><span class="n">t</span> <span class="n">a1</span> = (<span class="n">n</span>-<span class="mi">1</span>) % <span class="mi">26</span> + <span class="mi">1</span>;
    <span class="k">if</span> (!<span class="n">a2</span>)
        <span class="k">return</span> [<span class="n">alph</span>(<span class="n">a1</span>)];
    <span class="k">else</span>
        <span class="k">return</span> [<span class="n">alph</span>(<span class="n">a2</span>)] ~ <span class="n">alph</span>(<span class="n">a1</span>);
}

<span class="kt">void</span> <span class="n">main</span>() {
    <span class="k">for</span> (<span class="k">in</span><span class="n">t</span> <span class="n">i</span>=<span class="mi">1</span>; <span class="n">i</span>&lt;=<span class="mi">100</span>; <span class="n">i</span>++)
        <span class="n">writef</span>(<span class="s">&quot;%s &quot;</span>, <span class="n">alphabets</span>(<span class="n">i</span>));
    <span class="n">writefln</span>(<span class="s">&quot;&quot;</span>);
}
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
