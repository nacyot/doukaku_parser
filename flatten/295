69</pre></td><td class="code"><div class="highlight"><pre>import scala.util.Sorting

object Doukaku295 {
  
  implicit def option2ordered[A &lt;% Ordered[A]](opt: Option[A]): Ordered[Option[A]] =
  new Ordered[Option[A]] {
    def compare(that: Option[A]) = {
      if (opt.isEmpty) -1
      else if (that.isEmpty) 1
      else opt.get compare that.get
    }
  }
  
  val R = &quot;&quot;&quot;^(\P{Nd}*)(\p{Nd}*)(.*)$&quot;&quot;&quot;.r
  def toMixedNumbersList(str: String): List[(String, Option[BigInt])] = {
    if (str.length == 0) Nil
    else {
      str match {
        case R(s, &quot;&quot;, r) =&gt; (s, None           ) :: toMixedNumbersList(r)
        case R(s, n , r) =&gt; (s, Some(BigInt(n))) :: toMixedNumbersList(r)
      }
    }
  }
  
  def sortMixedNumbers(data: Seq[String]) = {
    val sa = data.map(d =&gt; (toMixedNumbersList(d) : Iterable[(String, Option[BigInt])], d))
    Sorting.stableSort(sa).map(_._2)
  }
  
  def main(args: Array[String]) = {
    
    // ソート対象 (適当にシャッフル済み)
    val data1: Seq[String] =
      Array(&quot;100.txt&quot;, &quot;1.txt&quot;, &quot;20.txt&quot;, &quot;2.txt&quot;, &quot;10.txt&quot;)
    val data2: Seq[String] =
      Array(&quot;x13&quot;, &quot;x1B&quot;, &quot;xAB&quot;, &quot;x12&quot;, &quot;x1A&quot;)
    val data3: Seq[String] =
      Array(&quot;A2B2&quot;, &quot;A10B1&quot;, &quot;A2B10&quot;, &quot;A10B10&quot;, &quot;A10B2&quot;, &quot;A1B1&quot;, &quot;A1B10&quot;, &quot;A1B2&quot;, &quot;A2B1&quot;)
    val data4: Seq[String] =
      Array(&quot;う５４う&quot;, &quot;い２３い&quot;, &quot;い１い&quot;, &quot;あ９あ&quot;, &quot;う１００う&quot;, &quot;あ８８あ&quot;)
    
    // 辞書順ソート
    val dic1 = Sorting.stableSort(data1)
    val dic2 = Sorting.stableSort(data2)
    val dic3 = Sorting.stableSort(data3)
    val dic4 = Sorting.stableSort(data4)
    
    // 数字混じり文字列ソート
    val num1 = sortMixedNumbers(data1)
    val num2 = sortMixedNumbers(data2)
    val num3 = sortMixedNumbers(data3)
    val num4 = sortMixedNumbers(data4)
    
    
    println(&quot;辞書順ソート: &quot; + dic1.mkString(&quot;, &quot;))
    println(&quot;数字混じり文字列ソート: &quot; + num1.mkString(&quot;, &quot;))
    println
    println(&quot;辞書順ソート: &quot; + dic2.mkString(&quot;, &quot;))
    println(&quot;数字混じり文字列ソート: &quot; + num2.mkString(&quot;, &quot;))
    println
    println(&quot;辞書順ソート: &quot; + dic3.mkString(&quot;, &quot;))
    println(&quot;数字混じり文字列ソート: &quot; + num3.mkString(&quot;, &quot;))
    println
    println(&quot;辞書順ソート: &quot; + dic4.mkString(&quot;, &quot;))
    println(&quot;数字混じり文字列ソート: &quot; + num4.mkString(&quot;, &quot;))
    
  }
  
}
</pre></div>
