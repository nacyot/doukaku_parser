<span class="c1">--               Just c&#39; -&gt; c&#39;</span>
<span class="c1">--               Nothing -&gt; c</span>

<span class="c1">-- import Maybe しとけば fromMaybe が使える</span>

<span class="nv">trc</span> <span class="nv">ccs</span> <span class="nv">c</span> <span class="o">=</span> <span class="nv">fromMaybe</span> <span class="nv">c</span> <span class="o">$</span> <span class="nv">lookup</span> <span class="nv">c</span> <span class="nv">ccs</span>
