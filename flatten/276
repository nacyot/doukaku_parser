#dct = 0;
$sp = &quot;　&quot;;
$line = &quot;&quot;;

#i = 0;
while (#i &lt; #w) {
    $line = $line+$sp;
    #i = #i + 1;
}
$line = $line+&quot;\n&quot;;

#i = 0;
while (#i &lt; #w) {
    insert $line;
    #i = #i + 1;
}
movetolineno #w,#w/2;

#i = 0;
while (1) {
    $ant = gettext(x, y, x+2, y);
    if ($ant == $sp) {
        overwrite &quot;○&quot;;
        #dct = (#dct+1)%4;
    } else {
        overwrite $sp;
        #dct = (((#dct+1)%4)+2)%4;
    }
    left;
    if (#dct == 0) {
        up;
    } else if (#dct == 1) {
        right;
    } else if (#dct == 2) {
        down;
    } else if (#dct == 3) {
        left;
    }
    if ((x &gt; #w*2-1)||(x == 0)||(y == 0)||(y &gt; #w-1)) {
        endmacro;
    }
}
endmacro;
</pre></div>
</td></tr></table>
