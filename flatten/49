<span class="k">using</span> <span class="nn">System.Text</span><span class="p">;</span>
<span class="k">class</span> <span class="nc">P</span>
<span class="k">{</span>
    <span class="k">static</span> <span class="k">void</span> <span class="nf">Main</span><span class="p">(</span><span class="kt">string</span><span class="p">[]</span> <span class="n">args</span><span class="p">)</span>
    <span class="k">{</span>
        <span class="kt">int</span><span class="p">[]</span> <span class="n">data</span> <span class="p">=</span> <span class="k">{</span> <span class="m">3</span><span class="p">,</span> <span class="m">1</span><span class="p">,</span> <span class="m">4</span><span class="p">,</span> <span class="m">1</span><span class="p">,</span> <span class="m">5</span><span class="p">,</span> <span class="m">9</span><span class="p">,</span> <span class="m">2</span><span class="p">,</span> <span class="m">6</span><span class="p">,</span> <span class="m">5</span> <span class="k">}</span><span class="p">;</span>
        <span class="kt">int</span><span class="p">[]</span> <span class="n">diff</span> <span class="p">=</span> <span class="n">Diff</span><span class="p">(</span><span class="n">data</span><span class="p">);</span>
        <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">(</span><span class="s">&quot;data = {0}&quot;</span><span class="p">,</span>
                         <span class="n">data</span><span class="p">.</span><span class="n">ToRawString</span><span class="p">());</span>
        <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">(</span><span class="s">&quot;diff = {0}&quot;</span><span class="p">,</span>
                         <span class="n">diff</span><span class="p">.</span><span class="n">ToRawString</span><span class="p">());</span>
    <span class="k">}</span>
