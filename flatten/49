    <span class="k">{</span>
        <span class="kt">string</span> <span class="n">str</span> <span class="p">=</span> <span class="n">ToRawString</span><span class="p">&lt;</span><span class="n">T</span><span class="p">&gt;(</span><span class="n">array</span><span class="p">);</span>
