<span class="kt">int</span> <span class="o">*</span><span class="n">diff</span><span class="p">(</span> <span class="kt">int</span> <span class="o">*</span><span class="n">list</span><span class="p">,</span> <span class="kt">int</span> <span class="n">len</span> <span class="p">){</span>
