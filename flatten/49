    Range(&quot;B1&quot;) = &quot;=IF(ISBLANK(A2),&quot;&quot;&quot;&quot;,A2-A1)&quot;
    Range(&quot;B1&quot;).Resize(Range(&quot;A1&quot;).End(xlDown).Row).FillDown
End Sub
</pre></div>
