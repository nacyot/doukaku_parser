    <span class="kt">int</span> <span class="n">price</span><span class="p">;</span>

    <span class="n">sin</span> <span class="o">&gt;&gt;</span> <span class="n">price</span><span class="p">;</span>

    <span class="n">price</span> <span class="o">*=</span> <span class="mf">1.05</span><span class="p">;</span>

