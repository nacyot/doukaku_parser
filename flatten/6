25
26</pre></td><td class="code"><div class="highlight"><pre>    call IncludeTax &quot;ダイコン150円、ハクサイ120円、ジャガイモ30円&quot;;
    message $$return;
    endmacro;

IncludeTax:
    $$figure = &quot;&quot;;
    $$result = &quot;&quot;;
    while( $$1 != &quot;&quot; ) {
        ##char = ascii( $$1 );
        if ( ##char &gt;= &#39;0&#39; &amp;&amp; ##char &lt;= &#39;9&#39; ) {
            $$figure = $$figure + char( ##char );
        } else {
            if ( $$figure != &quot;&quot; ) {
                $$result = $$result + str( val( $$figure ) * 105 / 100 );
                $$figure = &quot;&quot;;
            }
            $$result = $$result + char( ##char );
        }
        
        $$1 = rightstr( $$1, strlen( $$1 ) - strlen( char( ##char ) ) );
    }
    if ( $$figure != &quot;&quot; ) {
        $$result = $$result + str( val( $$figure ) * 105 / 100 );
        $$figure = &quot;&quot;;
    }
    return $$result;
</pre></div>
</td></tr></table>
    </div>
	

  
