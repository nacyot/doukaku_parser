15
16
17</pre></td><td class="code"><div class="highlight"><pre><span class="p">(</span><span class="nf">use</span> <span class="nv">srfi-13</span><span class="p">)</span>
<span class="p">(</span><span class="k">define </span><span class="nv">henkan</span>
  <span class="p">(</span><span class="k">lambda </span><span class="p">(</span><span class="nf">str</span><span class="p">)</span>
    <span class="p">(</span><span class="nf">string-concatenate</span> <span class="p">(</span><span class="nf">tokenize</span> <span class="nv">str</span><span class="p">))))</span>

<span class="p">(</span><span class="k">define </span><span class="nv">tokenize</span>
  <span class="p">(</span><span class="k">lambda </span><span class="p">(</span><span class="nf">str</span><span class="p">)</span> 
    <span class="p">(</span><span class="nf">let1</span> <span class="nv">m</span> <span class="p">(</span><span class="o">#</span><span class="nv">/</span><span class="err">\</span><span class="nv">d+/</span> <span class="nv">str</span><span class="p">)</span>
      <span class="p">(</span><span class="k">if </span><span class="nv">m</span>
          <span class="p">(</span><span class="k">if </span><span class="p">(</span><span class="nf">string-null?</span> <span class="p">(</span><span class="nf">rxmatch-after</span> <span class="nv">m</span><span class="p">))</span>
              <span class="p">(</span><span class="nb">cons </span><span class="p">(</span><span class="nf">rxmatch-before</span> <span class="nv">m</span><span class="p">)</span> <span class="p">(</span><span class="nb">cons </span><span class="p">(</span><span class="nf">taxation</span> <span class="p">(</span><span class="nf">m</span><span class="p">))</span> <span class="o">&#39;</span><span class="p">()))</span>
              <span class="p">(</span><span class="nb">cons </span><span class="p">(</span><span class="nf">rxmatch-before</span> <span class="nv">m</span><span class="p">)</span> <span class="p">(</span><span class="nb">cons </span><span class="p">(</span><span class="nf">taxation</span> <span class="p">(</span><span class="nf">m</span><span class="p">))</span> <span class="p">(</span><span class="nf">tokenize</span> <span class="p">(</span><span class="nf">rxmatch-after</span> <span class="nv">m</span><span class="p">)))))</span>
          <span class="p">(</span><span class="nb">cons </span><span class="nv">str</span> <span class="o">&#39;</span><span class="p">())))))</span>

<span class="p">(</span><span class="k">define </span><span class="nv">taxation</span>
  <span class="p">(</span><span class="k">lambda </span><span class="p">(</span><span class="nf">n</span><span class="p">)</span>
    <span class="p">(</span><span class="nb">number-&gt;string </span><span class="p">(</span><span class="nf">x-&gt;integer</span> <span class="p">(</span><span class="nb">floor </span><span class="p">(</span><span class="nb">* </span><span class="mf">1.05</span> <span class="p">(</span><span class="nb">string-&gt;number </span><span class="nv">n</span><span class="p">)))))))</span>
