	<a href="/web/20110320100230/http://ja.doukaku.org/comment/1702/">#1702</a>(<script language="JavaScript">print_time(new Date("2007/08/02 12:20 GMT"));</script><noscript>2007/08/02 12:20 GMT</noscript>)
	
	
	
	[<a href="/web/20110320100230/http://ja.doukaku.org/lang/pnuts/">
