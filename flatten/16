	<span class="k">public</span> <span class="k">static</span> <span class="n">Object</span><span class="o">[]</span> <span class="n">toUniqueArray</span><span class="o">(</span><span class="n">Object</span><span class="o">[]</span> <span class="n">array</span><span class="o">)</span> <span class="o">{</span>
