<span class="kr">open</span> <span class="n">Array</span><span class="k">;;</span>

<span class="kr">let</span> <span class="n">uniq</span> <span class="n">xs</span> <span class="k">=</span>
    <span class="kr">let</span> <span class="n">ars</span> <span class="k">=</span> <span class="n">create</span> <span class="k">(</span><span class="n">length</span> <span class="n">xs</span><span class="k">)</span> <span class="n">xs</span><span class="k">.(</span><span class="mi">0</span><span class="k">)</span> <span class="kr">in</span>
    <span class="kr">let</span> <span class="n">i</span> <span class="k">=</span> <span class="n">ref</span> <span class="mi">1</span> <span class="kr">in</span>
    <span class="kr">for</span> <span class="n">idx</span> <span class="k">=</span> <span class="mi">1</span> <span class="kr">to</span> <span class="k">(</span><span class="n">length</span> <span class="n">xs</span><span class="k">)-</span><span class="mi">1</span>
