<span class="k">private</span> <span class="k">import</span> <span class="n">std</span>.<span class="n">stdio</span>;
<span class="n">A</span>[] <span class="n">uniq</span>(<span class="n">A</span>)(<span class="n">A</span>[] <span class="n">ary</span>) {          <span class="c">// 型推論！</span>
    <span class="n">A</span>[<span class="n">A</span>] <span class="n">exists</span>;
    <span class="n">A</span>[] <span class="n">ret</span>;
    <span class="k">foreach</span>(<span class="n">x</span>; <span class="n">ary</span>) {
        <span class="k">if</span> (!(<span class="n">x</span> <span class="k">in</span> <span class="n">exists</span>)) {
            <span class="n">ret</span> ~= <span class="n">x</span>;
            <span class="n">exists</span>[<span class="n">x</span>] = <span class="mi">1</span>;
        }
    }
    <span class="k">return</span> <span class="n">ret</span>;
}

