	<span class="k">var</span> <span class="nx">h</span><span class="o">=</span><span class="p">{};</span>
	<span class="k">var</span> <span class="nx">tostr</span><span class="o">=</span><span class="k">function</span><span class="p">(</span><span class="nx">obj</span><span class="p">){</span>
