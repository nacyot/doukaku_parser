   <span class="kr">if</span> <span class="n">p</span> <span class="k">=</span> <span class="n">q</span> <span class="kr">then</span> <span class="kr">false</span>
   <span class="kr">else</span>
     <span class="kr">if</span> <span class="n">a</span><span class="k">.(</span><span class="n">p</span><span class="k">)</span> <span class="k">=</span> <span class="n">m</span> <span class="kr">then</span> <span class="kr">true</span>
     <span class="kr">else</span> <span class="n">appears_between</span> <span class="k">(</span><span class="n">succ</span> <span class="n">p</span><span class="k">)</span> <span class="n">q</span> <span class="n">m</span> <span class="n">a</span>

