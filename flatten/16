  <span class="kr">let</span> <span class="n">ys</span> <span class="k">=</span> <span class="n">Array</span><span class="k">.</span><span class="n">copy</span> <span class="n">xs</span> <span class="kr">in</span>
  <span class="kr">let</span> <span class="kr">rec</span> <span class="n">move</span> <span class="n">x</span> <span class="n">y</span> <span class="k">=</span>
    <span class="kr">if</span> <span class="k">(</span><span class="n">Array</span><span class="k">.</span><span class="n">length</span> <span class="n">xs</span><span class="k">)</span> <span class="k">=</span> <span class="n">x</span> <span class="kr">then</span>
      <span class="n">Array</span><span class="k">.</span><span class="n">sub</span> <span class="n">ys</span> <span class="mi">0</span> <span class="n">y</span>
    <span class="kr">else</span>
      <span class="kr">if</span> <span class="n">appears_between</span> <span class="mi">0</span> <span class="n">y</span> <span class="n">xs</span><span class="k">.(</span><span class="n">x</span><span class="k">)</span> <span class="n">ys</span> <span class="kr">then</span>
