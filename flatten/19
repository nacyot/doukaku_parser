	<a href="/web/20100617095748/http://ja.doukaku.org/comment/751/">#751</a>(<script language="JavaScript">print_time(new Date("2007/07/13 11:52 GMT"));</script><noscript>2007/07/13 11:52 GMT</noscript>)
	
	
	
