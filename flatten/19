<span class="k">import</span> <span class="n">tango</span>.<span class="n">net</span>.<span class="n">http</span>.<span class="n">HttpGet</span>;
<span class="k">import</span> <span class="n">tango</span>.<span class="n">text</span>.<span class="n">Regex</span>;

<span class="kt">void</span> <span class="n">main</span>(){
    <span class="k">auto</span> <span class="n">page</span> = <span class="k">new</span> <span class="n">HttpGet</span>(<span class="s">&quot;http://ja.doukaku.org/feeds/comments/&quot;</span>);
    <span class="k">auto</span> <span class="n">data</span> = <span class="k">cast</span>(<span class="kt">char</span>[])<span class="n">page</span>.<span class="n">read</span>;
    <span class="n">Stdout</span>(<span class="n">search</span>(<span class="n">data</span>, <span class="s">r&quot;&lt;lastBuildDate&gt;(.+?)&lt;/lastBuildDate&gt;&quot;</span>).<span class="n">match</span>(<span class="mi">1</span>)).<span class="n">newline</span>;
}
</pre></div>
