<span class="kr">import</span> <span class="n">Text</span><span class="o">.</span><span class="n">XML</span><span class="o">.</span><span class="n">HXT</span><span class="o">.</span><span class="n">Arrow</span><span class="o">.</span><span class="n">XmlArrow</span>
<span class="kr">import</span> <span class="n">Text</span><span class="o">.</span><span class="n">XML</span><span class="o">.</span><span class="n">HXT</span><span class="o">.</span><span class="n">Arrow</span><span class="o">.</span><span class="n">XmlIOStateArrow</span>
<span class="kr">import</span> <span class="n">Text</span><span class="o">.</span><span class="n">XML</span><span class="o">.</span><span class="n">HXT</span><span class="o">.</span><span class="n">DOM</span><span class="o">.</span><span class="n">XmlKeywords</span>

<span class="nv">uri</span> <span class="o">::</span> <span class="n">URI</span>
<span class="nv">uri</span> <span class="o">=</span> <span class="nv">fromJust</span> <span class="o">$</span> <span class="nv">parseURI</span> <span class="s">&quot;http://ja.doukaku.org/feeds/comments/&quot;</span>

<span class="nv">_data</span> <span class="o">::</span> <span class="n">String</span> <span class="c1">-- data は予約語なので使えません</span>
<span class="nv">_data</span> <span class="o">=</span> <span class="nv">unsafePerformIO</span> <span class="o">$</span>
        <span class="kr">do</span> <span class="n">Right</span> <span class="nv">resp</span> <span class="o">&lt;-</span> <span class="nv">simpleHTTP</span> <span class="o">$</span> <span class="n">Request</span> <span class="nv">uri</span> <span class="n">GET</span> <span class="p">[]</span> <span class="s">&quot;&quot;</span>
           <span class="nv">return</span> <span class="o">$</span> <span class="nv">rspBody</span> <span class="nv">resp</span>

<span class="nv">main</span> <span class="o">=</span> <span class="kr">do</span> <span class="nv">results</span> <span class="o">&lt;-</span> <span class="nv">runX</span> <span class="p">(</span> <span class="nv">readString</span> <span class="p">[</span> <span class="p">(</span><span class="nv">a_validate</span><span class="p">,</span> <span class="nv">v_0</span><span class="p">)</span> <span class="p">]</span> <span class="nv">_data</span>
                            <span class="o">&gt;&gt;&gt;</span>
                            <span class="nv">deep</span> <span class="p">(</span><span class="nv">isElem</span> <span class="o">&gt;&gt;&gt;</span> <span class="nv">hasName</span> <span class="s">&quot;lastBuildDate&quot;</span><span class="p">)</span>
                            <span class="o">&gt;&gt;&gt;</span>
                            <span class="nv">getChildren</span>
                            <span class="o">&gt;&gt;&gt;</span>
                            <span class="nv">getText</span>
                          <span class="p">)</span>
          <span class="nv">print</span> <span class="nv">results</span>
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
