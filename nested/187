  <span class="p">(</span><span class="k">let </span><span class="nv">loop1</span> <span class="p">((</span><span class="nf">i</span> <span class="nv">range-min</span><span class="p">)</span> <span class="p">(</span><span class="nf">result</span> <span class="o">&#39;</span><span class="p">()))</span>
    <span class="p">(</span><span class="k">if </span><span class="p">(</span><span class="nb">&gt;= </span><span class="nv">i</span> <span class="nv">range-max</span><span class="p">)</span>
      <span class="nv">result</span>
      <span class="p">(</span><span class="nf">loop1</span> <span class="p">(</span><span class="nb">+ </span><span class="nv">i</span> <span class="mi">1</span><span class="p">)</span> <span class="p">(</span><span class="nb">append </span><span class="nv">result</span>
