    def sort(min:Int,max:Int,data:Array[Int]):Array[Int] = {
        (min to max).flatMap { d =&gt; data.filter { _ == d } }.toArray
    }
    def main(args:Array[String]):Unit = {
        Console.println(BucketSort.sort(-1,10,Array(-1,9,4,8,9,6,3,9,5,2)).toString)
    }
}
