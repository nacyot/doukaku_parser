	<a href="/web/20080917035408/http://ja.doukaku.org/comment/6665/">#6665</a>(<script language="JavaScript">print_time(new Date("2008/07/04 15:38 GMT"));</script><noscript>2008/07/04 15:38 GMT</noscript>)
	
	
	
