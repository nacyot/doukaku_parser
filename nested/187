16
17
18
19</pre></td><td class="code"><div class="highlight"><pre><span class="k">private</span> <span class="k">import</span> <span class="n">std</span>.<span class="n">stdio</span>;

<span class="kt">void</span> <span class="n">main</span>() {
    <span class="kt">int</span>[] <span class="n">a</span> = [<span class="mi">1</span>, <span class="mi">9</span>, <span class="mi">4</span>, <span class="mi">8</span>, <span class="mi">9</span>, <span class="mi">6</span>, <span class="mi">3</span>, <span class="mi">9</span>, <span class="mi">5</span>, <span class="mi">2</span>];
    <span class="n">a</span>.<span class="n">countingSort</span>(<span class="mi">1</span>, <span class="mi">10</span>);
    <span class="n">writeln</span>(<span class="n">a</span>);
}

<span class="kt">void</span> <span class="n">countingSort</span>(<span class="kt">int</span>[] <span class="n">array</span>, <span class="kt">int</span> <span class="n">min</span>, <span class="kt">int</span> <span class="n">max</span>) {
    <span class="k">auto</span> <span class="n">counts</span> = <span class="k">new</span> <span class="kt">uint</span>[<span class="n">max</span> - <span class="n">min</span> + <span class="mi">1</span>];
    <span class="k">foreach</span>(<span class="n">n</span>; <span class="n">array</span>) {
        <span class="n">counts</span>[<span class="n">n</span> - <span class="n">min</span>]++;
    }
    <span class="k">auto</span> <span class="n">p</span> = <span class="n">array</span>.<span class="n">ptr</span>;
    <span class="k">foreach</span>(<span class="n">i</span>, <span class="n">c</span>; <span class="n">counts</span>) {
        <span class="n">p</span>[<span class="mf">0.</span>.<span class="n">c</span>] = <span class="n">i</span> + <span class="n">min</span>;
        <span class="n">p</span> += <span class="n">c</span>;
    }
}
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
