                                      <span class="kr">in</span> <span class="nv">evalState</span> <span class="p">(</span><span class="nv">countingSort_S</span> <span class="nv">min</span> <span class="nv">ctr</span><span class="p">)</span> <span class="p">(</span><span class="mi">0</span><span class="p">,</span><span class="nv">result</span><span class="p">)</span>
