      <table><tr><td class="linenos"><pre>1
2
3</pre></td><td class="code"><div class="highlight"><pre>$1 = &quot;l&quot;;
$2 = &quot;o&quot;;
insert &quot;H\x65&quot;+$1+$1+$2+&quot;, w&quot;+$2+&quot;\X72&quot;+$1+&quot;d!&quot;;
</pre></div>
</td></tr></table>
    </div>
	

  
