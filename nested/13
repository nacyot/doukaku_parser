      <table><tr><td class="linenos"><pre>1</pre></td><td class="code"><div class="highlight"><pre><span class="s2">&quot;Helo,Wrd!&quot;</span><span class="o">.</span><span class="n">gsub</span><span class="p">(</span><span class="sr">/(.{2})(.)(.)(.{2})(.)(.{2})/</span><span class="p">)</span> <span class="p">{</span><span class="vg">$1</span><span class="o">+</span><span class="vg">$2</span><span class="o">+</span><span class="vg">$2</span><span class="o">+</span><span class="vg">$3</span><span class="o">+</span><span class="vg">$4</span><span class="o">+</span><span class="vg">$3</span><span class="o">+</span><span class="vg">$5</span><span class="o">+</span><span class="vg">$2</span><span class="o">+</span><span class="vg">$6</span><span class="p">}</span>
</pre></div>
</td></tr></table>
    </div>
	

  
