<span class="k">use</span> <span class="n">strict</span><span class="p">;</span>
<span class="k">use</span> <span class="n">warnings</span><span class="p">;</span>

<span class="k">use</span> <span class="nn">List::</span><span class="n">Util</span> <span class="sx">qw( reduce )</span><span class="p">;</span>

<span class="k">my</span> <span class="nv">$max</span> <span class="o">=</span> <span class="mi">10000</span><span class="p">;</span>

<span class="k">for</span> <span class="k">my</span> <span class="nv">$orig</span> <span class="p">(</span> <span class="mi">10</span> <span class="o">..</span> <span class="nv">$max</span> <span class="p">)</span> <span class="p">{</span>
        <span class="k">my</span> <span class="nv">$num13</span> <span class="o">=</span> <span class="n">reduce</span> <span class="p">{</span> <span class="mi">13</span> <span class="o">*</span> <span class="nv">$a</span> <span class="o">+</span> <span class="nv">$b</span> <span class="p">}</span> <span class="nb">split</span> <span class="sr">//</span><span class="p">,</span> <span class="nv">$orig</span><span class="p">;</span>
        <span class="k">print</span> <span class="s">&quot;Found: $orig =&gt; $num13\n&quot;</span> <span class="ow">and</span> <span class="nb">exit</span> <span class="mi">0</span>
                <span class="k">if</span><span class="p">(</span> <span class="nv">$num13</span> <span class="nv">%</span> <span class="err">$</span><span class="nv">orig</span> <span class="o">==</span> <span class="mi">0</span> <span class="p">);</span>
<span class="p">}</span>
</pre></div>
