nagasa = it.toString().toList().size
youso =  it.toString().toList()
jyusanshin = 0
for (x in 1 .. nagasa) {
    jyusanshin = jyusanshin + (youso[-x].toInteger())*(13.power(x-1))
}
if (jyusanshin%it==0) { 
    println &quot;Found: &quot;+it +&quot;(&quot;+jyusanshin+&quot;)&quot;
    System.exit(0)
    }
}
</pre></div>
