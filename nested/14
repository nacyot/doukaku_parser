  <span class="kp">loop</span> <span class="p">{</span>
    <span class="n">n13</span> <span class="o">=</span> <span class="n">n</span><span class="o">.</span><span class="n">to_s</span><span class="o">.</span><span class="n">to_i</span><span class="p">(</span><span class="mi">13</span><span class="p">)</span>
