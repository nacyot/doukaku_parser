	for (x=10; !check(x); x++) ;
	print x; exit;
}
function check(x,  n,k,i)
{
	n = 0;
	k = length(x);
	for (i=1; i&lt;=k; i++) n = n * 13 + substr(x,i,1)
	return (n == x * 2)? 1 : 0;
}
