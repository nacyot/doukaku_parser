    message str( #i );
    endmacro;

convert:
    $$val = str( ##1 );
    ##result = 0;
    ##i = 0;
    while( ##i &lt; strlen( $$val ) ) {
        ##result = ##result * 13 + val( midstr( $$val, ##i, 1 ) );
        ##i = ##i + 1;
    }
    return ##result;
