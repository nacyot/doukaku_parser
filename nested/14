<span class="kr">let</span> <span class="n">solv</span> <span class="k">()</span> <span class="k">=</span>
    <span class="kr">let</span> <span class="kr">rec</span> <span class="n">trans</span> <span class="k">=</span> <span class="kr">function</span>
        <span class="k">|</span> <span class="mi">0</span> <span class="k">-&gt;</span> <span class="mi">0</span>
        <span class="k">|</span> <span class="n">n</span> <span class="k">-&gt;</span> <span class="k">(</span><span class="n">n</span> <span class="kr">mod</span> <span class="mi">10</span><span class="k">)</span> <span class="k">+</span> <span class="k">(</span><span class="n">trans</span> <span class="k">(</span><span class="n">n</span> <span class="o">/</span> <span class="mi">10</span><span class="k">))</span> <span class="k">*</span> <span class="n">base</span>
    <span class="kr">and</span> <span class="n">base</span> <span class="k">=</span> <span class="mi">13</span>
    <span class="kr">and</span> <span class="n">solving</span> <span class="n">m</span> <span class="k">=</span>
             <span class="kr">if</span> <span class="k">(</span><span class="n">trans</span> <span class="n">m</span><span class="k">)</span> <span class="kr">mod</span> <span class="n">m</span> <span class="k">=</span> <span class="mi">0</span>
             <span class="kr">then</span> <span class="n">m</span>
             <span class="kr">else</span> <span class="n">solving</span> <span class="k">(</span><span class="n">m</span><span class="k">+</span><span class="mi">1</span><span class="k">)</span>
    <span class="kr">in</span> <span class="n">solving</span> <span class="mi">10</span><span class="k">;;</span>

<span class="n">Console</span><span class="k">.</span><span class="n">WriteLine</span><span class="k">(</span> <span class="n">solv</span><span class="k">()</span> <span class="k">);;</span>
