				<span class="p">{</span>	<span class="c">// 発見</span>
					<span class="k">echo</span> <span class="s2">&quot;</span><span class="si">$a</span><span class="se">\n</span><span class="s2">&quot;</span><span class="p">;</span>
					<span class="k">break</span> <span class="m">2</span><span class="p">;</span>
				<span class="p">}</span>
