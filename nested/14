  <span class="n">rem2</span> <span class="o">=</span> <span class="n">rem</span><span class="o">[</span><span class="mi">1</span><span class="o">.</span><span class="n">.rem</span><span class="o">.</span><span class="n">length</span><span class="o">-</span><span class="mi">1</span><span class="o">]</span>
