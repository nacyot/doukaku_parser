<span class="nv">fromDecimal</span> <span class="o">::</span> <span class="p">(</span><span class="n">Integral</span> <span class="nv">a</span><span class="p">)</span> <span class="o">=&gt;</span> <span class="nv">a</span> <span class="o">-&gt;</span> <span class="nv">a</span> <span class="o">-&gt;</span> <span class="nv">a</span>
