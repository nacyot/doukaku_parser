      <span class="k">if</span> <span class="p">(</span><span class="n">m</span><span class="o">-</span><span class="n">x</span><span class="o">*</span><span class="n">ai</span><span class="p">)</span> <span class="o">%</span> <span class="n">rem2</span><span class="o">[</span><span class="mi">0</span><span class="o">]</span> <span class="o">==</span> <span class="mi">0</span>
