    <span class="k">for</span><span class="p">(</span><span class="nv">$i</span> <span class="o">=</span> <span class="m">10</span><span class="p">;</span> <span class="nv">$i</span> <span class="o">&lt;=</span> <span class="nv">$max_n</span><span class="p">;</span> <span class="nv">$i</span><span class="o">++</span><span class="p">){</span>
