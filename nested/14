<span class="k">struct</span> <span class="n">TransTri</span>
<span class="p">{</span>
    <span class="k">enum</span> <span class="p">{</span> <span class="n">value</span> <span class="o">=</span> <span class="n">BASE</span> <span class="o">*</span> <span class="n">TransTri</span><span class="o">&lt;</span><span class="n">n</span><span class="o">/</span><span class="n">DEC</span><span class="o">&gt;::</span><span class="n">value</span> <span class="o">+</span> <span class="n">n</span><span class="o">%</span><span class="n">DEC</span> <span class="p">};</span>
