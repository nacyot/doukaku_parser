		<span class="kt">char</span> <span class="n">str</span><span class="p">[</span><span class="mi">5</span><span class="p">];</span>
		<span class="n">itoa</span><span class="p">(</span><span class="n">idec</span><span class="p">,</span> <span class="n">str</span><span class="p">,</span><span class="mi">10</span><span class="p">);</span>
		<span class="n">i13</span> <span class="o">=</span> <span class="n">strtol</span><span class="p">(</span><span class="n">str</span><span class="p">,</span> <span class="nb">NULL</span><span class="p">,</span> <span class="mi">13</span><span class="p">);</span>
