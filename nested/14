			<span class="k">break</span><span class="p">;</span>
		<span class="p">}</span>
		<span class="k">for</span><span class="p">(</span><span class="nv">$a</span><span class="o">=</span><span class="nv">$c1</span><span class="p">;;)</span>
		<span class="p">{</span>	<span class="k">if</span><span class="p">((</span><span class="nv">$c</span><span class="o">/=</span><span class="m">10</span><span class="p">)</span><span class="o">&lt;</span><span class="m">1</span><span class="p">)</span>
