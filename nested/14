      <span class="n">search</span> <span class="n">m</span><span class="o">-</span><span class="n">x</span><span class="o">*</span><span class="n">ai</span><span class="p">,</span> <span class="n">rem2</span><span class="p">,</span> <span class="s2">&quot;</span><span class="si">#{</span><span class="n">result</span><span class="si">}#{</span><span class="n">x</span><span class="si">}</span><span class="s2">&quot;</span>
    <span class="k">end</span>
  <span class="k">end</span>
