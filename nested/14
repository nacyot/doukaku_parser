<span class="n">d</span><span class="o">=</span><span class="mi">3</span> <span class="c1"># keta - 1</span>
<span class="k">while</span> <span class="kp">true</span>
  <span class="nb">puts</span> <span class="s2">&quot;keta: </span><span class="si">#{</span><span class="n">d</span><span class="o">+</span><span class="mi">1</span><span class="si">}</span><span class="s2"> ==========================================================&quot;</span>
  <span class="n">max_n</span> <span class="o">=</span> <span class="mi">13</span><span class="o">**</span><span class="n">d</span> <span class="o">/</span> <span class="mi">10</span><span class="o">**</span><span class="n">d</span>
