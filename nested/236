<span class="k">if</span> <span class="n">flg</span>
  <span class="nb">puts</span> <span class="s2">&quot;17歳と</span><span class="si">#{</span><span class="n">date1</span> <span class="o">-</span> <span class="n">date2</span><span class="si">}</span><span class="s2">日&quot;</span>
  <span class="nb">puts</span> <span class="s2">&quot;17歳と</span><span class="si">#{</span><span class="n">m</span><span class="si">}</span><span class="s2">ヶ月と</span><span class="si">#{</span><span class="n">d</span><span class="si">}</span><span class="s2">日&quot;</span>
<span class="k">else</span>
  <span class="nb">puts</span> <span class="s2">&quot;17歳まであと</span><span class="si">#{</span><span class="n">date1</span> <span class="o">-</span> <span class="n">date2</span><span class="si">}</span><span class="s2">日&quot;</span>
  <span class="nb">puts</span> <span class="s2">&quot;17歳まであと</span><span class="si">#{</span><span class="n">m</span><span class="si">}</span><span class="s2">ヶ月と</span><span class="si">#{</span><span class="n">d</span><span class="si">}</span><span class="s2">日&quot;</span>
<span class="k">end</span>
</pre></div>
</td></tr></table>
    </div>
	

  
