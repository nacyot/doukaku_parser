DAY_OF_MONTH &lt;- [ 31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31 ];

class Date
{
    year=0;
    month=0;
    day=0;
    constructor(time=time())
    {
        local datetime = date(time);
        year = datetime.year;
        month= datetime.month+1;
        day  = datetime.day;
    }
    function parse(str)
    {
        local re = regexp(@&quot;^\s*(\d+)\s*/\s*(\d+)\s*/\s*(\d+)\s*$&quot;);
        local res = re.capture(str);
        year  = str.slice(res[1].begin, res[1].end).tointeger();
        month = str.slice(res[2].begin, res[2].end).tointeger();
        day   = str.slice(res[3].begin, res[3].end).tointeger();
    }
    // 1/1を0とする
    function yday()
    {
        local yday = DAY_OF_YEAR[month-1] + day - 1;
        if (month&gt;2 &amp;&amp; isLeapYear(year)) yday++;
        return yday;
    }
    // 1970/1/1を0とする
    function days()
    {
        local year0 = 1970;
        local function leapYears(year) { return year/4 - year/100 + year/400; }
        local days = 0;
        days += (year - year0)*365;
        days += yday();
        days += leapYears(year-1) - leapYears(year0-1); // これまでの閏年を加算
        return days;
    }
    function tostring()
    {
        return ::format(&quot;%d/%02d/%02d, (yday:%d, days:%d)&quot;, year, month, day, yday(), days());
    }
};

function isLeapYear(year)
{
    return (year%4 == 0 &amp;&amp; year%100 != 0 || year%400 == 0);
}

function println(...)
{
    foreach(str in vargv)
    {
        print(str.tostring());
        print(&quot; &quot;);
    }
    print(&quot;\n&quot;);
}

function calc17(birthday)
{
    local today = Date();
    
    local birthDate = clone today;
    birthDate.parse(birthday);
    
    local seventeenDate = clone birthDate;
    seventeenDate.year+=17;
    
    local mon = (today.year-seventeenDate.year)*12 + today.month-seventeenDate.month;
    local day = today.day-seventeenDate.day;
    if (day&lt;=0)
    {
        day += DAY_OF_MONTH[(today.month+10)%12]; // 前月の日数を加算
        mon--;
    }
    print(format(&quot;17歳と%dヶ月と%d日\n&quot;, mon, day));
    print(format(&quot;17歳と%d日\n&quot;, today.days() - seventeenDate.days()));
}

function main()
{
    local seventeens = 
    [
        { name=&quot;井上喜久子&quot;,    birthday=&quot;1964/ 9/25&quot; },
        { name=&quot;田村ゆかり&quot;,    birthday=&quot;1976/ 2/27&quot; },
        { name=&quot;堀江由衣&quot;,      birthday=&quot;1976/ 9/20&quot; },
        { name=&quot;野川さくら&quot;,    birthday=&quot;1978/ 3/ 1&quot; },
        { name=&quot;佐藤利奈&quot;,      birthday=&quot;1981/ 5/ 2&quot; },
        { name=&quot;石田燿子&quot;,      birthday=&quot;1973/10/ 7&quot; },
        { name=&quot;松澤由美&quot;,      birthday=&quot;1974/ 3/29&quot; },
        { name=&quot;こやまきみこ&quot;,  birthday=&quot;1979/ 1/27&quot; },
    ];
    foreach(foo in seventeens)
    {
        println(foo.name);
        calc17(foo.birthday);
        println();
    }
}

main();
</pre></div>
</td></tr></table>
