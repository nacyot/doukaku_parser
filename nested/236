11</pre></td><td class="code"><div class="highlight"><pre>seventeen.years &lt;- function(s=&quot;1964-09-25&quot;){
    d &lt;- c(as.POSIXlt(s), Sys.Date())
    d[1]$year &lt;- d[1]$year + 17
    d &lt;- as.Date(d)
    p &lt;- ifelse(diff(d)&gt;0, &quot;と&quot;, &quot;まで&quot;)
    if(diff(d)&lt;0) d[1:2] &lt;- d[2:1]
    l &lt;- seq(d[1], d[2], &quot;month&quot;)
    l &lt;- l[l&lt;d[2]]
    writeLines(sprintf(&quot;17歳%s%d日\n\nまたは17歳%s%dヶ月%d日\n&quot;, p, as.integer(diff(d)),
                       p, length(l)-1, as.integer(d[2]-l[length(l)])))
}
</pre></div>
