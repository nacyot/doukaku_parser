37
38</pre></td><td class="code"><div class="highlight"><pre><span class="c"># -*- coding: utf-8 -*-</span>

<span class="k">import</span> <span class="nn">sys</span>
<span class="k">import</span> <span class="nn">datetime</span>
<span class="k">import</span> <span class="nn">time</span>

<span class="k">def</span> <span class="nf">calc_seventeenth</span><span class="p">(</span><span class="n">birthday</span><span class="p">):</span>
  <span class="n">today</span> <span class="o">=</span> <span class="n">datetime</span><span class="o">.</span><span class="n">date</span><span class="o">.</span><span class="n">today</span><span class="p">()</span>
  <span class="n">seventeenth</span> <span class="o">=</span> <span class="n">datetime</span><span class="o">.</span><span class="n">date</span><span class="p">(</span>
      <span class="n">birthday</span><span class="o">.</span><span class="n">year</span> <span class="o">+</span> <span class="mi">17</span><span class="p">,</span> <span class="n">birthday</span><span class="o">.</span><span class="n">month</span><span class="p">,</span> <span class="n">birthday</span><span class="o">.</span><span class="n">day</span><span class="p">)</span>
  <span class="n">x</span> <span class="o">=</span> <span class="p">(</span><span class="n">today</span> <span class="o">-</span> <span class="n">seventeenth</span><span class="p">)</span><span class="o">.</span><span class="n">days</span>
  <span class="k">if</span> <span class="n">today</span><span class="o">.</span><span class="n">day</span> <span class="o">&gt;=</span> <span class="n">seventeenth</span><span class="o">.</span><span class="n">day</span><span class="p">:</span>
    <span class="n">y</span> <span class="o">=</span> <span class="p">(</span><span class="n">today</span><span class="o">.</span><span class="n">month</span> <span class="o">-</span> <span class="n">seventeenth</span><span class="o">.</span><span class="n">month</span><span class="p">)</span> <span class="o">+</span> \
        <span class="p">(</span><span class="n">today</span><span class="o">.</span><span class="n">year</span> <span class="o">-</span> <span class="n">seventeenth</span><span class="o">.</span><span class="n">year</span><span class="p">)</span> <span class="o">*</span> <span class="mi">12</span>
    <span class="n">z</span> <span class="o">=</span> <span class="n">today</span><span class="o">.</span><span class="n">day</span> <span class="o">-</span> <span class="n">seventeenth</span><span class="o">.</span><span class="n">day</span>
  <span class="k">else</span><span class="p">:</span>
    <span class="n">y</span> <span class="o">=</span> <span class="p">(</span><span class="n">today</span><span class="o">.</span><span class="n">month</span> <span class="o">-</span> <span class="n">seventeenth</span><span class="o">.</span><span class="n">month</span> <span class="o">-</span> <span class="mi">1</span><span class="p">)</span> <span class="o">+</span> \
        <span class="p">(</span><span class="n">today</span><span class="o">.</span><span class="n">year</span> <span class="o">-</span> <span class="n">seventeenth</span><span class="o">.</span><span class="n">year</span><span class="p">)</span> <span class="o">*</span> <span class="mi">12</span>
    <span class="k">if</span> <span class="n">today</span><span class="o">.</span><span class="n">month</span> <span class="o">==</span> <span class="mi">1</span><span class="p">:</span>
      <span class="n">z</span> <span class="o">=</span> <span class="p">(</span><span class="n">today</span> <span class="o">-</span> <span class="n">datetime</span><span class="o">.</span><span class="n">date</span><span class="p">(</span>
          <span class="n">today</span><span class="o">.</span><span class="n">year</span> <span class="o">-</span> <span class="mi">1</span><span class="p">,</span> <span class="mi">12</span><span class="p">,</span> <span class="n">seventeenth</span><span class="o">.</span><span class="n">day</span><span class="p">))</span><span class="o">.</span><span class="n">days</span>
    <span class="k">else</span><span class="p">:</span>
      <span class="n">z</span> <span class="o">=</span> <span class="p">(</span><span class="n">today</span> <span class="o">-</span> <span class="n">datetime</span><span class="o">.</span><span class="n">date</span><span class="p">(</span>
          <span class="n">today</span><span class="o">.</span><span class="n">year</span><span class="p">,</span> <span class="n">today</span><span class="o">.</span><span class="n">month</span> <span class="o">-</span> <span class="mi">1</span><span class="p">,</span> <span class="n">seventeenth</span><span class="o">.</span><span class="n">day</span><span class="p">))</span><span class="o">.</span><span class="n">days</span>
  <span class="k">return</span> <span class="n">x</span><span class="p">,</span> <span class="n">y</span><span class="p">,</span> <span class="n">z</span>

<span class="k">if</span> <span class="nb">len</span><span class="p">(</span><span class="n">sys</span><span class="o">.</span><span class="n">argv</span><span class="p">)</span> <span class="o">!=</span> <span class="mi">2</span><span class="p">:</span>
  <span class="k">print</span> <span class="s">&quot;usage: </span><span class="si">%s</span><span class="s"> Year/Month/Date&quot;</span> <span class="o">%</span> <span class="p">(</span><span class="n">sys</span><span class="o">.</span><span class="n">argv</span><span class="p">[</span><span class="mi">0</span><span class="p">])</span>
<span class="k">else</span><span class="p">:</span>
  <span class="n">birthday</span> <span class="o">=</span> <span class="n">datetime</span><span class="o">.</span><span class="n">date</span><span class="p">(</span>
      <span class="o">*</span><span class="n">time</span><span class="o">.</span><span class="n">strptime</span><span class="p">(</span><span class="n">sys</span><span class="o">.</span><span class="n">argv</span><span class="p">[</span><span class="mi">1</span><span class="p">],</span> <span class="s">&quot;%Y/%m/</span><span class="si">%d</span><span class="s">&quot;</span><span class="p">)[</span><span class="mi">0</span><span class="p">:</span><span class="mi">3</span><span class="p">])</span>
  <span class="n">seventeenth</span> <span class="o">=</span> <span class="n">calc_seventeenth</span><span class="p">(</span><span class="n">birthday</span><span class="p">)</span>
  <span class="k">print</span> <span class="s">u&quot;あなたの今日の年齢は&quot;</span>
  <span class="k">print</span> <span class="s">u&quot;17歳と&quot;</span> <span class="o">+</span> <span class="nb">str</span><span class="p">(</span><span class="n">seventeenth</span><span class="p">[</span><span class="mi">0</span><span class="p">])</span> <span class="o">+</span> <span class="s">u&quot;日&quot;</span>
  <span class="k">print</span> <span class="s">u&quot;または&quot;</span>
  <span class="k">print</span> <span class="s">u&quot;17歳と&quot;</span> <span class="o">+</span> <span class="nb">str</span><span class="p">(</span><span class="n">seventeenth</span><span class="p">[</span><span class="mi">1</span><span class="p">])</span> <span class="o">+</span> <span class="s">u&quot;ヶ月と&quot;</span> <span class="o">+</span> \
        <span class="nb">str</span><span class="p">(</span><span class="n">seventeenth</span><span class="p">[</span><span class="mi">2</span><span class="p">])</span> <span class="o">+</span> <span class="s">u&quot;日&quot;</span>
  <span class="k">print</span> <span class="s">u&quot;です。&quot;</span>
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
	<a href="/web/20090317035344/http://ja.doukaku.org/comment/8531/addtag/" class="addtag" id="addtag_8531">タグ</a>:
	
	タグなし
	&nbsp;

	
