<span class="n">b</span> <span class="o">=</span> <span class="s2">&quot;19640925&quot;</span><span class="c1">#生年月日</span>
<span class="n">b</span> <span class="o">=</span> <span class="no">Date</span><span class="o">.</span><span class="n">new</span><span class="p">(</span><span class="o">*[</span><span class="n">b</span><span class="o">[</span><span class="mi">0</span><span class="p">,</span><span class="mi">4</span><span class="o">]</span><span class="p">,</span><span class="n">b</span><span class="o">[</span><span class="mi">4</span><span class="p">,</span><span class="mi">2</span><span class="o">]</span><span class="p">,</span><span class="n">b</span><span class="o">[</span><span class="mi">6</span><span class="p">,</span><span class="mi">2</span><span class="o">]].</span><span class="n">map</span><span class="p">(</span><span class="o">&amp;</span><span class="ss">:to_i</span><span class="p">))</span>
