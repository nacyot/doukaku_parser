<span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">binary-clock</span><span class="p">)</span>
  <span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">integer-&gt;binary-string</span> <span class="nv">int</span><span class="p">)</span>
    <span class="p">(</span><span class="nf">regexp-replace-all</span>
     <span class="o">#</span><span class="nv">/1/</span>
     <span class="p">(</span><span class="nf">regexp-replace-all</span> <span class="o">#</span><span class="nv">/0/</span> <span class="p">(</span><span class="nb">number-&gt;string </span><span class="nv">int</span> <span class="mi">2</span><span class="p">)</span> <span class="s">&quot;o&quot;</span><span class="p">)</span>
     <span class="s">&quot;O&quot;</span><span class="p">))</span>
  <span class="p">(</span><span class="nf">let1</span> <span class="nv">cur</span> <span class="p">(</span><span class="nf">time-utc-&gt;date</span> <span class="p">(</span><span class="nf">current-time</span><span class="p">))</span>
        <span class="p">(</span><span class="nf">format</span> <span class="no">#t</span>
                <span class="s">&quot;\n ~2,&#39;0d:~2,&#39;0d\n~6,,,&#39;o@a\n~6,,,&#39;o@a&quot;</span>
                <span class="p">(</span><span class="nf">date-hour</span> <span class="nv">cur</span><span class="p">)</span>
                <span class="p">(</span><span class="nf">date-minute</span> <span class="nv">cur</span><span class="p">)</span>
                <span class="p">(</span><span class="nf">integer-&gt;binary-string</span> <span class="p">(</span><span class="nf">date-hour</span> <span class="nv">cur</span><span class="p">))</span>
                <span class="p">(</span><span class="nf">integer-&gt;binary-string</span> <span class="p">(</span><span class="nf">date-minute</span> <span class="nv">cur</span><span class="p">)))))</span>
<span class="p">(</span><span class="nf">binary-clock</span><span class="p">)</span>
