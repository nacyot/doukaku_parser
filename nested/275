<span class="nv">c</span><span class="o">=</span><span class="s2">&quot;　&quot;</span>; \
<span class="k">for </span>V in <span class="s">`echo ${d}`</span>;do \
  <span class="nv">V</span><span class="o">=</span><span class="s">`toBIN $V`</span>; \
  <span class="k">while</span> <span class="o">[</span> <span class="k">${#</span><span class="nv">V</span><span class="k">}</span> -lt <span class="m">6</span> <span class="o">]</span>;do \
    <span class="nv">V</span><span class="o">=</span><span class="k">${</span><span class="nv">c</span><span class="k">}${</span><span class="nv">V</span><span class="k">}</span>; \
  <span class="k">done</span>; \
  <span class="nb">echo</span> <span class="k">${</span><span class="nv">V</span><span class="k">}</span>|sed s/0/□/g|sed s/1/■/g; \
  <span class="nv">c</span><span class="o">=</span><span class="s2">&quot;0&quot;</span>; \
<span class="k">done</span>;
