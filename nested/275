<span class="nb">echo</span> -e <span class="s2">&quot;#define b_width 6\n\</span>
<span class="s2">#define b_height 3\n\</span>
<span class="s2">static char b_bits[] = {\n\</span>
<span class="s2">&quot;</span><span class="s">`printf &#39;0x%X,&#39; $date`</span><span class="s2">&quot;\n\</span>
<span class="s2">};&quot;</span> \
|display -flop -sample 120x240 -
