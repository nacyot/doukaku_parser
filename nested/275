  <span class="nv">v</span><span class="o">=</span><span class="nv">$1</span>; \
  <span class="nv">s</span><span class="o">=</span><span class="s2">&quot;&quot;</span>; \
  <span class="k">while</span> <span class="o">[</span> <span class="nv">$v</span> -gt <span class="m">0</span> <span class="o">]</span>;do \
    <span class="nb">let</span> <span class="s2">&quot;x = v&gt;&gt;1&quot;</span>; \
    <span class="nb">let</span> <span class="s2">&quot;b = v ^ x&lt;&lt;1&quot;</span>; \
    <span class="nv">s</span><span class="o">=</span><span class="nv">$b$s</span>; \
    <span class="nv">v</span><span class="o">=</span><span class="nv">$x</span>; \
  <span class="k">done</span>; \
  <span class="nb">echo</span> <span class="nv">$s</span>; \
