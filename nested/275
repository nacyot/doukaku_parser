    <span class="kt">void</span> <span class="n">output</span>(<span class="kt">uint</span> <span class="n">n</span>) {
        <span class="n">writeln</span>(<span class="n">format</span>(<span class="s">&quot;%06b&quot;</span>, <span class="n">n</span>).<span class="n">replace</span>(<span class="s">&quot;0&quot;</span>, <span class="s">&quot;□&quot;</span>).<span class="n">replace</span>(<span class="s">&quot;1&quot;</span>, <span class="s">&quot;■&quot;</span>));
