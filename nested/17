  <span class="p">(</span><span class="nf">receive</span> <span class="p">(</span><span class="nf">ss</span> <span class="nv">cs</span><span class="p">)</span> <span class="p">(</span><span class="nf">string-scan</span> <span class="nv">str</span> <span class="s">&quot;/*&quot;</span> <span class="ss">&#39;both</span><span class="p">)</span>
           <span class="p">(</span><span class="k">if </span><span class="nv">ss</span>
