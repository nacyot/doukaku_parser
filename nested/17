18</pre></td><td class="code"><div class="highlight"><pre><span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">remove-comment</span> <span class="nv">str</span><span class="p">)</span>
  <span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">cmnt-open-/</span> <span class="nv">l</span> <span class="nv">r</span><span class="p">)</span>
    <span class="p">(</span><span class="k">cond </span><span class="p">((</span><span class="nb">null? </span><span class="nv">l</span><span class="p">)</span> <span class="nv">r</span><span class="p">)</span>
          <span class="p">((</span><span class="nb">char=? </span><span class="sc">#\/</span> <span class="p">(</span><span class="nb">car </span><span class="nv">l</span><span class="p">))</span> <span class="p">(</span><span class="nf">cmnt-open-*</span> <span class="p">(</span><span class="nb">cdr </span><span class="nv">l</span><span class="p">)</span> <span class="nv">r</span><span class="p">))</span>
          <span class="p">(</span><span class="k">else </span><span class="p">(</span><span class="nf">cmnt-open-/</span> <span class="p">(</span><span class="nb">cdr </span><span class="nv">l</span><span class="p">)</span> <span class="p">(</span><span class="nb">cons </span><span class="p">(</span><span class="nb">car </span><span class="nv">l</span><span class="p">)</span> <span class="nv">r</span><span class="p">)))))</span>
  <span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">cmnt-open-*</span> <span class="nv">l</span> <span class="nv">r</span><span class="p">)</span>
    <span class="p">(</span><span class="k">cond </span><span class="p">((</span><span class="nb">null? </span><span class="nv">l</span><span class="p">)</span> <span class="p">(</span><span class="nb">cons </span><span class="sc">#\/</span> <span class="nv">r</span><span class="p">))</span>
          <span class="p">((</span><span class="nb">char=? </span><span class="o">#</span><span class="err">\</span><span class="nv">*</span> <span class="p">(</span><span class="nb">car </span><span class="nv">l</span><span class="p">))</span> <span class="p">(</span><span class="nf">cmnt-close-*</span> <span class="p">(</span><span class="nb">cdr </span><span class="nv">l</span><span class="p">)</span> <span class="nv">r</span><span class="p">))</span>
          <span class="p">(</span><span class="k">else </span><span class="p">(</span><span class="nf">cmnt-open-/</span> <span class="nv">l</span> <span class="p">(</span><span class="nb">cons </span><span class="sc">#\/</span> <span class="nv">r</span><span class="p">)))))</span>
  <span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">cmnt-close-*</span> <span class="nv">l</span> <span class="nv">r</span><span class="p">)</span>
    <span class="p">(</span><span class="k">cond </span><span class="p">((</span><span class="nb">null? </span><span class="nv">l</span><span class="p">)</span> <span class="nv">r</span><span class="p">)</span>
          <span class="p">((</span><span class="nb">char=? </span><span class="o">#</span><span class="err">\</span><span class="nv">*</span> <span class="p">(</span><span class="nb">car </span><span class="nv">l</span><span class="p">))</span> <span class="p">(</span><span class="nf">cmnt-close-/</span> <span class="p">(</span><span class="nb">cdr </span><span class="nv">l</span><span class="p">)</span> <span class="nv">r</span><span class="p">))</span>
          <span class="p">(</span><span class="k">else </span><span class="p">(</span><span class="nf">cmnt-close-*</span> <span class="p">(</span><span class="nb">cdr </span><span class="nv">l</span><span class="p">)</span> <span class="nv">r</span><span class="p">))))</span>
  <span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">cmnt-close-/</span> <span class="nv">l</span> <span class="nv">r</span><span class="p">)</span>
    <span class="p">(</span><span class="k">cond </span><span class="p">((</span><span class="nb">null? </span><span class="nv">l</span><span class="p">)</span> <span class="p">(</span><span class="nb">cons </span><span class="o">#</span><span class="err">\</span><span class="nv">*</span> <span class="nv">r</span><span class="p">))</span>
          <span class="p">((</span><span class="nb">char=? </span><span class="sc">#\/</span> <span class="p">(</span><span class="nb">car </span><span class="nv">l</span><span class="p">))</span> <span class="p">(</span><span class="nf">cmnt-open-/</span> <span class="p">(</span><span class="nb">cdr </span><span class="nv">l</span><span class="p">)</span> <span class="nv">r</span><span class="p">))</span>
          <span class="p">(</span><span class="k">else </span><span class="p">(</span><span class="nf">cmnt-close-*</span> <span class="nv">l</span> <span class="nv">r</span><span class="p">))))</span>
  <span class="p">(</span><span class="nb">list-&gt;string </span><span class="p">(</span><span class="nb">reverse </span><span class="p">(</span><span class="nf">cmnt-open-/</span> <span class="p">(</span><span class="nb">string-&gt;list </span><span class="nv">str</span><span class="p">)</span> <span class="o">&#39;</span><span class="p">()))))</span>
