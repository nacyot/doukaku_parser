  <span class="p">(</span><span class="nf">regexp-replace-all</span> <span class="o">#</span><span class="nv">/</span><span class="err">\</span><span class="nv">/</span><span class="err">\</span><span class="nv">*</span><span class="p">(</span><span class="o">.</span><span class="nv">+?</span><span class="p">)(</span><span class="err">\</span><span class="nv">*</span><span class="err">\</span><span class="nv">/</span><span class="err">|</span><span class="nv">$</span><span class="p">)</span><span class="nv">/</span> <span class="nv">str</span> <span class="s">&quot;&quot;</span><span class="p">))</span>

