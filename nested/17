    <div class="compact">
      <table><tr><td class="linenos"><pre>1
2
3</pre></td><td class="code"><div class="highlight"><pre><span class="k">function</span> <span class="nf">remove_comment</span><span class="p">(</span> <span class="n">str</span> <span class="p">)</span>
	<span class="k">return</span> <span class="p">(</span><span class="nf">string.gsub</span><span class="p">(</span> <span class="n">str</span><span class="p">,</span> <span class="s2">&quot;</span><span class="s">/%*.-%*/&quot;</span><span class="p">,</span> <span class="s2">&quot;</span><span class="s">&quot;</span> <span class="p">)</span> <span class="p">)</span>
