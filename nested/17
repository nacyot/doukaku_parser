<span class="nv">removeComment</span> <span class="o">=</span> <span class="nv">outOfComment</span>
  <span class="kr">where</span>
    <span class="nv">outOfComment</span> <span class="p">(</span><span class="sc">&#39;/&#39;</span><span class="o">:</span><span class="sc">&#39;*&#39;</span><span class="o">:</span><span class="nv">cs</span><span class="p">)</span> <span class="o">=</span> <span class="nv">inComment</span> <span class="nv">cs</span>
    <span class="nv">outOfComment</span> <span class="p">(</span><span class="nv">c</span><span class="o">:</span><span class="nv">cs</span><span class="p">)</span> <span class="o">=</span> <span class="nv">c</span> <span class="o">:</span> <span class="nv">outOfComment</span> <span class="nv">cs</span>
    <span class="nv">outOfComment</span> <span class="p">[]</span> <span class="o">=</span> <span class="p">[]</span>
    <span class="nv">inComment</span> <span class="p">(</span><span class="sc">&#39;*&#39;</span><span class="o">:</span><span class="sc">&#39;/&#39;</span><span class="o">:</span><span class="nv">cs</span><span class="p">)</span> <span class="o">=</span> <span class="nv">outOfComment</span> <span class="nv">cs</span>
    <span class="nv">inComment</span> <span class="p">(</span><span class="nv">c</span><span class="o">:</span><span class="nv">cs</span><span class="p">)</span> <span class="o">=</span> <span class="nv">inComment</span> <span class="nv">cs</span>
    <span class="nv">inComment</span> <span class="p">[]</span> <span class="o">=</span> <span class="p">[]</span>
</pre></div>
