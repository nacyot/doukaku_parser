  <span class="k">def</span> <span class="nf">remove_comment</span>
    <span class="nb">self</span><span class="o">.</span><span class="n">gsub</span><span class="p">(</span><span class="sr">/\/\*.*?(\*\/|\z)/m</span><span class="p">,</span> <span class="s1">&#39;&#39;</span><span class="p">)</span>
    <span class="c1">#self.gsub(/\/\*.*?(\*\/|$)/m, &#39;&#39;)</span>
  <span class="k">end</span>
<span class="k">end</span>

<span class="n">str</span> <span class="o">=</span> <span class="no">DATA</span><span class="o">.</span><span class="n">readlines</span><span class="o">.</span><span class="n">join</span>
