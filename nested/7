<span class="p">{</span>
	<span class="n">Gtk</span><span class="o">::</span><span class="n">Main</span> <span class="n">kit</span><span class="p">(</span><span class="n">argc</span><span class="p">,</span> <span class="n">argv</span><span class="p">);</span>
