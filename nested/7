<span class="k">import</span> <span class="nn">java.awt.event.WindowAdapter</span><span class="o">;</span>
<span class="k">import</span> <span class="nn">java.awt.event.WindowEvent</span><span class="o">;</span>

<span class="k">public</span> <span class="k">class</span> <span class="nc">WindowOpener</span> <span class="o">{</span>
	<span class="k">public</span> <span class="k">static</span> <span class="kt">void</span> <span class="n">main</span><span class="o">(</span><span class="n">String</span> <span class="n">args</span><span class="o">[])</span> <span class="o">{</span>
		<span class="kt">int</span> <span class="n">width</span> <span class="o">=</span> <span class="mi">400</span><span class="o">;</span>
