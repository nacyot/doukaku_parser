<span class="k">my</span> <span class="nv">$y</span> <span class="o">=</span> <span class="nb">int</span><span class="p">((</span><span class="nv">$mw</span><span class="o">-&gt;</span><span class="n">screenheight</span> <span class="o">-</span> <span class="n">MW_HEIGHT</span><span class="p">)</span> <span class="o">/</span> <span class="mi">2</span><span class="p">);</span>

<span class="nv">$mw</span><span class="o">-&gt;</span><span class="n">geometry</span><span class="p">(</span><span class="s">&quot;+$x+$y&quot;</span><span class="p">);</span>

