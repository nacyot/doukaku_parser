<span class="n">rTextBox</span><span class="k">.</span><span class="n">Dock</span> <span class="k">&lt;-</span> <span class="n">DockStyle</span><span class="k">.</span><span class="n">Fill</span><span class="k">;;</span>
<span class="n">rTextBox</span><span class="k">.</span><span class="n">Text</span> <span class="k">&lt;-</span> <span class="s2">&quot;こんにちは、GUI!&quot;</span><span class="k">;;</span>

<span class="kr">do</span> <span class="n">Application</span><span class="k">.</span><span class="n">Run</span><span class="k">(</span><span class="n">form</span><span class="k">);;</span> 
</pre></div>
</td></tr></table>
    </div>
	

  
