13</pre></td><td class="code"><div class="highlight"><pre><span class="kr">open</span> <span class="n">System</span><span class="k">.</span><span class="n">Drawing</span><span class="k">;;</span>
<span class="kr">open</span> <span class="n">System</span><span class="k">.</span><span class="n">Windows</span><span class="k">.</span><span class="n">Forms</span><span class="k">;;</span>

<span class="kr">let</span> <span class="n">form</span> <span class="k">=</span> <span class="kr">new</span> <span class="n">Form</span><span class="k">();;</span>
<span class="n">form</span><span class="k">.</span><span class="n">Visible</span> <span class="k">&lt;-</span> <span class="kr">true</span><span class="k">;;</span>
<span class="n">form</span><span class="k">.</span><span class="n">Size</span> <span class="k">&lt;-</span> <span class="kr">new</span> <span class="n">Size</span><span class="k">(</span><span class="mi">100</span><span class="k">,</span><span class="mi">75</span><span class="k">);;</span>

