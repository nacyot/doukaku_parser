<span class="k">use</span> <span class="n">Tk</span><span class="p">;</span>
<span class="k">use</span> <span class="n">constant</span> <span class="p">{</span>
    <span class="n">MW_WIDTH</span>    <span class="o">=&gt;</span> <span class="mi">400</span><span class="p">,</span>
    <span class="n">MW_HEIGHT</span>   <span class="o">=&gt;</span> <span class="mi">300</span>
<span class="p">};</span>

<span class="k">my</span> <span class="nv">$mw</span> <span class="o">=</span> <span class="k">new</span> <span class="nn">Tk::</span><span class="n">MainWindow</span><span class="p">(</span>
            <span class="o">-</span><span class="n">title</span> <span class="o">=&gt;</span> <span class="s">&#39;こんにちは、GUI！&#39;</span><span class="p">,</span>
