				<span class="k">public</span> <span class="kt">void</span> <span class="n">windowClosing</span><span class="o">(</span><span class="n">WindowEvent</span> <span class="n">e</span><span class="o">)</span> <span class="o">{</span>
					<span class="n">frame</span><span class="o">.</span><span class="na">dispose</span><span class="o">();</span>
				<span class="o">}</span>
