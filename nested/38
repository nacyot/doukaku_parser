	<a href="/web/20100318023313/http://ja.doukaku.org/comment/1784/">#1784</a>(<script language="JavaScript">print_time(new Date("2007/08/03 13:42 GMT"));</script><noscript>2007/08/03 13:42 GMT</noscript>)
	
	
	
