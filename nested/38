	<a href="/web/20100318023313/http://ja.doukaku.org/comment/1793/">#1793</a>(<script language="JavaScript">print_time(new Date("2007/08/03 15:52 GMT"));</script><noscript>2007/08/03 15:52 GMT</noscript>)
	
	
	
