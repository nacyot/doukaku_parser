  <span class="kr">where</span> <span class="nv">cmp</span> <span class="nv">a</span> <span class="nv">b</span> <span class="o">|</span> <span class="nv">length</span> <span class="nv">a</span> <span class="o">&lt;</span> <span class="nv">length</span> <span class="nv">b</span>  <span class="o">=</span> <span class="n">LT</span>
                <span class="o">|</span> <span class="nv">length</span> <span class="nv">a</span> <span class="o">==</span> <span class="nv">length</span> <span class="nv">b</span> <span class="o">=</span> <span class="n">EQ</span>
                <span class="o">|</span> <span class="nv">otherwise</span>            <span class="o">=</span> <span class="n">GT</span>
        <span class="nv">pttn</span> <span class="mi">0</span> <span class="kr">_</span> <span class="o">=</span> <span class="p">[[]]</span>
        <span class="nv">pttn</span> <span class="nv">n</span> <span class="nv">k</span> <span class="o">=</span> <span class="p">[(</span><span class="nv">n</span><span class="o">-</span><span class="nv">x</span><span class="p">)</span><span class="o">:</span><span class="nv">xs</span> <span class="o">|</span> <span class="nv">x</span> <span class="o">&lt;-</span> <span class="p">[</span><span class="nv">max</span> <span class="p">(</span><span class="nv">n</span><span class="o">-</span><span class="nv">k</span><span class="p">)</span> <span class="mi">0</span><span class="o">..</span><span class="nv">n</span><span class="o">-</span><span class="mi">1</span><span class="p">],</span> <span class="nv">xs</span> <span class="o">&lt;-</span> <span class="nv">pttn</span> <span class="nv">x</span> <span class="p">(</span><span class="nv">n</span><span class="o">-</span><span class="nv">x</span><span class="p">)]</span>

<span class="nv">main</span> <span class="o">=</span> <span class="n">U</span><span class="o">.</span><span class="nv">putStr</span> <span class="o">$</span> <span class="nv">unlines</span> <span class="o">$</span> <span class="nv">foldr</span> <span class="nv">chunk</span> <span class="p">[]</span> <span class="o">$</span> <span class="nv">partitionNum</span> <span class="mi">50</span>
  <span class="kr">where</span> <span class="nv">chunk</span> <span class="nv">ns</span> <span class="nv">r</span> <span class="o">=</span> <span class="nv">map</span> <span class="p">(</span><span class="o">\</span><span class="nv">n</span> <span class="o">-&gt;</span> <span class="nv">replicate</span> <span class="nv">n</span> <span class="sc">&#39;□&#39;</span><span class="p">)</span> <span class="nv">ns</span> <span class="o">++</span> <span class="p">[</span><span class="s">&quot;&quot;</span><span class="p">]</span> <span class="o">++</span> <span class="nv">r</span>
</pre></div>
</td></tr></table>
    </div>
	

  
