	&nbsp;
	<a href="/web/20080229025455/http://ja.doukaku.org/comment/4809/">#4809</a>(<script language="JavaScript">print_time(new Date("2007/12/14 14:20 GMT"));</script><noscript>2007/12/14 14:20 GMT</noscript>)
	
	
	
