 *Main&gt; mask2nbits "255.255.255.0"
 24
 *Main&gt; mask2nbits "255.255.255.128"
 25
 *Main&gt; mask2nbits "255.255.255.255"
 32
 *Main&gt; nbits2mask 24
 "255.255.255.0"
 *Main&gt; nbits2mask 25
 "255.255.255.128"
 *Main&gt; nbits2mask 28
 "255.255.255.240"
 *Main&gt; nbits2mask 32
 "255.255.255.255"

