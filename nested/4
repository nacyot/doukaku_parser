let
  val rnd = Random.rand (Time.toNanoseconds (Time.now ()), m)
  fun loop 0 m&#39; = m&#39;
    | loop n&#39; m&#39; =
  let
    val number = Random.randRange (1, n) rnd
  in
    if List.exists (fn x =&gt; x = number) m&#39; then loop n&#39; m&#39; else loop (n&#39; - 1) (number::m&#39;)
  end
in
  loop m []
end
