<span class="nv">main</span> <span class="o">=</span> <span class="kr">do</span> <span class="nv">xs</span> <span class="o">&lt;-</span> <span class="nv">getStdRandom</span> <span class="p">(</span><span class="nv">lot</span> <span class="mi">1000000</span> <span class="mi">999999</span><span class="p">)</span>
