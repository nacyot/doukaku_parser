   <span class="nv">$a</span> <span class="o">=</span> <span class="nf">range</span><span class="p">(</span><span class="m">1</span><span class="p">,</span> <span class="nv">$n</span><span class="p">);</span>
   <span class="nf">shuffle</span><span class="p">(</span><span class="nv">$a</span><span class="p">);</span>
   <span class="k">return</span> <span class="nf">array_slice</span><span class="p">(</span><span class="nv">$a</span><span class="p">,</span> <span class="o">-</span><span class="nv">$m</span><span class="p">);</span>
