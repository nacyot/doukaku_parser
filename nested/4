<span class="nv">shuffle</span> <span class="o">::</span> <span class="p">[</span><span class="nv">a</span><span class="p">]</span> <span class="o">-&gt;</span> <span class="n">StdGen</span> <span class="o">-&gt;</span> <span class="p">([</span><span class="nv">a</span><span class="p">],</span> <span class="n">StdGen</span><span class="p">)</span>
<span class="nv">shuffle</span> <span class="p">[]</span> <span class="nv">g</span> <span class="o">=</span> <span class="p">([],</span> <span class="nv">g</span><span class="p">)</span>
<span class="nv">shuffle</span> <span class="nv">xs</span> <span class="nv">g</span> <span class="o">=</span> <span class="kr">let</span> <span class="p">(</span><span class="nv">needle</span>  <span class="p">,</span> <span class="nv">g&#39;</span>          <span class="p">)</span> <span class="o">=</span> <span class="nv">randomR</span> <span class="p">(</span><span class="mi">0</span><span class="p">,</span> <span class="nv">length</span> <span class="nv">xs</span> <span class="o">-</span> <span class="mi">1</span><span class="p">)</span> <span class="nv">g</span>
                   <span class="p">(</span><span class="nv">left</span>    <span class="p">,</span> <span class="nv">picked</span><span class="o">:</span><span class="nv">right</span><span class="p">)</span> <span class="o">=</span> <span class="nv">splitAt</span> <span class="nv">needle</span> <span class="nv">xs</span>
                   <span class="p">(</span><span class="nv">unpicked</span><span class="p">,</span> <span class="nv">g&#39;&#39;</span>         <span class="p">)</span> <span class="o">=</span> <span class="nv">shuffle</span> <span class="p">(</span><span class="nv">left</span> <span class="o">++</span> <span class="nv">right</span><span class="p">)</span> <span class="nv">g&#39;</span>
               <span class="kr">in</span>
                 <span class="p">(</span><span class="nv">picked</span><span class="o">:</span><span class="nv">unpicked</span><span class="p">,</span> <span class="nv">g&#39;&#39;</span><span class="p">)</span>

<span class="nv">lot</span> <span class="o">::</span> <span class="n">Int</span> <span class="o">-&gt;</span> <span class="n">Int</span> <span class="o">-&gt;</span> <span class="n">StdGen</span> <span class="o">-&gt;</span> <span class="p">([</span><span class="n">Int</span><span class="p">],</span> <span class="n">StdGen</span><span class="p">)</span>
