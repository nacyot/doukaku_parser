<span class="p">{</span>
  <span class="kt">unsigned</span> <span class="kt">long</span> <span class="n">i</span><span class="p">,</span> <span class="n">orgN</span><span class="o">=</span><span class="n">n</span><span class="p">;</span>
  <span class="k">for</span><span class="p">(</span><span class="n">i</span><span class="o">=</span><span class="mi">1</span><span class="p">;</span> <span class="n">i</span><span class="o">&lt;=</span><span class="n">orgN</span><span class="p">;</span> <span class="o">++</span><span class="n">i</span><span class="p">){</span>
