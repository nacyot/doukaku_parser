    <span class="kt">int</span> <span class="n">idx</span> <span class="o">=</span> <span class="n">rand</span><span class="p">()</span> <span class="o">%</span> <span class="n">v</span><span class="p">.</span><span class="n">size</span><span class="p">();</span>
