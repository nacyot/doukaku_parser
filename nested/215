<span class="k">def</span> <span class="nf">put_iterator</span><span class="p">(</span><span class="n">it</span><span class="p">):</span>
    <span class="k">for</span> <span class="n">i</span> <span class="ow">in</span> <span class="n">it</span><span class="p">:</span>
        <span class="k">print</span> <span class="n">i</span>
        <span class="n">stackless</span><span class="o">.</span><span class="n">schedule</span><span class="p">()</span>

<span class="n">stackless</span><span class="o">.</span><span class="n">tasklet</span><span class="p">(</span><span class="n">put_iterator</span><span class="p">)(</span><span class="s">&#39;ABCDEFGHIJ&#39;</span><span class="p">)</span>
