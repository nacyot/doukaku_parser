<span class="nx">import</span> <span class="nx">System</span><span class="p">.</span><span class="nx">Threading</span><span class="o">;</span>

<span class="nx">class</span> <span class="nx">Sample</span> <span class="p">{</span>
    <span class="nx">static</span> <span class="k">function</span> <span class="nx">Main</span><span class="p">()</span>
    <span class="p">{</span>
        <span class="k">var</span> <span class="nx">t</span> <span class="o">=</span> <span class="k">new</span> <span class="nx">Sample</span><span class="p">();</span>
        <span class="nx">t</span><span class="p">.</span><span class="nx">doIt</span><span class="p">();</span>
    <span class="p">}</span>

    <span class="k">function</span> <span class="nx">doIt</span><span class="p">(){</span>
        <span class="k">var</span> <span class="nx">t1</span> <span class="o">=</span> <span class="k">new</span> <span class="nx">Thread</span><span class="p">(</span><span class="nx">numPrint</span><span class="p">);</span>
        <span class="k">var</span> <span class="nx">t2</span> <span class="o">=</span> <span class="k">new</span> <span class="nx">Thread</span><span class="p">(</span><span class="nx">charPrint</span><span class="p">);</span>
        <span class="nx">t1</span><span class="p">.</span><span class="nx">Start</span><span class="p">();</span>
        <span class="nx">t2</span><span class="p">.</span><span class="nx">Start</span><span class="p">();</span>
    <span class="p">}</span>

    <span class="k">function</span> <span class="nx">numPrint</span><span class="p">(){</span>
        <span class="k">for</span><span class="p">(</span><span class="k">var</span> <span class="nx">i</span><span class="o">=</span><span class="mi">0</span><span class="o">;</span><span class="nx">i</span><span class="o">&lt;</span><span class="mi">10</span><span class="o">;</span><span class="nx">i</span><span class="o">++</span><span class="p">){</span>
            <span class="nx">print</span><span class="p">(</span><span class="nx">i</span><span class="o">+</span><span class="mi">1</span><span class="p">);</span>
        <span class="p">}</span>
    <span class="p">}</span>
    <span class="k">function</span> <span class="nx">charPrint</span><span class="p">(){</span>
        <span class="k">var</span> <span class="nx">data</span> <span class="o">=</span> <span class="s2">&quot;abcdefghij&quot;</span>
        <span class="k">for</span><span class="p">(</span><span class="k">var</span> <span class="nx">i</span><span class="o">=</span><span class="mi">0</span><span class="o">;</span><span class="nx">i</span><span class="o">&lt;</span><span class="mi">10</span><span class="o">;</span><span class="nx">i</span><span class="o">++</span><span class="p">){</span>
            <span class="nx">print</span><span class="p">(</span><span class="nx">data</span><span class="p">.</span><span class="nx">charAt</span><span class="p">(</span><span class="nx">i</span><span class="p">));</span>
        <span class="p">}</span>
    <span class="p">}</span>
<span class="p">}</span>

<span class="nx">Sample</span><span class="p">.</span><span class="nx">Main</span><span class="p">();</span>
</pre></div>
</td></tr></table>
