numbers:
    @echo 0
    @echo 1
    @echo 2
    @echo 3
    @echo 4
    @echo 5
    @echo 6
    @echo 7
    @echo 8
    @echo 9

alphabets:
    @echo A
    @echo B
    @echo C
    @echo D
    @echo E
    @echo F
    @echo G
    @echo H
    @echo I
    @echo J
</pre></div>
