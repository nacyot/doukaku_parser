103</pre></td><td class="code"><div class="highlight"><pre><span class="cp">#include &lt;stdio.h&gt;</span>
<span class="cp">#include &lt;stdlib.h&gt;</span>

<span class="cp">#define TASK_END ((void*)-1)</span>
<span class="c">/*</span>
<span class="c">void put_num(){</span>
<span class="c">    int i;</span>
<span class="c">    for(i=0;i&lt;10;i++){</span>
<span class="c">        putchar(&#39;0&#39;+i);</span>
<span class="c">        fflush(stdout);</span>
<span class="c">    }</span>
<span class="c">}</span>
<span class="c">*/</span>
<span class="kt">void</span> <span class="n">put_num</span><span class="p">(</span><span class="kt">void</span> <span class="o">**</span><span class="n">local</span><span class="p">){</span>
    <span class="c">/*ローカル変数定義*/</span>
    <span class="k">struct</span> <span class="n">local_struct</span><span class="p">{</span>
        <span class="kt">int</span> <span class="n">i</span><span class="p">;</span>
    <span class="p">}</span> <span class="o">*</span><span class="n">pl</span><span class="p">;</span>
    <span class="k">if</span><span class="p">(</span><span class="o">*</span><span class="n">local</span><span class="o">==</span><span class="nb">NULL</span><span class="p">){</span>
        <span class="c">/*ローカル変数粋確保*/</span>
        <span class="o">*</span><span class="n">local</span> <span class="o">=</span><span class="n">malloc</span><span class="p">(</span><span class="k">sizeof</span><span class="p">(</span><span class="k">struct</span> <span class="n">local_struct</span><span class="p">));</span>
        <span class="n">pl</span><span class="o">=*</span><span class="n">local</span><span class="p">;</span>

        <span class="c">/*ローカル変数初期化*/</span>
        <span class="n">pl</span><span class="o">-&gt;</span><span class="n">i</span><span class="o">=</span><span class="mi">0</span><span class="p">;</span>
    <span class="p">}</span><span class="k">else</span> <span class="k">if</span><span class="p">(</span><span class="o">*</span><span class="n">local</span><span class="o">==</span><span class="n">TASK_END</span><span class="p">){</span>
        <span class="c">/*タスク終了済み*/</span>
        <span class="k">return</span><span class="p">;</span>
    <span class="p">}</span><span class="k">else</span>
        <span class="n">pl</span><span class="o">=*</span><span class="n">local</span><span class="p">;</span>

    <span class="c">/* タスク内容</span>
<span class="c">       キリのいいところでreturnする。</span>
<span class="c">    */</span>
    <span class="k">for</span><span class="p">(;</span><span class="n">pl</span><span class="o">-&gt;</span><span class="n">i</span><span class="o">&lt;</span><span class="mi">10</span><span class="p">;){</span>
        <span class="n">putchar</span><span class="p">(</span><span class="sc">&#39;0&#39;</span><span class="o">+</span><span class="n">pl</span><span class="o">-&gt;</span><span class="n">i</span><span class="p">);</span>
        <span class="n">fflush</span><span class="p">(</span><span class="n">stdout</span><span class="p">);</span>

        <span class="n">pl</span><span class="o">-&gt;</span><span class="n">i</span><span class="o">++</span><span class="p">;</span>
        <span class="c">//タスクスイッチ</span>
        <span class="k">return</span><span class="p">;</span>
    <span class="p">}</span>

    <span class="c">//タスク終了処理</span>
    <span class="n">free</span><span class="p">(</span><span class="n">pl</span><span class="p">);</span>
    <span class="o">*</span><span class="n">local</span><span class="o">=</span><span class="n">TASK_END</span><span class="p">;</span>
<span class="p">}</span>

<span class="c">/*</span>
<span class="c">void put_alpha(){</span>
<span class="c">    int i;</span>
<span class="c">    for(i=0;i&lt;26;i++){</span>
<span class="c">        putchar(&#39;A&#39;+i);</span>
<span class="c">        fflush(stdout);</span>
<span class="c">    }</span>
<span class="c">}</span>
<span class="c">*/</span>
<span class="kt">void</span> <span class="n">put_alpha</span><span class="p">(</span><span class="kt">void</span> <span class="o">**</span> <span class="n">local</span><span class="p">){</span>
    <span class="c">/*ローカル変数定義*/</span>
    <span class="k">struct</span> <span class="n">local_struct</span><span class="p">{</span>
        <span class="kt">int</span> <span class="n">i</span><span class="p">;</span>
    <span class="p">}</span> <span class="o">*</span><span class="n">pl</span><span class="p">;</span>
    <span class="k">if</span><span class="p">(</span><span class="o">*</span><span class="n">local</span><span class="o">==</span><span class="nb">NULL</span><span class="p">){</span>
        <span class="c">/*ローカル変数粋確保*/</span>
        <span class="o">*</span><span class="n">local</span> <span class="o">=</span><span class="n">malloc</span><span class="p">(</span><span class="k">sizeof</span><span class="p">(</span><span class="k">struct</span> <span class="n">local_struct</span><span class="p">));</span>
        <span class="n">pl</span><span class="o">=*</span><span class="n">local</span><span class="p">;</span>

        <span class="c">/*ローカル変数初期化*/</span>
        <span class="n">pl</span><span class="o">-&gt;</span><span class="n">i</span><span class="o">=</span><span class="mi">0</span><span class="p">;</span>
    <span class="p">}</span><span class="k">else</span> <span class="k">if</span><span class="p">(</span><span class="o">*</span><span class="n">local</span><span class="o">==</span><span class="n">TASK_END</span><span class="p">){</span>
        <span class="c">/*タスク終了済み*/</span>
        <span class="k">return</span><span class="p">;</span>
    <span class="p">}</span><span class="k">else</span>
        <span class="n">pl</span><span class="o">=*</span><span class="n">local</span><span class="p">;</span>

    <span class="c">/* タスク内容</span>
<span class="c">       キリのいいところでreturnする。</span>
<span class="c">    */</span>
    <span class="k">for</span><span class="p">(;</span><span class="n">pl</span><span class="o">-&gt;</span><span class="n">i</span><span class="o">&lt;</span><span class="mi">26</span><span class="p">;){</span>
        <span class="n">putchar</span><span class="p">(</span><span class="sc">&#39;A&#39;</span><span class="o">+</span><span class="n">pl</span><span class="o">-&gt;</span><span class="n">i</span><span class="p">);</span>
        <span class="n">fflush</span><span class="p">(</span><span class="n">stdout</span><span class="p">);</span>

        <span class="n">pl</span><span class="o">-&gt;</span><span class="n">i</span><span class="o">++</span><span class="p">;</span>
        <span class="c">//タスクスイッチ</span>
        <span class="k">return</span><span class="p">;</span>
    <span class="p">}</span>
    
    <span class="c">//タスク終了処理</span>
    <span class="n">free</span><span class="p">(</span><span class="n">pl</span><span class="p">);</span>
    <span class="o">*</span><span class="n">local</span><span class="o">=</span><span class="n">TASK_END</span><span class="p">;</span>
<span class="p">}</span>

<span class="kt">int</span> <span class="n">main</span><span class="p">(){</span>
    <span class="kt">void</span><span class="o">*</span> <span class="n">local_n</span><span class="o">=</span><span class="nb">NULL</span><span class="p">;</span>
    <span class="kt">void</span><span class="o">*</span> <span class="n">local_a</span><span class="o">=</span><span class="nb">NULL</span><span class="p">;</span>
    
    <span class="k">while</span><span class="p">(</span><span class="n">local_n</span><span class="o">!=</span><span class="n">TASK_END</span><span class="o">||</span><span class="n">local_a</span><span class="o">!=</span><span class="n">TASK_END</span><span class="p">){</span>
        <span class="n">put_alpha</span><span class="p">(</span><span class="o">&amp;</span><span class="n">local_a</span><span class="p">);</span>
        <span class="n">put_num</span><span class="p">(</span><span class="o">&amp;</span><span class="n">local_n</span><span class="p">);</span>
        <span class="n">put_alpha</span><span class="p">(</span><span class="o">&amp;</span><span class="n">local_a</span><span class="p">);</span>
    <span class="p">}</span>
    <span class="k">return</span> <span class="mi">0</span><span class="p">;</span>
<span class="p">}</span>
</pre></div>
</td></tr></table>
