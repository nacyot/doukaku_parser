  val n = Console.readInt
  val taro = List.tabulate(n, i =&gt; Console.readInt).sort((i,j) =&gt; i &lt; j)
  val hanako = List.range(1, n*2 + 1).filter(i =&gt; !taro.contains(i))
  
  def game(list1:List[int], list2:List[int], isList1Taro:boolean, p:int):String = {
   if(list1.length == 0 || list2.length == 0) 
     return if(isList1Taro) list2.length + &quot;\n&quot; + list1.length
     else list1.length + &quot;\n&quot; + list2.length
   val spanList = list1.span(i =&gt; i &lt;= p)
   if(spanList._2.length == 0) 
     return game(list2, list1, !isList1Taro, 0)
   else 
     return game(list2, spanList._1 ::: spanList._2.tail, !isList1Taro, spanList._2.head)
  }
  println(game(taro, hanako, true, 0))
}
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
