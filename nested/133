18</pre></td><td class="code"><div class="highlight"><pre>import scala.collection.mutable._

object JOI3 extends Application {
  val n = Console.readInt
  val taro = new ListBuffer ++ List.tabulate(n, (i) =&gt; Console.readInt).sort((i,j) =&gt; i &lt; j)
  val hanako = new ListBuffer ++ List.range(1, n*2 + 1).filter((i) =&gt; !taro.contains(i))
  
  var p = 0
  var isTaro = true
  while(taro.length &gt; 0 &amp;&amp; hanako.length &gt; 0) {
    val list = if(isTaro) taro else hanako
    val idx = list.findIndexOf((i) =&gt; i &gt; p)
    p = if(idx &gt;= 0) list.remove(idx) else 0
    isTaro = !isTaro
  }
  println(hanako.length)
  println(taro.length)
}
</pre></div>
</td></tr></table>
