                <span class="k">continue</span><span class="o">;</span>
            <span class="o">}</span>
            <span class="kt">int</span> <span class="n">card</span> <span class="o">=</span> <span class="n">tailSet</span><span class="o">.</span><span class="na">first</span><span class="o">();</span>
            <span class="n">cardList</span><span class="o">.</span><span class="na">remove</span><span class="o">(</span><span class="n">card</span><span class="o">);</span>
            <span class="n">board</span> <span class="o">=</span> <span class="n">card</span><span class="o">;</span>

            <span class="k">if</span> <span class="o">(</span><span class="n">cardList</span><span class="o">.</span><span class="na">isEmpty</span><span class="o">())</span> <span class="k">break</span><span class="o">;</span>
        <span class="o">}</span>

        <span class="k">return</span> <span class="k">new</span> <span class="kt">int</span><span class="o">[]</span> <span class="o">{</span><span class="n">hanakoCardList</span><span class="o">.</span><span class="na">size</span><span class="o">(),</span> <span class="n">tarouCardList_</span><span class="o">.</span><span class="na">size</span><span class="o">()};</span>
    <span class="o">}</span>
    <span class="k">private</span> <span class="n">SortedSet</span><span class="o">&lt;</span><span class="n">Integer</span><span class="o">&gt;</span> <span class="n">createOtherCardList</span><span class="o">(</span><span class="kt">int</span> <span class="n">max</span><span class="o">,</span> <span class="n">Set</span><span class="o">&lt;</span><span class="n">Integer</span><span class="o">&gt;</span> <span class="n">list</span><span class="o">)</span> <span class="o">{</span>
