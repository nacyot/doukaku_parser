<span class="k">public</span> <span class="k">class</span> <span class="nc">Sample133</span> <span class="o">{</span>
    <span class="k">private</span> <span class="k">final</span> <span class="kt">int</span> <span class="n">count</span><span class="o">;</span>
    <span class="k">private</span> <span class="k">final</span> <span class="n">SortedSet</span><span class="o">&lt;</span><span class="n">Integer</span><span class="o">&gt;</span> <span class="n">tarouCardList_</span> <span class="o">=</span> <span class="k">new</span> <span class="n">TreeSet</span><span class="o">&lt;</span><span class="n">Integer</span><span class="o">&gt;();</span>

