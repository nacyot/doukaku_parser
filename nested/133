            <span class="n">SortedSet</span><span class="o">&lt;</span><span class="n">Integer</span><span class="o">&gt;</span> <span class="n">tailSet</span> <span class="o">=</span> <span class="n">cardList</span><span class="o">.</span><span class="na">tailSet</span><span class="o">(</span><span class="n">board</span><span class="o">);</span>
            <span class="k">if</span> <span class="o">(</span><span class="n">tailSet</span><span class="o">.</span><span class="na">isEmpty</span><span class="o">())</span> <span class="o">{</span>
