<span class="k">using</span> <span class="nn">System</span><span class="p">;</span>
<span class="k">using</span> <span class="nn">System.Collections.Generic</span><span class="p">;</span>

<span class="k">class</span> <span class="nc">Program</span> <span class="k">{</span>
    <span class="k">static</span> <span class="k">void</span> <span class="nf">Main</span><span class="p">(</span><span class="kt">string</span><span class="p">[]</span> <span class="n">args</span><span class="p">)</span> <span class="k">{</span>
        <span class="kt">string</span> <span class="n">input</span> <span class="p">=</span>
        <span class="s">@&quot;5</span>
<span class="s">1</span>
<span class="s">7</span>
<span class="s">9</span>
<span class="s">6</span>
<span class="s">10&quot;</span><span class="p">;</span>
        <span class="n">Stack</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">&gt;</span> <span class="n">Bafuda</span> <span class="p">=</span> <span class="k">new</span> <span class="n">Stack</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">&gt;();</span><span class="c">//input.Split(Environment.NewLine.ToCharArray()).Length * 2);</span>
        <span class="n">List</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">&gt;</span> <span class="n">TarouCards</span> <span class="p">=</span> <span class="k">new</span> <span class="n">List</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">&gt;();</span>
        <span class="n">List</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">&gt;</span> <span class="n">HanakoCards</span> <span class="p">=</span> <span class="k">new</span> <span class="n">List</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">&gt;();</span>
        <span class="n">List</span><span class="p">&lt;</span><span class="n">List</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">&gt;&gt;</span> <span class="n">Player</span> <span class="p">=</span> <span class="k">new</span> <span class="n">List</span><span class="p">&lt;</span><span class="n">List</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">&gt;&gt;()</span> <span class="k">{</span> <span class="n">TarouCards</span><span class="p">,</span> <span class="n">HanakoCards</span> <span class="k">}</span><span class="p">;</span>
        <span class="n">Bafuda</span><span class="p">.</span><span class="n">Push</span><span class="p">(</span><span class="m">0</span><span class="p">);</span>

        <span class="c">//太郎手札</span>
        <span class="kt">bool</span> <span class="n">IsFirst</span> <span class="p">=</span> <span class="k">true</span><span class="p">;</span>
        <span class="k">foreach</span><span class="p">(</span><span class="kt">string</span> <span class="n">card</span> <span class="k">in</span> <span class="n">input</span><span class="p">.</span><span class="n">Trim</span><span class="p">().</span><span class="n">Split</span><span class="p">(</span><span class="k">new</span> <span class="kt">char</span><span class="p">[]</span> <span class="k">{</span> <span class="sc">&#39;\n&#39;</span> <span class="k">}</span><span class="p">))</span> <span class="k">{</span>
            <span class="k">if</span><span class="p">(!</span><span class="n">IsFirst</span><span class="p">)</span> <span class="k">{</span>
                <span class="n">TarouCards</span><span class="p">.</span><span class="n">Add</span><span class="p">(</span><span class="kt">int</span><span class="p">.</span><span class="n">Parse</span><span class="p">(</span><span class="n">card</span><span class="p">.</span><span class="n">Trim</span><span class="p">()));</span>
            <span class="k">}</span>
            <span class="n">IsFirst</span> <span class="p">=</span> <span class="k">false</span><span class="p">;</span>
        <span class="k">}</span>
        <span class="n">TarouCards</span><span class="p">.</span><span class="n">Sort</span><span class="p">();</span>
        <span class="c">//花子手札</span>
        <span class="k">for</span><span class="p">(</span><span class="kt">int</span> <span class="n">i</span> <span class="p">=</span> <span class="m">1</span><span class="p">;</span> <span class="n">i</span> <span class="p">&lt;=</span> <span class="n">TarouCards</span><span class="p">.</span><span class="n">Count</span> <span class="p">*</span> <span class="m">2</span><span class="p">;</span> <span class="n">i</span><span class="p">++)</span> <span class="k">{</span>
            <span class="k">if</span><span class="p">(!</span><span class="n">TarouCards</span><span class="p">.</span><span class="n">Contains</span><span class="p">(</span><span class="n">i</span><span class="p">))</span> <span class="n">HanakoCards</span><span class="p">.</span><span class="n">Add</span><span class="p">(</span><span class="n">i</span><span class="p">);</span>
        <span class="k">}</span>
        <span class="n">HanakoCards</span><span class="p">.</span><span class="n">Sort</span><span class="p">();</span>

        <span class="k">while</span><span class="p">(</span><span class="k">true</span><span class="p">)</span> <span class="k">{</span>
            <span class="kt">bool</span> <span class="n">IsEnd</span> <span class="p">=</span> <span class="k">false</span><span class="p">;</span>
            <span class="k">foreach</span><span class="p">(</span><span class="n">List</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">&gt;</span> <span class="n">player</span> <span class="k">in</span> <span class="n">Player</span><span class="p">)</span> <span class="k">{</span>
                <span class="kt">int</span> <span class="n">selectCard</span><span class="p">;</span>
                <span class="n">selectCard</span> <span class="p">=</span> <span class="n">SelectCard</span><span class="p">(</span><span class="n">player</span><span class="p">,</span> <span class="n">Bafuda</span><span class="p">.</span><span class="n">Peek</span><span class="p">());</span>
                <span class="n">player</span><span class="p">.</span><span class="n">Remove</span><span class="p">(</span><span class="n">selectCard</span><span class="p">);</span>
                <span class="n">Bafuda</span><span class="p">.</span><span class="n">Push</span><span class="p">(</span><span class="n">selectCard</span><span class="p">);</span>
                <span class="k">if</span><span class="p">(</span><span class="n">player</span><span class="p">.</span><span class="n">Count</span> <span class="p">==</span> <span class="m">0</span><span class="p">)</span> <span class="k">{</span>
                    <span class="n">IsEnd</span> <span class="p">=</span> <span class="k">true</span><span class="p">;</span>
                    <span class="k">break</span><span class="p">;</span>
                <span class="k">}</span>
            <span class="k">}</span>
            <span class="k">if</span><span class="p">(</span><span class="n">IsEnd</span><span class="p">)</span> <span class="k">break</span><span class="p">;</span>
        <span class="k">}</span>

        <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">(</span><span class="n">HanakoCards</span><span class="p">.</span><span class="n">Count</span><span class="p">);</span>
        <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">(</span><span class="n">TarouCards</span><span class="p">.</span><span class="n">Count</span><span class="p">);</span>
        <span class="n">Console</span><span class="p">.</span><span class="n">ReadLine</span><span class="p">();</span>
    <span class="k">}</span>

    <span class="k">static</span> <span class="kt">int</span> <span class="nf">SelectCard</span><span class="p">(</span><span class="n">List</span><span class="p">&lt;</span><span class="kt">int</span><span class="p">&gt;</span> <span class="n">cards</span><span class="p">,</span> <span class="kt">int</span> <span class="n">fieldCard</span><span class="p">)</span> <span class="k">{</span>
        <span class="k">foreach</span><span class="p">(</span><span class="kt">int</span> <span class="n">card</span> <span class="k">in</span> <span class="n">cards</span><span class="p">)</span> <span class="k">{</span>
            <span class="k">if</span><span class="p">(</span><span class="n">card</span> <span class="p">&gt;</span> <span class="n">fieldCard</span><span class="p">)</span> <span class="k">{</span>
                <span class="k">return</span> <span class="n">card</span><span class="p">;</span>
            <span class="k">}</span>
        <span class="k">}</span>
        <span class="k">return</span> <span class="m">0</span><span class="p">;</span>
    <span class="k">}</span>
<span class="k">}</span>
