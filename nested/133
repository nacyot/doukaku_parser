<span class="k">class</span> <span class="nc">Program</span>
<span class="k">{</span>
    <span class="k">enum</span> <span class="n">Status</span> <span class="p">:</span> <span class="kt">int</span> <span class="k">{</span> <span class="n">NONE</span> <span class="p">=</span> <span class="m">0</span><span class="p">,</span> <span class="n">TARO</span><span class="p">,</span> <span class="n">HANAKO</span><span class="k">}</span><span class="p">;</span>
    <span class="k">static</span> <span class="k">void</span> <span class="nf">Main</span><span class="p">(</span><span class="kt">string</span><span class="p">[]</span> <span class="n">args</span><span class="p">)</span>
    <span class="k">{</span>
        <span class="kt">int</span> <span class="n">n</span> <span class="p">=</span> <span class="m">0</span><span class="p">,</span> <span class="n">TaroNum</span>  <span class="p">=</span> <span class="m">0</span><span class="p">,</span> <span class="n">HanakoNum</span> <span class="p">=</span> <span class="m">0</span><span class="p">;</span>
        <span class="n">Status</span><span class="p">[]</span> <span class="n">cache</span><span class="p">;</span>
        <span class="c">//入力 (nの値）</span>
        <span class="k">try</span>
        <span class="k">{</span> 
            <span class="n">n</span> <span class="p">=</span> <span class="kt">int</span><span class="p">.</span><span class="n">Parse</span><span class="p">(</span><span class="n">Console</span><span class="p">.</span><span class="n">ReadLine</span><span class="p">());</span>
            <span class="k">if</span> <span class="p">(</span><span class="n">n</span> <span class="p">&lt;</span> <span class="m">1</span> <span class="p">||</span> <span class="n">n</span> <span class="p">&gt;</span> <span class="m">100</span><span class="p">)</span>
                <span class="k">throw</span> <span class="k">new</span> <span class="n">Exception</span><span class="p">();</span>
        <span class="k">}</span>
        <span class="k">catch</span>
        <span class="k">{</span>
            <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">(</span><span class="s">&quot;nの値が間違ってます&quot;</span><span class="p">);</span>
            <span class="k">return</span><span class="p">;</span>
        <span class="k">}</span>

        <span class="c">//入力（カードの値）</span>
        <span class="n">cache</span> <span class="p">=</span> <span class="k">new</span> <span class="n">Status</span><span class="p">[</span><span class="m">2</span> <span class="p">*</span> <span class="n">n</span><span class="p">];</span>
        <span class="k">do</span>
        <span class="k">{</span>
            <span class="k">try</span>
            <span class="k">{</span>
                <span class="kt">int</span> <span class="n">temp</span> <span class="p">=</span> <span class="kt">int</span><span class="p">.</span><span class="n">Parse</span><span class="p">(</span><span class="n">Console</span><span class="p">.</span><span class="n">ReadLine</span><span class="p">());</span>
                <span class="k">if</span> <span class="p">(</span><span class="n">temp</span> <span class="p">-</span> <span class="m">1</span> <span class="p">&lt;</span> <span class="m">0</span> <span class="p">||</span> <span class="n">temp</span> <span class="p">-</span> <span class="m">1</span> <span class="p">&gt;</span> <span class="n">cache</span><span class="p">.</span><span class="n">Length</span><span class="p">)</span>
                <span class="k">{</span>
                    <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">(</span><span class="s">&quot;その値は範囲外です&quot;</span><span class="p">);</span>
                    <span class="k">continue</span><span class="p">;</span>
                <span class="k">}</span>
                <span class="k">if</span> <span class="p">(</span><span class="n">cache</span><span class="p">[</span><span class="n">temp</span> <span class="p">-</span> <span class="m">1</span><span class="p">]</span> <span class="p">!=</span> <span class="n">Status</span><span class="p">.</span><span class="n">NONE</span><span class="p">)</span>
                <span class="k">{</span>
                    <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">(</span><span class="s">&quot;もうそのカードは持っています&quot;</span><span class="p">);</span>
                    <span class="k">continue</span><span class="p">;</span>
                <span class="k">}</span>
                <span class="n">cache</span><span class="p">[</span><span class="n">temp</span> <span class="p">-</span> <span class="m">1</span><span class="p">]</span> <span class="p">=</span> <span class="n">Status</span><span class="p">.</span><span class="n">TARO</span><span class="p">;</span>
                <span class="n">TaroNum</span><span class="p">++;</span>
                <span class="n">n</span><span class="p">--;</span>
            <span class="k">}</span>
            <span class="k">catch</span>
            <span class="k">{</span>
                <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">(</span><span class="s">&quot;その値は入力できません&quot;</span><span class="p">);</span>
                <span class="k">continue</span><span class="p">;</span>
            <span class="k">}</span>
        <span class="k">}</span> <span class="k">while</span> <span class="p">(</span><span class="n">n</span> <span class="p">&gt;</span> <span class="m">0</span><span class="p">);</span>

        <span class="c">//花子にカードを渡す</span>
        <span class="k">for</span> <span class="p">(</span><span class="kt">int</span> <span class="n">i</span> <span class="p">=</span> <span class="m">0</span><span class="p">;</span> <span class="n">i</span> <span class="p">&lt;</span> <span class="n">cache</span><span class="p">.</span><span class="n">GetLength</span><span class="p">(</span><span class="m">0</span><span class="p">);</span> <span class="n">i</span><span class="p">++)</span>
            <span class="k">if</span> <span class="p">(</span><span class="n">cache</span><span class="p">[</span><span class="n">i</span><span class="p">]</span> <span class="p">==</span> <span class="n">Status</span><span class="p">.</span><span class="n">NONE</span><span class="p">)</span>
            <span class="k">{</span>
                <span class="n">cache</span><span class="p">[</span><span class="n">i</span><span class="p">]</span> <span class="p">=</span> <span class="n">Status</span><span class="p">.</span><span class="n">HANAKO</span><span class="p">;</span>
                <span class="n">HanakoNum</span><span class="p">++;</span>
            <span class="k">}</span>

        <span class="c">//対戦開始</span>
        <span class="c">//int field = 0;</span>
        <span class="n">Status</span> <span class="n">turn</span> <span class="p">=</span> <span class="n">Status</span><span class="p">.</span><span class="n">TARO</span><span class="p">;</span>
        <span class="k">while</span> <span class="p">(</span><span class="n">TaroNum</span> <span class="p">&gt;</span> <span class="m">0</span> <span class="p">&amp;&amp;</span> <span class="n">HanakoNum</span> <span class="p">&gt;</span> <span class="m">0</span><span class="p">)</span>
        <span class="k">{</span>
            
            <span class="k">for</span> <span class="p">(</span><span class="kt">int</span> <span class="n">i</span> <span class="p">=</span> <span class="m">0</span><span class="p">;</span> <span class="n">i</span> <span class="p">&lt;</span> <span class="n">cache</span><span class="p">.</span><span class="n">GetLength</span><span class="p">(</span><span class="m">0</span><span class="p">);</span> <span class="n">i</span><span class="p">++)</span>
            <span class="k">{</span>
                <span class="k">if</span> <span class="p">(</span><span class="n">turn</span> <span class="p">==</span> <span class="n">cache</span><span class="p">[</span><span class="n">i</span><span class="p">])</span>
                <span class="k">{</span>
                    <span class="n">cache</span><span class="p">[</span><span class="n">i</span><span class="p">]</span> <span class="p">=</span> <span class="n">Status</span><span class="p">.</span><span class="n">NONE</span><span class="p">;</span>
                    <span class="k">if</span> <span class="p">(</span><span class="n">turn</span> <span class="p">==</span> <span class="n">Status</span><span class="p">.</span><span class="n">TARO</span><span class="p">)</span>
                    <span class="k">{</span> 
                            <span class="n">TaroNum</span><span class="p">--;</span>
                            <span class="n">turn</span> <span class="p">=</span> <span class="n">Status</span><span class="p">.</span><span class="n">HANAKO</span><span class="p">;</span>
                            <span class="c">//Console.WriteLine(&quot;Taro Drop : &quot; + (i + 1));</span>
                            <span class="k">if</span> <span class="p">(</span><span class="n">TaroNum</span> <span class="p">==</span> <span class="m">0</span><span class="p">)</span> <span class="k">break</span><span class="p">;</span>
                    <span class="k">}</span>
                    <span class="k">else</span> <span class="nf">if</span> <span class="p">(</span><span class="n">turn</span> <span class="p">==</span> <span class="n">Status</span><span class="p">.</span><span class="n">HANAKO</span><span class="p">)</span>
                    <span class="k">{</span>
                            <span class="n">HanakoNum</span><span class="p">--;</span>
                            <span class="n">turn</span> <span class="p">=</span> <span class="n">Status</span><span class="p">.</span><span class="n">TARO</span><span class="p">;</span>
                            <span class="c">//Console.WriteLine(&quot;Hanako Drop : &quot; + (i + 1));</span>
                            <span class="k">if</span> <span class="p">(</span><span class="n">HanakoNum</span> <span class="p">==</span> <span class="m">0</span><span class="p">)</span> <span class="k">break</span><span class="p">;</span>
                    <span class="k">}</span>
                <span class="k">}</span>
            <span class="k">}</span>
            <span class="c">//Console.WriteLine(&quot;Field Flush&quot;);</span>
            <span class="k">if</span> <span class="p">(</span><span class="n">turn</span> <span class="p">==</span> <span class="n">Status</span><span class="p">.</span><span class="n">TARO</span><span class="p">)</span> <span class="n">turn</span> <span class="p">=</span> <span class="n">Status</span><span class="p">.</span><span class="n">HANAKO</span><span class="p">;</span>
            <span class="k">else</span> <span class="n">turn</span> <span class="p">=</span> <span class="n">Status</span><span class="p">.</span><span class="n">TARO</span><span class="p">;</span>
        <span class="k">}</span>

        <span class="c">//結果発表</span>
        <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">(</span><span class="s">&quot;Taro   : &quot;</span> <span class="p">+</span> <span class="n">TaroNum</span><span class="p">);</span>
        <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">(</span><span class="s">&quot;Hanako : &quot;</span> <span class="p">+</span> <span class="n">HanakoNum</span><span class="p">);</span>  
    <span class="k">}</span>
<span class="k">}</span>
</pre></div>
</td></tr></table>
    </div>
	

  
