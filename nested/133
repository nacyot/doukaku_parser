1 to: 5 do: [:入力＃ |
    | 入力 データ サイズ 太郎 花子 二人 プレイヤ 場の数 |
    入力 := FileStream fileNamed: (&#39;2008-yo-t3-in{1}.txt&#39; format: {入力＃}).
    データ := Scanner new scanTokens: 入力 contentsOfEntireFile.
    サイズ := データ first.
    太郎 := データ allButFirst sort asOrderedCollection.
    self assert: [サイズ = 太郎 size].
    花子 := ((1 to: サイズ * 2) difference: 太郎) asOrderedCollection.
    二人 := {太郎. 花子}.
    場の数 := nil.
    プレイヤ := 太郎.
    [二人 allSatisfy: [:各々 | 各々 notEmpty]] whileTrue: [
        場の数 ifNil: [場の数 := プレイヤ removeFirst] ifNotNil: [
            場の数 := プレイヤ detect: [:各々 | 各々 &gt; 場の数] ifNone: [nil].
            場の数 ifNotNil: [プレイヤ remove: 場の数]].
        プレイヤ := 二人 detect: [:各々 | (各々 == プレイヤ) not]].
    Transcript cr; cr; show: 入力 localName.
    {花子. 太郎} do: [:各々 | Transcript cr; show: 各々 size]]
</pre></div>
