n=.{.y
m=.+:n
c=.0(&lt;:}.y)}m$1
cn=.n,n
p=.0
while. 1 do.
  for_i. i.m do.
    if. p=i{c do.
      c=._1(i)}c
      cn=.(&lt;:p{cn)(p)}cn
      if. 0=p{cn do. ,.|.cn return. end.
      p=.-.p
    end.
  end.
  p=.-.p
end.
)
exit echo&quot;0 cardgame &quot;.;._2 stdin&#39;&#39;
