    <span class="o">}</span>

    <span class="k">public</span> <span class="kt">int</span><span class="o">[]</span> <span class="n">calc</span><span class="o">()</span> <span class="o">{</span>
        <span class="n">SortedSet</span><span class="o">&lt;</span><span class="n">Integer</span><span class="o">&gt;</span> <span class="n">hanakoCardList</span> <span class="o">=</span> <span class="n">createOtherCardList</span><span class="o">(</span><span class="n">count</span> <span class="o">*</span> <span class="mi">2</span><span class="o">,</span> <span class="n">tarouCardList_</span><span class="o">);</span>
        <span class="kt">int</span> <span class="n">board</span> <span class="o">=</span> <span class="mi">0</span><span class="o">;</span>
