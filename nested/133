<span class="kt">int</span> <span class="n">main</span> <span class="p">(</span> <span class="kt">void</span> <span class="p">){</span>

   <span class="kt">int</span> <span class="n">n</span><span class="p">[</span><span class="mi">3</span><span class="p">],</span> <span class="n">i</span><span class="p">,</span> <span class="o">*</span><span class="n">card</span><span class="p">,</span> <span class="n">turn</span> <span class="o">=</span> <span class="n">TARO</span><span class="p">;</span>

