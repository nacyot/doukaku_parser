  <span class="k">|</span> <span class="k">(</span><span class="n">Nine</span><span class="k">,</span> <span class="n">Nine</span><span class="k">)</span> <span class="k">-&gt;</span> <span class="n">raise</span> <span class="k">(</span><span class="n">Invalid_argument</span> <span class="s2">&quot;succ, ninety-nine&quot;</span><span class="k">)</span>
