  <span class="p">(</span><span class="k">syntax-rules </span><span class="p">()</span>
    <span class="p">((</span><span class="nf">_</span> <span class="p">(</span><span class="nf">?v</span> <span class="nv">?l</span><span class="p">)</span> <span class="nv">body</span> <span class="o">...</span><span class="p">)</span>
     <span class="p">(</span><span class="k">do </span><span class="p">((</span><span class="nf">lim</span> <span class="nv">?l</span><span class="p">)</span>
          <span class="p">(</span><span class="nf">?v</span> <span class="p">(</span><span class="nf">+</span><span class="p">)</span> <span class="p">(</span><span class="nb">+ </span><span class="nv">?v</span> <span class="p">(</span><span class="nf">*</span><span class="p">))))</span>
         <span class="p">((</span><span class="nb">&gt;= </span><span class="nv">?v</span> <span class="nv">lim</span><span class="p">))</span>
       <span class="nv">body</span> <span class="o">...</span><span class="p">))))</span>

<span class="p">(</span><span class="k">let* </span><span class="p">((</span><span class="nf">n-str</span> <span class="s">&quot;○一二三四五六七八九&quot;</span><span class="p">)</span>
       <span class="p">(</span><span class="nf">ten</span> <span class="p">(</span><span class="nb">string-length </span><span class="nv">n-str</span><span class="p">))</span>
       <span class="p">(</span><span class="nf">nine</span> <span class="p">(</span><span class="nb">- </span><span class="nv">ten</span> <span class="p">(</span><span class="nf">*</span><span class="p">))))</span>
  <span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">kan-suzi</span> <span class="nv">n</span><span class="p">)</span>
    <span class="p">(</span><span class="k">let </span><span class="nv">loop</span> <span class="p">((</span><span class="nf">n</span> <span class="nv">n</span><span class="p">)</span>
               <span class="p">(</span><span class="nf">rs</span> <span class="o">&#39;</span><span class="p">()))</span>
      <span class="p">(</span><span class="k">if </span><span class="p">(</span><span class="nb">&lt; </span><span class="nv">n</span> <span class="nv">ten</span><span class="p">)</span>
          <span class="p">(</span><span class="nb">apply </span><span class="nv">string</span>
                 <span class="p">(</span><span class="nb">reverse </span><span class="p">(</span><span class="nb">map </span><span class="p">(</span><span class="k">lambda </span><span class="p">(</span><span class="nf">i</span><span class="p">)</span> <span class="p">(</span><span class="nb">string-ref </span><span class="nv">n-str</span> <span class="nv">i</span><span class="p">))</span> <span class="p">(</span><span class="nb">cons </span><span class="nv">n</span> <span class="nv">rs</span><span class="p">))))</span>
          <span class="p">(</span><span class="k">let </span><span class="p">((</span><span class="nf">q</span> <span class="p">(</span><span class="nb">quotient </span><span class="nv">n</span> <span class="nv">ten</span><span class="p">))</span>
                <span class="p">(</span><span class="nf">r</span> <span class="p">(</span><span class="nb">remainder </span><span class="nv">n</span> <span class="nv">ten</span><span class="p">)))</span>
            <span class="p">(</span><span class="nf">loop</span> <span class="nv">r</span> <span class="p">(</span><span class="nb">cons </span><span class="nv">q</span> <span class="nv">rs</span><span class="p">))))))</span>
  <span class="p">(</span><span class="nf">dotimes</span> <span class="p">(</span><span class="nf">i</span> <span class="nv">nine</span><span class="p">)</span>
    <span class="p">(</span><span class="nf">dotimes</span> <span class="p">(</span><span class="nf">j</span> <span class="nv">nine</span><span class="p">)</span>
      <span class="p">(</span><span class="nb">display </span><span class="s">&quot; &quot;</span><span class="p">)</span>
