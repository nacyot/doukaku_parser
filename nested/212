<span class="k">private</span> <span class="k">import</span> <span class="n">std</span>.<span class="n">string</span> : <span class="n">repeat</span>;
<span class="k">private</span> <span class="k">import</span> <span class="n">std</span>.<span class="n">typecons</span> : <span class="n">defineEnum</span>;

