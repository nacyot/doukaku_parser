30</pre></td><td class="code"><div class="highlight"><pre><span class="c">#!/bin/bash</span>
<span class="nv">digit</span><span class="o">=(</span>○ 一 二 三 四 五 六 七 八 九<span class="o">)</span>

漢数字出力<span class="o">()</span> <span class="o">{</span>
    <span class="nb">local </span><span class="nv">n</span><span class="o">=</span><span class="nv">$*</span>
    <span class="nb">local</span> -i i
    <span class="o">((</span>n &lt; <span class="k">${#</span><span class="nv">digit</span><span class="p">[@]</span><span class="k">}</span><span class="o">))</span> &amp;&amp; <span class="nb">echo</span> -n <span class="s1">&#39;　&#39;</span>
    <span class="k">for</span> <span class="o">((</span>;i &lt; <span class="k">${#</span><span class="nv">n</span><span class="k">}</span>; i++<span class="o">))</span> <span class="o">{</span>
        <span class="nb">echo</span> -n <span class="k">${</span><span class="nv">digit</span><span class="p">[${n:$i:$#</span><span class="k">}</span><span class="o">]}</span>
    <span class="o">}</span>
<span class="o">}</span>

九九一段<span class="o">()</span> <span class="o">{</span>
    <span class="nb">local</span> -i <span class="nv">m</span><span class="o">=</span><span class="nv">$*</span>
    <span class="nb">local</span> -i n
    <span class="k">for</span> <span class="o">((</span>;++n &lt; <span class="k">${#</span><span class="nv">digit</span><span class="p">[@]</span><span class="k">}</span>;<span class="o">))</span> <span class="o">{</span>
        漢数字出力 <span class="k">$(</span><span class="o">(</span>m * n<span class="k">)</span><span class="o">)</span>
        <span class="nb">echo</span> -n <span class="s1">&#39; &#39;</span>
    <span class="o">}</span>
<span class="o">}</span>

九九<span class="o">()</span> <span class="o">{</span>
    <span class="nb">local</span> -i m
    <span class="k">for</span> <span class="o">((</span>;++m &lt; <span class="k">${#</span><span class="nv">digit</span><span class="p">[@]</span><span class="k">}</span>;<span class="o">))</span> <span class="o">{</span>
        九九一段 <span class="nv">$m</span>
        <span class="nb">echo</span>
    <span class="o">}</span>
<span class="o">}</span>

九九
</pre></div>
