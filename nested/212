48</pre></td><td class="code"><div class="highlight"><pre>implement dZIZ;

include &quot;sys.m&quot;;
include &quot;draw.m&quot;;

dZIZ: module{
    sys: Sys;
    init: fn(ctx: ref Draw-&gt;Context, argv: list of string);
    main: fn();
};

main()
{
    num:= array[] of {
        &quot;〇&quot;, &quot;一&quot;, &quot;二&quot;, &quot;三&quot;, &quot;四&quot;, &quot;五&quot;, &quot;六&quot;, &quot;七&quot;, &quot;八&quot;, &quot;九&quot;
    };
    spc := &quot;　&quot;;

    zero, one, ten: int;
    i, j: int;
    u, l: int;

    zero = len &quot;&quot;;
    one = zero; one++;
    ten = len num;

    for(i = one; i &lt; ten; i++){
        for(j = one; j &lt; ten; j++){
            l = (i * j) % ten;
            u = (i * j) / ten;
            sys-&gt;print(&quot;%s&quot;, spc);
            if(u == zero){
                sys-&gt;print(&quot;%s&quot;, spc);
            }else{
                sys-&gt;print(&quot;%s&quot;, num[u]);
            }
            sys-&gt;print(&quot;%s&quot;, num[l]);
        }
        sys-&gt;print(&quot;\n&quot;);
    }
}

init(ctx: ref Draw-&gt;Context, argv: list of string)
{
    sys = load Sys Sys-&gt;PATH;

    main ();
}
</pre></div>
</td></tr></table>
