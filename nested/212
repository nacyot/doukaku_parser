<span class="c"># encoding: UTF</span>

<span class="n">num_ten</span> <span class="o">=</span> <span class="s">u&quot;　一二三四五六七八九&quot;</span>
<span class="n">num_one</span> <span class="o">=</span> <span class="s">u&quot;〇一二三四五六七八九&quot;</span>

<span class="n">nums</span> <span class="o">=</span> <span class="p">[</span><span class="n">i</span><span class="o">+</span><span class="n">j</span> <span class="k">for</span> <span class="n">i</span> <span class="ow">in</span> <span class="n">num_ten</span> <span class="k">for</span> <span class="n">j</span> <span class="ow">in</span> <span class="n">num_one</span><span class="p">]</span>

<span class="k">for</span> <span class="n">i</span> <span class="ow">in</span> <span class="n">num_ten</span><span class="p">[</span><span class="n">num_ten</span><span class="o">.</span><span class="n">find</span><span class="p">(</span><span class="s">u&quot;一&quot;</span><span class="p">):]:</span>
        <span class="k">for</span> <span class="n">j</span> <span class="ow">in</span> <span class="n">num_one</span><span class="p">[</span><span class="n">num_one</span><span class="o">.</span><span class="n">find</span><span class="p">(</span><span class="s">u&quot;一&quot;</span><span class="p">):]:</span>
                <span class="k">print</span> <span class="n">nums</span><span class="p">[</span><span class="n">num_ten</span><span class="o">.</span><span class="n">index</span><span class="p">(</span><span class="n">i</span><span class="p">)</span><span class="o">*</span><span class="n">num_one</span><span class="o">.</span><span class="n">index</span><span class="p">(</span><span class="n">j</span><span class="p">)],</span>
        <span class="k">print</span> <span class="s">&quot;&quot;</span>
