16
17
18
19</pre></td><td class="code"><div class="highlight"><pre><span class="x">$nums = array(&quot;〇&quot;, &quot;一&quot;, &quot;二&quot;, &quot;三&quot;, &quot;四&quot;, &quot;五&quot;, &quot;六&quot;, &quot;七&quot;, &quot;八&quot;, &quot;九&quot;, &quot;十&quot;, &quot;十一&quot;);</span>
<span class="x">$start = array_search(&quot;一&quot;, $nums);</span>
<span class="x">end($nums);</span>
<span class="x">$digit = strlen(key($nums) * key($nums));</span>
<span class="x">for ($i = $start; $i &lt; count($nums); $i++) {</span>
<span class="x">    for ($j = $start; $j &lt; count($nums); $j++) {</span>
<span class="x">        $res = str_pad($i * $j, $digit, &quot; &quot;, STR_PAD_LEFT);</span>
<span class="x">        for ($k = $digit - $digit; $k &lt; $digit; $k++) {</span>
<span class="x">            $num = substr($res, $k, $start);</span>
<span class="x">            if ($num == &quot; &quot;) {</span>
<span class="x">                print(&quot;　&quot;);</span>
<span class="x">            } else {</span>
<span class="x">                print($nums[$num]);</span>
<span class="x">            }</span>
<span class="x">        }</span>
<span class="x">        print(&quot;　&quot;);</span>
<span class="x">    }</span>
<span class="x">    print(&quot;\n&quot;);</span>
<span class="x">}</span>
</pre></div>
</td></tr></table>
