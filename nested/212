	<a href="/web/20090103120250/http://ja.doukaku.org/comment/7917/">#7917</a>(<script language="JavaScript">print_time(new Date("2008/11/01 10:21 GMT"));</script><noscript>2008/11/01 10:21 GMT</noscript>)
	
	
	
