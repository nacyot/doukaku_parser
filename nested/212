漢数字(一,[_]).
漢数字(二,[_,_]).
漢数字(三,[_,_,_]).
漢数字(四,[_,_,_,_]).
漢数字(五,[_,_,_,_,_]).
漢数字(六,[_,_,_,_,_,_]).
漢数字(七,[_,_,_,_,_,_,_]).
漢数字(八,[_,_,_,_,_,_,_,_]).
漢数字(九,[_,_,_,_,_,_,_,_,_]).

九九 :-
   漢数字(U,A),
   漢数字(W,B),
   not(U=〇),
   not(W=〇),
   九九(A,B,[],[],X,Y),
   九九解表示(B,X,Y),
   fail.
九九.

九九([],_,[],A,&#39;　&#39;,Y) :-
    漢数字(Y,A),!.
九九([],_,U,W,X,Y) :-
    漢数字(X,U),漢数字(Y,W),!.
九九(A,B,C,[_,_,_,_,_,_,_,_,_,_|R],X,Y) :-
    九九(A,B,[_|C],R,X,Y),!.
九九([_|R],B,C,D,X,Y) :-
    append(B,D,E),
    九九(R,B,C,E,X,Y).

九九解表示([_,_,_,_,_,_,_,_,_],X,Y) :-
   write_formatted(&#39;%t%t\n&#39;,[X,Y]),!.
九九解表示(_,X,Y) :-
   write_formatted(&#39;%t%t &#39;,[X,Y]).

:- 九九.
</pre></div>
</td></tr></table>
    </div>
	

  
