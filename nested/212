  <span class="k">|</span> <span class="k">(</span><span class="n">x</span><span class="k">,</span> <span class="n">y</span><span class="k">)</span>       <span class="k">-&gt;</span> <span class="k">(</span><span class="n">x</span><span class="k">,</span> <span class="n">next_digit</span> <span class="n">y</span><span class="k">)</span>
