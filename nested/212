(defun 亜剌比亜数字至漢数字 (n)
  (map &#39;string (lambda (x) (aref +漢数字+ (parse-integer (string x))))
       (princ-to-string n)))

(let ((一 (position #\一 +漢数字+))
      (三 (position #\三 +漢数字+))
      (九 (position #\九 +漢数字+)))
