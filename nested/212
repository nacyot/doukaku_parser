    <span class="k">static</span> <span class="n">String</span><span class="o">[]</span> <span class="n">kuku</span><span class="o">={</span> <span class="s">&quot;一&quot;</span><span class="o">,</span> <span class="s">&quot;二&quot;</span><span class="o">,</span> <span class="s">&quot;三&quot;</span><span class="o">,</span> <span class="s">&quot;四&quot;</span><span class="o">,</span> <span class="s">&quot;五&quot;</span><span class="o">,</span> <span class="s">&quot;六&quot;</span><span class="o">,</span> <span class="s">&quot;七&quot;</span><span class="o">,</span> <span class="s">&quot;八&quot;</span><span class="o">,</span> <span class="s">&quot;九&quot;</span><span class="o">,</span> <span class="o">};</span>
    <span class="k">static</span> <span class="n">String</span><span class="o">[]</span> <span class="n">Wten</span><span class="o">={</span> <span class="s">&quot;○&quot;</span><span class="o">,</span> <span class="s">&quot;一&quot;</span><span class="o">,</span> <span class="s">&quot;二&quot;</span><span class="o">,</span> <span class="s">&quot;三&quot;</span><span class="o">,</span> <span class="s">&quot;四&quot;</span><span class="o">,</span> <span class="s">&quot;五&quot;</span><span class="o">,</span> <span class="s">&quot;六&quot;</span><span class="o">,</span> <span class="s">&quot;七&quot;</span><span class="o">,</span> <span class="s">&quot;八&quot;</span><span class="o">,</span> <span class="s">&quot;九&quot;</span><span class="o">,</span> <span class="o">};</span>
    <span class="k">static</span> <span class="k">final</span> <span class="n">String</span> <span class="n">num</span><span class="o">=</span><span class="s">&quot;〇一二三四五六七八九&quot;</span><span class="o">;</span>
    <span class="k">static</span> <span class="n">String</span> <span class="n">real</span><span class="o">=</span><span class="n">num</span><span class="o">;</span>
    <span class="k">static</span> <span class="n">String</span> <span class="n">ans</span><span class="o">;</span>
    <span class="k">public</span> <span class="k">static</span> <span class="kt">void</span> <span class="n">main</span><span class="o">(</span><span class="n">String</span><span class="o">[]</span> <span class="n">args</span><span class="o">)</span> <span class="o">{</span>

        <span class="k">for</span><span class="o">(</span><span class="n">String</span> <span class="nl">s:</span><span class="n">kuku</span><span class="o">){</span>
            <span class="kt">char</span> <span class="n">ch</span><span class="o">;</span>
            <span class="kt">int</span> <span class="n">interval</span><span class="o">=</span><span class="n">num</span><span class="o">.</span><span class="na">indexOf</span><span class="o">(</span><span class="n">s</span><span class="o">);</span>
            <span class="kt">int</span> <span class="n">ten</span><span class="o">=</span><span class="n">Wten</span><span class="o">.</span><span class="na">length</span><span class="o">-</span><span class="n">Wten</span><span class="o">.</span><span class="na">length</span><span class="o">;</span>
            
            <span class="n">real</span><span class="o">=</span><span class="n">num</span><span class="o">;</span>
            <span class="k">for</span><span class="o">(</span><span class="kt">int</span> <span class="n">i</span><span class="o">=</span><span class="n">interval</span><span class="o">;</span><span class="n">i</span><span class="o">&lt;</span><span class="n">kuku</span><span class="o">.</span><span class="na">length</span><span class="o">+</span><span class="n">interval</span><span class="o">;</span><span class="n">i</span><span class="o">++){</span>
                
                <span class="k">if</span><span class="o">(</span><span class="n">real</span><span class="o">.</span><span class="na">length</span><span class="o">()&lt;=</span><span class="n">interval</span><span class="o">){</span>
                    <span class="n">real</span><span class="o">+=</span><span class="n">num</span><span class="o">;</span>
                    <span class="n">ten</span><span class="o">++;</span>
                <span class="o">}</span><span class="n">ch</span><span class="o">=</span><span class="n">real</span><span class="o">.</span><span class="na">charAt</span><span class="o">(</span><span class="n">interval</span><span class="o">);</span>
