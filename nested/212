def nums = (1..9) as List
println nums.collect{i-&gt;
  nums.collect{
    new String(it*i+&quot;&quot;).collect{n-&gt; kanji[(n+&quot;&quot;).toInteger()]}.join().padLeft(2,&quot;　&quot;)+&quot; &quot;
