17</pre></td><td class="code"><div class="highlight"><pre><span class="k">var</span> <span class="nx">k</span><span class="o">=</span><span class="p">[</span><span class="s2">&quot;○&quot;</span><span class="o">,</span> <span class="s2">&quot;一&quot;</span><span class="o">,</span> <span class="s2">&quot;二&quot;</span><span class="o">,</span> <span class="s2">&quot;三&quot;</span><span class="o">,</span> <span class="s2">&quot;四&quot;</span><span class="o">,</span> <span class="s2">&quot;五&quot;</span><span class="o">,</span> <span class="s2">&quot;六&quot;</span><span class="o">,</span> <span class="s2">&quot;七&quot;</span><span class="o">,</span> <span class="s2">&quot;八&quot;</span><span class="o">,</span> <span class="s2">&quot;九&quot;</span><span class="p">];</span>
<span class="k">var</span> <span class="nx">size</span><span class="o">=</span><span class="nx">k</span><span class="p">.</span><span class="nx">length</span><span class="o">;</span>
<span class="k">var</span> <span class="nx">step</span><span class="o">=</span><span class="kc">null</span><span class="o">;</span>
<span class="k">var</span> <span class="nx">l</span><span class="o">;</span>

<span class="k">while</span><span class="p">(</span><span class="o">++</span><span class="nx">step</span><span class="o">&lt;</span><span class="p">(</span><span class="nx">l</span><span class="o">=</span><span class="nx">size</span><span class="p">)){</span>
    <span class="k">var</span> <span class="nx">out_line</span> <span class="o">=</span> <span class="s2">&quot;&quot;</span><span class="o">;</span>
    <span class="k">var</span> <span class="nx">over</span> <span class="o">=</span> <span class="kc">null</span><span class="o">;</span>
    <span class="k">for</span><span class="p">(</span><span class="k">var</span> <span class="nx">pos</span><span class="o">=</span><span class="nx">step</span><span class="o">;--</span><span class="nx">l</span><span class="o">;</span><span class="nx">pos</span><span class="o">+=</span><span class="nx">step</span><span class="p">){</span>
        <span class="k">if</span><span class="p">(</span><span class="nx">pos</span><span class="o">&gt;=</span><span class="nx">size</span><span class="p">){</span>
            <span class="nx">over</span><span class="o">++;</span>
            <span class="nx">pos</span> <span class="o">-=</span> <span class="nx">size</span><span class="o">;</span>
        <span class="p">}</span>
        <span class="nx">out_line</span> <span class="o">+=</span> <span class="p">(</span><span class="nx">over</span> <span class="o">?</span> <span class="nx">k</span><span class="p">[</span><span class="nx">over</span><span class="p">]</span> <span class="o">:</span> <span class="s2">&quot;&quot;</span><span class="p">)</span> <span class="o">+</span> <span class="nx">k</span><span class="p">[</span><span class="nx">pos</span><span class="p">]</span> <span class="o">+</span> <span class="s2">&quot;\t&quot;</span><span class="o">;</span>
    <span class="p">}</span>
