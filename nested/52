  <div class="comment_content" >
	<div class="comment_body">
	  <pre class='compact'></pre>
	</div>
	
	
	
    <div class="compact">
      <table><tr><td class="linenos"><pre>1
2
3
4
5
6
7
8</pre></td><td class="code"><div class="highlight"><pre>(require &#39;cl)
(defun pyramid (str)
  (interactive &quot;sPyramid string: &quot;)
  (with-output-to-temp-buffer &quot;*Pyramid*&quot;
    (loop for i from (1- (length str)) downto 0 do
          (princ (make-string i ? ))
          (princ (mapconcat &#39;identity (split-string (substring str i) &quot;&quot;) &quot; &quot;))
          (terpri))))
</pre></div>
</td></tr></table>
