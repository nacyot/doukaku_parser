15</pre></td><td class="code"><div class="highlight"><pre><span class="k">private</span> <span class="k">import</span> <span class="n">std</span>.<span class="n">stdio</span>;

<span class="kt">void</span> <span class="n">pyramid</span>(<span class="kt">char</span>[] <span class="n">str</span>) {
    <span class="k">in</span><span class="n">t</span> <span class="n">i</span>,<span class="n">j</span>;
    <span class="k">for</span> (<span class="n">i</span>=<span class="n">str</span>.<span class="n">length</span>-<span class="mi">1</span>; <span class="n">i</span>&gt;=<span class="mi">0</span>; <span class="n">i</span>--) {
        <span class="k">for</span>(<span class="n">j</span>=<span class="mi">0</span>; <span class="n">j</span>&lt;<span class="n">i</span>; <span class="n">j</span>++) <span class="n">writef</span>(<span class="s">&quot; &quot;</span>);
        <span class="k">for</span>(<span class="n">j</span>=<span class="n">i</span>; <span class="n">j</span>&lt;<span class="n">str</span>.<span class="n">length</span>; <span class="n">j</span>++) <span class="n">writef</span>(<span class="s">&quot;%s%s&quot;</span>, <span class="n">str</span>[<span class="n">j</span>], <span class="s">&quot; &quot;</span>);
        <span class="n">writef</span>(<span class="s">&quot;\n&quot;</span>);
    }
}

<span class="k">in</span><span class="n">t</span> <span class="n">main</span>() {
    <span class="n">pyramid</span>(<span class="s">&quot;abracadabra&quot;</span>);
    <span class="k">return</span> <span class="mi">0</span>;
}
