	<a href="/web/20080315053939/http://ja.doukaku.org/comment/2548/">#2548</a>(<script language="JavaScript">print_time(new Date("2007/08/26 14:59 GMT"));</script><noscript>2007/08/26 14:59 GMT</noscript>)
	
	
	
