         loop x y
       end

<span class="gd">-      fun f x = let</span>
<span class="gi">+      fun f [] = ()</span>
<span class="gi">+        | f (x::xs) = let</span>
         val pp = pow (p, x)
       in
         if pp &lt;= bound then
