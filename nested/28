<span class="p">(</span><span class="k">define </span><span class="nv">call-trace</span> <span class="p">(</span><span class="nf">make-parameter</span> <span class="o">&#39;</span><span class="p">()))</span>

<span class="p">(</span><span class="nf">define-class</span> <span class="nv">&lt;tracer&gt;</span> <span class="p">(</span><span class="nf">&lt;generic&gt;</span><span class="p">)</span> <span class="p">())</span>

<span class="p">(</span><span class="nf">define-method</span> <span class="nv">apply-generic</span> <span class="p">((</span><span class="nf">gf</span> <span class="nv">&lt;tracer&gt;</span><span class="p">)</span> <span class="nv">args</span><span class="p">)</span>
  <span class="p">(</span><span class="nf">parameterize</span> <span class="p">((</span><span class="nf">call-trace</span> <span class="p">(</span><span class="nb">cons </span><span class="p">(</span><span class="nf">ref</span> <span class="nv">gf</span><span class="ss">&#39;name</span><span class="p">)</span> <span class="p">(</span><span class="nf">call-trace</span><span class="p">))))</span>
    <span class="p">(</span><span class="nf">next-method</span><span class="p">)))</span>

<span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">foo</span><span class="p">)</span> <span class="p">((</span><span class="nf">every-pred</span> <span class="nv">pair?</span> <span class="nv">car</span><span class="p">)</span> <span class="p">(</span><span class="nf">call-trace</span><span class="p">)))</span>
</pre></div>
</td></tr></table>
