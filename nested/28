    <span class="k">return</span> <span class="nx">foo</span><span class="p">();</span>
<span class="p">}</span>
<span class="k">function</span> <span class="nx">baz</span><span class="p">(){</span>
    <span class="k">return</span> <span class="nx">foo</span><span class="p">();</span>
<span class="p">}</span>

