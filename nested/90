         <span class="n">spawn</span> <span class="n">search&#39;&#39;</span><span class="k">(</span><span class="n">i</span><span class="k">)</span>
      <span class="kr">done</span><span class="k">;</span>
      <span class="n">reply</span> <span class="n">wait</span><span class="k">()</span> <span class="kr">to</span> <span class="n">search&#39;</span>
   <span class="kr">in</span>
   <span class="n">search&#39;</span><span class="k">(</span><span class="n">radix</span> <span class="k">-</span> <span class="mi">1</span><span class="k">,</span> <span class="mi">1</span><span class="k">,</span> <span class="k">[])</span>

