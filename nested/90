   <span class="kr">let</span> <span class="n">str</span> <span class="k">=</span> <span class="n">List</span><span class="k">.</span><span class="n">flatten</span> <span class="k">(</span><span class="n">slist_of_alist</span> <span class="n">radix</span> <span class="n">alist</span><span class="k">)</span> <span class="kr">in</span>
   <span class="n">List</span><span class="k">.</span><span class="n">for_all</span> <span class="kr">begin</span> <span class="kr">fun</span> <span class="n">pair</span> <span class="k">-&gt;</span>
      <span class="kr">match</span> <span class="n">pair</span> <span class="kr">with</span>
      <span class="k">|</span> <span class="k">[</span><span class="n">i</span><span class="k">;</span> <span class="n">num</span><span class="k">]</span> <span class="k">-&gt;</span> <span class="kr">begin</span>
           <span class="kr">match</span> <span class="n">lstring_of_radix_num</span> <span class="n">radix</span> <span class="n">i</span> <span class="kr">with</span>
           <span class="k">|</span> <span class="k">[</span><span class="n">ch</span><span class="k">]</span> <span class="k">-&gt;</span>
                <span class="n">num</span> <span class="k">=</span>
                   <span class="n">List</span><span class="k">.</span><span class="n">fold_left</span> <span class="kr">begin</span> <span class="kr">fun</span> <span class="n">sum</span> <span class="n">ch&#39;</span> <span class="k">-&gt;</span>
