                 <span class="k">(</span><span class="n">count_digits</span> <span class="n">radix</span> <span class="k">(</span><span class="n">slist_of_alist</span> <span class="n">radix</span> <span class="n">acc&#39;</span><span class="k">)</span> <span class="n">next</span><span class="k">)</span> <span class="k">+</span> <span class="mi">1</span>
