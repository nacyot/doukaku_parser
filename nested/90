   <span class="kr">let</span> <span class="n">str</span> <span class="k">=</span>
      <span class="n">List</span><span class="k">.</span><span class="n">fold_left</span> <span class="kr">begin</span> <span class="kr">fun</span> <span class="n">acc</span> <span class="n">pair</span> <span class="k">-&gt;</span>
         <span class="kr">match</span> <span class="n">List</span><span class="k">.</span><span class="n">map</span> <span class="k">(</span><span class="n">string_of_radix_num</span> <span class="n">radix</span><span class="k">)</span> <span class="n">pair</span> <span class="kr">with</span>
         <span class="k">|</span> <span class="k">[</span><span class="n">i</span><span class="k">;</span> <span class="n">n</span><span class="k">]</span> <span class="k">-&gt;</span>
              <span class="n">acc</span> <span class="o">^</span> <span class="n">i</span> <span class="o">^</span> <span class="s2">&quot;が&quot;</span> <span class="o">^</span> <span class="n">n</span> <span class="o">^</span> <span class="s2">&quot;個&quot;</span> <span class="o">^</span> <span class="s2">&quot;, &quot;</span>
