    <span class="nv">cs</span> <span class="o">=</span> <span class="nv">map</span> <span class="p">(</span><span class="o">+</span><span class="mi">1</span><span class="p">)</span> <span class="o">$</span> <span class="nv">countDigs&#39;</span> <span class="nv">radix</span> <span class="nv">xs&#39;</span>

<span class="nv">showResult</span> <span class="nv">radix</span> <span class="nv">xs</span> <span class="o">=</span> <span class="p">(</span><span class="s">&quot;この文は&quot;</span> <span class="o">++</span><span class="p">)</span> <span class="o">$</span> <span class="p">(</span><span class="o">++</span> <span class="s">&quot;あります。&quot;</span><span class="p">)</span>
  <span class="o">$</span> <span class="nv">concat</span> <span class="o">$</span> <span class="nv">intersperse</span> <span class="s">&quot;, &quot;</span>
  <span class="o">$</span> <span class="nv">map</span> <span class="nv">ct</span> <span class="p">[</span><span class="mi">0</span><span class="o">..</span><span class="nv">radix</span><span class="o">-</span><span class="mi">1</span><span class="p">]</span>
