      <span class="n">def</span> <span class="n">result</span><span class="k">(</span><span class="n">alist</span><span class="k">)</span> <span class="k">&amp;</span> <span class="n">wait</span><span class="k">()</span> <span class="k">=</span> <span class="n">reply</span> <span class="n">alist</span> <span class="kr">to</span> <span class="n">wait</span> <span class="kr">in</span>
