<span class="kr">import</span> <span class="n">Maybe</span>

<span class="nv">count</span> <span class="nv">pred</span> <span class="nv">xs</span> <span class="o">=</span> <span class="nv">length</span> <span class="o">$</span> <span class="nv">filter</span> <span class="nv">pred</span> <span class="nv">xs</span>

<span class="nv">countDigs&#39;</span> <span class="nv">radix</span> <span class="nv">ks</span> <span class="o">=</span> <span class="nv">map</span> <span class="nv">sum</span> <span class="o">$</span> <span class="nv">transpose</span> <span class="o">$</span> <span class="nv">map</span> <span class="p">(</span><span class="nv">countDigs</span> <span class="nv">radix</span><span class="p">)</span> <span class="nv">ks</span>

<span class="nv">combListsS</span> <span class="nv">n</span> <span class="nv">lim</span> <span class="o">=</span> <span class="nv">sequence</span> <span class="o">$</span> <span class="nv">replicate</span> <span class="nv">n</span> <span class="p">[</span><span class="mi">1</span><span class="o">..</span><span class="nv">lim</span><span class="o">-</span><span class="mi">1</span><span class="p">]</span>

<span class="nv">combLists&#39;&#39;</span> <span class="nv">n</span> <span class="o">=</span> <span class="p">[</span><span class="mi">1</span><span class="o">..</span><span class="p">]</span> <span class="o">&gt;&gt;=</span> <span class="nv">combListsM</span> <span class="nv">n</span>

<span class="nv">check</span> <span class="nv">radix</span> <span class="nv">xss</span> <span class="o">=</span> <span class="nv">mapMaybe</span> <span class="nv">f</span> <span class="nv">xss</span>  <span class="kr">where</span>
  <span class="nv">f</span> <span class="nv">xs</span> <span class="o">=</span> <span class="kr">if</span> <span class="nv">xs&#39;</span> <span class="o">==</span> <span class="nv">cs</span> <span class="kr">then</span> <span class="n">Just</span> <span class="nv">xs&#39;</span> <span class="kr">else</span> <span class="n">Nothing</span>  <span class="kr">where</span>
