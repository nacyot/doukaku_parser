   <span class="kr">in</span>
   <span class="kr">let</span> <span class="n">str&#39;</span> <span class="k">=</span> <span class="k">(</span><span class="n">String</span><span class="k">.</span><span class="n">sub</span> <span class="n">str</span> <span class="mi">0</span> <span class="k">((</span><span class="n">String</span><span class="k">.</span><span class="n">length</span> <span class="n">str</span><span class="k">)</span> <span class="k">-</span> <span class="mi">2</span><span class="k">))</span> <span class="o">^</span> <span class="s2">&quot;あります。&quot;</span> <span class="kr">in</span>
