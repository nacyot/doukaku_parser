        <span class="kr">end</span> <span class="mi">0</span> <span class="k">(</span><span class="n">List</span><span class="k">.</span><span class="n">flatten</span> <span class="n">str_list</span><span class="k">)</span>
   <span class="k">|</span> <span class="k">_</span> <span class="k">-&gt;</span> <span class="n">failwith</span> <span class="s2">&quot;unknown&quot;</span>

