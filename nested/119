 class MeMonthPanel extends JPanel {
     public MeMonthPanel(int year, int month) {
         setLayout(new BorderLayout());
<span class="gd">-        add(new JLabel(String.format(&quot;%dŒŽ&quot;, month), JLabel.CENTER), BorderLayout.NORTH);</span>
