	<a href="/web/20090222042728/http://ja.doukaku.org/comment/5144/">#5144</a>(<script language="JavaScript">print_time(new Date("2008/01/03 11:55 GMT"));</script><noscript>2008/01/03 11:55 GMT</noscript>)
	
	
	
	
	
