<span class="k">\begin</span><span class="nb">{</span>document<span class="nb">}</span>

<span class="k">\def\list</span><span class="nb">{</span>1,2,3,4,5<span class="nb">}</span>

<span class="k">\makeatletter</span>
<span class="k">\setbox</span>0=<span class="k">\hbox</span><span class="nb">{</span><span class="k">\list</span><span class="nb">}</span><span class="c">%</span>
<span class="k">\null\hskip\wd</span>0
<span class="k">\newlength\TempDim</span>
<span class="k">\TempDim</span> 0pt
<span class="k">\@</span>for<span class="k">\c</span>:=<span class="k">\list\do</span><span class="nb">{</span><span class="c">%</span>
  <span class="k">\llap</span><span class="nb">{</span><span class="k">\c</span>,<span class="k">\hskip\TempDim</span><span class="nb">}</span><span class="c">%</span>
  <span class="k">\advance\TempDim</span> 1em<span class="nb">}</span><span class="c">%</span>
<span class="k">\makeatother</span>

<span class="k">\end</span><span class="nb">{</span>document<span class="nb">}</span>
