<span class="cm">{$APPTYPE CONSOLE}</span>

<span class="k">procedure</span> <span class="nf">WriteReverse</span><span class="p">(</span><span class="k">const</span> <span class="n">given_list</span><span class="o">:</span> <span class="k">array</span> <span class="k">of</span> <span class="kt">Integer</span><span class="p">)</span><span class="o">;</span>
