  <span class="k">while</span> <span class="p">(</span> <span class="n">n</span><span class="o">%</span><span class="mi">2</span><span class="o">==</span><span class="mi">0</span> <span class="p">)</span> <span class="p">{</span> <span class="n">n</span><span class="o">/=</span><span class="mi">2</span><span class="p">;</span> <span class="o">++</span><span class="n">r</span><span class="p">[</span><span class="mi">2</span><span class="p">];</span> <span class="p">}</span>
