    <span class="o">|</span> <span class="p">(</span><span class="nv">b</span><span class="o">^</span><span class="nv">n</span><span class="p">)</span> <span class="o">&gt;</span> <span class="nv">max</span>   <span class="o">=</span>   <span class="p">[]</span>
    <span class="o">|</span> <span class="nv">otherwise</span> <span class="o">=</span> <span class="nv">n</span> <span class="o">:</span> <span class="nv">list</span> <span class="nv">max</span> <span class="nv">b</span> <span class="p">(</span><span class="nv">n</span><span class="o">+</span><span class="mi">1</span><span class="p">)</span>

<span class="nv">scan</span> <span class="nv">n</span> <span class="o">=</span> <span class="p">[(</span><span class="nv">n</span><span class="p">,</span><span class="nv">i</span><span class="p">,</span><span class="nv">j</span><span class="p">,</span><span class="nv">k</span><span class="p">)</span><span class="o">|</span><span class="nv">i</span><span class="o">&lt;-</span><span class="nv">list</span> <span class="nv">n</span> <span class="mi">2</span> <span class="mi">0</span><span class="p">,</span><span class="nv">j</span><span class="o">&lt;-</span><span class="nv">list</span> <span class="nv">n</span> <span class="mi">3</span> <span class="mi">0</span><span class="p">,</span><span class="nv">k</span><span class="o">&lt;-</span><span class="nv">list</span> <span class="nv">n</span> <span class="mi">5</span> <span class="mi">0</span><span class="p">,</span><span class="nv">n</span> <span class="o">==</span> <span class="p">(</span><span class="mi">2</span><span class="o">^</span><span class="nv">i</span><span class="o">*</span><span class="mi">3</span><span class="o">^</span><span class="nv">j</span><span class="o">*</span><span class="mi">5</span><span class="o">^</span><span class="nv">k</span><span class="p">)]</span>

<span class="nv">result</span> <span class="o">=</span> <span class="nv">take</span> <span class="mi">100</span> <span class="o">$</span> <span class="nv">concatMap</span> <span class="p">(</span><span class="nv">scan</span><span class="p">)</span> <span class="o">$</span> <span class="nv">filter</span> <span class="p">(</span><span class="nv">check</span><span class="p">)</span> <span class="p">[</span><span class="mi">1</span><span class="o">..</span><span class="p">]</span>

<span class="nv">check</span> <span class="nv">n</span>
    <span class="o">|</span> <span class="nv">n</span> <span class="o">==</span> <span class="mi">1</span>            <span class="o">=</span>   <span class="n">True</span>
    <span class="o">|</span> <span class="p">(</span><span class="nv">n</span> <span class="p">`</span><span class="nv">mod</span><span class="p">`</span> <span class="mi">2</span><span class="p">)</span> <span class="o">==</span> <span class="mi">0</span>  <span class="o">=</span>   <span class="n">True</span>
    <span class="o">|</span> <span class="p">(</span><span class="nv">n</span> <span class="p">`</span><span class="nv">mod</span><span class="p">`</span> <span class="mi">3</span><span class="p">)</span> <span class="o">==</span> <span class="mi">0</span>  <span class="o">=</span>   <span class="n">True</span>
    <span class="o">|</span> <span class="p">(</span><span class="nv">n</span> <span class="p">`</span><span class="nv">mod</span><span class="p">`</span> <span class="mi">5</span><span class="p">)</span> <span class="o">==</span> <span class="mi">0</span>  <span class="o">=</span>   <span class="n">True</span>
    <span class="o">|</span> <span class="nv">otherwise</span>         <span class="o">=</span>   <span class="n">False</span>

<span class="nv">main</span><span class="o">=</span><span class="kr">do</span>
    <span class="nv">mapM</span> <span class="p">(</span><span class="o">\</span><span class="nv">x</span><span class="o">-&gt;</span> <span class="nv">putStrLn</span> <span class="o">$</span> <span class="nv">format</span> <span class="nv">x</span><span class="p">)</span> <span class="nv">result</span>
    <span class="kr">where</span>
        <span class="nv">format</span> <span class="p">(</span><span class="nv">n</span><span class="p">,</span><span class="nv">i</span><span class="p">,</span><span class="nv">j</span><span class="p">,</span><span class="nv">k</span><span class="p">)</span> <span class="o">=</span> <span class="nv">concat</span> <span class="p">[</span><span class="nv">show</span> <span class="nv">n</span><span class="p">,</span><span class="s">&quot; = &quot;</span><span class="p">,</span><span class="nv">fmt</span> <span class="mi">2</span> <span class="nv">i</span><span class="p">,</span><span class="s">&quot; * &quot;</span><span class="p">,</span><span class="nv">fmt</span> <span class="mi">3</span> <span class="nv">j</span><span class="p">,</span><span class="s">&quot; * &quot;</span><span class="p">,</span><span class="nv">fmt</span> <span class="mi">5</span> <span class="nv">k</span><span class="p">]</span>
        <span class="nv">fmt</span> <span class="nv">b</span> <span class="nv">n</span> <span class="o">=</span> <span class="nv">show</span> <span class="nv">b</span> <span class="o">++</span> <span class="s">&quot;^&quot;</span> <span class="o">++</span> <span class="nv">show</span> <span class="nv">n</span>
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
	<a href="/web/20091213020708/http://ja.doukaku.org/comment/7640/addtag/" class="addtag" id="addtag_7640">タグ</a>:
	
