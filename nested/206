<span class="k">for</span> <span class="o">((</span><span class="nv">i</span> <span class="o">=</span> 0; i &lt; 100; i++<span class="o">))</span>;do
    <span class="nb">echo</span> <span class="nv">$n</span>
    ary<span class="o">[</span><span class="k">$(</span><span class="o">(</span>n*2<span class="k">)</span><span class="o">)]=</span>1
    ary<span class="o">[</span><span class="k">$(</span><span class="o">(</span>n*3<span class="k">)</span><span class="o">)]=</span>1
    ary<span class="o">[</span><span class="k">$(</span><span class="o">(</span>n*5<span class="k">)</span><span class="o">)]=</span>1
