	<a href="/web/20081120001525/http://ja.doukaku.org/comment/2110/">#2110</a>(<script language="JavaScript">print_time(new Date("2007/08/13 13:23 GMT"));</script><noscript>2007/08/13 13:23 GMT</noscript>)
	
	
	
