  <span class="p">(</span><span class="nf">cond</span>
   <span class="p">((</span><span class="o">#</span><span class="nv">/</span><span class="p">(</span><span class="err">[</span><span class="nv">a-zA-Z</span><span class="err">]</span><span class="nv">+</span><span class="p">)(</span><span class="nf">-hidden</span><span class="p">)</span><span class="nv">?</span><span class="p">(</span><span class="nf">?:-</span><span class="p">(</span><span class="nf">small</span><span class="err">|</span><span class="nv">big</span><span class="p">))</span><span class="nv">?</span><span class="err">\</span><span class="o">.</span><span class="p">(</span><span class="err">[</span><span class="nv">a-zA-Z</span><span class="err">]</span><span class="nv">+</span><span class="p">)</span><span class="nv">/</span> <span class="nv">string</span><span class="p">)</span>
    <span class="k">=&gt; </span><span class="p">(</span><span class="k">lambda </span><span class="p">(</span><span class="nf">m</span><span class="p">)</span>
         <span class="p">(</span><span class="nf">print</span> <span class="s">&quot;name: &#39;&quot;</span><span class="p">(</span><span class="nf">m</span> <span class="mi">1</span><span class="p">)</span><span class="s">&quot;&#39;, ext: &#39;&quot;</span><span class="p">(</span><span class="nf">m</span> <span class="mi">4</span><span class="p">)</span><span class="s">&quot;&#39;, size: &quot;</span><span class="p">(</span><span class="k">or </span><span class="p">(</span><span class="nf">m</span> <span class="mi">3</span><span class="p">)</span> <span class="s">&quot;normal&quot;</span><span class="p">)</span>
                <span class="s">&quot;, hidden: &quot;</span><span class="p">(</span><span class="k">if </span><span class="p">(</span><span class="nf">m</span> <span class="mi">2</span><span class="p">)</span><span class="s">&quot;True&quot;</span> <span class="s">&quot;False&quot;</span><span class="p">))</span>
         <span class="p">(</span><span class="nf">pick</span> <span class="p">(</span><span class="nf">m</span><span class="ss">&#39;after</span><span class="p">))))))</span>
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
