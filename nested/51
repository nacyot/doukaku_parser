  <span class="kr">_</span>  <span class="o">-&gt;</span> <span class="kr">case</span> <span class="nv">readP_to_S</span> <span class="nv">p</span> <span class="nv">str</span> <span class="kr">of</span>
    <span class="p">[]</span>         <span class="o">-&gt;</span> <span class="nv">head</span> <span class="nv">str</span> <span class="o">:</span> <span class="nv">replacer</span> <span class="nv">p</span> <span class="nv">s</span> <span class="p">(</span><span class="nv">tail</span> <span class="nv">str</span><span class="p">)</span>
    <span class="p">(</span><span class="nv">a</span><span class="p">,</span><span class="nv">str&#39;</span><span class="p">)</span><span class="o">:</span><span class="kr">_</span> <span class="o">-&gt;</span> <span class="nv">s</span> <span class="nv">a</span> <span class="o">++</span> <span class="nv">replacer</span> <span class="nv">p</span> <span class="nv">s</span> <span class="nv">str&#39;</span>
