    <span class="k">my</span> <span class="nv">$d</span> <span class="o">=</span> <span class="nv">$list</span><span class="p">[</span><span class="nv">$idx</span><span class="p">]</span> <span class="o">-</span> <span class="nv">$list</span><span class="p">[</span><span class="nv">$idx</span> <span class="o">-</span> <span class="mi">1</span><span class="p">];</span>

    <span class="k">if</span> <span class="p">(</span> <span class="nv">$prevd</span> <span class="o">&amp;&amp;</span> <span class="nv">$prevd</span> <span class="o">!=</span> <span class="nv">$d</span> <span class="p">)</span> <span class="p">{</span>
