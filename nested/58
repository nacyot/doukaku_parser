reverse_string:
    $$result = &quot;&quot;;
    while( $$1 != &quot;&quot; ) {
        $$char = unichar( unicode( $$1 ) );
        $$result = $$char + $$result;
        $$1 = rightstr( $$1, strlen( $$1 ) - strlen( $$char ) );
    }
    return $$result;
