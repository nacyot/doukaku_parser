<span class="p">{</span>
	<span class="k">return</span> <span class="nf">mb_convert_encoding</span><span class="p">(</span><span class="nf">strrev</span><span class="p">(</span><span class="nf">mb_convert_encoding</span><span class="p">(</span><span class="nv">$str</span><span class="p">,</span><span class="s2">&quot;UTF-16BE&quot;</span><span class="p">,</span><span class="nv">$encoding</span><span class="p">)),</span><span class="nv">$encoding</span><span class="p">,</span><span class="s2">&quot;UTF-16LE&quot;</span><span class="p">);</span>
