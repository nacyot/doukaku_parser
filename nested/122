    <span class="k">return</span> <span class="k">super</span> <span class="k">unless</span> <span class="vi">@infinit</span>
    <span class="k">return</span> <span class="kp">true</span> <span class="k">if</span> <span class="nb">self</span><span class="o">.</span><span class="n">first</span> <span class="o">&lt;=</span> <span class="n">other</span>
