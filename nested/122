<span class="kr">let</span> <span class="n">conv2</span> <span class="n">year</span> <span class="n">month</span> <span class="n">day</span> <span class="k">=</span>
    <span class="kr">match</span> <span class="n">year</span> <span class="k">*</span> <span class="mi">10000</span> <span class="k">+</span> <span class="n">month</span> <span class="k">*</span> <span class="mi">100</span> <span class="k">+</span> <span class="n">day</span> <span class="kr">with</span>
    <span class="k">|</span> <span class="n">ymd</span> <span class="kr">when</span> <span class="n">ymd</span> <span class="k">&gt;</span> <span class="mi">19890107</span> <span class="k">-&gt;</span> <span class="n">prt1</span> <span class="n">heisei</span>  <span class="k">(</span><span class="n">year</span> <span class="k">-</span> <span class="mi">1988</span><span class="k">)</span> <span class="k">(</span><span class="n">month</span> <span class="k">*</span> <span class="mi">2</span><span class="k">)</span> <span class="n">day</span>
    <span class="k">|</span> <span class="n">ymd</span> <span class="kr">when</span> <span class="n">ymd</span> <span class="k">&gt;</span> <span class="mi">19261225</span> <span class="k">-&gt;</span> <span class="n">prt1</span> <span class="n">showa</span>   <span class="k">(</span><span class="n">year</span> <span class="k">-</span> <span class="mi">1925</span><span class="k">)</span> <span class="k">(</span><span class="n">month</span> <span class="k">*</span> <span class="mi">2</span><span class="k">)</span> <span class="n">day</span>
    <span class="k">|</span> <span class="mi">19261225</span>                <span class="k">-&gt;</span> <span class="n">prt2</span> <span class="n">taishou</span> <span class="mi">15</span> <span class="n">showa</span> <span class="mi">1</span>    <span class="k">(</span><span class="n">month</span> <span class="k">*</span> <span class="mi">2</span><span class="k">)</span> <span class="n">day</span>
    <span class="k">|</span> <span class="n">ymd</span> <span class="kr">when</span> <span class="n">ymd</span> <span class="k">&gt;</span> <span class="mi">19120730</span> <span class="k">-&gt;</span> <span class="n">prt1</span> <span class="n">taishou</span> <span class="k">(</span><span class="n">year</span> <span class="k">-</span> <span class="mi">1911</span><span class="k">)</span> <span class="k">(</span><span class="n">month</span> <span class="k">*</span> <span class="mi">2</span><span class="k">)</span> <span class="n">day</span>
    <span class="k">|</span> <span class="mi">19120730</span>                <span class="k">-&gt;</span> <span class="n">prt2</span> <span class="n">meiji</span> <span class="mi">45</span> <span class="n">taishou</span> <span class="mi">1</span>    <span class="k">(</span><span class="n">month</span> <span class="k">*</span> <span class="mi">2</span><span class="k">)</span> <span class="n">day</span>
    <span class="k">|</span> <span class="n">ymd</span> <span class="kr">when</span> <span class="n">ymd</span> <span class="k">&gt;</span> <span class="mi">18730100</span> <span class="k">-&gt;</span> <span class="n">prt1</span> <span class="n">meiji</span>   <span class="k">(</span><span class="n">year</span> <span class="k">-</span> <span class="mi">1867</span><span class="k">)</span> <span class="k">(</span><span class="n">month</span> <span class="k">*</span> <span class="mi">2</span><span class="k">)</span> <span class="n">day</span>
    <span class="k">|</span> <span class="n">ymd</span> <span class="kr">when</span> <span class="n">ymd</span> <span class="k">&gt;</span> <span class="mi">18681022</span> <span class="k">-&gt;</span> <span class="n">taiin</span> <span class="n">year</span> <span class="n">month</span> <span class="n">day</span>
    <span class="k">|</span> <span class="k">_</span>                       <span class="k">-&gt;</span> <span class="n">errmsg</span>

<span class="kr">let</span> <span class="n">conv</span> <span class="n">s</span> <span class="k">=</span> <span class="kr">match</span> <span class="n">split</span> <span class="n">s</span> <span class="sc">&#39;/&#39;</span> <span class="kr">with</span>
    <span class="k">|</span> <span class="n">y</span> <span class="k">::</span> <span class="n">m</span> <span class="k">::</span> <span class="n">d</span> <span class="k">::</span> <span class="k">[]</span> <span class="k">-&gt;</span>
        <span class="kr">let</span> <span class="n">f</span> <span class="n">x</span> <span class="k">=</span> <span class="kr">try</span> <span class="n">int_of_string</span> <span class="n">x</span> <span class="kr">with</span> <span class="k">_</span> <span class="k">-&gt;</span> <span class="mi">0</span> <span class="kr">in</span>
        <span class="kr">let</span> <span class="n">year</span><span class="k">,</span> <span class="n">month</span><span class="k">,</span> <span class="n">day</span> <span class="k">=</span> <span class="n">f</span> <span class="n">y</span><span class="k">,</span> <span class="n">f</span> <span class="n">m</span><span class="k">,</span> <span class="n">f</span> <span class="n">d</span> <span class="kr">in</span>
