       <span class="o">[</span><span class="s1">&#39;昭和&#39;</span><span class="p">,</span> <span class="o">[</span><span class="s1">&#39;1926/12/25&#39;</span><span class="p">,</span> <span class="s1">&#39;1989/01/07&#39;</span><span class="o">]]</span><span class="p">,</span>
       <span class="o">[</span><span class="s1">&#39;平成&#39;</span><span class="p">,</span> <span class="o">[</span><span class="s1">&#39;1989/01/08&#39;</span><span class="p">,</span> <span class="s1">&#39;1970/01/01&#39;</span><span class="o">]]].</span>
      <span class="n">map</span> <span class="p">{</span><span class="o">|</span><span class="nb">name</span><span class="p">,</span> <span class="n">r</span><span class="o">|</span> <span class="o">[</span><span class="nb">name</span><span class="p">,</span> <span class="no">DateRange</span><span class="o">.</span><span class="n">new</span><span class="p">(</span><span class="o">*</span><span class="n">r</span><span class="p">)</span><span class="o">]</span> <span class="p">}</span>
