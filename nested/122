         <span class="p">(</span><span class="n">date</span> <span class="o">&gt;=</span> <span class="s1">&#39;1873/01/01&#39;</span><span class="p">)</span>
    <span class="k">return</span> <span class="kp">false</span>
  <span class="k">end</span>
  <span class="k">return</span> <span class="kp">true</span>
<span class="k">end</span>

<span class="k">def</span> <span class="nf">usage</span>
  <span class="nb">puts</span> <span class="s2">&quot;usage: </span><span class="si">#{</span><span class="no">File</span><span class="o">.</span><span class="n">basename</span><span class="p">(</span><span class="vg">$0</span><span class="p">)</span><span class="si">}</span><span class="s2"> &lt;date&gt;&quot;</span>
<span class="k">end</span>

<span class="k">class</span> <span class="nc">DateOutOfRange</span> <span class="o">&lt;</span> <span class="no">Exception</span><span class="p">;</span> <span class="k">end</span>

<span class="n">era</span> <span class="o">=</span> <span class="o">[[</span><span class="s1">&#39;明治&#39;</span><span class="p">,</span> <span class="o">[</span><span class="s1">&#39;1868/01/25&#39;</span><span class="p">,</span> <span class="s1">&#39;1912/07/30&#39;</span><span class="o">]]</span><span class="p">,</span>
