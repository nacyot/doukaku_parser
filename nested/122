  <span class="n">y</span><span class="p">,</span> <span class="n">m</span><span class="p">,</span> <span class="n">d</span> <span class="o">=</span> <span class="sr">%r!(\d+)[/-](\d+)[/-](\d+)!</span><span class="o">.</span><span class="n">match</span><span class="p">(</span><span class="n">date</span><span class="p">)</span><span class="o">.</span><span class="n">to_a</span><span class="o">[</span><span class="mi">1</span><span class="o">.</span><span class="n">.</span><span class="o">-</span><span class="mi">1</span><span class="o">].</span><span class="n">map</span> <span class="p">{</span><span class="o">|</span><span class="n">x</span><span class="o">|</span> <span class="n">x</span><span class="o">.</span><span class="n">to_i</span><span class="p">}</span>
  <span class="n">date</span> <span class="o">=</span> <span class="s2">&quot;%04d/%02d/%02d&quot;</span> <span class="o">%</span> <span class="o">[</span><span class="n">y</span><span class="p">,</span> <span class="n">m</span><span class="p">,</span> <span class="n">d</span><span class="o">]</span>
  <span class="k">raise</span> <span class="no">DateOutOfRange</span> <span class="k">unless</span> <span class="n">acceptable?</span><span class="p">(</span><span class="n">date</span><span class="p">,</span> <span class="n">y</span><span class="p">,</span> <span class="n">m</span><span class="p">,</span> <span class="n">d</span><span class="p">)</span>
  <span class="n">result</span> <span class="o">=</span> <span class="n">era</span><span class="o">.</span><span class="n">select</span> <span class="p">{</span><span class="o">|</span><span class="n">_</span><span class="p">,</span> <span class="n">r</span><span class="o">|</span> <span class="n">r</span> <span class="o">===</span> <span class="n">date</span><span class="p">}</span><span class="o">.</span>
               <span class="n">map</span> <span class="p">{</span><span class="o">|</span><span class="nb">name</span><span class="p">,</span> <span class="n">r</span><span class="o">|</span> <span class="o">[</span><span class="nb">name</span><span class="p">,</span> <span class="n">y</span> <span class="o">-</span> <span class="n">r</span><span class="o">.</span><span class="n">first</span><span class="o">.</span><span class="n">split</span><span class="p">(</span><span class="s1">&#39;/&#39;</span><span class="p">)</span><span class="o">[</span><span class="mi">0</span><span class="o">].</span><span class="n">to_i</span> <span class="o">+</span> <span class="mi">1</span><span class="o">]</span> <span class="p">}</span><span class="o">.</span>
               <span class="n">map</span> <span class="p">{</span><span class="o">|</span><span class="nb">name</span><span class="p">,</span> <span class="n">y</span><span class="o">|</span> <span class="s2">&quot;%s%d年%d月%d日&quot;</span> <span class="o">%</span> <span class="o">[</span><span class="nb">name</span><span class="p">,</span> <span class="n">y</span><span class="p">,</span> <span class="n">m</span><span class="p">,</span> <span class="n">d</span><span class="o">]</span> <span class="p">}</span>
  <span class="k">raise</span> <span class="no">DateOutOfRange</span> <span class="k">if</span> <span class="n">result</span><span class="o">.</span><span class="n">empty?</span>
  <span class="nb">puts</span> <span class="n">result</span><span class="o">.</span><span class="n">join</span><span class="p">(</span><span class="s1">&#39; &#39;</span><span class="p">)</span>
<span class="k">rescue</span> <span class="no">DateOutOfRange</span>
  <span class="nb">puts</span> <span class="s1">&#39;範囲外&#39;</span>
<span class="k">rescue</span>
  <span class="n">usage</span>
  <span class="nb">exit</span><span class="p">(</span><span class="mi">1</span><span class="p">)</span>
<span class="k">end</span>
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
	<a href="/web/20080418011411/http://ja.doukaku.org/comment/5261/addtag/" class="addtag" id="addtag_5261">タグ</a>:
	
