<span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">calcYear</span> <span class="nv">base</span> <span class="nv">d</span><span class="p">)</span> <span class="p">(</span><span class="nb">- </span><span class="p">(</span><span class="nf">getYear</span> <span class="nv">d</span><span class="p">)</span> <span class="p">(</span><span class="nb">- </span><span class="p">(</span><span class="nf">getYear</span> <span class="p">(</span><span class="nb">cadr </span><span class="nv">base</span><span class="p">))</span> <span class="mi">1</span><span class="p">)))</span>

<span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">okDate?</span> <span class="nv">date</span><span class="p">)</span>
 <span class="p">(</span><span class="k">define </span><span class="nv">y</span> <span class="p">(</span><span class="nf">getYear</span> <span class="nv">date</span><span class="p">))</span>
 <span class="p">(</span><span class="k">define </span><span class="nv">m</span> <span class="p">(</span><span class="nf">getMonth</span> <span class="nv">date</span><span class="p">))</span>
