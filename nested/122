  <span class="p">((</span><span class="nb">&lt; </span><span class="nv">m</span> <span class="mi">1</span><span class="p">)</span>  <span class="no">#f</span><span class="p">)</span>
  <span class="p">((</span><span class="nb">&lt; </span><span class="mi">12</span> <span class="nv">m</span><span class="p">)</span> <span class="no">#f</span><span class="p">)</span>
  <span class="p">((</span><span class="nb">&lt; </span><span class="nv">d</span> <span class="mi">1</span><span class="p">)</span>  <span class="no">#f</span><span class="p">)</span>
