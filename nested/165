    <span class="k">end</span>

    <span class="k">def</span> <span class="nf">frame_rate</span><span class="o">=</span><span class="p">(</span><span class="n">v</span><span class="p">)</span>
        <span class="vi">@frame_rate</span> <span class="o">=</span> <span class="n">v</span>
    <span class="k">end</span>

    <span class="k">def</span> <span class="nf">to_s</span>
        <span class="k">if</span> <span class="vi">@frame_rate</span> <span class="o">==</span> <span class="s2">&quot;ntsc&quot;</span> <span class="o">&amp;&amp;</span> <span class="vi">@m</span> <span class="o">!=</span> <span class="mi">10</span> <span class="o">&amp;&amp;</span> <span class="vi">@s</span> <span class="o">==</span> <span class="mi">0</span> <span class="o">&amp;&amp;</span> <span class="p">(</span><span class="vi">@frame</span> <span class="o">==</span> <span class="mi">0</span> <span class="o">||</span> <span class="vi">@frame</span> <span class="o">==</span> <span class="mi">1</span><span class="p">)</span>
            <span class="s2">&quot;%02d:%02d:%02d;%02d&quot;</span> <span class="o">%</span> <span class="o">[</span><span class="vi">@h</span><span class="p">,</span> <span class="vi">@m</span><span class="p">,</span> <span class="vi">@s</span><span class="p">,</span> <span class="vi">@frame</span><span class="o">]</span>
        <span class="k">else</span>
            <span class="s2">&quot;%02d:%02d:%02d:%02d&quot;</span> <span class="o">%</span> <span class="o">[</span><span class="vi">@h</span><span class="p">,</span> <span class="vi">@m</span><span class="p">,</span> <span class="vi">@s</span><span class="p">,</span> <span class="vi">@frame</span><span class="o">]</span>
        <span class="k">end</span>
    <span class="k">end</span>

<span class="k">end</span>

<span class="nb">p</span> <span class="no">TimeCode</span><span class="o">.</span><span class="n">new</span><span class="p">(</span> <span class="mi">0</span><span class="p">,</span>  <span class="mi">0</span><span class="p">,</span>  <span class="mi">0</span><span class="p">,</span>  <span class="mi">0</span><span class="p">,</span> <span class="s2">&quot;ntsc&quot;</span><span class="p">)</span><span class="o">.</span><span class="n">to_s</span> <span class="c1">#=&gt; &quot;00:00:00;00&quot;</span>
<span class="nb">p</span> <span class="no">TimeCode</span><span class="o">.</span><span class="n">new</span><span class="p">(</span> <span class="mi">0</span><span class="p">,</span>  <span class="mi">0</span><span class="p">,</span>  <span class="mi">0</span><span class="p">,</span>  <span class="mi">1</span><span class="p">,</span> <span class="s2">&quot;ntsc&quot;</span><span class="p">)</span><span class="o">.</span><span class="n">to_s</span> <span class="c1">#=&gt; &quot;00:00:00;01&quot;</span>
<span class="nb">p</span> <span class="no">TimeCode</span><span class="o">.</span><span class="n">new</span><span class="p">(</span> <span class="mi">0</span><span class="p">,</span>  <span class="mi">0</span><span class="p">,</span>  <span class="mi">0</span><span class="p">,</span>  <span class="mi">2</span><span class="p">,</span> <span class="s2">&quot;ntsc&quot;</span><span class="p">)</span><span class="o">.</span><span class="n">to_s</span> <span class="c1">#=&gt; &quot;00:00:00:02&quot;</span>
