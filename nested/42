<span class="k">private</span> <span class="k">import</span> <span class="n">std</span>.<span class="n">stdio</span>;

<span class="kt">void</span> <span class="n">main</span>() {
    <span class="k">static</span> <span class="k">if</span> (<span class="mi">0</span>) {
        <span class="n">these_lines</span>();
        <span class="n">are_not</span>();
        <span class="n">executed</span>();
        <span class="k">static</span> <span class="k">if</span> (<span class="mi">0</span>) {
            <span class="n">nesting</span>();
            <span class="k">is</span><span class="n">_allowed</span>();
        }
    }
    <span class="c">/* C-style</span>
<span class="c">       comment</span>
