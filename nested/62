18
19
20
21</pre></td><td class="code"><div class="highlight"><pre>Module Universe
     implicit none
     Private
         integer::left,right
     Public :: Mult
     contains
         Subroutine Mult
             do left = 1,9
                 do right = 1,9
                     Print &#39;(i1,&quot; * &quot;,i1,&quot; = &quot;,i2)&#39;,&amp;
                             left, right, left*right
                 end do
             end do
         End Subroutine
End Module

Program Main
     use Universe
     implicit none
     call Mult
End Program
</pre></div>
</td></tr></table>
