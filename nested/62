setlocal enabledelayedexpansion
for /l %%x in (1,1,9) do (
  for /l %%y in (1,1,9) do (
    set /a a=%%x*%%y
    set a= !a!
    echo %%x * %%y = !a:~-2!
  )
)
</pre></div>
