    <span class="n">a</span><span class="p">[</span><span class="mi">1</span><span class="p">]</span> <span class="o">=</span> <span class="mi">540876849L</span> <span class="o">+</span> <span class="n">i</span><span class="o">%</span><span class="mi">9</span><span class="p">;</span>
