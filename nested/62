  (cond ((and (= x 9) (&gt; y 9)) nil)
        ((&gt; y 9) (terpri) (kuku (1+ x) 1))
        (t (format t &quot;~D * ~D = ~2D~%&quot; x y (* x y))
           (kuku x (1+ y)))))
