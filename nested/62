15</pre></td><td class="code"><div class="highlight"><pre><span class="cp">&lt;?php</span>
<span class="k">function</span> <span class="nf">print_kuku</span><span class="p">(</span><span class="nv">$max</span><span class="p">)</span> <span class="p">{</span>
    <span class="nv">$len1</span> <span class="o">=</span> <span class="nf">strlen</span><span class="p">(</span><span class="nv">$max</span><span class="p">);</span>
    <span class="nv">$len2</span> <span class="o">=</span> <span class="nf">strlen</span><span class="p">(</span><span class="nf">pow</span><span class="p">(</span><span class="nv">$max</span><span class="p">,</span> <span class="m">2</span><span class="p">));</span>
    
    <span class="k">for</span> <span class="p">(</span><span class="nv">$i</span> <span class="o">=</span> <span class="m">1</span><span class="p">;</span> <span class="nv">$i</span> <span class="o">&lt;=</span> <span class="nv">$max</span><span class="p">;</span> <span class="nv">$i</span><span class="o">++</span><span class="p">)</span> <span class="p">{</span>
      <span class="k">for</span> <span class="p">(</span><span class="nv">$j</span> <span class="o">=</span> <span class="m">1</span><span class="p">;</span> <span class="nv">$j</span> <span class="o">&lt;=</span> <span class="nv">$max</span><span class="p">;</span> <span class="nv">$j</span><span class="o">++</span><span class="p">)</span> <span class="p">{</span>
        <span class="nf">printf</span><span class="p">(</span><span class="s2">&quot;%</span><span class="si">{</span><span class="nv">$len1</span><span class="si">}</span><span class="s2">s * %</span><span class="si">{</span><span class="nv">$len1</span><span class="si">}</span><span class="s2">s = %</span><span class="si">{</span><span class="nv">$len2</span><span class="si">}</span><span class="s2">s</span><span class="se">\n</span><span class="s2">&quot;</span><span class="p">,</span> <span class="nv">$i</span><span class="p">,</span> <span class="nv">$j</span><span class="p">,</span> <span class="nv">$i</span><span class="o">*</span><span class="nv">$j</span><span class="p">);</span>
      <span class="p">}</span>
    <span class="p">}</span>
<span class="p">}</span>

<span class="nx">print_kuku</span><span class="p">(</span><span class="m">9</span><span class="p">);</span> <span class="c">//日本</span>
<span class="nx">print_kuku</span><span class="p">(</span><span class="m">20</span><span class="p">);</span> <span class="c">//インド</span>
<span class="cp">?&gt;</span><span class="x"></span>
