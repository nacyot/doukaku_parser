    <span class="p">(</span><span class="nb">for-each </span><span class="nv">gl-vertex</span> <span class="p">(</span><span class="nb">list </span><span class="o">&#39;</span><span class="no">#f</span><span class="mi">32</span><span class="p">(</span> <span class="mf">0.0</span>  <span class="mf">0.45</span><span class="p">)</span> <span class="o">&#39;</span><span class="no">#f</span><span class="mi">32</span><span class="p">(</span> <span class="mf">0.0</span>  <span class="mf">-0.45</span><span class="p">)</span>
