      <table><tr><td class="linenos"><pre>1</pre></td><td class="code"><div class="highlight"><pre><span class="nv">main</span><span class="o">=</span><span class="nv">mapM_</span><span class="p">(</span><span class="o">\</span><span class="nv">n</span><span class="o">-&gt;</span><span class="nv">putStrLn</span><span class="o">.</span><span class="nv">take</span> <span class="nv">n</span><span class="o">$</span><span class="nv">repeat</span> <span class="sc">&#39;*&#39;</span><span class="p">)</span><span class="o">$</span><span class="p">[</span><span class="mi">1</span><span class="o">..</span><span class="mi">4</span><span class="p">]</span><span class="o">++</span><span class="nv">reverse</span><span class="p">[</span><span class="mi">1</span><span class="o">..</span><span class="mi">3</span><span class="p">]</span>
</pre></div>
</td></tr></table>
    </div>
	

  
