<span class="p">(</span><span class="nf">use</span> <span class="nv">gl</span><span class="o">.</span><span class="nv">glut</span><span class="p">)</span>

<span class="p">(</span><span class="k">define </span><span class="nv">*height*</span> <span class="no">#f</span><span class="p">)</span>
<span class="p">(</span><span class="k">define </span><span class="nv">*size*</span> <span class="no">#f</span><span class="p">)</span>
