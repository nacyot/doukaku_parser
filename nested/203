while(y-#h+1&lt;#h){
    #a=y-#h+1;
    if(#a&lt;0)#a=#a*-1;
    $p=&quot;&quot;;
    while(strlen($p)&lt;#h-#a)$p=$p+&quot;*&quot;;
    insert$p+&quot;\n&quot;;
}
