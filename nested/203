<span class="p">}</span>
<span class="c">/*</span>
<span class="c">pira(int n){int i=n*2,m=1;for(;i;m=--i&lt;n?i:n*2-i)for(;m+1;)putchar(m--?&#39;*&#39;:&#39;\n&#39;);}</span>
<span class="c">*/</span>
<span class="n">main</span><span class="p">(){</span>
    <span class="n">pira</span><span class="p">(</span><span class="mi">4</span><span class="p">);</span>
