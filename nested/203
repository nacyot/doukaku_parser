  <span class="nb">echo</span> -n \*
  <span class="o">((</span>++j&gt;m<span class="o">))</span>&amp;&amp;echo&amp;&amp;<span class="o">((</span><span class="nv">j</span><span class="o">=</span>0,i&lt;<span class="o">(</span><span class="nv">$1</span>+1<span class="o">)</span>*<span class="nv">$1</span>/2?m++:m--<span class="o">))</span>
