19</pre></td><td class="code"><div class="highlight"><pre><span class="o">#</span><span class="nv">!/usr/bin/gosh</span>

<span class="p">(</span><span class="nf">use</span> <span class="nv">gauche</span><span class="o">.</span><span class="nv">selector</span><span class="p">)</span>
<span class="p">(</span><span class="nf">use</span> <span class="nv">gauche</span><span class="o">.</span><span class="nv">process</span><span class="p">)</span>
<span class="p">(</span><span class="nf">use</span> <span class="nv">file</span><span class="o">.</span><span class="nv">util</span><span class="p">)</span>

<span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">main</span> <span class="nv">args</span><span class="p">)</span>
  <span class="p">(</span><span class="k">let </span><span class="p">((</span><span class="nf">ino</span> <span class="p">(</span><span class="nb">number-&gt;string </span><span class="p">(</span><span class="nf">file-ino</span> <span class="nv">*program-name*</span><span class="p">)))</span>
       <span class="p">(</span><span class="nf">selector</span> <span class="p">(</span><span class="nf">make</span> <span class="nv">&lt;selector&gt;</span><span class="p">)))</span>
    <span class="p">(</span><span class="nf">selector-add!</span> <span class="nv">selector</span> 
                   <span class="p">(</span><span class="nf">standard-input-port</span><span class="p">)</span>
                   <span class="p">(</span><span class="k">lambda </span><span class="p">(</span><span class="nf">port</span> <span class="nv">flag</span><span class="p">)</span>
                     <span class="p">(</span><span class="k">if </span><span class="p">(</span><span class="nb">not </span><span class="p">(</span><span class="nb">eof-object? </span><span class="p">(</span><span class="nf">read-line</span> <span class="nv">port</span><span class="p">)))</span>
                         <span class="p">(</span><span class="nf">let1</span> <span class="nv">p</span> <span class="p">(</span><span class="nf">run-process</span> <span class="p">(</span><span class="nb">list </span><span class="s">&quot;find&quot;</span> <span class="s">&quot;./&quot;</span> <span class="s">&quot;-inum&quot;</span> <span class="nv">ino</span><span class="p">)</span> <span class="nv">:output</span> <span class="nv">:pipe</span><span class="p">)</span>
                           <span class="p">(</span><span class="nb">display </span><span class="p">(</span><span class="nf">sys-basename</span> <span class="p">(</span><span class="nf">read-line</span> <span class="p">(</span><span class="nf">process-output</span> <span class="nv">p</span><span class="p">))))</span>
                           <span class="p">(</span><span class="nf">newline</span><span class="p">))</span>
                         <span class="p">(</span><span class="nf">exit</span><span class="p">)))</span>
                   <span class="o">&#39;</span><span class="p">(</span><span class="nv">r</span><span class="p">))</span>
    <span class="p">(</span><span class="k">do </span><span class="p">()</span> <span class="p">(</span><span class="no">#f</span><span class="p">)</span> <span class="p">(</span><span class="nf">selector-select</span> <span class="nv">selector</span><span class="p">))))</span>
