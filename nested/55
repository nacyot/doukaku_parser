               <span class="k">else</span> <span class="n">f</span><span class="p">(((</span><span class="n">x</span><span class="o">/</span><span class="n">y</span><span class="o">**</span><span class="mi">2</span><span class="p">)</span><span class="o">+</span><span class="p">(</span><span class="n">y</span><span class="o">*</span><span class="mi">2</span><span class="p">))</span> <span class="o">/</span> <span class="mf">3.0</span><span class="p">))(</span><span class="mf">1.0</span><span class="p">))</span>

