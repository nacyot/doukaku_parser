  <span class="n">y</span> <span class="o">=</span> <span class="mf">0.0</span> 
  <span class="n">y_next</span> <span class="o">=</span> <span class="n">x</span> 
  <span class="k">while</span> <span class="n">y</span> <span class="o">!=</span> <span class="n">y_next</span><span class="p">:</span>
