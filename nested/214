<span class="k">Imports</span> <span class="n">CSharp</span>
<span class="k">Class</span> <span class="nc">ResultTrue</span>
    <span class="k">Inherits</span> <span class="n">Exception</span>
<span class="k">End</span> <span class="k">Class</span>

<span class="nc">Class</span> <span class="n">ResultFalse</span>
    <span class="k">Inherits</span> <span class="n">Exception</span>
<span class="k">End</span> <span class="k">Class</span>

<span class="nc">Module</span> <span class="n">Vb</span>
    <span class="k">Sub</span> <span class="n">MaIn</span>
        <span class="k">Dim</span> <span class="n">s</span> <span class="o">=</span> <span class="n">Console</span><span class="p">.</span><span class="n">ReadLIne</span>
        <span class="k">Dim</span> <span class="n">n</span> <span class="o">=</span> <span class="mi">0</span>

        <span class="k">For</span> <span class="k">Each</span> <span class="n">c</span> <span class="ow">In</span> <span class="n">s</span>
            <span class="k">If</span> <span class="n">c</span> <span class="o">=</span> <span class="n">Paren</span><span class="p">.</span><span class="n">Open</span> <span class="k">Then</span>
                <span class="n">n</span> <span class="o">=</span> <span class="n">n</span> <span class="o">+</span> <span class="mi">1</span>
            <span class="k">Else</span> <span class="k">If</span> <span class="n">c</span> <span class="o">=</span> <span class="n">Paren</span><span class="p">.</span><span class="n">Close</span> <span class="k">Then</span>
                <span class="n">n</span> <span class="o">=</span> <span class="n">n</span> <span class="o">-</span> <span class="mi">1</span>
            <span class="k">End</span> <span class="k">If</span>

            <span class="k">If</span> <span class="n">n</span> <span class="o">&lt;</span> <span class="mi">0</span> <span class="k">Then</span>
                <span class="k">Throw</span> <span class="k">New</span> <span class="n">ResultFalse</span>
            <span class="k">End</span> <span class="k">If</span>
        <span class="k">Next</span> <span class="n">c</span>

        <span class="k">If</span> <span class="n">n</span> <span class="o">&lt;&gt;</span> <span class="mi">0</span> <span class="k">Then</span>
            <span class="k">Throw</span> <span class="k">New</span> <span class="n">ResultFalse</span>
        <span class="k">Else</span>
            <span class="k">Throw</span> <span class="k">New</span> <span class="n">ResultTrue</span>
        <span class="k">End</span> <span class="k">If</span>
    <span class="k">End</span> <span class="k">Sub</span>
<span class="k">End</span> <span class="k">Module</span>

<span class="o">====</span> <span class="n">CSharp</span><span class="p">.</span><span class="n">cs</span> <span class="o">====</span>
<span class="k">namespace</span> <span class="n">CSharp</span> <span class="p">{</span>
    <span class="k">public</span> <span class="k">class</span> <span class="nc">Paren</span> <span class="p">{</span>
        <span class="k">public</span> <span class="k">static</span> <span class="kt">char</span> <span class="n">Open</span> <span class="o">=</span> <span class="c">&#39;\x28&#39;;</span>
        <span class="k">public</span> <span class="k">static</span> <span class="kt">char</span> <span class="n">Close</span> <span class="o">=</span> <span class="c">&#39;\x29&#39;;</span>
    <span class="p">}</span>
<span class="p">}</span>

