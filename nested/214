  &lt;loop with match? = 0; 
        with lp = &lt;code-char 40&gt;; 
        with rp = &lt;code-char 41&gt;;

        for c = &lt;read-char in nil nil&gt; while c
        if &lt;minusp match?&gt;
          return nil;
        else if &lt;char= lp c&gt;
                do &lt;incf match?&gt;;
             else if &lt;char= rp c&gt;
                    do &lt;decf match?&gt;;
                  end;
             end;
        end;
        finally &lt;return &lt;zerop match?&gt;&gt;&gt;&gt;
