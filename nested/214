38</pre></td><td class="code"><div class="highlight"><pre>{pkd,[[],[],[]]} :- !.
{pkd,[[A|R1],R,Chars]} :-
    {char_code,[A,40]},
    {pekd,[R1,R2,Chars1]}
    {pkd,[R2,R,Chars2]},
    {append,[Chars1,Chars2,Chars]},!.
{pkd,[[A|R1],R,[A|R2]]} :-
    {pkd,[R1,R,R2]}.

{pekd,[[A|R],R,[]]} :-
    {char_code,[A,41]},!.
{pekd,[[A|R1],R,Chars]} :-
    {char_code,[A,40]},
    {pkd,[[A|R1],R2,Chars1]},
    {pekd,[R2,R,Chars2]},
    {append,[Chars1,Chars2,Chars]},!.
{pekd,[[A|R1],R,[A|R2]} :-
    {pekd,[R1,R,R2]}.

{readfile,[File,Chars]} :-
    {see,[File]},
    {get_char,[Char]},
    {readfile_2,[Char,Chars]},
    {seen,[]}.

{readfile_2,[end_of_file,[]]} :- !.
{readfile_2,[A,[A|R]} :-
    {get_char,[B]}
    {readfile_2,[B,R]}.

{odai,[File,Atom]} :-
    {readfile,[File,Chars]},
    {pkd,[Chars,_,Chars_2]},
    {atom_codes,[Atom,Chars_2]}.

{Functor,Args} :- Q =.. [Functor|Args],Q.

:- {odai,[&#39;foo.txt&#39;,Atom]}.
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
	<a href="/web/20081226035308/http://ja.doukaku.org/comment/8112/addtag/" class="addtag" id="addtag_8112">タグ</a>:
	
