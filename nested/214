<span class="k">if</span> <span class="o">[</span> <span class="s">`echo &quot;$3&quot; | tr -d &quot;[^$1]&quot; | wc -c`</span> \
   <span class="o">=</span> <span class="s">`echo &quot;$3&quot; | tr -d &quot;[^$2]&quot; | wc -c`</span> <span class="o">]</span>; <span class="k">then</span>
<span class="k">    </span><span class="nb">echo </span>True
