    <div class="compact">
      <table><tr><td class="linenos"><pre>1
2
3
4
5
6
7
8</pre></td><td class="code"><div class="highlight"><pre>boolean aho
1.upto(30) { 
    if (it%3 == 0) {aho=true}
    it.toString().toList().each {if (it==&quot;3&quot;){aho=true} }
    print it
    if (aho) {println &quot; ーっ&quot;} else {println &quot;&quot;}
    aho=false
}
</pre></div>
</td></tr></table>
