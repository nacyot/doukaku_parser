  var s = n.toString()
  if(n % 3 == 0 || s.contains(&#39;3&#39;)) { s += &quot; Aho&quot; }
  if(n % 5 == 0) { s += &quot; Doko&quot; }
  if(n % 8 == 0) { s += &quot; Naru&quot; }
  s
}) reduceLeft ((s,e) =&gt; { s + &quot;,&quot; + e }) )
