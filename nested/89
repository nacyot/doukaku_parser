55</pre></td><td class="code"><div class="highlight"><pre><span class="nb">require</span> <span class="s1">&#39;pp&#39;</span>

<span class="k">class</span> <span class="nc">NumericArray</span> <span class="o">&lt;</span> <span class="nb">Array</span>
  <span class="k">def</span> <span class="nf">initialize</span><span class="p">(</span><span class="n">maxvalue</span><span class="p">,</span> <span class="n">level</span><span class="p">)</span>
    <span class="nb">self</span><span class="o">[</span><span class="mi">0</span><span class="p">,</span> <span class="mi">0</span><span class="o">]</span> <span class="o">=</span> <span class="o">[</span><span class="mi">0</span><span class="o">]</span> <span class="o">*</span> <span class="n">level</span>
    <span class="vi">@maxvalue</span> <span class="o">=</span> <span class="n">maxvalue</span>
  <span class="k">end</span>

  <span class="k">def</span> <span class="nf">succ</span>
    <span class="nb">self</span><span class="o">.</span><span class="n">dup</span><span class="o">.</span><span class="n">succ!</span>
  <span class="k">end</span>

  <span class="k">def</span> <span class="nf">succ!</span>
    <span class="n">result</span> <span class="o">=</span> <span class="o">[]</span>
    <span class="n">carry</span> <span class="o">=</span> <span class="n">reverse</span><span class="o">.</span><span class="n">inject</span><span class="p">(</span><span class="mi">1</span><span class="p">)</span> <span class="k">do</span> <span class="o">|</span><span class="n">carry</span><span class="p">,</span> <span class="n">cur</span><span class="o">|</span>
      <span class="k">if</span> <span class="n">cur</span> <span class="o">+</span> <span class="n">carry</span> <span class="o">&lt;</span> <span class="vi">@maxvalue</span> <span class="k">then</span>
        <span class="n">result</span> <span class="o">&lt;&lt;</span> <span class="n">cur</span> <span class="o">+</span> <span class="n">carry</span>
        <span class="n">carry</span> <span class="o">=</span> <span class="mi">0</span>
      <span class="k">else</span>
        <span class="n">result</span> <span class="o">&lt;&lt;</span> <span class="mi">0</span>
        <span class="n">carry</span> <span class="o">=</span> <span class="mi">1</span>
      <span class="k">end</span>
    <span class="k">end</span>
    <span class="k">return</span> <span class="kp">nil</span> <span class="k">if</span> <span class="n">carry</span> <span class="o">==</span> <span class="mi">1</span>
    <span class="n">replace</span><span class="p">(</span><span class="n">result</span><span class="o">.</span><span class="n">reverse</span><span class="p">)</span>
  <span class="k">end</span>

  <span class="k">def</span> <span class="nf">sum</span>
    <span class="n">inject</span><span class="p">(</span><span class="mi">0</span><span class="p">)</span> <span class="p">{</span><span class="o">|</span><span class="n">sum</span><span class="p">,</span> <span class="n">i</span><span class="o">|</span> <span class="n">i</span> <span class="o">+</span> <span class="n">sum</span><span class="p">}</span>
  <span class="k">end</span>
<span class="k">end</span>


<span class="k">begin</span>
  <span class="k">raise</span> <span class="s1">&#39;2 arguments required.&#39;</span> <span class="k">if</span> <span class="no">ARGV</span><span class="o">.</span><span class="n">size</span> <span class="o">!=</span> <span class="mi">2</span>
  <span class="n">sum</span><span class="p">,</span> <span class="n">maxlevel</span> <span class="o">=</span> <span class="no">ARGV</span><span class="o">.</span><span class="n">map</span> <span class="p">{</span><span class="o">|</span><span class="n">n</span><span class="o">|</span> <span class="n">n</span><span class="o">.</span><span class="n">to_i</span> <span class="p">}</span>

  <span class="k">if</span> <span class="n">sum</span> <span class="o">&lt;=</span> <span class="mi">0</span> <span class="ow">or</span> <span class="n">maxlevel</span> <span class="o">&lt;=</span> <span class="mi">0</span> <span class="k">then</span>
    <span class="k">raise</span> <span class="s1">&#39;Arguments out of range.&#39;</span>
  <span class="k">end</span>
<span class="k">rescue</span>
  <span class="nb">puts</span> <span class="o">&lt;&lt;-</span><span class="no">_EOD_</span>
<span class="sh">#{File.basename($0)}: ERROR: #{$!}</span>
<span class="sh">usage: #{File.basename($0)} n m</span>
<span class="no">  _EOD_</span>
  <span class="nb">exit</span><span class="p">(</span><span class="mi">1</span><span class="p">)</span>
<span class="k">end</span>

<span class="n">xs</span> <span class="o">=</span> <span class="no">NumericArray</span><span class="o">.</span><span class="n">new</span><span class="p">(</span><span class="n">sum</span> <span class="o">+</span> <span class="mi">1</span><span class="p">,</span> <span class="n">maxlevel</span><span class="p">)</span>
<span class="n">result</span> <span class="o">=</span> <span class="o">[]</span>
<span class="k">begin</span>
  <span class="n">result</span> <span class="o">&lt;&lt;</span> <span class="n">xs</span><span class="o">.</span><span class="n">dup</span>
<span class="k">end</span> <span class="k">while</span> <span class="n">xs</span><span class="o">.</span><span class="n">succ!</span><span class="o">.</span><span class="n">nil?</span> <span class="o">==</span> <span class="kp">false</span>

<span class="n">pp</span> <span class="n">result</span><span class="o">.</span><span class="n">select</span> <span class="p">{</span><span class="o">|</span><span class="n">list</span><span class="o">|</span> <span class="n">list</span><span class="o">.</span><span class="n">sum</span> <span class="o">==</span> <span class="n">sum</span><span class="p">}</span><span class="o">.</span><span class="n">sort</span><span class="o">.</span><span class="n">reverse</span>
</pre></div>
</td></tr></table>
