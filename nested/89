<span class="nv">partitionNum</span> <span class="nv">n</span> <span class="nv">m</span> <span class="o">=</span> <span class="p">[(</span><span class="nv">n</span><span class="o">-</span><span class="nv">x</span><span class="p">)</span><span class="o">:</span><span class="nv">xs</span> <span class="o">|</span> <span class="nv">x</span> <span class="o">&lt;-</span> <span class="p">[</span><span class="mi">0</span><span class="o">..</span><span class="nv">n</span><span class="p">],</span> <span class="nv">xs</span> <span class="o">&lt;-</span> <span class="nv">partitionNum</span> <span class="nv">x</span> <span class="p">(</span><span class="nv">m</span><span class="o">-</span><span class="mi">1</span><span class="p">)]</span>
