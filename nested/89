<span class="gd">-            for (int i = 0; i &lt;= n/2; i++) {</span>
<span class="gi">+            for (int i = 0; i &lt;= n; i++) {</span>
                 answer.set.add(new int[]{i, n-i});
<span class="gu">@@ -27,3 +27,3 @@</span>
         } else {
<span class="gd">-            for (int i = 0; i &lt;= n/m; i++) {</span>
<span class="gi">+            for (int i = 0; i &lt;= n; i++) {</span>
                 answer.set.addAll(execute(n-i, m-1).marge(key, i).set);
