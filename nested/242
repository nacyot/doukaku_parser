Dim sh
Set sh = WScript.CreateObject(&quot;WScript.Shell&quot;)
WScript.Echo &quot;メモ帳を起動（待たない）&quot;
sh.Run &quot;notepad.exe&quot;, 1, False
WScript.Echo &quot;メモ帳を起動（待つ）&quot;
sh.Run &quot;notepad.exe&quot;, 1, True
</pre></div>
