10</pre></td><td class="code"><div class="highlight"><pre><span class="k">public</span> <span class="k">class</span> <span class="nc">Sample242</span> <span class="o">{</span>
    <span class="k">public</span> <span class="k">static</span> <span class="kt">int</span> <span class="nf">execute</span><span class="o">(</span><span class="kt">boolean</span> <span class="n">isWait</span><span class="o">,</span> <span class="n">String</span><span class="o">...</span> <span class="n">command</span><span class="o">)</span> <span class="k">throws</span> <span class="n">Exception</span> <span class="o">{</span>
        <span class="n">Process</span> <span class="n">process</span> <span class="o">=</span> <span class="k">new</span> <span class="n">ProcessBuilder</span><span class="o">(</span><span class="n">command</span><span class="o">).</span><span class="na">start</span><span class="o">();</span>
        <span class="k">return</span> <span class="n">isWait</span><span class="o">?</span> <span class="n">process</span><span class="o">.</span><span class="na">waitFor</span><span class="o">():</span> <span class="mi">0</span><span class="o">;</span>
    <span class="o">}</span>
    <span class="k">public</span> <span class="k">static</span> <span class="kt">void</span> <span class="nf">main</span><span class="o">(</span><span class="n">String</span><span class="o">[]</span> <span class="n">args</span><span class="o">)</span> <span class="k">throws</span> <span class="n">Exception</span> <span class="o">{</span>
        <span class="n">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="n">execute</span><span class="o">(</span><span class="kc">false</span><span class="o">,</span> <span class="s">&quot;notepad&quot;</span><span class="o">,</span> <span class="s">&quot;src/Sample242.java&quot;</span><span class="o">));</span>
