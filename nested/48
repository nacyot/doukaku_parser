	<a href="/web/20100327185244/http://ja.doukaku.org/comment/2334/">#2334</a>(<script language="JavaScript">print_time(new Date("2007/08/19 23:51 GMT"));</script><noscript>2007/08/19 23:51 GMT</noscript>)
	
	
	
