<span class="k">from</span> <span class="nn">numpy</span> <span class="k">import</span> <span class="n">array</span>
<span class="k">from</span> <span class="nn">numpy.linalg</span> <span class="k">import</span> <span class="n">eig</span>

<span class="p">(</span><span class="n">v</span><span class="p">,</span> <span class="n">d</span><span class="p">)</span> <span class="o">=</span> <span class="n">eig</span><span class="p">(</span><span class="n">array</span><span class="p">(</span><span class="n">m</span><span class="p">))</span>

