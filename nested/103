<span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">first-state</span> <span class="nv">line</span><span class="p">)</span>
  <span class="p">(</span><span class="nb">map </span><span class="nv">string-&gt;symbol</span> <span class="p">(</span><span class="nf">string-split</span> <span class="nv">line</span> <span class="sc">#\space</span><span class="p">)))</span>

<span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">update-state</span> <span class="nv">line</span> <span class="nv">prev-state</span><span class="p">)</span>
  <span class="p">(</span><span class="nf">let1</span> <span class="nv">amida</span> <span class="p">(</span><span class="nb">cdr </span><span class="p">(</span><span class="nf">string-split</span> <span class="nv">line</span> <span class="o">#</span><span class="err">\|</span><span class="p">))</span>
    <span class="p">(</span><span class="nf">fold-right</span> <span class="p">(</span><span class="k">lambda </span><span class="p">(</span><span class="nf">s</span> <span class="nv">a</span> <span class="nv">k</span><span class="p">)</span>
                  <span class="p">(</span><span class="k">if </span><span class="p">(</span><span class="nb">string=? </span><span class="nv">a</span> <span class="s">&quot;-&quot;</span><span class="p">)</span>
                    <span class="p">(</span><span class="nf">cons*</span> <span class="p">(</span><span class="nb">car </span><span class="nv">k</span><span class="p">)</span> <span class="nv">s</span> <span class="p">(</span><span class="nb">cdr </span><span class="nv">k</span><span class="p">))</span>
                    <span class="p">(</span><span class="nb">cons </span><span class="nv">s</span> <span class="nv">k</span><span class="p">)))</span>
                <span class="o">&#39;</span><span class="p">()</span> <span class="nv">prev-state</span> <span class="nv">amida</span><span class="p">)))</span>

<span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">solve</span> <span class="nv">amida</span><span class="p">)</span>
  <span class="p">(</span><span class="nf">call-with-input-string</span> <span class="nv">amida</span>
    <span class="p">(</span><span class="k">lambda </span><span class="p">(</span><span class="nf">in</span><span class="p">)</span>
      <span class="p">(</span><span class="nf">port-fold</span>
       <span class="p">(</span><span class="nf">cut</span> <span class="nv">update-state</span> <span class="nv">&lt;&gt;</span> <span class="nv">&lt;&gt;</span><span class="p">)</span>
       <span class="p">(</span><span class="nf">first-state</span> <span class="p">(</span><span class="nf">read-line</span> <span class="nv">in</span><span class="p">))</span>
       <span class="p">(</span><span class="nf">cut</span> <span class="nv">read-line</span> <span class="nv">in</span><span class="p">)))))</span>

<span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">print-result</span> <span class="nv">amida</span><span class="p">)</span>
  <span class="p">(</span><span class="nb">display </span><span class="nv">amida</span><span class="p">)</span>
  <span class="p">(</span><span class="nf">print</span> <span class="p">(</span><span class="nf">string-join</span> <span class="p">(</span><span class="nb">map </span><span class="nv">symbol-&gt;string</span> <span class="p">(</span><span class="nf">solve</span> <span class="nv">amida</span><span class="p">))</span> <span class="s">&quot; &quot;</span><span class="p">)))</span>
</pre></div>
</td></tr></table>
    </div>
	

  
