26
27</pre></td><td class="code"><div class="highlight"><pre>val s = [
&quot;A B C D E&quot;,
&quot;| | |-| |&quot;,
&quot;|-| | |-|&quot;,
&quot;| |-| |-|&quot;,
&quot;|-| |-| |&quot;,
&quot;|-| | | |&quot;
]

fun amida [] = []
  | amida (a as (x::xs)) =
  let
    fun loop [] y = y
      | loop (s::ss) y =
      let
        fun f ((i, _), v) =
          substring (v, 0, i - 1) ^
          (implode o rev o explode o substring) (v, i - 1, 3) ^
          String.extract (v, i + 2, NONE)
      in
        loop ss (foldl f y (global_find &quot;-&quot; s))
      end
  in
    a @ [loop xs x]
  end

val _ = app println (amida s)
</pre></div>
</td></tr></table>
    </div>
	

  
  <div align = "right" class="banner">
	
