23
24</pre></td><td class="code"><div class="highlight"><pre><span class="k">import</span> <span class="n">std</span>.<span class="n">stdio</span>;
<span class="k">import</span> <span class="n">std</span>.<span class="n">string</span>;

<span class="kt">void</span> <span class="n">main</span>(){
    <span class="n">string</span>[] <span class="n">amida</span> = [<span class="s">&quot;A B C D E&quot;</span>,
                      <span class="s">&quot;| | |-| |&quot;</span>,
                      <span class="s">&quot;|-| | |-|&quot;</span>,
                      <span class="s">&quot;| |-| |-|&quot;</span>,
                      <span class="s">&quot;|-| |-| |&quot;</span>,
                      <span class="s">&quot;|-| | | |&quot;</span>];

    <span class="k">auto</span> <span class="n">pos</span> = <span class="k">cast</span>(<span class="kt">char</span>[])<span class="n">amida</span>[<span class="mi">0</span>].<span class="n">dup</span>;
    <span class="k">foreach</span>(<span class="n">s</span>; <span class="n">amida</span>[<span class="mf">1.</span>.$]){
        <span class="k">foreach</span>(<span class="n">i</span>, <span class="n">c</span>; <span class="n">s</span>[<span class="mf">0.</span>.($ - <span class="mi">1</span>)]){
            <span class="k">if</span>(<span class="n">c</span> == <span class="sc">&#39;-&#39;</span>){
                <span class="k">auto</span> <span class="n">tmp</span> = <span class="n">pos</span>[<span class="n">i</span> - <span class="mi">1</span>];
                <span class="n">pos</span>[<span class="n">i</span> - <span class="mi">1</span>] = <span class="n">pos</span>[<span class="n">i</span> + <span class="mi">1</span>];
                <span class="n">pos</span>[<span class="n">i</span> + <span class="mi">1</span>] = <span class="n">tmp</span>;
            }
        }
    }
    <span class="n">amida</span> ~= <span class="k">cast</span>(<span class="n">string</span>)<span class="n">pos</span>;
    <span class="n">writefln</span>(<span class="n">amida</span>.<span class="n">join</span>(<span class="s">&quot;\n&quot;</span>));
}
</pre></div>
</td></tr></table>
    </div>
	

  
