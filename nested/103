    <span class="kr">let</span> <span class="nv">z</span><span class="o">:</span><span class="nv">zs</span> <span class="o">=</span> <span class="nv">repl</span> <span class="nv">xs</span> <span class="nv">ys</span> <span class="kr">in</span>
    <span class="kr">case</span> <span class="nv">y</span> <span class="kr">of</span>
      <span class="sc">&#39;-&#39;</span> <span class="o">-&gt;</span> <span class="nv">z</span><span class="o">:</span><span class="sc">&#39; &#39;</span><span class="o">:</span><span class="nv">x</span><span class="o">:</span><span class="nv">zs</span>
      <span class="kr">_</span> <span class="o">-&gt;</span> <span class="nv">x</span><span class="o">:</span><span class="sc">&#39; &#39;</span><span class="o">:</span><span class="nv">z</span><span class="o">:</span><span class="nv">zs</span>
<span class="nv">repl</span> <span class="nv">xs</span> <span class="kr">_</span> <span class="o">=</span> <span class="nv">xs</span>

<span class="nv">main</span> <span class="o">=</span> <span class="nv">putStrLn</span> <span class="o">$</span> <span class="p">(</span><span class="nv">unlines</span> <span class="nv">s</span><span class="p">)</span> <span class="o">++</span> <span class="nv">foldl1</span> <span class="nv">repl</span> <span class="nv">s</span>
</pre></div>
</td></tr></table>
    </div>
	

  
