    <span class="k">public</span> <span class="k">static</span> <span class="n">List</span><span class="o">&lt;</span><span class="n">String</span><span class="o">&gt;</span> <span class="n">solveAmida</span><span class="o">(</span><span class="n">String</span><span class="o">[]</span> <span class="n">input</span><span class="o">)</span> <span class="o">{</span>
