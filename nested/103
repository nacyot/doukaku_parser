  <span class="p">(</span><span class="nf">string-append</span>
   <span class="s">&quot;A B C D E\n&quot;</span>
   <span class="s">&quot;| | |-| |\n&quot;</span>
   <span class="s">&quot;|-| | |-|\n&quot;</span>
