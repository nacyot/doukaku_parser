    <div class="compact">
      <table><tr><td class="linenos"><pre>1
2
3</pre></td><td class="code"><div class="highlight"><pre><span class="nv">$_</span><span class="o">=&lt;&gt;</span><span class="p">;</span><span class="k">print</span><span class="p">;</span><span class="k">while</span><span class="p">(</span><span class="nv">$b</span><span class="o">=</span><span class="nv">$a</span><span class="o">=&lt;&gt;</span><span class="p">){</span><span class="k">print</span><span class="nv">$a</span><span class="p">;</span><span class="nb">chomp</span><span class="nv">$a</span><span class="p">;</span><span class="nv">$i</span><span class="o">=</span><span class="mi">1</span><span class="p">;</span><span class="nv">$a</span><span class="o">=~</span><span class="sr">s/(-|\|)/$1eq&#39;-&#39;?&#39; &#39;:&#39;(.)&#39;</span>
<span class="sr">/eg</span><span class="p">;</span><span class="nv">$b</span><span class="o">=~</span><span class="sr">s/(\|-\||\|)/$1eq&#39;|&#39;?&#39;$&#39;.$i++:&#39;$&#39;.++$i.&#39; $&#39;.($i++ -1)/eg</span><span class="p">;</span><span class="nb">eval</span><span class="s">&quot;s/$a/$b/&quot;</span>
<span class="p">}</span><span class="k">print</span>
</pre></div>
</td></tr></table>
