     <span class="s">&quot;| | |-| |&quot;</span><span class="p">,</span>
     <span class="s">&quot;|-| | |-|&quot;</span><span class="p">,</span>
     <span class="s">&quot;| |-| |-|&quot;</span><span class="p">,</span>
