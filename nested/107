<span class="k">using</span> <span class="nn">System.IO</span><span class="p">;</span>
<span class="k">using</span> <span class="nn">System.Text</span><span class="p">;</span>

<span class="k">class</span> <span class="nc">Program</span> <span class="k">{</span>
    <span class="k">static</span> <span class="kt">byte</span><span class="p">[]</span> <span class="n">SjisToEuc</span><span class="p">(</span><span class="kt">byte</span><span class="p">[]</span> <span class="n">bytes</span><span class="p">)</span> <span class="k">{</span>
        <span class="k">return</span> <span class="n">Encoding</span><span class="p">.</span><span class="n">Convert</span><span class="p">(</span><span class="n">Encoding</span><span class="p">.</span><span class="n">GetEncoding</span><span class="p">(</span><span class="s">&quot;shift-jis&quot;</span><span class="p">),</span> <span class="n">Encoding</span><span class="p">.</span><span class="n">GetEncoding</span><span class="p">(</span><span class="s">&quot;euc-jp&quot;</span><span class="p">),</span> <span class="n">bytes</span><span class="p">);</span>
