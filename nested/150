  <span class="k">my</span> <span class="nv">$G</span> <span class="o">=</span> <span class="nb">shift</span><span class="p">;</span> 
  <span class="k">return</span> <span class="nv">$G</span><span class="o">-&gt;</span><span class="p">(</span> <span class="k">sub </span><span class="p">{</span> <span class="k">my</span> <span class="nv">$x</span> <span class="o">=</span> <span class="nb">shift</span><span class="p">;</span> <span class="k">return</span> <span class="n">fix</span><span class="p">(</span><span class="nv">$G</span><span class="p">)</span><span class="o">-&gt;</span><span class="p">(</span><span class="nv">$x</span><span class="p">);}</span> <span class="p">);</span> 
