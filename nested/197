14
15</pre></td><td class="code"><div class="highlight"><pre><span class="k">private</span> <span class="k">import</span> <span class="n">std</span>.<span class="n">stdio</span>, <span class="n">std</span>.<span class="n">date</span>;

<span class="k">enum</span> <span class="n">Friday</span> = <span class="mi">5</span>;

<span class="kt">void</span> <span class="n">main</span>(){
    <span class="k">auto</span> <span class="n">count</span> = <span class="mi">0</span>;
    <span class="k">foreach</span>(<span class="n">day</span>; <span class="n">Day</span>(<span class="n">getUTCtime</span>) .. <span class="n">MakeDay</span>(<span class="mi">2014</span>, <span class="mi">1</span>, <span class="mi">1</span>)) {
        <span class="k">auto</span> <span class="n">date</span> = <span class="n">MakeDate</span>(<span class="n">day</span>, <span class="mi">0</span>);
        <span class="k">if</span>(<span class="n">WeekDay</span>(<span class="n">date</span>) == <span class="n">Friday</span> &amp;&amp; <span class="n">DateFromTime</span>(<span class="n">date</span>) == <span class="mi">13</span>) {
            <span class="n">writefln</span>(<span class="s">&quot;%04d-%02d-%02d&quot;</span>, <span class="n">YearFromTime</span>(<span class="n">date</span>), <span class="n">MonthFromTime</span>(<span class="n">date</span>) + <span class="mi">1</span>, <span class="n">DateFromTime</span>(<span class="n">date</span>));
            ++<span class="n">count</span>;
        }
    }
    <span class="n">writeln</span>(<span class="n">count</span>);
}
</pre></div>
</td></tr></table>
