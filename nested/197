11</pre></td><td class="code"><div class="highlight"><pre><span class="nb">eval</span> <span class="s">`date &#39;+y=%Y;m=$((%-m+(%e&gt;13)))&#39;`</span>
<span class="k">for </span>i in <span class="s">`seq $y 2013`</span>;do
<span class="k">for </span>j in <span class="s">`seq $m 12`</span>;do
<span class="k">if</span> <span class="o">[</span> <span class="s">`cal $j $i|awk &#39;$6==13&amp;&amp;$_=$6&#39;`</span> <span class="o">]</span>;then
<span class="nv">c</span><span class="o">=</span><span class="k">$(</span><span class="o">(</span>c+1<span class="k">)</span><span class="o">)</span>
<span class="nb">printf</span> <span class="nv">$i</span>-%02d-13\\n <span class="nv">$j</span>
<span class="k">fi</span>
<span class="k">done</span>
<span class="nv">m</span><span class="o">=</span>1
<span class="k">done</span>
<span class="nb">echo</span> <span class="nv">$c</span>
</pre></div>
