  <span class="n">d</span> <span class="o">=</span> <span class="n">start</span>
  <span class="k">while</span> <span class="n">d</span><span class="o">.</span><span class="n">toordinal</span><span class="p">()</span> <span class="o">&lt;</span> <span class="n">end</span><span class="o">.</span><span class="n">toordinal</span><span class="p">():</span>
    <span class="n">d</span> <span class="o">=</span> <span class="n">next_friday</span><span class="p">(</span><span class="n">d</span><span class="p">)</span>
    <span class="k">if</span> <span class="ow">not</span> <span class="n">d</span><span class="p">:</span>
      <span class="k">break</span>
    <span class="k">yield</span> <span class="n">d</span>

<span class="o">----</span> <span class="n">after</span>
  <span class="n">d</span> <span class="o">=</span> <span class="n">next_friday</span><span class="p">(</span><span class="n">start</span><span class="p">)</span>
  <span class="k">while</span> <span class="n">d</span><span class="o">.</span><span class="n">toordinal</span><span class="p">()</span> <span class="o">&lt;</span> <span class="n">end</span><span class="o">.</span><span class="n">toordinal</span><span class="p">():</span>
    <span class="k">yield</span> <span class="n">d</span>
    <span class="n">d</span> <span class="o">=</span> <span class="n">next_friday</span><span class="p">(</span><span class="n">d</span><span class="p">)</span>
</pre></div>
