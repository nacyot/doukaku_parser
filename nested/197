  <span class="kr">where</span> <span class="nv">thirteen</span> <span class="o">=</span> <span class="s">&quot;31&quot;</span> <span class="o">==</span> <span class="p">(</span><span class="nv">take</span> <span class="mi">2</span> <span class="o">$</span> <span class="nv">reverse</span> <span class="o">$</span> <span class="nv">show</span> <span class="nv">day</span><span class="p">)</span>
