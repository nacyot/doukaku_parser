16</pre></td><td class="code"><div class="highlight"><pre><span class="c1">#!/usr/bin/env ruby</span>
<span class="nb">require</span> <span class="s1">&#39;date&#39;</span>

<span class="n">from</span> <span class="o">=</span> <span class="no">DateTime</span><span class="o">.</span><span class="n">now</span>
<span class="n">to</span> <span class="o">=</span> <span class="no">DateTime</span><span class="o">.</span><span class="n">parse</span><span class="p">(</span><span class="s2">&quot;2013-12-31&quot;</span><span class="p">)</span>

<span class="n">friday</span> <span class="o">=</span> <span class="p">(</span><span class="n">from</span><span class="o">.</span><span class="n">.to</span><span class="p">)</span><span class="o">.</span><span class="n">inject</span><span class="p">(</span><span class="mi">0</span><span class="p">)</span> <span class="k">do</span> <span class="o">|</span><span class="n">friday</span><span class="p">,</span> <span class="n">date</span><span class="o">|</span>
    <span class="k">if</span> <span class="n">date</span><span class="o">.</span><span class="n">mday</span> <span class="o">==</span> <span class="mi">13</span> <span class="ow">and</span> <span class="n">date</span><span class="o">.</span><span class="n">wday</span> <span class="o">==</span> <span class="mi">5</span> <span class="k">then</span>
        <span class="nb">puts</span> <span class="n">date</span><span class="o">.</span><span class="n">strftime</span><span class="p">(</span><span class="s1">&#39;%Y-%m-%d&#39;</span><span class="p">)</span>
        <span class="n">friday</span> <span class="o">+</span> <span class="mi">1</span>
    <span class="k">else</span>
        <span class="n">friday</span>
    <span class="k">end</span>
<span class="k">end</span>

<span class="nb">puts</span> <span class="n">friday</span>
</pre></div>
</td></tr></table>
