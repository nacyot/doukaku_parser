<span class="kr">import</span> <span class="n">Time</span>
<span class="kr">import</span> <span class="n">Control</span><span class="o">.</span><span class="n">Monad</span>
<span class="kr">import</span> <span class="n">System</span><span class="o">.</span><span class="n">IO</span><span class="o">.</span><span class="n">Unsafe</span>

<span class="nv">crossProduct</span> <span class="o">::</span> <span class="p">[</span><span class="nv">a</span><span class="p">]</span> <span class="o">-&gt;</span> <span class="p">[</span><span class="nv">b</span><span class="p">]</span><span class="o">-&gt;</span> <span class="p">[(</span><span class="nv">a</span><span class="p">,</span><span class="nv">b</span><span class="p">)]</span>
<span class="nv">crossProduct</span> <span class="o">=</span> <span class="nv">liftM2</span> <span class="nv">p</span>
    <span class="kr">where</span>
        <span class="nv">p</span> <span class="nv">x</span> <span class="nv">y</span> <span class="o">=</span> <span class="p">(</span><span class="nv">x</span><span class="p">,</span><span class="nv">y</span><span class="p">)</span>

<span class="nv">isJason</span> <span class="o">::</span> <span class="p">(</span><span class="n">Int</span><span class="p">,</span><span class="n">Int</span><span class="p">)</span> <span class="o">-&gt;</span> <span class="n">Bool</span>
<span class="nv">isJason</span> <span class="p">(</span><span class="nv">year</span><span class="p">,</span><span class="nv">month</span><span class="p">)</span> <span class="o">=</span> <span class="nv">week</span> <span class="o">==</span> <span class="n">Friday</span>
    <span class="kr">where</span>
        <span class="nv">mon</span> <span class="o">=</span> <span class="nv">toEnum</span> <span class="p">(</span><span class="nv">month</span><span class="o">-</span><span class="mi">1</span><span class="p">)</span> <span class="o">::</span> <span class="n">Month</span>
        <span class="nv">clock</span> <span class="o">=</span> <span class="nv">toClockTime</span> <span class="o">$</span> <span class="n">CalendarTime</span> <span class="nv">year</span> <span class="nv">mon</span> <span class="mi">13</span> <span class="mi">0</span> <span class="mi">0</span> <span class="mi">0</span> <span class="mi">0</span> <span class="n">Sunday</span> <span class="mi">0</span> <span class="s">&quot;JST&quot;</span> <span class="mi">9</span> <span class="n">False</span>
