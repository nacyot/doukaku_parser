<span class="kr">import</span> <span class="n">Data</span><span class="o">.</span><span class="n">Time</span><span class="o">.</span><span class="n">Calendar</span><span class="o">.</span><span class="n">OrdinalDate</span>

<span class="nv">isFriday</span> <span class="nv">d</span> <span class="o">=</span>
  <span class="kr">case</span> <span class="nv">sundayStartWeek</span> <span class="nv">d</span> <span class="kr">of</span>
    <span class="p">(</span><span class="kr">_</span><span class="p">,</span> <span class="mi">5</span><span class="p">)</span>    <span class="o">-&gt;</span> <span class="n">True</span>
    <span class="nv">otherwise</span> <span class="o">-&gt;</span> <span class="n">False</span>

<span class="nv">main</span> <span class="o">=</span> <span class="kr">do</span>
  <span class="nv">today</span> <span class="o">&lt;-</span> <span class="nv">getZonedTime</span> <span class="o">&gt;&gt;=</span> <span class="o">\</span> <span class="nv">zt</span> <span class="o">-&gt;</span> <span class="p">(</span><span class="nv">return</span><span class="o">.</span><span class="nv">localDay</span><span class="o">.</span><span class="nv">zonedTimeToLocalTime</span><span class="p">)</span> <span class="nv">zt</span>
  <span class="kr">let</span> <span class="nv">fridays</span> <span class="o">=</span> <span class="nv">filter</span> <span class="nv">isFriday</span> <span class="o">$</span> <span class="nv">filter</span> <span class="p">(</span><span class="nv">today</span> <span class="o">&lt;=</span><span class="p">)</span> <span class="p">[</span><span class="nv">fromGregorian</span> <span class="nv">y</span> <span class="nv">m</span> <span class="mi">13</span> <span class="o">|</span> <span class="nv">y</span> <span class="o">&lt;-</span> <span class="p">[</span><span class="mi">2008</span><span class="o">..</span><span class="mi">2013</span><span class="p">],</span> <span class="nv">m</span> <span class="o">&lt;-</span><span class="p">[</span><span class="mi">1</span><span class="o">..</span><span class="mi">12</span><span class="p">]]</span>
