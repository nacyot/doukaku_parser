<span class="nv">isAfter</span> <span class="o">::</span> <span class="n">CalendarTime</span> <span class="o">-&gt;</span> <span class="p">(</span><span class="n">Int</span><span class="p">,</span><span class="n">Int</span><span class="p">)</span> <span class="o">-&gt;</span> <span class="n">Bool</span>
