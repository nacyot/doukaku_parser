        <span class="nv">err</span> <span class="nv">i</span> <span class="o">=</span> <span class="p">(</span><span class="nv">i</span> <span class="o">==</span> <span class="nv">lZERROR_BADINHANDLE</span><span class="p">)</span> <span class="o">||</span>
