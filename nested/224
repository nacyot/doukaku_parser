<span class="nv">oF_READ</span> <span class="o">=</span> <span class="mi">0</span> <span class="o">::</span> <span class="n">WORD</span>
<span class="nv">oF_CREATE</span> <span class="o">=</span> <span class="mh">0x1000</span> <span class="o">::</span> <span class="n">WORD</span>

