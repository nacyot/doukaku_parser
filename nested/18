<span class="nb">require</span> <span class="s1">&#39;open-uri&#39;</span>
<span class="nb">require</span> <span class="s1">&#39;uconv&#39;</span>

<span class="vg">$cp</span> <span class="o">=</span> <span class="no">Win32</span><span class="o">::</span><span class="no">Console</span><span class="o">.</span><span class="n">OutputCP</span><span class="p">()</span>
<span class="c1">#ENV[&#39;HTTP_PROXY&#39;] = &quot;http://proxy.example.com:8080/&quot;</span>

<span class="n">uri</span> <span class="o">=</span> <span class="s2">&quot;http://ja.doukaku.org/feeds/comments/&quot;</span>

<span class="k">begin</span>
  <span class="n">str</span> <span class="o">=</span> <span class="nb">open</span><span class="p">(</span><span class="n">uri</span><span class="p">)</span><span class="o">.</span><span class="n">read</span>
<span class="k">rescue</span>
  <span class="vg">$stderr</span><span class="o">.</span><span class="n">puts</span> <span class="vg">$!</span>
<span class="k">else</span>
  <span class="k">if</span> <span class="vg">$cp</span> <span class="o">==</span> <span class="mi">932</span>
    <span class="n">output</span> <span class="o">=</span> <span class="no">Uconv</span><span class="o">.</span><span class="n">u8tosjis</span><span class="p">(</span><span class="n">str</span><span class="p">)</span>
  <span class="k">else</span>
    <span class="n">output</span> <span class="o">=</span> <span class="n">str</span>
  <span class="k">end</span>
  <span class="nb">print</span> <span class="n">output</span>
<span class="k">end</span>
