        <span class="n">WebResponse</span> <span class="n">res</span> <span class="p">=</span> <span class="n">WebRequest</span><span class="p">.</span><span class="n">Create</span><span class="p">(</span><span class="s">&quot;http://ja.doukaku.org&quot;</span><span class="p">).</span><span class="n">GetResponse</span><span class="p">();</span>
