	<a href="/web/20110831022558/http://ja.doukaku.org/comment/1352/">#1352</a>(<script language="JavaScript">print_time(new Date("2007/07/22 21:05 GMT"));</script><noscript>2007/07/22 21:05 GMT</noscript>)
	
	
	
	
	
