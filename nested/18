<span class="nf">curl_setopt</span><span class="p">(</span><span class="nv">$ch</span><span class="p">,</span> <span class="nx">CURLOPT_URL</span><span class="p">,</span> <span class="s2">&quot;http://ja.doukaku.org/feeds/comments/&quot;</span><span class="p">);</span>
<span class="nf">curl_setopt</span><span class="p">(</span><span class="nv">$ch</span><span class="p">,</span> <span class="nx">CURLOPT_HEADER</span><span class="p">,</span> <span class="m">0</span><span class="p">);</span>
<span class="nf">curl_exec</span><span class="p">(</span><span class="nv">$ch</span><span class="p">);</span>
<span class="nf">curl_close</span><span class="p">(</span><span class="nv">$ch</span><span class="p">);</span>
