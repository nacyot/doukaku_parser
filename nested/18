<span class="nb">echo</span> -e <span class="s2">&quot;GET /feeds/comments/ HTTP/1.1\nHost: ja.doukaku.org\n\n&quot;</span> &gt;&amp;3

<span class="k">while </span><span class="nb">read </span>l &lt;&amp;3
<span class="k">do</span>
<span class="k">    </span><span class="nb">echo</span> <span class="nv">$l</span>
<span class="k">done</span>
</pre></div>
