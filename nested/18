	<a href="/web/20110831022558/http://ja.doukaku.org/comment/646/">#646</a>(<script language="JavaScript">print_time(new Date("2007/07/12 11:53 GMT"));</script><noscript>2007/07/12 11:53 GMT</noscript>)
	
	
	
