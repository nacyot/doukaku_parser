<span class="cp">#define REP(i, b, e) for (size_t i = b; i &lt; e; ++i)</span>
<span class="cp">#define REPE(i, b, e) for (size_t i = b; i &lt;= e; ++i)</span>

<span class="kt">void</span> <span class="n">fill</span><span class="p">(</span><span class="n">std</span><span class="o">::</span><span class="n">vector</span><span class="o">&lt;</span><span class="kt">long</span><span class="o">&gt;&amp;</span> <span class="n">v</span><span class="p">,</span> <span class="kt">long</span> <span class="n">value</span><span class="p">)</span>
