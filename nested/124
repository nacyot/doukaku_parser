20</pre></td><td class="code"><div class="highlight"><pre><span class="k">public</span> <span class="k">class</span> <span class="nc">LeapYear</span> <span class="o">{</span>
    <span class="k">public</span> <span class="k">static</span> <span class="kt">void</span> <span class="n">main</span><span class="o">(</span><span class="n">String</span><span class="o">[]</span> <span class="n">args</span><span class="o">)</span> <span class="o">{</span>
        <span class="n">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">&quot;A.D. 1900: &quot;</span> <span class="o">+</span> <span class="n">isLeapYear</span><span class="o">(</span><span class="mi">1900</span><span class="o">));</span>
        <span class="n">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">&quot;A.D. 2000: &quot;</span> <span class="o">+</span> <span class="n">isLeapYear</span><span class="o">(</span><span class="mi">2000</span><span class="o">));</span>
        <span class="n">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">&quot;A.D. 2008: &quot;</span> <span class="o">+</span> <span class="n">isLeapYear</span><span class="o">(</span><span class="mi">2008</span><span class="o">));</span>
        <span class="n">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">&quot;A.D. 2009: &quot;</span> <span class="o">+</span> <span class="n">isLeapYear</span><span class="o">(</span><span class="mi">2009</span><span class="o">));</span>
        <span class="n">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">&quot;A.D. 2100: &quot;</span> <span class="o">+</span> <span class="n">isLeapYear</span><span class="o">(</span><span class="mi">2100</span><span class="o">));</span>
    <span class="o">}</span>

    <span class="k">public</span> <span class="k">static</span> <span class="kt">boolean</span> <span class="n">isLeapYear</span><span class="o">(</span><span class="kt">int</span> <span class="n">aYear</span><span class="o">)</span> <span class="o">{</span>
        <span class="k">return</span> <span class="n">isDivisible</span><span class="o">(</span><span class="n">aYear</span><span class="o">,</span> <span class="mi">4</span><span class="o">)</span> <span class="o">?</span> <span class="o">(</span><span class="n">isDivisible</span><span class="o">(</span><span class="n">aYear</span><span class="o">,</span> <span class="mi">100</span><span class="o">)</span> <span class="o">?</span> <span class="o">(</span><span class="n">isDivisible</span><span class="o">(</span><span class="n">aYear</span><span class="o">,</span> <span class="mi">400</span><span class="o">)</span> <span class="o">?</span> <span class="kc">true</span> <span class="o">:</span> <span class="kc">false</span><span class="o">)</span> <span class="o">:</span> <span class="kc">true</span><span class="o">)</span> <span class="o">:</span> <span class="kc">false</span><span class="o">;</span>
    <span class="o">}</span>

    <span class="k">public</span> <span class="k">static</span> <span class="kt">boolean</span> <span class="n">isDivisible</span><span class="o">(</span><span class="kt">int</span> <span class="n">aLeft</span><span class="o">,</span> <span class="kt">int</span> <span class="n">aRight</span><span class="o">)</span> <span class="o">{</span>
        <span class="k">while</span> <span class="o">(</span><span class="n">aLeft</span> <span class="o">&gt;=</span> <span class="n">aRight</span><span class="o">)</span> <span class="o">{</span>
            <span class="n">aLeft</span> <span class="o">-=</span> <span class="n">aRight</span><span class="o">;</span>
        <span class="o">}</span>
        <span class="k">return</span> <span class="n">aLeft</span> <span class="o">==</span> <span class="mi">0</span><span class="o">;</span>
    <span class="o">}</span>
<span class="o">}</span>
