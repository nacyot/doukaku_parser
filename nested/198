	&nbsp;
	<a href="/web/20081202220346/http://ja.doukaku.org/comment/7090/">#7090</a>(<script language="JavaScript">print_time(new Date("2008/08/10 06:05 GMT"));</script><noscript>2008/08/10 06:05 GMT</noscript>)
	
	
	
