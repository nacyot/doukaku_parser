<span class="k">use</span> <span class="n">warnings</span><span class="p">;</span>

<span class="k">use</span> <span class="n">DateTime</span><span class="p">;</span>
<span class="k">use</span> <span class="nn">DateTime::</span><span class="n">Duration</span><span class="p">;</span>

<span class="k">sub </span><span class="nf">date_later</span> <span class="p">{</span>
    <span class="n">DateTime</span><span class="o">-&gt;</span><span class="n">today</span> <span class="o">+</span> <span class="nn">DateTime::</span><span class="n">Duration</span><span class="o">-&gt;</span><span class="k">new</span><span class="p">(</span><span class="n">days</span> <span class="o">=&gt;</span> <span class="nb">shift</span><span class="p">);</span>
<span class="p">}</span>

