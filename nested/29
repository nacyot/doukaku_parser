<span class="cp">#if GNU_RUNTIME</span>
<span class="cp">#define MetaClass( a ) [a class]-&gt;class_pointer</span>
<span class="k">extern</span> <span class="kt">void</span> <span class="n">class_add_method_list</span> <span class="p">(</span><span class="n">Class</span> <span class="n">class</span><span class="p">,</span> <span class="n">MethodList_t</span> <span class="n">list</span><span class="p">);</span>
<span class="cp">#else</span>
<span class="cp">#import &lt;objc/objc-class.h&gt;</span>
<span class="cp">#import &lt;objc/objc-runtime.h&gt;</span>
<span class="cp">#define MetaClass( a ) [a class]-&gt;isa</span>
<span class="cp">#endif</span>

<span class="n">NSCalendarDate</span><span class="o">*</span> <span class="nf">daysLater</span><span class="p">(</span> <span class="kt">id</span> <span class="n">self</span><span class="p">,</span> <span class="kt">SEL</span> <span class="n">_cmd</span><span class="p">,</span> <span class="n">NSCalendarDate</span><span class="o">*</span> <span class="n">date</span> <span class="p">)</span> <span class="p">{</span>
	<span class="kt">int</span> <span class="n">nDays</span> <span class="o">=</span> <span class="p">[[</span><span class="n">NSString</span> <span class="nl">stringWithUTF8String:</span><span class="n">sel_getName</span><span class="p">(</span> <span class="n">_cmd</span> <span class="p">)]</span> <span class="n">intValue</span><span class="p">];</span>
	<span class="k">return</span> <span class="p">[</span><span class="n">date</span> <span class="nl">dateByAddingYears:</span><span class="mi">0</span> <span class="nl">months:</span><span class="mi">0</span> <span class="nl">days:</span><span class="n">nDays</span> <span class="nl">hours:</span><span class="mi">0</span> <span class="nl">minutes:</span><span class="mi">0</span> <span class="nl">seconds:</span><span class="mi">0</span><span class="p">];</span>
<span class="p">}</span>

<span class="kt">SEL</span> <span class="nf">createMethod</span><span class="p">(</span> <span class="kt">int</span> <span class="n">n</span> <span class="p">)</span> <span class="p">{</span>
	<span class="k">const</span> <span class="kt">char</span><span class="o">*</span> <span class="n">days</span> <span class="o">=</span> <span class="p">[[[</span><span class="n">NSNumber</span> <span class="nl">numberWithInt:</span><span class="n">n</span><span class="p">]</span> <span class="n">stringValue</span><span class="p">]</span> <span class="n">UTF8String</span><span class="p">];</span>
	<span class="kt">SEL</span> <span class="n">selector</span> <span class="o">=</span> <span class="n">sel_registerName</span><span class="p">(</span> <span class="n">days</span> <span class="p">);</span>				<span class="c">//数値nの文字列表現をセレクタとして登録する</span>
	<span class="k">struct</span> <span class="n">objc_method_list</span><span class="o">*</span> <span class="n">methodList</span> <span class="o">=</span>					<span class="c">//割り当てたメモリはプログラム終了まで解放しない</span>
			<span class="n">calloc</span><span class="p">(</span> <span class="k">sizeof</span> <span class="p">(</span><span class="k">struct</span> <span class="n">objc_method_list</span><span class="p">),</span> <span class="mi">1</span> <span class="p">);</span>
	<span class="n">methodList</span><span class="o">-&gt;</span><span class="n">method_count</span> <span class="o">=</span> <span class="mi">1</span><span class="p">;</span>							<span class="c">//登録するメソッド数は１</span>
	<span class="n">methodList</span><span class="o">-&gt;</span><span class="n">method_list</span><span class="p">[</span><span class="mi">0</span><span class="p">].</span><span class="n">method_name</span> <span class="o">=</span>				<span class="c">//メソッド名はセレクタ（数値nの文字列表現）</span>
<span class="cp">#if GNU_RUNTIME</span>
			<span class="p">(</span><span class="kt">SEL</span><span class="p">)</span><span class="n">sel_get_name</span><span class="p">(</span> <span class="n">selector</span> <span class="p">);</span>
<span class="cp">#else</span>
			<span class="n">selector</span><span class="p">;</span>
<span class="cp">#endif</span>
	<span class="n">methodList</span><span class="o">-&gt;</span><span class="n">method_list</span><span class="p">[</span><span class="mi">0</span><span class="p">].</span><span class="n">method_types</span> <span class="o">=</span> <span class="s">&quot;@0@0:0@0&quot;</span><span class="p">;</span>	<span class="c">//オブジェクトの引数を１つ取り、返り値はオブジェクト</span>
	<span class="n">methodList</span><span class="o">-&gt;</span><span class="n">method_list</span><span class="p">[</span><span class="mi">0</span><span class="p">].</span><span class="n">method_imp</span> <span class="o">=</span> <span class="p">(</span><span class="n">IMP</span><span class="p">)</span><span class="n">daysLater</span><span class="p">;</span>
	<span class="n">class_addMethods</span><span class="p">(</span> <span class="n">MetaClass</span><span class="p">(</span> <span class="n">NSObject</span> <span class="p">),</span> <span class="n">methodList</span> <span class="p">);</span>	<span class="c">//NSObjectのクラスメソッドとして登録</span>
	<span class="k">return</span> <span class="n">selector</span><span class="p">;</span>
<span class="p">}</span>

<span class="kt">int</span> <span class="nf">main</span> <span class="p">(</span><span class="kt">int</span> <span class="n">argc</span><span class="p">,</span> <span class="k">const</span> <span class="kt">char</span> <span class="o">*</span> <span class="n">argv</span><span class="p">[])</span> <span class="p">{</span>
    <span class="n">NSAutoreleasePool</span> <span class="o">*</span> <span class="n">pool</span> <span class="o">=</span> <span class="p">[[</span><span class="n">NSAutoreleasePool</span> <span class="n">alloc</span><span class="p">]</span> <span class="n">init</span><span class="p">];</span>
	
	<span class="kt">SEL</span> <span class="n">selector</span> <span class="o">=</span> <span class="n">createMethod</span><span class="p">(</span> <span class="mi">5</span> <span class="p">);</span>						<span class="c">//５日後の日付を返す関数を作成</span>
	<span class="n">NSCalendarDate</span><span class="o">*</span> <span class="n">now</span> <span class="o">=</span> <span class="p">[</span><span class="n">NSCalendarDate</span> <span class="n">calendarDate</span><span class="p">];</span>	<span class="c">//現在の日付を作成</span>
	<span class="n">NSLog</span><span class="p">(</span> <span class="p">[</span><span class="n">now</span> <span class="n">description</span><span class="p">]</span> <span class="p">);</span>								<span class="c">//現在の日付を表示</span>
	<span class="n">NSCalendarDate</span><span class="o">*</span> <span class="n">fiveDaysLater</span> <span class="o">=</span>							<span class="c">//作成した関数を呼び出して５日後の日付を取得</span>
		<span class="p">[</span><span class="n">NSObject</span> <span class="nl">performSelector:</span><span class="n">selector</span> <span class="nl">withObject:</span><span class="n">now</span><span class="p">];</span>
	<span class="n">NSLog</span><span class="p">(</span> <span class="p">[</span><span class="n">fiveDaysLater</span> <span class="n">description</span><span class="p">]</span> <span class="p">);</span>					<span class="c">//５日後の日付を表示</span>
	
    <span class="p">[</span><span class="n">pool</span> <span class="n">release</span><span class="p">];</span>
    <span class="k">return</span> <span class="mi">0</span><span class="p">;</span>
<span class="p">}</span>
</pre></div>
