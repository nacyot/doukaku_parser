let
  open Date
in
  date { year = year d,
         month = month d,
         day = day d + n,
         hour = hour d,
         minute = minute d,
         second = second d,
         offset = offset d
       }
end;

val five_days_later = n_days_later 5;

print (Date.toString (five_days_later (Date.fromTimeLocal (Time.now()))))
</pre></div>
</td></tr></table>
