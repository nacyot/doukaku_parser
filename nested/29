fiveDaysLater := [:n | [:date | date + n days]] value: 5.
now := DateAndTime now.
^now -&gt; (fiveDaysLater value: now)

&quot;=&gt; 2007-07-21T23:47:36+09:00-&gt;2007-07-26T23:47:36+09:00 &quot;
