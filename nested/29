<span class="k">def</span> <span class="nf">n_days_later</span><span class="p">(</span><span class="n">n</span><span class="p">):</span>
 <span class="k">return</span> <span class="k">lambda</span> <span class="n">d</span><span class="p">:</span> <span class="n">d</span> <span class="o">+</span> <span class="n">timedelta</span><span class="p">(</span><span class="n">days</span><span class="o">=</span><span class="n">n</span><span class="p">)</span>

