<span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">n-days-later</span> <span class="nv">n</span><span class="p">)</span>
  <span class="p">(</span><span class="k">lambda </span><span class="p">(</span><span class="nf">date</span><span class="p">)</span> <span class="p">(</span><span class="nf">julian-day-&gt;date</span> <span class="p">(</span><span class="nb">+ </span><span class="p">(</span><span class="nf">date-&gt;julian-day</span> <span class="nv">date</span><span class="p">)</span> <span class="nv">n</span><span class="p">))))</span>

