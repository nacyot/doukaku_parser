26</pre></td><td class="code"><div class="highlight"><pre><span class="k">use</span> <span class="n">strict</span><span class="p">;</span>
<span class="k">use</span> <span class="n">warnings</span><span class="p">;</span>

<span class="k">my</span> <span class="p">(</span><span class="nv">$n</span><span class="p">,</span> <span class="nv">$k</span><span class="p">,</span> <span class="nv">$m</span><span class="p">)</span> <span class="o">=</span> <span class="nv">@ARGV</span><span class="p">;</span>
<span class="k">my</span> <span class="nv">@cards</span> <span class="o">=</span> <span class="p">(</span><span class="mi">1</span><span class="o">..</span><span class="nv">$n</span><span class="p">);</span>
<span class="k">my</span> <span class="nv">$target</span> <span class="o">=</span> <span class="nv">$m</span> <span class="o">-</span> <span class="mi">1</span><span class="p">;</span>    <span class="c1">#カードの数字は1から始まる</span>

<span class="k">sub </span><span class="nf">choice</span> <span class="p">{</span>
    <span class="k">my</span> <span class="p">(</span><span class="nv">$start</span><span class="p">,</span> <span class="nv">$step</span><span class="p">)</span> <span class="o">=</span> <span class="nv">@_</span><span class="p">;</span>
    <span class="nv">$step</span> <span class="nv">%</span><span class="err">=</span> <span class="err">@</span><span class="nv">cards</span><span class="p">;</span>
    <span class="nv">$start</span> <span class="o">+=</span> <span class="nv">$step</span><span class="p">;</span>
    <span class="nv">$start</span> <span class="nv">%</span><span class="err">=</span> <span class="err">@</span><span class="nv">cards</span><span class="p">;</span>
    <span class="nv">$start</span><span class="p">;</span>
<span class="p">}</span>

<span class="c1">#一枚になるまで繰り返す</span>
<span class="k">while</span> <span class="p">(</span><span class="nv">@cards</span> <span class="o">!=</span> <span class="mi">1</span><span class="p">)</span> <span class="p">{</span>
    <span class="c1">#カードを取り除く</span>
    <span class="k">my</span> <span class="nv">$deleted</span> <span class="o">=</span> <span class="nb">delete</span> <span class="nv">$cards</span><span class="p">[</span><span class="nv">$target</span><span class="p">];</span>
    <span class="nv">@cards</span> <span class="o">=</span> <span class="nb">grep</span> <span class="p">{</span> <span class="nb">defined</span><span class="p">(</span><span class="nv">$_</span><span class="p">)</span> <span class="p">}</span> <span class="nv">@cards</span><span class="p">;</span>
    <span class="c1">#print $deleted, &quot; is deleted.\n&quot;;    </span>
    <span class="c1">#取り除くカードを選ぶ</span>
    <span class="nv">$target</span> <span class="o">=</span> <span class="n">choice</span><span class="p">(</span><span class="nv">$target</span><span class="o">-</span><span class="mi">1</span><span class="p">,</span> <span class="nv">$k</span><span class="p">);</span>
<span class="p">}</span>

<span class="k">print</span> <span class="nv">@cards</span><span class="p">,</span> <span class="s">&quot; remained.\n&quot;</span><span class="p">;</span>
</pre></div>
</td></tr></table>
    </div>
	

  
