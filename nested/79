        <span class="n">xs</span><span class="o">.</span><span class="n">insert</span><span class="p">(</span><span class="n">r</span> <span class="o">+</span> <span class="mi">1</span><span class="p">,</span> <span class="n">i</span><span class="p">)</span>
        <span class="n">r</span> <span class="o">+=</span> <span class="mi">1</span>

<span class="c">#    print xs</span>
<span class="c">#    print xs.index(0),</span>
<span class="c">#    print xs.index(n - 1)</span>
