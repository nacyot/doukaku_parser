19</pre></td><td class="code"><div class="highlight"><pre>import swing._
object CmdParam extends SimpleGUIApplication {
  var args = Array[String]()
  override def main(args:Array[String]):Unit = {
    this.args = args
    super.main(args)
  }
  
  def top=new MainFrame{
    title = &quot;CmdParam&quot;
    object label extends Label{
      text= args.toList.toString
    }
    contents = new FlowPanel{
      for {arg&lt;-args
           label=new Label{ text=arg } } contents += label
    }
  }
}
</pre></div>
</td></tr></table>
