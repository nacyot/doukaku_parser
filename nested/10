<span class="k">class</span> <span class="nc">Program</span>
<span class="k">{</span>
  <span class="k">static</span> <span class="k">void</span> <span class="nf">Main</span><span class="p">()</span>
  <span class="k">{</span>
    <span class="kt">string</span> <span class="n">s</span><span class="p">;</span>
    <span class="k">while</span> <span class="p">((</span><span class="n">s</span> <span class="p">=</span> <span class="n">Console</span><span class="p">.</span><span class="n">ReadLine</span><span class="p">())</span> <span class="p">!=</span> <span class="k">null</span><span class="p">)</span>
      <span class="k">if</span> <span class="p">(!</span><span class="n">s</span><span class="p">.</span><span class="n">StartsWith</span><span class="p">(</span><span class="s">&quot;#&quot;</span><span class="p">))</span> <span class="n">Console</span><span class="p">.</span><span class="n">WriteLine</span><span class="p">(</span><span class="n">s</span><span class="p">);</span>
