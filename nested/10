<span class="k">function</span> <span class="nf">removeComment</span><span class="p">(</span><span class="nv">$infile</span><span class="p">,</span> <span class="nv">$outfile</span><span class="p">,</span> <span class="nv">$startwith</span> <span class="o">=</span><span class="s1">&#39;#&#39;</span><span class="p">)</span> <span class="p">{</span>
