      <span class="n">c0</span><span class="o">.</span><span class="n">zip</span><span class="p">(</span><span class="n">c1</span><span class="p">)</span><span class="o">.</span><span class="n">each</span><span class="p">{</span><span class="o">|</span><span class="n">e0</span><span class="p">,</span><span class="n">e1</span><span class="o">|</span> <span class="k">break</span> <span class="n">e0</span> <span class="o">&lt;=&gt;</span> <span class="n">e1</span> <span class="k">unless</span> <span class="n">e0</span> <span class="o">==</span> <span class="n">e1</span><span class="p">}}</span>
