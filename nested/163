<span class="p">);</span>

<span class="nx">data</span><span class="p">.</span><span class="nx">sort</span><span class="p">(</span><span class="k">function</span><span class="p">(</span><span class="nx">a</span><span class="o">,</span><span class="nx">b</span><span class="p">){</span> <span class="k">return</span><span class="p">(</span><span class="nx">a</span><span class="p">.</span><span class="nx">size</span> <span class="o">-</span> <span class="nx">b</span><span class="p">.</span><span class="nx">size</span><span class="p">)</span> <span class="p">});</span>
