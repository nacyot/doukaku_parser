#元データ
$list = array(
    array( &quot;x&quot;=&gt;3,&quot;y&quot;=&gt;3)
    ,array( &quot;x&quot;=&gt;2,&quot;y&quot;=&gt;10)
    ,array( &quot;x&quot;=&gt;2,&quot;y&quot;=&gt;1)
    ,array( &quot;x&quot;=&gt;1,&quot;y&quot;=&gt;2)
    ,array( &quot;x&quot;=&gt;1,&quot;y&quot;=&gt;1)
    ,array( &quot;x&quot;=&gt;3,&quot;y&quot;=&gt;1)
    ,array( &quot;x&quot;=&gt;1,&quot;y&quot;=&gt;10)
);

#辞書順
usort(
    $list,
    create_function(
        &#39;$a,$b&#39;,
        &#39;return ($a[&quot;x&quot;] == $b[&quot;x&quot;]) ? ($a[&quot;y&quot;] &gt; $b[&quot;y&quot;]) : ($a[&quot;x&quot;] &gt; $b[&quot;x&quot;] );&#39;
    )
);
var_dump($list);
#距離順
usort(
    $list,
    create_function(
        &#39;$a,$b&#39;,
        &#39;return pow($a[&quot;x&quot;],2)+pow($a[&quot;y&quot;],2) &gt; pow($b[&quot;x&quot;],2)+pow($b[&quot;y&quot;],2);&#39;
    )
);
var_dump($list);
?&gt;
