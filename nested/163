22</pre></td><td class="code"><div class="highlight"><pre><span class="k">import</span> <span class="n">std</span>.<span class="n">stdio</span>;
<span class="k">import</span> <span class="n">std</span>.<span class="n">algorithm</span>;

<span class="kt">void</span> <span class="n">main</span>(){
    <span class="kt">int</span>[][] <span class="n">array</span> = [[<span class="mi">1</span>, <span class="mi">3</span>], [-<span class="mi">1</span>, <span class="mi">0</span>], [-<span class="mi">2</span>, <span class="mi">3</span>], [<span class="mi">1</span>, -<span class="mi">9</span>], [<span class="mi">1</span>, -<span class="mi">19</span>],
                     [<span class="mi">4</span>, <span class="mi">2</span>], [<span class="mi">3</span>, <span class="mi">104</span>], [<span class="mi">4</span>, -<span class="mi">91</span>]];

    <span class="kt">bool</span> <span class="n">lexical</span>(<span class="kt">int</span>[] <span class="n">a</span>, <span class="kt">int</span>[] <span class="n">b</span>){
        <span class="k">return</span> (<span class="n">a</span>[<span class="mi">0</span>] == <span class="n">b</span>[<span class="mi">0</span>]) ? (<span class="n">a</span>[<span class="mi">1</span>] &lt; <span class="n">b</span>[<span class="mi">1</span>]) : (<span class="n">a</span>[<span class="mi">0</span>] &lt; <span class="n">b</span>[<span class="mi">0</span>]);
    }
    <span class="kt">bool</span> <span class="n">distance</span>(<span class="kt">int</span>[] <span class="n">a</span>, <span class="kt">int</span>[] <span class="n">b</span>){
        <span class="k">return</span> (<span class="n">a</span>[<span class="mi">0</span>] * <span class="n">a</span>[<span class="mi">0</span>] + <span class="n">a</span>[<span class="mi">1</span>] * <span class="n">a</span>[<span class="mi">1</span>]) &lt; (<span class="n">b</span>[<span class="mi">0</span>] * <span class="n">b</span>[<span class="mi">0</span>] + <span class="n">b</span>[<span class="mi">1</span>] * <span class="n">b</span>[<span class="mi">1</span>]);
    }

    <span class="k">auto</span> <span class="n">a1</span> = <span class="n">array</span>.<span class="n">dup</span>;
    <span class="k">auto</span> <span class="n">a2</span> = <span class="n">array</span>.<span class="n">dup</span>;
    <span class="n">sort</span>!(<span class="n">lexical</span>)(<span class="n">a1</span>);
    <span class="n">sort</span>!(<span class="n">distance</span>)(<span class="n">a2</span>);

    <span class="n">writeln</span>(<span class="n">a1</span>);  <span class="c">// [[-2 3] [-1 0] [1 -19] [1 -9] [1 3] [3 104] [4 -91] [4 2]]</span>
    <span class="n">writeln</span>(<span class="n">a2</span>);  <span class="c">// [[-1 0] [1 3] [-2 3] [4 2] [1 -9] [1 -19] [4 -91] [3 104]]</span>
}
</pre></div>
