      <table><tr><td class="linenos"><pre>1
2
3
4</pre></td><td class="code"><div class="highlight"><pre><span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">pyramid</span> <span class="nv">n</span><span class="p">)</span>
  <span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">p</span> <span class="nv">s</span> <span class="nv">n</span><span class="p">)</span> <span class="p">(</span><span class="nf">q</span> <span class="p">(</span><span class="nf">regexp-replace*</span> <span class="nv">s</span> <span class="o">#</span><span class="nv">/</span> <span class="err">\</span><span class="nv">*/</span> <span class="s">&quot;***&quot;</span> <span class="o">#</span><span class="nv">/</span> <span class="nv">$/</span> <span class="s">&quot;*&quot;</span><span class="p">)</span> <span class="nv">n</span><span class="p">))</span>
  <span class="p">(</span><span class="k">define </span><span class="p">(</span><span class="nf">q</span> <span class="nv">s</span> <span class="nv">n</span><span class="p">)</span> <span class="p">(</span><span class="nf">when</span> <span class="p">(</span><span class="nb">&gt; </span><span class="nv">n</span> <span class="mi">0</span><span class="p">)</span> <span class="p">(</span><span class="nf">print</span> <span class="nv">s</span><span class="p">)</span> <span class="p">(</span><span class="nf">p</span> <span class="nv">s</span> <span class="p">(</span><span class="nb">- </span><span class="nv">n</span> <span class="mi">1</span><span class="p">))))</span>
